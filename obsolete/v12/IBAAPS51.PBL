. Solutions 6 Export
.--------------------
WRTEXP10  UNPACK   FMTRTDAT,CCENT,CYEAR,CMON,CDAY
          MOVE     "J  ",SOL6JTYP
          MATCH    "PY",FMTRTYPE
          IF       @EQUAL
            MOVE     "P  ",SOL6JTYP
          ENDIF
          MATCH    "CR",FMTRTYPE
          IF       @EQUAL
            MOVE     "R  ",SOL6JTYP
          ENDIF
          MOVE     FMTRORD,SOL6CHQ
          MATCH    "1",FMAMTYPE
          GOTO     WRTEXP15 IF EQUAL
          MATCH    "2",FMAMTYPE
          GOTO     WRTEXP20 IF NOT EQUAL
WRTEXP15  UNPACK   FMAMACCT,SOL6SUB,SOL6ACC
          PACK     SOL6ACCT,SOL6ACC,SOL6SUB,SP70
          GOTO     WRTEXP25
.
WRTEXP20  UNPACK   FMAMACCT,SOL6ACC
          PACK     SOL6ACCT,SOL6ACC,SP70
.
WRTEXP25  CLEAR    SOL6REF
          APPEND   "Batch:",SOL6REF
          APPEND   FMTRBAT,SOL6REF
          APPEND   " Line:",SOL6REF
          APPEND   FMTRUNIQ,SOL6REF
          APPEND   " Ref:",SOL6REF
          APPEND   FMTRDOCR,SOL6REF
          APPEND   " Des:",SOL6REF
          APPEND   FMTRDESC,SOL6REF
          RESET    SOL6REF
          CALL     FAMT0000
          MOVE     FMTRAMT,SOL6AMT
          MOVE     ZERO,SOL6QTY
          WRITE    EXPORTFL,SEQ;CDAY,"/",CMON,"/",CYEAR:
                                SOL6JTYP:
                                SP3:
                                SOL6CHQ:
                                SOL6REF:
                                SOL6ACCT:
