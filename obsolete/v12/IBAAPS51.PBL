. Create Export File
.------------------------------------------------------------
PROC0000  CALL      OPEN0000
          CALL      MAKEXP00
          MOVE      ZERO,TRANCNT
          DISPLAY   *P1:24,*EL,"Processing Account..."
          PACK      KEY14,FMLALEDG,SP70
          CALL      RSFMAM1
          CALL      RPFMAM1
PROC1000  CALL      RKFMAM1              * read next
          BRANCH    OVRCD,PROC9500       * end of report if over
          MATCH     FMAMLEDG,FMLALEDG    * Test if same ledger
          GOTO      PROC9500 IF NOT EQUAL
          MATCH     "6",FMAMTYPE         * Test if posting FMAMACCT type
          GOTO      PROC1000 IF NOT LESS
          ADD       ONE,TRANCNT
          IF        TRANCNT%100=0
            DISPLAY   *P40:24,*V2LON,FMAMLEDG,"/",FMAMACCT
          ENDIF
.
.-----  read through the cash or accrual transaction file ----
.
PROC1600  PACK      KEY32,FMAMLEDG,FMAMACCT,STRDAT,SP20
          CALL      RSFMTR2
PROC1700  CALL      RKFMTR2
          BRANCH    OVRCD,PROC1000
          MATCH     FMAMLEDG,FMTRLEDG    * test for same ledger
          GOTO      PROC1000 IF NOT EQUAL
          MATCH     FMAMACCT,FMTRACCT    * test if same FMAMACCT
          GOTO      PROC1000 IF NOT EQUAL
          MATCH     FMTRPDAT,ENDDAT      * test if in date range
          GOTO      PROC1000 IF LESS
          CALL      WRTEXP00             * write export line
          GOTO      PROC1700
.
PROC9500  CLEAR     DISPMSG
          APPEND    TRANCNT,DISPMSG
          APPEND    " Records Exported to File ",DISPMSG
          APPEND    EXPORTFN,DISPMSG
          APPEND    " - ",DISPMSG
          RESET     DISPMSG
          CALL      MESSAGE1
          CALL      TRAN0000
.
PROC9999  RETURN
