          PACK      KEY5,TCODE,CCIMCOD                                 * V90200
          CALL      RDCODE1                                            * V90200
          IF        OVRCD=0
            PACK      PRTDESC,TDESC,SP70                               * V90200
          ELSE
            MOVE      "Invalid Code",PRTDESC
          ENDIF
          GOTO      REPT1900
.
REPT1600  MOVE      "BT",TCODE                                         * V90200
          PACK      KEY5,TCODE,CCIMCOD                                 * V90200
          CALL      RDCODE1                                            * V90200
          IF        OVRCD=0
            PACK      PRTDESC,TDESC,SP70                               * V90200
          ELSE
            MOVE      "Invalid Code",PRTDESC
