REPT0000  DISPLAY   *P1:24,*EL,"Printing Report : ";
          MOVE      "64",SECTOR
          READ      CONTROLF,SECTOR;*59,CCCOCD1,CCCOCD2,CCCOCD3,CCCOCD4:
                                        CCCOCD5,CCCOIA1,CCCOIA2,CCCOIA3:
                                        CCCOIM1,CCCOIM2,CCCOIM3
          MOVE      "99",CLNO
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
.
          MOVE      SP70,LASTTYP
          PACK      KEY10,SP70
          CALL      RDCCIM1
          IF        OVRCD=0
            CALL      RPCCIM1
          ENDIF
REPT1000  CALL      RKCCIM1
          BRANCH    OVRCD,REPT9000
.
          MATCH     LASTTYP,CCIMTYP
          IF        !@EQUAL
            CALL      HEAD0000
            MOVE      CCIMTYP,LASTTYP
          ENDIF
.
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          MOVE      CCIMTYP,F1
          BRANCH    F1,REPT1100,REPT1200,REPT1300,REPT1400,REPT1500,REPT1600
REPT1100  MOVE      "FI",TCODE                                         * V90200
          PACK      KEY5,TCODE,CCIMCOD                                 * V90200
          CALL      RDCODE1                                            * V90200
          IF        OVRCD=0
            PACK      PRTDESC,TDESC,SP70                               * V90200
          ELSE
            MOVE      "Invalid Code",PRTDESC
          ENDIF
          GOTO      REPT1900
.
REPT1200  MOVE      "0  ",MCLMCOD                                      * V90200
          MOVE      SP70,MHFUND                                        * V90200
          MOVE      SP70,PTMCHFTT                                     *C-233046
          PACK      CURRDATE,CCC,CYY,CMM,CDD
          REP       " 0",CURRDATE
          PACK      KEY29,MCLMCOD,SP6,SP3,CCIMCOD,CURRDATE,SP10       *C-233046
          CALL      PATMCHRD                * read Misc.Charges file
          COMPARE   ZERO,EXIT
          GOTO      REPT1280 IF EQUAL
.
          GOTO      REPT1290
.
REPT1280  PACK      PRTDESC,MDESC,SP70                               * V90200
          GOTO      REPT1900
.
REPT1290  MOVE      "Invalid Code",PRTDESC
          GOTO      REPT1900
.
REPT1300  MOVE      "J ",TCODE                                         * V90200
          PACK      KEY5,TCODE,CCIMCOD                                 * V90200
          CALL      RDCODE1                                            * V90200
          IF        OVRCD=0
            PACK      PRTDESC,TDESC,SP70                               * V90200
          ELSE
            MOVE      "Invalid Code",PRTDESC
          ENDIF
          GOTO      REPT1900
.
REPT1400  PACK      KEY6,CCIMCOD
          CALL      RDWARD1                                            * V90200
          IF        OVRCD=0
            PACK      PRTDESC,WBDESC,SP70                              * V90200
          ELSE
            MOVE      "Invalid Code",PRTDESC
          ENDIF
          GOTO      REPT1900
.
