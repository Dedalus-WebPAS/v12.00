.-------------------------------------------------------------
. Save Record
.-------------------------------------------------------------
POST0000  PACK     KEY4,CCAGDEF,CCAGGRP * Setup Key
          CALL     RACCAG1              * Check for record on file
          IF       OVRCD=0
            CALL     UPCCAG1            * Update Changes
            CALL     UUCCAG1            * Unlock
          ELSE
            CALL     WRCCAG1            * Write New Record
          ENDIF
.
          MOVE     CCAGDEF,F3
          ADD      "35",F3
          MOVE     F3,KEY3
          REP      " 0",KEY3
          PACK     KEY13,KEY3,CCAGGRP,SP70
          UNPACK   KEY13,CCXHSTY,CCXHKEY
          MOVE     CCAGDES,CCXHDES
          CALL     RACCXH1
          IF       OVRCD=0
            CALL     UPCCXH1            * Update Changes
          ELSE
            CALL     WRCCXH1            * Write New Record
          ENDIF
          RETURN                        * Return to program.
.-------------------------------------------------------------
. Cancel Changes
.-------------------------------------------------------------
CANC0000  PACK     KEY4,CCAGDEF,CCAGGRP  * Setup Key for file.
          CALL     RACCAG1               * Check to see if record on file.
          IF       OVRCD=0
            CALL     UUCCAG1             * Unlock.
          ENDIF                          * If not on file then there is no
          RETURN                         * need to do anything.
.-------------------------------------------------------------
. Delete Record
.-------------------------------------------------------------
DELT0000  MOVE     SP70,DISPMSG         * Clear Line 24 Display Message
          CALL     MESSAGE2             * Save and Clear Line 24
          CALL     DELQST               * Delete (Y/N/C) CEXIT 1/2/3
          CALL     MESSAGE3             * Restore Line 24
          BRANCH   CEXIT,DELT1000,DELT9000,DELT9100
.
DELT1000  PACK     KEY4,CCAGDEF,CCAGGRP * Setup Key to File
          CALL     RACCAG1              * Read Record
          IF       OVRCD=0
            CALL     DECCAG1            * Delete Record
          ENDIF
.
          MOVE     CCAGDEF,F3
          ADD      "35",F3
          MOVE     F3,KEY3
          REP      " 0",KEY3
          PACK     KEY13,KEY3,CCAGGRP,SP70
          CALL     RACCXH1
          IF       OVRCD=0
            CALL     DECCXH1            * Delete Cross Reference
          ENDIF
.
          MOVE     TWO,EXIT             * Restart Screen
          GOTO     DELT9999
DELT9000  MOVE     ONE,EXIT             * Return to Prompt on "No"
          GOTO     DELT9999
DELT9100  MOVE     TWO,EXIT             * Restart Screen on "Cancel"
          GOTO     DELT9999
DELT9999  RETURN
