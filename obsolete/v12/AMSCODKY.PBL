.
          MATCH     SP70,AMCDCAT 
          GOTO      KAMCDB90 IF EQUAL
.
          CMATCH    EXITCHAR,AMCDCAT 
          GOTO      KAMCDB95 IF EQUAL
.
          CMATCH    QUEST,AMCDCAT 
          GOTO      KAMCDB10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HAMCDB00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    AMCDCAT 
          APPEND    SP70,AMCDCAT 
          RESET     AMCDCAT 
.
          MATCH     SP70,AMCDCAT 
          GOTO      KAMCDB00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,AMCDCAT                                 
KAMCDB10
          PACK      KEY5,AMCDCOD,AMCDCAT                                        
          CALL      RAAMCD2 
          BRANCH    OVRCD,KAMCDB80
          BRANCH    CKEYTYP,KAMCDB75
          CALL      RDAMCD2 
KAMCDB70
          MOVE      ZERO,EXIT
          GOTO      KAMCDB99
KAMCDB75
          MOVE      "Category Already Exist - ",DISPMSG                                              
          CALL      MESSAGE1
          GOTO      KAMCDB00
KAMCDB80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KAMCDB99,KAMCDB99
          MOVE      "Category Does Not Exist - ",DISPMSG                                             
          CALL      MESSAGE1
          GOTO      KAMCDB00
KAMCDB90
          MOVE      ONE,EXIT
          GOTO      KAMCDB99
KAMCDB95
          MOVE      TWO,EXIT
KAMCDB99 TOPIC    PRGID
          RETURN
