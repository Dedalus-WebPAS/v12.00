.---------------------------------------------------------------------------
.   System    :   Casemix Expense Analysis           
.   Include   :   CEASPCKY
.   Routine   :   HCESPA00 Service Provider    
.   Return    :   CEPSPSC 
.-------------------------------------------------------------------------------
HCESPA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HCESPA04
          CALL      GSCRN000
          BRANCH    EXIT,HCESPA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Code"               
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 5",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HCESPA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CESPSPC 
          PACK      KEY3,CESPSPC,SP70                                           
.                                                                               
.                                                                               
          CALL      RSCESP1 
HCESPA10
          CALL      RKCESP1 
          BRANCH    OVRCD,HCESPA15
          PACK      KEY3,CESPSPC,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HCESPA12
          CALL      HCESPA60
          BRANCH    OVRCD,HCESPA10
HCESPA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CESPSPC 
          DISPLAY   *PSTDCOL02:STDROW,CESPDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCESPA10 IF NOT EQUAL
HCESPA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCESPA20
          CALL      HSCRI000
          BRANCH    EXIT,HCESPA30,HCESPA35,HCESPA40,HCESPA45,HCESPA04:
                              HCESPA91,HCESPA50
          GOTO      HCESPA92
HCESPA30
          CALL      RKCESP1 
          BRANCH    OVRCD,HCESPA85
          PACK      KEY3,CESPSPC,SP70                                           
.                                                                               
.                                                                               
          CALL      RPCESP1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCESPA10
HCESPA35
          MOVE      ST1,KEY3 
          CALL      RDCESP1 
          CALL      RPCESP1 
          BRANCH    OVRCD,HCESPA85
          PACK      KEY3,CESPSPC,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCESPA36
          CALL      RPCESP1 
          BRANCH    OVRCD,HCESPA37
          PACK      KEY3,CESPSPC,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCESPA36 IF NOT EQUAL
HCESPA37
          MOVE      ZERO,XITEMNO
          GOTO      HCESPA10
HCESPA40
          MOVE      ST1,KEY3 
          CALL      RDCESP1 
          CALL      RPCESP1 
          BRANCH    OVRCD,HCESPA85
          PACK      KEY3,CESPSPC,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCESPA08
HCESPA45
          CALL      RKCESP1 
          BRANCH    OVRCD,HCESPA85
          PACK      KEY3,CESPSPC,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY3,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCESP1 
HCESPA46
          CALL      RPCESP1 
          BRANCH    OVRCD,HCESPA47
          PACK      KEY3,CESPSPC,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCESPA46 IF NOT EQUAL
HCESPA47
          MOVE      ZERO,XITEMNO
          GOTO      HCESPA10
HCESPA50  COMPARE   ZERO,CSEARCH
          GOTO      HCESPA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCESPA08
HCESPA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCESPA08
HCESPA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCESPA61 IF EQUAL
          MATCH     SCHFLD01,CESPSPC 
          GOTO      HCESPA72 IF NOT EQUAL
HCESPA61
          MATCH     SP70,SCHFLD02
          GOTO      HCESPA62 IF EQUAL
          MATCH     SCHFLD02,CESPDES 
          GOTO      HCESPA72 IF NOT EQUAL
HCESPA62
HCESPA63
HCESPA64
HCESPA65
HCESPA66
HCESPA67
HCESPA68
HCESPA69
HCESPA70
          GOTO      HCESPA75
HCESPA72  MOVE      ONE,OVRCD
HCESPA75  RETURN
HCESPA85
          LOAD      KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,CESPSPC                                                
          CALL      RDCESP1 
HCESPA90
.                                                                               
          BEEP
          GOTO      HCESPA20
HCESPA91
          MOVE      SP70,CESPDES 
          MOVE      SP70,CESPUR1 
          MOVE      SP70,CESPUR2 
          MOVE      SP70,CESPUD1 
          MOVE      SP70,CESPUD2 
          MOVE      SP70,CESPUY1 
          MOVE      SP70,CESPUY2 
          MOVE      SP70,CESPSPA 
          MOVE      SP70,KEY3 
          MOVE      SP70,CESPSPC 
          GOTO      HCESPA96
HCESPA92
          MOVE      SP70,KEY3 
          MOVE      SP70,CESPSPC 
          LOAD      KEY3,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,CESPSPC                                                
HCESPA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCESPA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Casemix Expense Analysis           
.   Include   :   CEASPCKY
.   Routine   :   KCESPA00 Keyin Service Provider    
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCESPA00 TOPIC     "CESPSPC "
          MOVE      UNDLN70,CESPSPC 
          KEYIN     *PCCOL:CVERT,*DV,CESPSPC:                                   
                    *PCCOL:CVERT,*V2LON,CESPSPC:                                
                    *PCCOL:CVERT,*DV,CESPSPC 
.
          ENDSET    CESPSPC 
          APPEND    SP70,CESPSPC 
          RESET     CESPSPC 
.
          MATCH     SP70,CESPSPC 
          GOTO      KCESPA90 IF EQUAL
.
          CMATCH    EXITCHAR,CESPSPC 
          GOTO      KCESPA95 IF EQUAL
.
          CMATCH    QUEST,CESPSPC 
          GOTO      KCESPA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCESPA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CESPSPC 
          APPEND    SP70,CESPSPC 
          RESET     CESPSPC 
.
          MATCH     SP70,CESPSPC 
          GOTO      KCESPA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CESPSPC                                 
KCESPA10
          PACK      KEY3,CESPSPC                                                
          CALL      RACESP1 
          BRANCH    OVRCD,KCESPA80
          BRANCH    CKEYTYP,KCESPA75
          CALL      RDCESP1 
KCESPA70
          MOVE      ZERO,EXIT
          GOTO      KCESPA99
KCESPA75
          MOVE      "Service Provider Already Exist - ",DISPMSG                                      
          CALL      MESSAGE1
          GOTO      KCESPA00
KCESPA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCESPA99,KCESPA99
          MOVE      "Service Provider Does Not Exist - ",DISPMSG                                     
          CALL      MESSAGE1
          GOTO      KCESPA00
KCESPA90
          MOVE      ONE,EXIT
          GOTO      KCESPA99
KCESPA95
          MOVE      TWO,EXIT
KCESPA99 TOPIC    PRGID
          RETURN
