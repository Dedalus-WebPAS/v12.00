. *----------------------------------------------------------------------
. * Include Name  :   CCSCEBTM.PBL
. *
. * Function      :   Display routine for CCSCEBFD fields.
. *
. * Mods          :   
. *      V9.03.00  20.02.2004  Sylvek Litewka  CAR 46508
. *                Created include.
. *----------------------------------------------------------------------
CCEB0000  BRANCH    FLDITMNO,CCEB0100,CCEB0200,CCEB0300,CCEB0400,CCEB0500:
                             CCEB0600,CCEB0700,CCEB0800,CCEB0900,CCEB1000:
                             CCEB1100,CCEB1200,CCEB1300,CCEB1400,CCEB1500:
                             CCEB1600,CCEB1700,CCEB1800,CCEB1900,CCEB2000:
                             CCEB2100,CCEB2200,CCEB2300
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      CCEB9999
.
CCEB0100  WRITE     HTMLFILE;CCEBLV1;       * Level 1 Extract Code
          GOTO      CCEB9999
.
CCEB0200  WRITE     HTMLFILE;CCEBNEP;       * Total Number of Episodes
          GOTO      CCEB9999
.
CCEB0300  WRITE     HTMLFILE;CCEBFTC;       * Comp. From Total Cost
          GOTO      CCEB9999
.
CCEB0400  WRITE     HTMLFILE;CCEBFMN;       * Comp. From Minimum Cost
          GOTO      CCEB9999
.
CCEB0500  WRITE     HTMLFILE;CCEBFMX;       * Comp. From Maximum Cost
          GOTO      CCEB9999
.
CCEB0600  WRITE     HTMLFILE;CCEBFSV;       * Comp. From Statistical Variance
          GOTO      CCEB9999
.
CCEB0700  WRITE     HTMLFILE;CCEBTTC;       * Comp. To Total Cost
          GOTO      CCEB9999
.
CCEB0800  WRITE     HTMLFILE;CCEBTMN;       * Comp. To Minimum Cost
          GOTO      CCEB9999
.
CCEB0900  WRITE     HTMLFILE;CCEBTMX;       * Comp. To Maximum Cost
          GOTO      CCEB9999
.
CCEB1000  WRITE     HTMLFILE;CCEBTSV;       * Comp. To Statistical Variance
          GOTO      CCEB9999
.
CCEB1100  WRITE     HTMLFILE;CCEBLTO;       * Total Length of Stay (L.O.S)
          GOTO      CCEB9999
.
CCEB1200  WRITE     HTMLFILE;CCEBLMN;       * Minimum L.O.S
          GOTO      CCEB9999
.
CCEB1300  WRITE     HTMLFILE;CCEBLMX;       * Maximum L.O.S
          GOTO      CCEB9999
.
CCEB1400  WRITE     HTMLFILE;CCEBLSV;       * L.O.S Statistical Variance
          GOTO      CCEB9999
.
CCEB1500  ASSIGN    CCEBFTC/CCEBNEP,RESULT  * From Cost Mean (Income/Model)
          WRITE     HTMLFILE;RESULT;       
          GOTO      CCEB9999
.
CCEB1600  ASSIGN    CCEBFMX-CCEBFMN,RESULT  * From Cost Range (Income/Model)
          WRITE     HTMLFILE;RESULT;       
          GOTO      CCEB9999
.
CCEB1700  MOVE      CCEBFSV,CCVARIAN        * From Cost Standard Deviation
          CALL      CALSD000
          WRITE     HTMLFILE;CCSTDDEV;
          GOTO      CCEB9999
.
CCEB1800  ASSIGN    CCEBTTC/CCEBNEP,RESULT  * To Cost Mean (Cost)
          WRITE     HTMLFILE;RESULT;
          GOTO      CCEB9999
.
CCEB1900  ASSIGN    CCEBTMX-CCEBTMN,RESULT  * To Cost Range (Cost)
          WRITE     HTMLFILE;RESULT;
          GOTO      CCEB9999
.
CCEB2000  MOVE      CCEBTSV,CCVARIAN        * To Cost Standard Deviation
          CALL      CALSD000
          WRITE     HTMLFILE;CCSTDDEV;
          GOTO      CCEB9999
.
CCEB2100  ASSIGN    CCEBLTO/CCEBNEP,RESULT  * Length of Stay Mean
          WRITE     HTMLFILE;RESULT;
          GOTO      CCEB9999
.
CCEB2200  ASSIGN    CCEBLMX-CCEBLMN,RESULT  * Length of Stay Range 
          WRITE     HTMLFILE;RESULT;
          GOTO      CCEB9999
.
CCEB2300  MOVE      CCEBLSV,CCVARIAN        * Length of Stay Standard Deviation
          CALL      CALSD000
          WRITE     HTMLFILE;CCSTDDEV;
          GOTO      CCEB9999
.
CCEB9999  RETURN
