.---------------------------------------------------------------------------
.   System    :   Financial Management System
.   Include   :   FMSPRCKY
.   Routine   :   HFMPRA00 Print Code
.   Return    :
.-------------------------------------------------------------------------------
KPRCFM00
          MOVE      ZERO,CKEYTYP
          CALL      KFMPRA00
          RETURN
.
APRCFM00
          MOVE      ONE,CKEYTYP
          CALL      KFMPRA00
          RETURN
.
.-------------------------------------------------------------------------------
.
HFMPRA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "30",HLEF
            MOVE      "80",HRIG
          ENDIF
HFMPRA04
          CALL      GSCRN000
          BRANCH    EXIT,HFMPRA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Print Code"
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 5",STDCOL02
HFMPRA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,FMPRCODE
          PACK      KEY3,FMPRCODE,SP70
.
.
          CALL      RSFMPR1
HFMPRA10
          CALL      RKFMPR1
          BRANCH    OVRCD,HFMPRA15
          PACK      KEY3,FMPRCODE,SP70
.
.
          BRANCH    CSEARCH,HFMPRA12
          CALL      HFMPRA60
          BRANCH    OVRCD,HFMPRA10
HFMPRA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,FMPRCODE
          DISPLAY   *PSTDCOL02:STDROW,FMPRDESC
          COMPARE   MAXITEM,XITEMNO
          GOTO      HFMPRA10 IF NOT EQUAL
HFMPRA15
.
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HFMPRA20
          CALL      HSCRI000
          BRANCH    EXIT,HFMPRA30,HFMPRA35,HFMPRA40,HFMPRA45,HFMPRA04:
                              HFMPRA91,HFMPRA50
          GOTO      HFMPRA92
HFMPRA30
          CALL      RKFMPR1
          BRANCH    OVRCD,HFMPRA85
          PACK      KEY3,FMPRCODE,SP70
.
.
          CALL      RPFMPR1
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMPRA10
HFMPRA35
          MOVE      ST1,KEY3
          CALL      RDFMPR1
          CALL      RPFMPR1
          BRANCH    OVRCD,HFMPRA85
          PACK      KEY3,FMPRCODE,SP70
.
.
.
.
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HFMPRA36
          CALL      RPFMPR1
          BRANCH    OVRCD,HFMPRA37
          PACK      KEY3,FMPRCODE,SP70
.
.
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HFMPRA36 IF NOT EQUAL
HFMPRA37
          MOVE      ZERO,XITEMNO
          GOTO      HFMPRA10
HFMPRA40
          MOVE      ST1,KEY3
          CALL      RDFMPR1
          CALL      RPFMPR1
          BRANCH    OVRCD,HFMPRA85
          PACK      KEY3,FMPRCODE,SP70
.
.
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMPRA08
HFMPRA45
          CALL      RKFMPR1
          BRANCH    OVRCD,HFMPRA85
          PACK      KEY3,FMPRCODE,SP70
.
.
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY3,Z70
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSFMPR1
HFMPRA46
          CALL      RPFMPR1
          BRANCH    OVRCD,HFMPRA47
          PACK      KEY3,FMPRCODE,SP70
.
.
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HFMPRA46 IF NOT EQUAL
HFMPRA47
          MOVE      ZERO,XITEMNO
          GOTO      HFMPRA10
HFMPRA50  COMPARE   ZERO,CSEARCH
          GOTO      HFMPRA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          KEYIN     *PSTDCOL03:STDROW,*V2LON,SCHFLD03
          RESET     SCHFLD03
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMPRA08
HFMPRA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMPRA08
HFMPRA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HFMPRA61 IF EQUAL
          MATCH     SCHFLD01,FMPRCODE
          GOTO      HFMPRA72 IF NOT EQUAL
HFMPRA61
          MATCH     SP70,SCHFLD02
          GOTO      HFMPRA62 IF EQUAL
          MATCH     SCHFLD02,FMPRDESC
          GOTO      HFMPRA72 IF NOT EQUAL
HFMPRA62
          MATCH     SP70,SCHFLD03
          GOTO      HFMPRA63 IF EQUAL
HFMPRA63
HFMPRA64
HFMPRA65
HFMPRA66
HFMPRA67
HFMPRA68
HFMPRA69
HFMPRA70
          GOTO      HFMPRA75
HFMPRA72  MOVE      ONE,OVRCD
HFMPRA75  RETURN
HFMPRA85
          LOAD      KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,FMPRCODE
          CALL      RDFMPR1
HFMPRA90
.
          BEEP
          GOTO      HFMPRA20
HFMPRA91
          MOVE      SP70,FMPRDESC
          MOVE      SP70,FMPRSPAR
          MOVE      SP70,KEY3
          MOVE      SP70,FMPRCODE
          GOTO      HFMPRA96
HFMPRA92
          MOVE      SP70,KEY3
          MOVE      SP70,FMPRCODE
          LOAD      KEY3,CCITEM,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,FMPRCODE
HFMPRA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HFMPRA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Financial Management System
.   Include   :   FMSPRCKY
.   Routine   :   KFMPRA00 Keyin Print Code
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KFMPRA00
          MOVE      UNDLN70,FMPRCODE
          KEYIN     *PCCOL:CVERT,*DV,FMPRCODE:
                    *PCCOL:CVERT,*V2LON,FMPRCODE:
                    *PCCOL:CVERT,*DV,FMPRCODE
.
          ENDSET    FMPRCODE
          APPEND    SP70,FMPRCODE
          RESET     FMPRCODE
.
          MATCH     SP70,FMPRCODE
          GOTO      KFMPRA90 IF EQUAL
.
          CMATCH    EXITCHAR,FMPRCODE
          GOTO      KFMPRA95 IF EQUAL
.
          CMATCH    QUEST,FMPRCODE
          GOTO      KFMPRA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HFMPRA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    FMPRCODE
          APPEND    SP70,FMPRCODE
          RESET     FMPRCODE
.
          MATCH     SP70,FMPRCODE
          GOTO      KFMPRA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,FMPRCODE
KFMPRA10
          PACK      KEY3,FMPRCODE
          CALL      RAFMPR1
          BRANCH    OVRCD,KFMPRA80
          BRANCH    CKEYTYP,KFMPRA75
          CALL      RDFMPR1
KFMPRA70
          MOVE      ZERO,EXIT
          GOTO      KFMPRA99
KFMPRA75
          MOVE      "Print Code Already Exists - ",DISPMSG
          CALL      MESSAGE1
          GOTO      KFMPRA00
KFMPRA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KFMPRA99,KFMPRA99
          MOVE      "Print Code Does Not Exist - ",DISPMSG
          CALL      MESSAGE1
          GOTO      KFMPRA00
KFMPRA90
          MOVE      ONE,EXIT
          GOTO      KFMPRA99
KFMPRA95
          MOVE      TWO,EXIT
KFMPRA99
          RETURN
