.----------------------------------------
. Patient Service Analysis
.----------------------------------------
REPT0000  MOVE      "Printing : ",DISPMSG
          CALL      MESSAGE2
          MOVE      ZERO,CPAGENO
          MOVE      ZERO,TOTALQTY
          MOVE      ZERO,TOTALCST
          MOVE      ZERO,GRANDQTY
          MOVE      ZERO,GRANDCST
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
.
          MOVE      CCCAYEAR,KEY4
          PACK      KEY6,KEY4,FPERIOD
          CALL      RDCCCA1
          MATCH     "  ",FPERIOD
          IF        @EQUAL
            CALL      RKCCCA1
          ENDIF
          MOVE      CCCADES,FROMPNAM
          MOVE      KEY4,CCCAYEAR
.
          PACK      KEY6,KEY4,LPERIOD
          CALL      RDCCCA1
          MATCH     "ZZ",LPERIOD
          IF        @EQUAL
            CALL      RPCCCA1
          ENDIF
          MOVE      CCCADES,TOPNAM
          MOVE      KEY4,CCCAYEAR
.
          PACK      KEY10,CCHCHCD,FDEPART
          CALL      RDCCDP1
          IF        OVRCD=0
            CALL      RPCCDP1
          ENDIF
.
REPT1000  CALL      RKCCDP1
          BRANCH    OVRCD,REPT9000
          MATCH     CCHCHCD,CCDPHCD
          GOTO      REPT9000 IF NOT EQUAL
          MATCH     CCDPDPT,LDEPART
          GOTO      REPT9000 IF LESS
.
          MATCH     "1",CCDPDTY
          GOTO      REPT1000 IF EQUAL
.
          DISPLAY   *P12:24,CCDPDPT
          CALL      HEAD0000        * Page Break for each department
.
REPT2000  MOVE      ZERO,GRANDQTY
          PACK      KEY10,CCDPHCD,CCDPDPT,SP70
          PACK      KEY21,CCDPHCD,CCDPDPT,SP70
          CALL      RSCCPD4
REPT2100  CALL      RKCCPD4
          BRANCH    OVRCD,REPT4000
          PACK      KEY18,CCPDHCD,CCPDDPT,CCPDPCD
          MATCH     KEY10,KEY18
          GOTO      REPT4000 IF NOT EQUAL
.
          MOVE      ZERO,TOTALQTY
          MOVE      ZERO,TOTALCST
          MOVE      ONE,KEYFLAG
          PACK      KEY22,CCDPHCD,CCDPDPT,CCPDPCD,CCCAYEAR,SP70
          PACK      KEY24,CCDPHCD,CCDPDPT,CCPDPCD,CCCAYEAR,SP70
          CALL      RSCCME2
REPT2200  CALL      RKCCME2
          BRANCH    OVRCD,REPT2300
          PACK      KEY24,CCMEHCD,CCMEDPT,CCMEPCD,CCMEYEAR,CCMEPER,SP70
          MATCH     KEY22,KEY24
          GOTO      REPT2300 IF NOT EQUAL
          MATCH     CCMEPER,LPERIOD
          GOTO      REPT2300 IF LESS
          MATCH     FPERIOD,CCMEPER
          GOTO      REPT2200 IF LESS
          MOVE      ZERO,KEYFLAG
.
          ADD       CCMEQTY,TOTALQTY
          MOVE      CCMEQTY,F12P6
          ASSIGN    TOTALCST+(CCMECST*F12P6),TOTALCST
          GOTO      REPT2200
.
REPT2300  IF        PRINTZER=0
            BRANCH    KEYFLAG,REPT2100
            IF        TOTALQTY=0
              GOTO      REPT2100
            ENDIF
          ENDIF
          ADD       ONE,CLNO
          COMPARE   "60",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          MOVE      CCFED1,FTOTAL1
          MOVE      CCFED1,FTOTAL2
          MOVE      CCFED1,FTOTAL3
.
          MOVE      TOTALQTY,F12P2
          FEDIT     F12P2,FTOTAL1
.
          MOVE      TOTALCST,F12P2
          FEDIT     F12P2,FTOTAL2
.
          ADD       TOTALQTY,GRANDQTY
          ADD       TOTALCST,GRANDCST
          ASSIGN    TOTALCST/TOTALQTY,F12P2
          FEDIT     F12P2,FTOTAL3
          MOVE      CCPDDES,KEY32
          PRINT     CCPDSEQ,SP1,CCPDDES,*41,FTOTAL1,FTOTAL2,FTOTAL3
          GOTO      REPT2100
.
. Print Department Totals
.
REPT4000  MOVE      CCFED1,FTOTAL1
          MOVE      CCFED1,FTOTAL2
          MOVE      GRANDQTY,F12P2
          FEDIT     F12P2,FTOTAL1
          MOVE      GRANDCST,F12P2
          FEDIT     F12P2,FTOTAL2
          ADD       THREE,CLNO
          COMPARE   "60",CLNO
          CALL      HEAD0000 IF NOT LESS
          PRINT    *42,"------------ ------------"
          PRINT    "Department Total",*41,FTOTAL1,FTOTAL2
          PRINT    *42,"------------ ------------"
          MOVE      ZERO,GRANDCST
          MOVE      ZERO,GRANDQTY
          GOTO      REPT1000
.
REPT9000  CALL      UND80
          PRINT     *25,"*** End of Report ***"
          CALL      MESSAGE3
          MOVE      "Report Completed - ",DISPMSG
          CALL      MESSAGE1
          RETURN
.
HEAD0000  CALL      HEAD80
          MOVE      "9",CLNO
          PRINT     "Costing Model    : ",CCMACOM,SP5,CCMADES
          PRINT     "Hospital         : ",CCHCHCD,SP7,CCHCDES
          PRINT     "Department       : ",CCDPDPT,SP1,CCDPDES
          PRINT     "Year             : ",CCCAYEAR:
                    "   Period from : ",FROMPNAM," to ",TOPNAM
          IF        CPAGENO=1
            PRINT     "First Department : ",FROMDEPT
            PRINT     "Last  Department : ",TODEPT
            ADD       "2",CLNO
          ENDIF
          CALL      UND80
          PRINT     "Seq Patient Service                          Quantity":
                    "   Total Cost    Avg. Cost"
          CALL      UND80
HEAD9999  RETURN
