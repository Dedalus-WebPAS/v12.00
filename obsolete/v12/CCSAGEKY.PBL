.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSAGEKY
.   Routine   :   HCCAGA00 Age Range           
.   Return    :           
.-------------------------------------------------------------------------------
HCCAGA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCAGA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCAGA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Group"              
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 6",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"To Age"             
.
          MOVE      STDCOL02,STDCOL03
          ADD       " 7",STDCOL03
          DISPLAY   *PSTDCOL03:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL03,STDCOL04
          ADD       "35",STDCOL04
HCCAGA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCAGGRP 
          PACK      KEY4,CCAGDEF,CCAGGRP,SP70                                   
          PACK      SAVKEY80,CCAGDEF,CCAGGRP,SP70                               
          SETLPTR   SAVKEY80,1                                                  
          CALL      RSCCAG1 
HCCAGA10
          CALL      RKCCAG1 
          BRANCH    OVRCD,HCCAGA15
          PACK      KEY4,CCAGDEF,CCAGGRP,SP70                                   
          MATCH     SAVKEY80,KEY4                                               
          GOTO      HCCAGA15 IF NOT EQUAL                                       
          BRANCH    CSEARCH,HCCAGA12
          CALL      HCCAGA60
          BRANCH    OVRCD,HCCAGA10
HCCAGA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY4,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCAGGRP 
          DISPLAY   *PSTDCOL02:STDROW,CCAGTAGE
          DISPLAY   *PSTDCOL03:STDROW,CCAGDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCAGA10 IF NOT EQUAL
HCCAGA15
          UNPACK    SAVKEY80,CCAGDEF,CCAGGRP                                    
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCAGA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCAGA30,HCCAGA35,HCCAGA40,HCCAGA45,HCCAGA04:
                              HCCAGA91,HCCAGA50
          GOTO      HCCAGA92
HCCAGA30
          CALL      RKCCAG1 
          BRANCH    OVRCD,HCCAGA85
          PACK      KEY4,CCAGDEF,CCAGGRP,SP70                                   
          MATCH     SAVKEY80,KEY4                                               
          GOTO      HCCAGA85 IF NOT EQUAL                                       
          CALL      RPCCAG1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCAGA10
HCCAGA35
          MOVE      ST1,KEY4 
          CALL      RDCCAG1 
          CALL      RPCCAG1 
          BRANCH    OVRCD,HCCAGA85
          PACK      KEY4,CCAGDEF,CCAGGRP,SP70                                   
          MATCH     SAVKEY80,KEY4                                               
          GOTO      HCCAGA85 IF NOT EQUAL                                       
          MATCH     SP70,CCAGGRP                                                
          GOTO      HCCAGA85 IF EQUAL                                           
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCAGA36
          CALL      RPCCAG1 
          BRANCH    OVRCD,HCCAGA37
          PACK      KEY4,CCAGDEF,CCAGGRP,SP70                                   
          MATCH     SAVKEY80,KEY4                                               
          GOTO      HCCAGA37 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCAGA36 IF NOT EQUAL
HCCAGA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCAGA10
HCCAGA40
          MOVE      ST1,KEY4 
          CALL      RDCCAG1 
          CALL      RPCCAG1 
          BRANCH    OVRCD,HCCAGA85
          PACK      KEY4,CCAGDEF,CCAGGRP,SP70                                   
          MATCH     SAVKEY80,KEY4                                               
          GOTO      HCCAGA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCAGA08
HCCAGA45
          CALL      RKCCAG1 
          BRANCH    OVRCD,HCCAGA85
          PACK      KEY4,CCAGDEF,CCAGGRP,SP70                                   
          MATCH     SAVKEY80,KEY4                                               
          GOTO      HCCAGA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY4,SAVKEY80,Z70                                           
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCAG1 
HCCAGA46
          CALL      RPCCAG1 
          BRANCH    OVRCD,HCCAGA47
          PACK      KEY4,CCAGDEF,CCAGGRP,SP70                                   
          MATCH     SAVKEY80,KEY4                                               
          GOTO      HCCAGA47 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCAGA46 IF NOT EQUAL
HCCAGA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCAGA10
HCCAGA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCAGA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          KEYIN     *PSTDCOL03:STDROW,*V2LON,SCHFLD03
          RESET     SCHFLD03
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCAGA08
HCCAGA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCAGA08
HCCAGA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCAGA61 IF EQUAL
          MATCH     SCHFLD01,CCAGGRP 
          GOTO      HCCAGA72 IF NOT EQUAL
HCCAGA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCAGA62 IF EQUAL
          MATCH     SCHFLD02,CCAGTAGE
          GOTO      HCCAGA72 IF NOT EQUAL
HCCAGA62
          MATCH     SP70,SCHFLD03
          GOTO      HCCAGA63 IF EQUAL
          MATCH     SCHFLD03,CCAGDES 
          GOTO      HCCAGA72 IF NOT EQUAL
HCCAGA63
HCCAGA64
HCCAGA65
HCCAGA66
HCCAGA67
HCCAGA68
HCCAGA69
HCCAGA70
          GOTO      HCCAGA75
HCCAGA72  MOVE      ONE,OVRCD
HCCAGA75  RETURN
HCCAGA85
          LOAD      KEY4,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY4,CCAGDEF,CCAGGRP                                        
          CALL      RDCCAG1 
HCCAGA90
          UNPACK    SAVKEY80,CCAGDEF,CCAGGRP                                    
          BEEP
          GOTO      HCCAGA20
HCCAGA91
          MOVE      SP70,CCAGDES 
          MOVE      SP70,CCAGTAGE
          MOVE      SP70,CCAGSPA 
          MOVE      SP70,KEY4 
          MOVE      SP70,CCAGGRP 
          GOTO      HCCAGA96
HCCAGA92
          MOVE      SP70,KEY4 
          MOVE      SP70,CCAGGRP 
          LOAD      KEY4,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY4,CCAGDEF,CCAGGRP                                        
HCCAGA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCAGA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSAGEKY
.   Routine   :   KCCAGA00 Keyin Age Group           
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCAGA00 TOPIC     "CCAGGRP "
          MOVE      UNDLN70,CCAGGRP 
          KEYIN     *PCCOL:CVERT,*DV,CCAGGRP:                                   
                    *PCCOL:CVERT,*V2LON,CCAGGRP:                                
                    *PCCOL:CVERT,*DV,CCAGGRP 
.
          ENDSET    CCAGGRP 
          APPEND    SP70,CCAGGRP 
          RESET     CCAGGRP 
.
          MATCH     SP70,CCAGGRP 
          GOTO      KCCAGA90 IF EQUAL
.
          CMATCH    EXITCHAR,CCAGGRP 
          GOTO      KCCAGA95 IF EQUAL
.
          CMATCH    QUEST,CCAGGRP 
          GOTO      KCCAGA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCCAGA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCAGGRP 
          APPEND    SP70,CCAGGRP 
          RESET     CCAGGRP 
.
          MATCH     SP70,CCAGGRP 
          GOTO      KCCAGA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CCAGGRP                                 
KCCAGA10
          PACK      KEY4,CCAGDEF,CCAGGRP                                        
          CALL      RACCAG1 
          BRANCH    OVRCD,KCCAGA80
          BRANCH    CKEYTYP,KCCAGA75
          CALL      RDCCAG1 
KCCAGA70
          MOVE      ZERO,EXIT
          GOTO      KCCAGA99
KCCAGA75
          MOVE      "Age Group Already Exist - ",DISPMSG                                             
          CALL      MESSAGE1
          GOTO      KCCAGA00
KCCAGA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCAGA99,KCCAGA99
          MOVE      "Age Group Does Not Exist - ",DISPMSG                                            
          CALL      MESSAGE1
          GOTO      KCCAGA00
KCCAGA90
          MOVE      ONE,EXIT
          GOTO      KCCAGA99
KCCAGA95
          MOVE      TWO,EXIT
KCCAGA99 TOPIC    PRGID
          RETURN
