.---------------------------------------------------------------------------
.   System    :   IBA Software                       
.   Include   :   IBADBSKY
.   Routine   :   HIBDBA00 Database ID         
.   Return    :           
.-------------------------------------------------------------------------------
HIBDBA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HIBDBA04
          CALL      GSCRN000
          BRANCH    EXIT,HIBDBA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"ID"                 
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 4",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HIBDBA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,IBDBDBS 
          PACK      KEY3,IBDBDBS,SP70                                           
.                                                                               
.                                                                               
          CALL      RSIBDB1 
HIBDBA10
          CALL      RKIBDB1 
          BRANCH    OVRCD,HIBDBA15
          PACK      KEY3,IBDBDBS,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HIBDBA12
          CALL      HIBDBA60
          BRANCH    OVRCD,HIBDBA10
HIBDBA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,IBDBDBS 
          DISPLAY   *PSTDCOL02:STDROW,IBDBDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HIBDBA10 IF NOT EQUAL
HIBDBA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HIBDBA20
          CALL      HSCRI000
          BRANCH    EXIT,HIBDBA30,HIBDBA35,HIBDBA40,HIBDBA45,HIBDBA04:
                              HIBDBA91,HIBDBA50
          GOTO      HIBDBA92
HIBDBA30
          CALL      RKIBDB1 
          BRANCH    OVRCD,HIBDBA85
          PACK      KEY3,IBDBDBS,SP70                                           
.                                                                               
.                                                                               
          CALL      RPIBDB1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBDBA10
HIBDBA35
          MOVE      ST1,KEY3 
          CALL      RDIBDB1 
          CALL      RPIBDB1 
          BRANCH    OVRCD,HIBDBA85
          PACK      KEY3,IBDBDBS,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HIBDBA36
          CALL      RPIBDB1 
          BRANCH    OVRCD,HIBDBA37
          PACK      KEY3,IBDBDBS,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HIBDBA36 IF NOT EQUAL
HIBDBA37
          MOVE      ZERO,XITEMNO
          GOTO      HIBDBA10
HIBDBA40
          MOVE      ST1,KEY3 
          CALL      RDIBDB1 
          CALL      RPIBDB1 
          BRANCH    OVRCD,HIBDBA85
          PACK      KEY3,IBDBDBS,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBDBA08
HIBDBA45
          CALL      RKIBDB1 
          BRANCH    OVRCD,HIBDBA85
          PACK      KEY3,IBDBDBS,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY3,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSIBDB1 
HIBDBA46
          CALL      RPIBDB1 
          BRANCH    OVRCD,HIBDBA47
          PACK      KEY3,IBDBDBS,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HIBDBA46 IF NOT EQUAL
HIBDBA47
          MOVE      ZERO,XITEMNO
          GOTO      HIBDBA10
HIBDBA50  COMPARE   ZERO,CSEARCH
          GOTO      HIBDBA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBDBA08
HIBDBA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBDBA08
HIBDBA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HIBDBA61 IF EQUAL
          MATCH     SCHFLD01,IBDBDBS 
          GOTO      HIBDBA72 IF NOT EQUAL
HIBDBA61
          MATCH     SP70,SCHFLD02
          GOTO      HIBDBA62 IF EQUAL
          MATCH     SCHFLD02,IBDBDES 
          GOTO      HIBDBA72 IF NOT EQUAL
HIBDBA62
HIBDBA63
HIBDBA64
HIBDBA65
HIBDBA66
HIBDBA67
HIBDBA68
HIBDBA69
HIBDBA70
          GOTO      HIBDBA75
HIBDBA72  MOVE      ONE,OVRCD
HIBDBA75  RETURN
HIBDBA85
          LOAD      KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,IBDBDBS                                                
          CALL      RDIBDB1 
HIBDBA90
.                                                                               
          BEEP
          GOTO      HIBDBA20
HIBDBA91
          MOVE      SP70,IBDBDES 
          MOVE      SP70,IBDBDB01
          MOVE      SP70,IBDBDB02
          MOVE      SP70,IBDBDB03
          MOVE      SP70,IBDBDB04
          MOVE      SP70,IBDBDB05
          MOVE      SP70,IBDBDB06
          MOVE      SP70,IBDBDB07
          MOVE      SP70,IBDBDB08
          MOVE      SP70,IBDBDB09
          MOVE      SP70,IBDBDB10
          MOVE      SP70,IBDBUR1 
          MOVE      SP70,IBDBUR2 
          MOVE      SP70,IBDBUD1 
          MOVE      SP70,IBDBUD2 
          MOVE      SP70,IBDBUY1 
          MOVE      SP70,IBDBUY2 
          MOVE      SP70,IBDBSPA 
          MOVE      SP70,KEY3 
          MOVE      SP70,IBDBDBS 
          GOTO      HIBDBA96
HIBDBA92
          MOVE      SP70,KEY3 
          MOVE      SP70,IBDBDBS 
          LOAD      KEY3,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,IBDBDBS                                                
HIBDBA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HIBDBA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   IBA Software                       
.   Include   :   IBADBSKY
.   Routine   :   KIBDBA00 Keyin Database ID         
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KIBDBA00 TOPIC     "IBDBDBS "
          MOVE      UNDLN70,IBDBDBS 
          KEYIN     *PCCOL:CVERT,*DV,IBDBDBS:                                   
                    *PCCOL:CVERT,*V2LON,IBDBDBS:                                
                    *PCCOL:CVERT,*DV,IBDBDBS 
.
          ENDSET    IBDBDBS 
          APPEND    SP70,IBDBDBS 
          RESET     IBDBDBS 
.
          MATCH     SP70,IBDBDBS 
          GOTO      KIBDBA90 IF EQUAL
.
          CMATCH    EXITCHAR,IBDBDBS 
          GOTO      KIBDBA95 IF EQUAL
.
          CMATCH    QUEST,IBDBDBS 
          GOTO      KIBDBA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HIBDBA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    IBDBDBS 
          APPEND    SP70,IBDBDBS 
          RESET     IBDBDBS 
.
          MATCH     SP70,IBDBDBS 
          GOTO      KIBDBA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,IBDBDBS                                 
KIBDBA10
          PACK      KEY3,IBDBDBS                                                
          CALL      RAIBDB1 
          BRANCH    OVRCD,KIBDBA80
          BRANCH    CKEYTYP,KIBDBA75
          CALL      RDIBDB1 
KIBDBA70
          MOVE      ZERO,EXIT
          GOTO      KIBDBA99
KIBDBA75
          MOVE      "Database ID Already Exist - ",DISPMSG                                           
          CALL      MESSAGE1
          GOTO      KIBDBA00
KIBDBA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KIBDBA99,KIBDBA99
          MOVE      "Database ID Does Not Exist - ",DISPMSG                                          
          CALL      MESSAGE1
          GOTO      KIBDBA00
KIBDBA90
          MOVE      ONE,EXIT
          GOTO      KIBDBA99
KIBDBA95
          MOVE      TWO,EXIT
KIBDBA99 TOPIC    PRGID
          RETURN
