           MOVE     STRNAM,VAR
           KEYIN    *PCCOL:CVERT,*V2LON,*MASK,*RV,VAR
           MOVE     VAR,STRNAM
           SFORMAT  VAR,127
           ENDSET   STRNAM
           APPEND   SP70,STRNAM
           RESET    STRNAM
           CMATCH   EXITCHAR,STRNAM
           GOTO     RNAM0999 IF EQUAL
           MATCH    SP70,STRNAM
           GOTO     RNAM0100 IF EQUAL
           PACK     FROMNAM,STRNAM,SP70
           GOTO     RNAM0140
RNAM0100   MOVE     "Start             ",FROMNAM
           MOVE     SP70,STRNAM
.
RNAM0140   MOVE     FROMNAM,VAR
           CALL     DISPITEM
.
