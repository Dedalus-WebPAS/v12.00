.----------------------------------------------------------------------
. PHED0000 - Print Page Heading
.----------------------------------------------------------------------
PHED0000  CALL      HEAD132
          DISPLAY   *P1:24,*EL,"Printing Page ... ",CPAGENO
          IF        FALLITM=0
            PRINT     "Item Code  From  : ",FROMITM," to ",TOITM
            ADD       ONE,CLNO
          ENDIF
          IF        FALLDEP=0
            PRINT     "Department From  : ",FROMDEP," to ",TODEP
            ADD       ONE,CLNO
          ENDIF
          IF        FALLPTY=0
            PRINT     "Prod. Type From  : ",FROMPTY," to ",TOPTY
            ADD       ONE,CLNO
          ENDIF
          IF        FALLNAM=0
            PRINT     "Item Name  From  : ",FROMNAM," to ",TONAM
            ADD       ONE,CLNO
          ENDIF
          MOVE      NO,YESNO
          LOAD      YESNO,REPTACT,YES
          PRINT     "Report Sequence  : ",SEQDESC:
                    "   Active Only      : ",YESNO
          ADD       ONE,CLNO
          IF        REPTSEQ=3
            PRINT     "Product Type     : ",DBPTPTY,SP1,DBPTDES
            ADD       ONE,CLNO
          ENDIF
          IF        REPTSEQ=4
            PRINT     "Department       : ",DBDEDEP,SP1,DBDEDES
            PRINT     "Product Type     : ",DBPTPTY,SP1,DBPTDES
            ADD       TWO,CLNO
          ENDIF
.
          CALL      UND132
          PRINT     *3,"Sales Item",*44,"Invoice Description":
                    *73,"Income Account",*89,"Debtors Control":
                    *121,"Price":
                    *N,*12,"Product Type",*73,"GST Code":
                    *89,"GST Control"
          CALL      UND132
          ADD       ONE,CLNO
          RETURN
