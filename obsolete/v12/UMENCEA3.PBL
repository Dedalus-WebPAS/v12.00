.*************************************************************************
.*                          HEAD0000                                     *
.*                display screen heading line 1 & 2                      *
.*************************************************************************
HEAD0000  READ      CONTROLF,TWO;*4,CONAME
          MOVE      CONAME,CNAME
.
          STRIP     CNAME
          MOVELPTR  CNAME,CCOL
          DIV       TWO,CCOL
          MULT      SEQ,CCOL                                     Date : 12/12/12
          ADD       "40",CCOL
          DISPLAY   *ES,*HON,SP30,SP30,SP10,SP10:
                    *P1:1,PRGID,"/",VERSION,*PCCOL:1,CNAME:
                    *P64:1,"Date : ",CDATE
          HLINE     *G37,2,1,80
          DISPLAY   *P65:2," Port : ",PORT,SP1
HEAD9999  RETURN
