.********************************************************************
.*                          KDIR0000                                *
.*  Keyin directories                                               *
.********************************************************************
KDIR0000  MOVE      ONE,SAVEFLG        * Default to keep file
          DISPLAY   *P5:5,"This program will validate the Catalog Code in ":
                    "Table 'sinciaaf'.":
                    *P5:6,"It may also make changes to Tables 'sincicaf', ":
                    "'sincifaf' and 'sinimpaf'.":
                    *P5:7,"All four tables will be backed up before changes."
          KEYIN     *P1:10,*EL,"Do you want to save the original file (Y/N) ? ":
                    ANS;
          REP       "yYnN",ANS
          CMATCH    "Y",ANS
          GOTO      KDIR1000 IF EQUAL
          MOVE      ZERO,SAVEFLG       * not to keep file
          CMATCH    "N",ANS
          GOTO      KDIR1000 IF EQUAL
          BEEP
          GOTO      KDIR0000
.
KDIR1000  CALL      DEFT0000                * Get the default path
          BRANCH    EXIT,KDIR9999
          MOVE      SP60,OLDPATH
.
          DISPLAY   *P1:24,*EL,"The directory path must end with a slash (/) ";
.
          MOVE      DEFPATH,OLDPATH
          BRANCH    SAVEFLG,KDIR2000
.
.         Keyin the path for the original file
.
          KEYIN     *P1:12,*EL,"Keyin path for temporarily saving original ":
                    "file: ":
                    *P10:13,*EL,*DV,*RV,OLDPATH:
                    *P10:13,OLDPATH;
          GOTO      KDIR3000
.
KDIR2000  KEYIN     *P1:12,*EL,"Keyin path for saving original file: ":
                    *P10:13,*EL,*DV,*RV,OLDPATH:
                    *P10:13,OLDPATH;
.
KDIR3000  ENDSET    OLDPATH
          APPEND    SP60,OLDPATH
          RESET     OLDPATH
.
          MATCH     SP60,OLDPATH
          IF        @EQUAL
            PACK      OLDPATH,DEFPATH      * use the default
            DISPLAY   *P10:13,*EL,*V2LON,OLDPATH;
          ENDIF
.
          SQUEEZE   OLDPATH
          CMATCH    EXITCHAR,OLDPATH
          GOTO      KDIR9000 IF EQUAL
.
          PACK      DIM60,OLDPATH,SP60
          CALL      VALD0000         * check if it's a valid directory
          BRANCH    EXIT,KDIR1000
.
.         Keyin the path for the new file
.
KDIR5000  MOVE      SP60,NEWPATH
          MOVE      DEFPATH,NEWPATH
          KEYIN     *P1:15,*EL,"Keyin path for the new file: ":
                    *P10:16,*EL,*DV,*RV,NEWPATH:
                    *P10:16,NEWPATH;
          ENDSET    NEWPATH
          APPEND    SP60,NEWPATH
          RESET     NEWPATH
.
          MATCH     SP60,NEWPATH
          IF        @EQUAL
            PACK      NEWPATH,DEFPATH      * use the default
            DISPLAY   *P10:16,*V2LON,NEWPATH;
          ENDIF
.
          SQUEEZE   NEWPATH
          CMATCH    EXITCHAR,NEWPATH
          GOTO      KDIR9000 IF EQUAL
.
          PACK      DIM60,NEWPATH,SP60
          CALL      VALD0000         * check if it's a valid directory
          BRANCH    EXIT,KDIR5000
          GOTO      KDIR9999
.
KDIR9000  MOVE      ONE,EXIT
KDIR9999  DISPLAY   *P1:24,*EL;
          RETURN
+
.
.********************************************************************
.*                          PREP0000                                *
.*  Preparing files                                                 *
.********************************************************************
PREP0000 
. open old file
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      OLDFILE1,"sinciaaf"
          TRAPCLR   IO
          BRANCH    OVRCD,PREP5000       * Original file does not exist
          CLOSE     OLDFILE1
          DISPLAY   *P1:24,*EL,"Copying files ... Please wait !! ";
.
. Copy old file to the keyin directory
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "cp `ldat sinciaaf.dat` ",CMDLINE
          APPEND    OLDPATH,CMDLINE
          APPEND    "ssiciaaf.dat",CMDLINE
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
          CMATCH    "0",TASKID
          IF        !@EQUAL
            GOTO      PREP8000
          ENDIF
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "cp `ldat sinciaaf.idx` ",CMDLINE
          APPEND    OLDPATH,CMDLINE
          APPEND    "ssiciaaf.idx",CMDLINE
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
          CMATCH    "0",TASKID
          IF        !@EQUAL
            GOTO      PREP8000
          ENDIF
.
. --------------------------------- * backup "sincicaf" "sincifaf" & sinimpaf"
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "cp `ldat sincicaf.dat` ",CMDLINE
          APPEND    OLDPATH,CMDLINE
          APPEND    "ssicicaf.dat",CMDLINE
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "cp `ldat sincicaf.idx` ",CMDLINE
          APPEND    OLDPATH,CMDLINE
          APPEND    "ssicicaf.idx",CMDLINE
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "cp `ldat sincifaf.dat` ",CMDLINE
          APPEND    OLDPATH,CMDLINE
          APPEND    "ssicifaf.dat",CMDLINE
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "cp `ldat sincifaf.idx` ",CMDLINE
          APPEND    OLDPATH,CMDLINE
          APPEND    "ssicifaf.idx",CMDLINE
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "cp `ldat sinimpaf.dat` ",CMDLINE
          APPEND    OLDPATH,CMDLINE
          APPEND    "ssiimpaf.dat",CMDLINE
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "cp `ldat sinimpaf.idx` ",CMDLINE
          APPEND    OLDPATH,CMDLINE
          APPEND    "ssiimpaf.idx",CMDLINE
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
. ----------------------------------------------------------
.
. remove the original files
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "iserase ",CMDLINE
          APPEND    DEFPATH,CMDLINE
          APPEND    "sinciaaf",CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
          CMATCH    "0",TASKID
          IF        !@EQUAL
            DISPLAY   *P1:20,*EL,*B,"Unable to remove the original file.  ":
                      *P15:22,*EL,*B,"ALLWEB03, SINWEB01 to 03 or COMWEB80 ":
                      "may still be running":
                      *P1:24," ";
            CALL      HITENTER
            GOTO      PREP9000
          ENDIF
.
. Opening old file after being copied
.
          MOVE      SP60,DIM60
          CLEAR     DIM60
          APPEND    OLDPATH,DIM60
          APPEND    "ssiciaaf",DIM60
          APPEND    SP60,DIM60
          RESET     DIM60
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      OLDFILE1,DIM60
          TRAPCLR   IO
          IF        OVRCD = 1
            DISPLAY   *P1:24,*EL,*B,"Unable to open saved original file.  ";
            CALL      HITENTER
            GOTO      PREP9000
          ENDIF
.
. Create the new file
.
          DISPLAY   *P1:24,*EL,"Creating new files ... Please wait !! ";
          PACK      NEWFILE,SP60
          CLEAR     NEWFILE
          APPEND    NEWPATH,NEWFILE
          APPEND    "sinciaaf",NEWFILE
          APPEND    SP60,NEWFILE
          RESET     NEWFILE
          SQUEEZE   NEWFILE
.
PREP1000  PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "isbuild ",CMDLINE
          APPEND    NEWFILE,CMDLINE
.
.  The files new record length and unique key must be written here
.      eg    APPEND    " 195 UC9-16",CMDLINE
.
          APPEND    " 250 UC1-7",CMDLINE              * <==== change
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
.  If the file has more than one index, this piece of code below has 
.  to be repeated with for each individual index
.
           PACK      CMDLINE,SP60,SP60
           CLEAR     CMDLINE
           APPEND    "isadd ",CMDLINE
           APPEND    NEWFILE,CMDLINE
           APPEND    " UC68-72,1-7",CMDLINE          * <==== change
           APPEND    SP60,CMDLINE
           RESET     CMDLINE
           EXECUTE   CMDLINE,TASKID
.
           PACK      CMDLINE,SP60,SP60
           CLEAR     CMDLINE
           APPEND    "isadd ",CMDLINE
           APPEND    NEWFILE,CMDLINE
           APPEND    " UC8-67,1-7",CMDLINE          * <==== change
           APPEND    SP60,CMDLINE
           RESET     CMDLINE
           EXECUTE   CMDLINE,TASKID
.
          OPEN      SINCIAA1,NEWFILE
.
. set the flag to say we want to continue
.
          DISPLAY   *P1:24,*EL;
          MOVE      ZERO,EXIT
          CALL      IBACLOCK
          REP       " 0",CDATE
          DISPLAY   *P1:18,"Started : ",CDATE,SP1,CTIMEIS:
                    *P1:20,"Record : "
          GOTO      PREP9999
.
PREP5000  DISPLAY   *P1:24,*EL,*B,"Old file not found.  ";
          CALL      HITENTER
          GOTO      PREP9000
.
PREP8000  DISPLAY   *P1:24,*EL,*B,"Unable to copy original file.  ":
                    "(Error: ",TASKID,")  ";
          CALL      HITENTER
.
PREP9000  MOVE      ONE,EXIT
PREP9999  RETURN
+
.**********************************************************************
.*                               READ0000                             *
.*             loop thru old file and write each record               *
.**********************************************************************
READ0000  DISPLAY   *P1:20,*EL,"Record No. : ";
          MOVE      "99",CLNO
.
          OPEN      SINCICA1,"sincicaf"           * KEY27
          OPEN      SINCIFA2,"sincifaf"           * KEY67
          OPEN      SINIMPA2,"sinimpaf"           * KEY12
.
          MOVE      ZERO,ERRNUMB
          MOVE      "was ",DIM4A
          PACK      KEY7,SP60
          CALL      READSOLD                      * position at start
READ1000  CALL      READKOLD                      * read next record
          BRANCH    OVRCD,READ6000                * no more records
          ADD       ONE,RECNUM                    * update record counter
.
.  All necessary changes to the files field lengths, date changes
.  or any other sort of modification must be made here
.
.    eg    PACK      DSPARE,SP50                      * <==== change
.
          MOVE      SIIACAT,SSIIACAT
          MOVE      SIIACAT,KEY7
          COMPARE   ZERO,INCOCATE         * 0=Alpha left-justified
          IF        @EQUAL
            SQUEEZE   KEY7   
            PACK      DIM7A,KEY7,SP10
            PACK      KEY7,DIM7A,SP50
            CALL      RASIIA1
            IF        OVRCD = 0	
              MOVE      "Z",ANS
              PACK      DIM7A,ANS,KEY7,SP10
              MOVE      "- Code has been changed to avoid duplicates",ERRMESS
              ADD       ONE,ERRNUMB
              CALL      FORMERR0
            ENDIF
.
          ELSE                            * 1=Numeric right-just no lead zeros
            MOVE      KEY7,DIM7A     
            CALL      RJUS0000            * right justify Catalogue No.
            TYPE      DIM7A
            IF        !@EQUAL
              MOVE      SP4,DIM4A
              MOVE      SIIACAT,DIM7A
              MOVE      "- Code is not Numeric - Code not changed",ERRMESS
              ADD       ONE,ERRNUMB
              CALL      FORMERR0
             ELSE
              MOVE      DIM7A,DIM7B
              MOVE      DIM7A,F7
              MOVE      F7,DIM7A          * remove leading zeros
              PACK      KEY7,DIM7A,SP50         * <==== change
              CALL      RASIIA1
              IF        OVRCD = 0	
                MOVE      SP4,DIM4A
                MATCH     DIM7A,DIM7B
                MOVE      "- Right adjusting Code will cause duplicates",ERRMESS
                IF        !@EQUAL
                  MOVE      "- Removing leading zeros will cause duplicates",ERRMESS
                ENDIF
                MOVE      SIIACAT,DIM7A
                ADD       ONE,ERRNUMB
                CALL      FORMERR0
              ENDIF
            ENDIF
          ENDIF
.                                           * check if new Code exists
READ5000  MOVE      DIM7A,SIIACAT
          PACK      KEY7,SIIACAT,SP10
          CALL      WRSIIA1                    * write to new file
.
          MATCH     SIIACAT,SSIIACAT
          IF        !@EQUAL
READ5100    MOVE      "- Catalog Code changed",ERRMESS
            CALL      FORMERR0
            CALL      FIXCIC00              * fixit for "sincicaf"
            CALL      FIXCIF00              * fixit for "sincifaf"
            CALL      FIXIMP00              * fixit for "sinimpaf"
          ENDIF
.
          IF        (RECNUM%1000) = 0
            DISPLAY   *P15:20,*V2LON,RECNUM
          ENDIF
.
          GOTO      READ1000                      * get next record
.
READ6000  MOVE      ZERO,EXIT
          DISPLAY   *P15:20,*V2LON,RECNUM
          CALL      IBACLOCK
          REP       " 0",CDATE
.
READ9999  RETURN
+
.******************************************************************************
.*                                  RJUS0000                                  *
.*                         Right Justify a field                              *
.******************************************************************************
RJUS0000  MOVE      SP70,FLD10OUT
          MOVE      DIM7A,FLD10IN
          SQUEEZE   FLD10IN
          IF        @EOS
            MOVE      "0",FLD10IN
          ENDIF
          MOVELPTR  FLD10IN,LENIN
          MOVE      "7",LENOUT
          COMPARE   LENIN,LENOUT
          IF        @EQUAL
            MOVE      FLD10IN,FLD10OUT
          ELSE
            SUB       LENIN,LENOUT
            SUB       ONE,LENOUT
            BUMP      FLD10OUT,LENOUT
            APPEND    FLD10IN,FLD10OUT
          ENDIF
          RESET     FLD10OUT
          MOVE      FLD10OUT,DIM7A
.
RJUS9999  RETURN
.**********************************************************************
.*                               FORMERR0                             *
.*                       Print any Code Errors                        *
.**********************************************************************
FORMERR0  COMPARE   "56",CLNO
          IF        !@LESS
            MOVE      ONE,CNOUNDLN
            MOVE      "- Error report",CPHDROPT
            CALL      HEAD132                 * Print the report header
            PRINT     *N:
                      *1,"Cat.Code    Description",*68,"Message",*N,*N
            MOVE      "5",CLNO
          ENDIF
.
          MOVE      SIIADES,DIM50
          PRINT     *1,DIM7A,SP5,DIM50,SP5,DIM4A,"'",SSIIACAT,"' ",ERRMESS
.
          MOVE      "was",DIM4A
          ADD       ONE,CLNO
.
FORMERR9  RETURN
