          APPEND    SP70,DBDBAD3
          RESET     DBDBAD3
          CMATCH    EXITCHAR,DBDBAD3
          GOTO      KADD3900 IF EQUAL
.
          MOVE      THREE,SUBKYLIN
          PACK      IBAADLN2,DBDBAD2,SP30,SP30
          PACK      IBAADLN3,DBDBAD3,SP30,SP30
          PROC      AUTOPCOD
          BRANCH    EXIT,KADD3000,KADD3500
.
          MOVE      IBAPCOD,DBDBPC
          MOVE      "   12",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      DBDBPC,VAR
            CALL      DISPITEM
          ENDIF
          MOVE      "   10",FLDID
