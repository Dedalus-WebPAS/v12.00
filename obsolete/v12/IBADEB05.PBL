.-------------------------------------------------------------
. Audit & Locking Standard Routine to Post Changes.
.-------------------------------------------------------------
POST0000  MATCH    "@",DBDBDEB
          GOTO     POST1000 IF NOT EQUAL
.
POST0500  READ     CONTROLF,EIGHTY1;*233,CFDBNDB
          ADD      ONE,CFDBNDB
          WRITAB   CONTROLF,EIGHTY1;*233,CFDBNDB
          MOVE     CFDBNDB,KEY8
          SQUEEZE  KEY8
.
          PACK     DBDBDEB,KEY8,SP70
          CALL     RADBDB1
          COMPARE  ZERO,OVRCD
          GOTO     POST0500 IF EQUAL
.
          CLEAR    DISPMSG
          APPEND   "New Debtor Number Generated ",DISPMSG
          APPEND   DBDBDEB,DISPMSG
          APPEND   " - ",DISPMSG
          RESET    DISPMSG
          CALL     MESSAGE1
.
POST1000  PACK     KEY8,DBDBDEB,SP70  * Setup Key
          CALL     RADBDB1            * Check for record on file
          IF       OVRCD=0
            CALL     UPDBDB1          * Update Changes
            MOVE     THREE,AUDTTYPE   * After Change Type
            CALL     WADBDB00         * Write Audit Record
            CALL     UUDBDB1          * Unloc
          ELSE
            CALL     WRDBDB1          * Write New Record
            MOVE     ONE,AUDTTYPE     * New Record Audit Type
            CALL     WADBDB00         * Write Audit Record
          ENDIF
.
          PROC      DEBDKIUP
.
          CALL      PNOTE000          * Save Notes
.
          RETURN                      * Return to program.
.---------------------------
. Save Notes
.---------------------------
PNOTE000  PACK      KEY11,DBDBDEB,SP70
          CALL      RSDBDC1
PNOTE200  CALL      RKDBDC1
          BRANCH    OVRCD,PNOTE250
          MATCH     DBDBDEB,DBDCDEB
          GOTO      PNOTE250 IF NOT EQUAL
          PACK      KEY11,DBDCDEB,DBDCLIN
          CALL      DEDBDC1
          GOTO      PNOTE200
.
PNOTE250  MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      TEMPWORD,FILDBDC1
          TRAPCLR   IO
          BRANCH    OVRCD,PNOTE999
          MOVE      ZERO,F3
PNOTE260  READ      TEMPWORD,SEQ;DBDCCOM
          GOTO      PNOTE999 IF OVER
          MOVE      F3,DBDCLIN
          PACK      KEY11,DBDBDEB,DBDCLIN
          UNPACK    KEY11,DBDCDEB,DBDCLIN
          CALL      WRDBDC1
          ADD       ONE,F3
          GOTO      PNOTE260
.
PNOTE999  RETURN
.
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Cancel Changes.
.-------------------------------------------------------------
CANC0000  PACK     KEY8,DBDBDEB,SP70   * Setup Key for file.
          CALL     RADBDB1             * Check to see if record on file.
          IF       OVRCD=0
            CALL     DADBDB00          * Delete Before Audit.
            CALL     UUDBDB1           * Unlock.
          ENDIF
          RETURN
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Delete Record.
.-------------------------------------------------------------
DELT0000  MOVE     "Checking Transaction Files ...",DISPMSG
          CALL     MESSAGE2            * Save and Clear Line 24
          CALL     CHKITM00            * Check for Related Transactions
          CALL     MESSAGE3            * Restore Line 24
          BRANCH   EXIT,DELT9100
.
          MOVE     SP70,DISPMSG        * Clear Line 24 Display Message
          CALL     MESSAGE2            * Save and Clear Line 24
          CALL     DELQST              * Delete (Y/N/C) CEXIT 1/2/3
          CALL     MESSAGE3            * Restore Line 24
          BRANCH   CEXIT,DELT1000,DELT9000,DELT9100
.
DELT1000  PACK     KEY8,DBDBDEB,SP70   * Setup Key to File
          CALL     RADBDB1             * Read Record
          IF       OVRCD=0
            CALL     DADBDB00          * Delete Before Audit
            MOVE     FOUR,AUDTTYPE     * Delete Audit Type
            CALL     WADBDB00          * Write Delete Audit
            CALL     DEDBDB1           * Delete Record
          ENDIF
