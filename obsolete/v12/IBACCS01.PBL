.------------------------------------------------------------
. Enter Hospital Code
.------------------------------------------------------------
EHCD0000  CALL    KCCHCA00
          BRANCH  EXIT,EHCD9999,EHCD9999
.
          MOVE    CCHCHCD,KEY2
          CALL    RDCCHL1
          CALL    RDCCHC1
.
EHCD9999  RETURN
.------------------------------------
. Write/Update Hospital Code Record
.------------------------------------
WCCHC000  PACK      KEY2,CCHCHCD
          CALL      RACCHC1
          IF        OVRCD=0
            CALL      UPCCHC1
          ELSE
            CALL      WRCCHC1
          ENDIF
.
          MOVE      CCHCHCD,CCHLHCD
          PACK      KEY2,CCHLHCD
          CALL      RACCHL1
          IF        OVRCD=0
            CALL      UPCCHL1
          ELSE
            CALL      WRCCHL1
          ENDIF
.
          RETURN
.------------------------------------
. Delete Hospital Code Record
.------------------------------------
DCCHC000  CALL      DELTYORN
          COMPARE   ONE,FYESNO
          GOTO      DCCHC999 IF NOT EQUAL
          PACK      KEY10,CCHCHCD,SP70
          CALL      RSCCDP1
          CALL      RKCCDP1
          BRANCH    OVRCD,DCCHC100
          MATCH     CCHCHCD,CCDPHCD
          GOTO      DCCHC100 IF NOT EQUAL
          MOVE      "Dept. Still Exist for Hospital. Delete Invalid - ",DISPMSG
          CALL      MESSAGE1
          GOTO      DCCHC999
.
DCCHC100  PACK      KEY2,CCHCHCD
          CALL      RDCCHC1
          IF        OVRCD=0
            CALL      DECCHC1
          ENDIF
.
          CALL      RDCCHC1
          IF        OVRCD=0
            CALL      DECCHC1
          ENDIF
.
DCCHC999  RETURN
