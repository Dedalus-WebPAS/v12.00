.------------------------------------------------------------
. Print Patient Service Chart for a Range of Departments
.------------------------------------------------------------
REPT0000  MOVE      ZERO,CPAGENO
          MOVE      "99",CLNO
          CLOCK    TIME,CTIMEIS
          DISPLAY  *P1:24,*EL,"Printing : "
          PACK     KEY10,CCHCHCD,STRDPT,SP70
          CALL     RDCCDP1
          IF       OVRCD=0
            CALL     RPCCDP1
          ENDIF
.
REPT1000  CALL     RKCCDP1
          BRANCH   OVRCD,REPT9000
          MATCH    CCHCHCD,CCDPHCD
          GOTO     REPT9000 IF NOT EQUAL
          MATCH    CCDPDPT,ENDDPT
          GOTO     REPT9000 IF LESS
          DISPLAY  *P12:24,CCDPHCD,"/",CCDPDPT
.
          CALL     PDPT0000    * Print Department Details
          CALL     EDPT0000    * Print Cost Type Details
.
          GOTO     REPT1000
.
REPT9000  COMPARE  ZERO,CPAGENO
          CALL     HEAD132 IF EQUAL
          CALL     UND132
          PRINT    *50,"****** End of Report ******"
.
REPT9999  RETURN
.------------------------------------------------------------
. Print Patient Service Chart
.------------------------------------------------------------
PDPT0000  CALL     HEAD0000
          PACK     KEY14,CCDPHCD,CCDPDPT,SP70
          CALL     RSCCDA1
          MOVE     ZERO,SECTOR
          GOTO     PDPT1000
.
PDPT0100  CALL     HEAD0000
          MOVE     ZERO,SECTOR
.
PDPT1000  CALL     RKCCDA1
          BRANCH   OVRCD,PDPT1900
          MATCH    CCDAHCD,CCDPHCD
          GOTO     PDPT1900 IF NOT EQUAL
          MATCH    CCDADPT,CCDPDPT
          GOTO     PDPT1900 IF NOT EQUAL
          ADD      ONE,SECTOR
          MOVE     CCDACTY,COSTTYPE[SECTOR]
          PRINT    " |    ",CCDACTY;
          COMPARE  "10",SECTOR
          GOTO     PDPT1000 IF NOT EQUAL
          GOTO     PDPT2000
.
PDPT1900
          MOVE     SECTOR,F2
          REPEAT
            ADD      ONE,F2
            PRINT    " |    ",SP4;
            MOVE     SP4,COSTTYPE[F2]
          UNTIL    F2=10
.
PDPT2000  PRINT    " |"
          COMPARE  ZERO,SECTOR
          GOTO     PDPT9999 IF EQUAL
          PACK     KEY21,CCDPHCD,CCDPDPT,SP70
          CALL     RSCCPD4
PDPT3000  CALL     RKCCPD4
          BRANCH   OVRCD,PDPT3900
          MATCH    CCPDHCD,CCDPHCD
          GOTO     PDPT3900 IF NOT EQUAL
          MATCH    CCPDDPT,CCDPDPT
          GOTO     PDPT3900 IF NOT EQUAL
.
          COMPARE  "55",CLNO
          CALL     CHEAD000 IF NOT LESS
          ADD      THREE,CLNO
          CALL     UND132
          MOVE     CCPDDES,KEY26
          PRINT    CCPDSEQ,SP1,KEY26;
          MOVE     ZERO,F2
          REPEAT
            ADD      ONE,F2
            MATCH    SP70,COSTTYPE[F2]
            IF       @EQUAL
              PRINT    " |",SP8;
            ELSE
              PACK     KEY22,CCPDHCD,CCPDDPT,CCPDPCD,COSTTYPE[F2]
              CALL     RDCCPV1
              IF       OVRCD=0
                MOVE     CCPVRVI,F12P2
                MOVE     "99999.99",KEY8
                FEDIT    F12P2,KEY8
                PRINT    " |",KEY8;
              ELSE
                PRINT    " |",SP8;
              ENDIF
            ENDIF
         UNTIL     (F2=10)
         PRINT    " |"
         MOVE     CCPDRVI,F12P2
         MOVE     "99999.99",KEY8
         FEDIT    F12P2,KEY8
         PRINT    CCPDPCD,"              ",KEY8," |         |":
                  "         |         |         |         |":
                  "         |         |         |         |":
                  "         |"
         GOTO     PDPT3000
.
PDPT3900 COMPARE  "10",SECTOR
         GOTO     PDPT0100 IF EQUAL
.
PDPT9999 RETURN
.------------------------------------------------------------
. Contuation Heading
.------------------------------------------------------------
CHEAD000  CALL     HEAD0000
          MOVE     ZERO,F2
          REPEAT
            ADD      ONE,F2
            PRINT    " |    ",COSTTYPE[F2];
          UNTIL     (F2=10)
          PRINT      " |"
          RETURN
.
HEAD0000  CALL     HEAD132
          ADD      "5",CLNO
          PRINT    "Hospital    : ",CCHCHCD,SP7,CCHCDES
          PRINT    "Department  : ",CCDPDPT,SP1,CCDPDES
          CALL     UND132
          PRINT    "Patient Service   Indirect RVI";
          RETURN
.
. End Department so List Cost Type Legend
.----------------------------------------
EDPT0000  PACK     KEY14,CCDPHCD,CCDPDPT,SP70
          IF       CLNO>40
            CALL     HEAD132
            ADD      "5",CLNO
            PRINT    "Hospital    : ",CCHCHCD,SP7,CCHCDES
            PRINT    "Department  : ",CCDPDPT,SP1,CCDPDES
          ENDIF
          CALL     UND132
          PRINT    "   Code Description                        ":
                   "   Code Description                       ":
                   "    Code Description"
          CALL     UND132
          MOVE     ZERO,SECTOR
          CALL     RSCCDA1
EDPT1000  CALL     RKCCDA1
          BRANCH   OVRCD,EDPT9999
          MATCH    CCDAHCD,CCDPHCD
          GOTO     EDPT9999 IF NOT EQUAL
          MATCH    CCDADPT,CCDPDPT
          GOTO     EDPT9999 IF NOT EQUAL
          MOVE     CCDACTY,KEY4
          CALL     RDCCCT1
          BRANCH   OVRCD,EDPT1000
          PRINT    SP3,CCCTCTY,SP1,CCCTDES;
          ADD      ONE,SECTOR
          COMPARE  "3",SECTOR
          GOTO     EDPT1000 IF NOT EQUAL
          PRINT    "  "
          MOVE     ZERO,SECTOR
          GOTO     EDPT1000
.
EDPT9999  PRINT    "  "
          RETURN
RDPT0000   MOVE     "    5",FLDID
           CALL     GETPOSIT
           MOVE     CCHCHCD,CCDPHCD
           CALL     KCCDPA00
           BRANCH   EXIT,RDPT0100,RDPT0999
           MOVE     CCDPDPT,STRDPT
           PACK     FROMDPT,CCDPDPT,SP1,CCDPDES
           GOTO     RDPT0140
.
RDPT0100   MOVE     "Start             ",FROMDPT
           MOVE     SP70,STRDPT
.
RDPT0140   MOVE     FROMDPT,VAR
           CALL     DISPITEM
.
RDPT0150   MOVE     "    6",FLDID
           CALL     GETPOSIT
           MOVE     CCHCHCD,CCDPHCD
           CALL     KCCDPA00
           BRANCH   EXIT,RDPT0200,RDPT0999
           MOVE     CCDPDPT,ENDDPT
           PACK     TODPT,CCDPDPT,SP1,CCDPDES
           MATCH    STRDPT,ENDDPT
           GOTO     RDPT0999 IF EQUAL
           GOTO     RDPT0999 IF NOT LESS
           BEEP
           MOVE     "To Department Must Be > From Department - ",DISPMSG
           CALL     MESSAGE1
           GOTO     RDPT0000
.
RDPT0200   MOVE     "Finish             ",TODPT
           MOVE     "ZZZZZZZZ",ENDDPT
.
RDPT0999   MOVE     "    5",FLDID
           CALL     GETPOSIT
           RETURN
