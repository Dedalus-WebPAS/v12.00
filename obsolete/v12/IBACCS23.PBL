.---------------------------------------------
. General Ledger Load Remainders Report
.---------------------------------------------
PRINT000  PACK      REMFNAME,REMDFILE,CCCAYEAR,CCCAPER
          REP       " 0",REMFNAME
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      CCSREMAF,REMFNAME
          TRAPCLR   IO
.
          BRANCH    OVRCD,PRINT090
          MOVE      ZERO,CPAGENO
          MOVE      ZERO,REPTOTAL
          MOVE      "99",CLNO
          CLOCK     TIME,CTIMEIS
.
. Print Costs from Tempory File
.--------------------------------------------------
PRINT010  READ      CCSREMAF,SEQ;CCGKKEY,CCTMTOT
          GOTO      PRINT080 IF OVER
          DISPLAY   *P30:24,CCGKKEY
          IF        CLNO>60
            CALL      HEAD80
            PRINT     "Year : ",CCCAYEAR,"   Period : ",CCCAPER
            CALL      UND80
            PRINT     "| General Ledger Key   | Description":
                      "                         | Remainder       |"
            CALL      UND80
            MOVE      "6",CLNO
          ENDIF
          MOVE      CCGKKEY,KEY20
          CALL      RDCCGK1
          IF        OVRCD=1
            MOVE      "Key Not on File   ",CCGKDES
          ENDIF
          PRINT     "| ",CCGKKEY," | ",CCGKDES," | ",CCTMTOT," |"
          ADD       ONE,CLNO
          ADD       CCTMTOT,REPTOTAL
          GOTO      PRINT010
.
PRINT080
          CALL      UND80
          PRINT     "| Report Total                      ":
                    "                         | ",REPTOTAL," |"
          CALL      UND80
          CALL      MESSAGE3
          MOVE      "Completed - ",DISPMSG
          CALL      MESSAGE1
          GOTO      PRINT099
PRINT090  MOVE      "File Not Found. - ",DISPMSG
          CALL      MESSAGE1
          GOTO      PRINT099
.
PRINT099  RETURN
