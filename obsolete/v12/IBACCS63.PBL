.----------------------------------------
. Move one to accept to for insert mode
.----------------------------------------
MVACCEPT  MOVE      ONE,ACCEPT
          RETURN
EDEPT000   MOVE     "    3",FLDID
           CALL     GETPOSIT
           MOVE     CCHCHCD,CCDPHCD
           CALL     KCCDPA00
           BRANCH   EXIT,EDEPT100,EDEPT999
           MOVE     CCDPDPT,FDEPART
           PACK     FROMDEPT,CCDPDPT,SP1,CCDPDES
           GOTO     EDEPT150
EDEPT100   MOVE     "Start             ",FROMDEPT
           MOVE     SP70,FDEPART
.
EDEPT150   MOVE     "    4",FLDID
           CALL     GETPOSIT
           MOVE     CCHCHCD,CCDPHCD
           CALL     KCCDPA00
           BRANCH   EXIT,EDEPT200,EDEPT999
           PACK     TODEPT,CCDPDPT,SP1,CCDPDES
           MOVE     CCDPDPT,LDEPART
           MATCH    FDEPART,LDEPART
           GOTO     EDEPT999 IF EQUAL
           GOTO     EDEPT999 IF NOT LESS
           BEEP
           MOVE     "To Department Must Be > From Department - ",DISPMSG
           CALL     MESSAGE1
           GOTO     EDEPT000
EDEPT200   MOVE     "Finish             ",TODEPT
           MOVE     "ZZZZZZZZ",LDEPART
.
EDEPT999   MOVE     "    3",FLDID
           CALL     GETPOSIT
           RETURN
ECTY0000   MOVE     ZERO,CKEYTYP
           CALL     KCCCTA00
           BRANCH   EXIT,ECTY1000,ECTY9999
           MOVE     CCCTCTY,FROMTYPE
           MOVE     CCCTDES,FROMDESC
           GOTO     ECTY9999
ECTY1000   MOVE     "Default RVI",FROMDESC
           MOVE     SP70,FROMTYPE
ECTY9999   RETURN
POST0000  DISPLAY   *P1:24,*EL,"Copying : "
          PACK      KEY10,CCHCHCD,FDEPART
          CALL      RDCCDP1
          IF        OVRCD=0
            CALL      RPCCDP1
          ENDIF
POST1000  CALL      RKCCDP1
          BRANCH    OVRCD,POST9000
          MATCH     CCHCHCD,CCDPHCD
          GOTO      POST9000 IF NOT EQUAL
          MATCH     CCDPDPT,LDEPART
          GOTO      POST9000 IF LESS
          DISPLAY   *P12:24,CCDPDPT
          PACK      KEY18,CCDPHCD,CCDPDPT,SP70
          CALL      RSCCPD1
POST2000  CALL      RKCCPD1
          BRANCH    OVRCD,POST1000
          MATCH     CCDPHCD,CCPDHCD
          GOTO      POST1000 IF NOT EQUAL
          MATCH     CCDPDPT,CCPDDPT
          GOTO      POST1000 IF NOT EQUAL
          MATCH     SP70,FROMTYPE
          IF        !(@EQUAL)
            PACK       KEY22,CCPDHCD,CCPDDPT,CCPDPCD,FROMTYPE
            CALL       RDCCPV1
            BRANCH     OVRCD,POST2000
            MOVE       CCPVRVI,CCPDRVI
          ENDIF
.
          COMPARE    ZERO,CCPDRVI
          GOTO       POST2000 IF EQUAL
          PACK       KEY22,CCPDHCD,CCPDDPT,CCPDPCD,TOTYPE
          CALL       RDCCPV1
          IF         OVRCD=0
            MOVE       CCPDRVI,CCPVRVI
            CALL       UPCCPV1
          ELSE
            UNPACK     KEY22,CCPVHCD,CCPVDPT,CCPVPCD,CCPVCTY
            MOVE       CCPDRVI,CCPVRVI
            CALL       WRCCPV1
          ENDIF
          GOTO      POST2000
.
POST9000  MOVE      "Copy Complete - ",DISPMSG
          CALL      MESSAGE1
          RETURN
.----------------------------------------------------------------------
. Process Details :  Update Default Relative Values with Cost Details
.----------------------------------------------------------------------
PROC0000  DISPLAY  *P1:24,*EL,"Processing : ",CCDPHCD,"/",CCDPDPT
          PACK     KEY18,CCDPHCD,CCDPDPT,SP70
          PACK     KEY10,CCDPHCD,CCDPDPT,SP70
          CALL     RSCCPD1
PROC1000  CALL     RKCCPD1
          BRANCH   OVRCD,PROC9999
          PACK     KEY18,CCPDHCD,CCPDDPT,SP70
          MATCH    KEY10,KEY18
          GOTO     PROC9999 IF NOT EQUAL
.
          MATCH    SP70,CCMACOM
          IF       @EQUAL
            CALL     CACT0000
          ELSE
            CALL     CBUD0000
          ENDIF
          ASSIGN   TOTALCST/TOTALQTY,CCPDRVI
          CALL     UPCCPD1
          GOTO     PROC1000
.
PROC9999  RETURN
.----------------------------------------------------------------------
. Calculate Model Cost Excluding Indirect Cost Components
.----------------------------------------------------------------------
CBUD0000  MOVE      ZERO,TOTALCST
          MOVE      ZERO,TOTALQTY
          PACK      KEY22,CCDPHCD,CCDPDPT,CCPDPCD,CCCAYEAR,SP70
          PACK      KEY24,CCDPHCD,CCDPDPT,CCPDPCD,CCCAYEAR,SP70
          CALL      RSCCME2
CBUD1000  CALL      RKCCME2
          BRANCH    OVRCD,CBUD9999
          PACK      KEY24,CCMEHCD,CCMEDPT,CCMEPCD,CCMEYEAR,SP70
          MATCH     KEY22,KEY24
          GOTO      CBUD9999 IF NOT EQUAL
.
          ADD       CCMEQTY,TOTALQTY
.
          PACK      KEY28,CCMEHCD,CCMEDPT,CCMEYEAR,CCMEPER,CCMEPCD,SP70
          PACK      KEY24,CCMEHCD,CCMEDPT,CCMEYEAR,CCMEPER,CCMEPCD,SP70
          CALL      RSCCMD1
CBUD1500  CALL      RKCCMD1
          BRANCH    OVRCD,CBUD1000
          PACK      KEY28,CCMDHCD,CCMDDPT,CCMDYEAR,CCMDPER,CCMDPCD,SP70
          MATCH     KEY24,KEY28
          GOTO      CBUD1000 IF NOT EQUAL
.
          MOVE      "0",CCCTIND
          MOVE      CCMDCTY,KEY4
          CALL      RDCCCT1
.
          MATCH     "1",CCCTIND       * Indirect 1=Yes
          GOTO      CBUD1500 IF EQUAL
.
          ASSIGN    TOTALCST+(CCMEQTY*CCMDCST),TOTALCST
          GOTO      CBUD1500
.
CBUD9999  RETURN
.----------------------------------------------------------------------
. Calculate Actual Cost Excluding Indirect Cost Components
.----------------------------------------------------------------------
CACT0000  MOVE      ZERO,TOTALCST
          MOVE      ZERO,TOTALQTY
          PACK      KEY22,CCDPHCD,CCDPDPT,CCPDPCD,CCCAYEAR,SP70
          PACK      KEY24,CCDPHCD,CCDPDPT,CCPDPCD,CCCAYEAR,SP70
          CALL      RSCCPS2
CACT2000  CALL      RKCCPS2
          BRANCH    OVRCD,CACT9999
          PACK      KEY24,CCPSHCD,CCPSDPT,CCPSPCD,CCPSYEAR,SP70
          MATCH     KEY22,KEY24
          GOTO      CACT9999 IF NOT EQUAL
.
          ADD       CCPSQTY,TOTALQTY
.
          PACK      KEY28,CCPSHCD,CCPSDPT,CCPSYEAR,CCPSPER,CCPSPCD,SP70
          PACK      KEY24,CCPSHCD,CCPSDPT,CCPSYEAR,CCPSPER,CCPSPCD,SP70
          CALL      RSCCPC1
CACT2500  CALL      RKCCPC1
          BRANCH    OVRCD,CACT2000
          PACK      KEY28,CCPCHCD,CCPCDPT,CCPCYEAR,CCPCPER,CCPCPCD,SP70
          MATCH     KEY24,KEY28
          GOTO      CACT2000 IF NOT EQUAL
.
          MOVE      "0",CCCTIND
          MOVE      CCPCCTY,KEY4
          CALL      RDCCCT1
.
          MATCH     "1",CCCTIND       * Indirect 1=Yes
          GOTO      CACT2500 IF EQUAL
.
          ASSIGN    TOTALCST+(CCPSQTY*CCPCCST),TOTALCST
          GOTO      CACT2500
.
CACT9999  RETURN
.
.
ECMOD000  MOVE      ZERO,CKEYTYP
