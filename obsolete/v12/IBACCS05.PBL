            ADD      ONE,SECTOR
          DO
          CALL     DP02L000
          GOTO     LP02L999
LP02L105
LP02L999  RETURN
.--------------------------------------------------------
. Display Previous Page  
.--------------------------------------------------------
PP02L000
.
PP02L100
          MATCH    "CCAW1",NAMSSFFI
          GOTO     PP02L115 IF NOT EQUAL
PP02L105
          MOVE     SP70,CCAW1NXT
          PACK     KEY5,SP1,ONE,SP70
          CALL     TSCCAW11
          CALL     TKCCAW11 * First on Page
          MOVE     KEY4,CCAW1NXT
          CALL     TSCCAW12
          CALL     TPCCAW12 * Get Key
          IF       OVRCD=1
            BEEP
            GOTO      PP02L999
          ENDIF
.
          CALL     CT02L000
.
. Position and Display Page
.--------------------------
PP02L110
          MOVE     CCAW1NXT,KEY4   
          CALL     TSCCAW12
          MOVE     ZERO,SECTOR
          WHILE    SECTOR<=CCAW1SNO
            CALL     TPCCAW12
            ADD      ONE,SECTOR
          DO
          CALL     DP02L000
          GOTO     PP02L999
PP02L115
PP02L999  RETURN
.--------------------------------------------------------
. Display Next Page of Multi Items
.--------------------------------------------------------
NP02L000
          MATCH    "CCAW1",NAMSSFFI
          GOTO     NP02L115 IF NOT EQUAL
NP02L105
          MOVE     SP70,CCAW1NXT
          PACK     KEY5,Z70
          CALL     TSCCAW11
          CALL     TPCCAW11
          MOVE     KEY4,CCAW1NXT
          CALL     TSCCAW12
          CALL     TKCCAW12
          IF       OVRCD=1
            BEEP
            GOTO      NP02L999
          ENDIF
.
          CALL     CT02L000
.
. Position and Display Page
.--------------------------
NP02L110
          MOVE     CCAW1NXT,KEY4   
          CALL     TSCCAW12
          CALL     DP02L000
          GOTO     NP02L999
NP02L115
NP02L999  RETURN
.--------------------------------------------------------
. Clear Page of Multi Items from Temp File
.--------------------------------------------------------
CT02L000
          MATCH    "CCAW1",NAMSSFFI
          GOTO     CT02L115 IF NOT EQUAL
CT02L105
          PACK     KEY5,SP1,ONE,SP70
          CALL     TSCCAW11
          CALL     TKCCAW11
          BRANCH   OVRCD,CT02L110
          CALL     TRCCAW12
          MOVE     SP70,SCTMROW
          MOVE     SP70,SCTMCOL
          CALL     TUCCAW12
          GOTO     CT02L105
CT02L110
          GOTO     CT02L999
CT02L115
CT02L999  RETURN
.----------------------------------
. Clear Pages of Multi Items 
.----------------------------------
CP02L000
          CALL     CT02L000
          PACK     KEY15,PRGID,SCRID,SP70
          CALL     RSSCPS1
CP02L010
          CALL     RKSCPS1
          BRANCH   OVRCD,CP02L999
          MATCH    PRGID,SCPSPID
          GOTO     CP02L999 IF NOT EQUAL
          MATCH    SCRID,SCPSSID
          GOTO     CP02L999 IF NOT EQUAL
          MATCH    NAMSSFFI,SCPSMTY
          GOTO     CP02L010 IF NOT EQUAL
          MOVE     SCPSITM,FIELDNO
          BRANCH    FIELDNO,CP02L010,CP02L010,CP02L010,CP02L010:
                            CP02L100,CP02L010,CP02L010,CP02L010:
                           CP02L010,CP02L100,CP02L100,CP02L010:
                            CP02L010,CP02L010,CP02L010,CP02L010:
                            CP02L010,CP02L010,CP02L010,CP02L010
.
CP02L100
          PACK     VAR,SP70,SP70
          CALL     DISPITEM  * Display Blank Item
          GOTO     CP02L010
CP02L999  RETURN
.---------------------------------------------------------
. Add Items
.---------------------------------------------------------
AD02L000
          CALL      CP02L000 * Clear Screen of Items
          PACK      KEY20,PRGID,SCRID,ITMSSFFI,SP70
          CALL      RSSCPS2
AD02L010  CALL      RKSCPS2
          BRANCH    OVRCD,AD02L000
          MATCH     PRGID,SCPSPID
          GOTO      AD02L000 IF NOT EQUAL
          MATCH     SCRID,SCPSSID
          GOTO      AD02L000 IF NOT EQUAL
          MATCH     ITMSSFFI,SCPSITM
          GOTO      AD02L000 IF NOT EQUAL
          PACK      SKSCPS20,SCPSPID,SCPSSID,SCPSITM,SCPSROW,SCPSCOL
          MOVE      ACCEPT,SACCEPT
          MOVE      ONE,ACCEPT
          CALL      SM02L000
          MOVE      SACCEPT,ACCEPT
          MOVE      SKSCPS20,KEY20
          CALL      RDSCPS2
          MATCH     "CCAW1",NAMSSFFI
          IF        @EQUAL
            COMPARE   ZERO,CCAW1AFL
            GOTO      AD02L010 IF EQUAL
            CALL      FP02L000
            GOTO      AD02L999
          ENDIF
AD02L999
          RETURN
.---------------------------------------------------------
. Delete Items
.---------------------------------------------------------
DL02L000  
          MOVE      SCSLDES,DISPMSG
          CALL      MESSAGE2
DL02L005  
          CALL      MDELQ000
          BRANCH    EXIT,DL02L900,DL02L900
          MOVE      SCSLITM,FIELDNO
          MATCH     ITMSSFFI,SCSLITM
          GOTO      DL02L100 IF NOT EQUAL
          CALL      SELECTED
          MATCH     "CCAW1",SCPSMTY
          IF        @EQUAL
            PACK      KEY5,SCSLROW,SCSLCOL
            CALL      TRCCAW11
            IF        OVRCD=0
              CALL      TDCCAW12
            ELSE
              BEEP
              GOTO      DL02L005
            ENDIF
            PACK      KEY5,SP1,ONE,SP70
            CALL      TSCCAW11
            CALL      TKCCAW11
            MOVE     KEY4,CCAW1NXT
            CALL      CT02L000
            MOVE      CCAW1NXT,KEY4   
            CALL      TSCCAW12
            CALL      TPCCAW12
            CALL      DP02L000
            GOTO      DL02L005
          ENDIF
DL02L100  MOVE      "Invalid Selection. - ",DISPMSG
          CALL      MESSAGE1
          GOTO      DL02L005
DL02L900  CALL      MESSAGE3
          RETURN
