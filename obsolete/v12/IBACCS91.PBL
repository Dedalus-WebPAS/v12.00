              MOVE      SCPSROW,SCTMROW
              MOVE      SCPSCOL,SCTMCOL
              CALL      TUCCDC22
              MOVE      CCDCPER,VAR      
              CALL      DISPITEM
          CALL      LINDET00
            ENDIF
          ENDIF
          IF        CCDC2END=1
            PACK      VAR,SP70,SP70
            CALL      DISPITEM
          ENDIF
          GOTO      DP01L010
.
DP01L102
          IF        CCDC2END=0
            MOVE      CCDCDIR,VAR      
            CALL      DISPITEM
          ELSE
            PACK      VAR,SP70,SP70
            CALL      DISPITEM
          ENDIF
          GOTO      DP01L010
