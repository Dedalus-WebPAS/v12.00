          IF       @EQUAL
            PACK     KEY20,CCDCHCD,CCDCDPT,CCDCYEAR,CCDCPER,CCDCCTY
            RESET    KEY20,17
            MOVE     KEY20,KEY4
            CALL      TACCDC12
            IF        OVRCD=0
              CALL      TUCCDC12
            ELSE
              CALL      TWCCDC12
            ENDIF
            GOTO      SM02L999
          ENDIF
          GOTO      SM02L999
SM02L990
          MATCH    "CCDC1",NAMSSFFI
          IF       @EQUAL
            MOVE     ONE,CCDC1AFL
          ENDIF
SM02L999
