          ENDIF
TOTANN10  CALL      TKCCDC21
          BRANCH    OVRCD,TOTANN90
          ADD       CCDCDIR,ANNDIR
          GOTO      TOTANN10
TOTANN90  MOVE      "    5",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      ANNDIR,VAR
            CALL      DISPITEM
          ENDIF
          RETURN
LINDET00   PACK      KEY6,CCDCYEAR,CCDCPER
           CALL      RDCCCA1
           RETURN
EANNQ000  MOVE     CCCAYEAR,SAVEYEAR
          MOVE     ZERO,SECTOR
.
          PACK     KEY6,SAVEYEAR,SP70
          CALL     RDCCCA1
EANNQ100  CALL     RKCCCA1
          BRANCH   OVRCD,EANNQ200
          MATCH    SAVEYEAR,CCCAYEAR
          GOTO     EANNQ200 IF NOT EQUAL
          ADD      ONE,SECTOR
          GOTO     EANNQ100
.
EANNQ200  MOVE     "    5",FLDID
          CALL     GETPOSIT
          BRANCH   EXIT,EANNQ999
          MOVE     SP70,VAR
          CALL     DISPITEM
          KEYIN    *PCCOL:CVERT,*NOMASK,*V2LON,*RV,ANNDIR
          MOVE     ANNDIR,VAR
          CALL     DISPITEM
.
          CALL     DELALL00
.
          ASSIGN   (ANNDIR/SECTOR),TOTAL
          ASSIGN   ANNDIR-(TOTAL*SECTOR)+TOTAL,F12P2
          MOVE     ONE,KEYFLAG
.
          PACK     KEY6,SAVEYEAR,SP70
          CALL     RDCCCA1
EANNQ300  CALL     RKCCCA1
          BRANCH   OVRCD,EANNQ400
          MATCH    SAVEYEAR,CCCAYEAR
          GOTO     EANNQ400 IF NOT EQUAL
          MOVE     CCCAPER,KEY2
          PACK     KEY24,CCHCHCD,CCDPDPT,CCCTCTY,CCCAYEAR,CCCAPER
          UNPACK   KEY24,CCDCHCD,CCDCDPT,CCDCCTY,CCDCYEAR,CCDCPER
          CALL     RDCCDC2
          IF       OVRCD=1
            MOVE      ZERO,CCDCIND
            MOVE      ZERO,CCDCCAL
            MOVE      ZERO,CCDCDBU
            MOVE      ZERO,CCDCIBU
            MOVE      ZERO,CCDCCBU
            MOVE      ZERO,CCDCFBU
            MOVE      SP70,CCDCSPA
          ENDIF
          ASSIGN    ANNDIR/SECTOR,CCDCDIR
          IF        KEYFLAG=1
            MOVE      ZERO,KEYFLAG
            MOVE      F12P2,CCDCDIR
          ENDIF
          MOVE      SP70,SCTMROW
          MOVE      SP70,SCTMCOL
          CALL      TWCCDC22
          GOTO      EANNQ300
.
EANNQ400  MOVE     SAVEYEAR,CCCAYEAR
EANNQ999  RETURN
.
DELALL00  MOVE      SP70,KEY2
          CALL      TRCCDC22
          IF        OVRCD=0
            CALL      TDCCDC22
          ENDIF
DELALL10  CALL      TKCCDC22
          BRANCH    OVRCD,DELALL99
          CALL      TDCCDC22
          CALL      TSCCDC22
          GOTO      DELALL10
DELALL99  RETURN
EDEPT000  MATCH    SP70,SAVEDPT
          IF       @EQUAL
            MOVE     CCHCHCD,CCDPHCD
            CALL     KCCDPA00
            BRANCH   EXIT,EDEPT999,EDEPT999
            MOVE     CCDPDPT,SAVEDPT
          ELSE
            PACK     KEY10,CCHCHCD,SAVEDPT
            CALL     RDCCDP1
            IF       OVRCD=1
              MOVE     SP70,SAVEDPT
              GOTO     EDEPT000
            ENDIF
          ENDIF
.
EDEPT999  RETURN

ETYPE000  MOVE     CCHCHCD,CCDAHCD
          MOVE     CCDPDPT,CCDADPT
          CALL     KCCDAA00
          BRANCH   EXIT,ETYPE900,ETYPE900
          MOVE     CCDACTY,CCDCCTY
          MOVE     CCDACTY,KEY4
          CALL     RDCCCT1
          GOTO     ETYPE999
ETYPE900  MOVE     SP70,SAVEDPT
ETYPE999  RETURN
