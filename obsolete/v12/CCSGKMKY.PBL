.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSGKMKY
.   Routine   :   HCCGKA00 G/L Key             
.   Return    :   CCECECC 
.-------------------------------------------------------------------------------
HCCGKA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "30",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCGKA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCGKA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"General Ledger Key" 
.
          MOVE      STDCOL01,STDCOL02
          ADD       "21",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HCCGKA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCGKKEY 
          PACK      KEY20,CCGKKEY,SP70                                          
.                                                                               
.                                                                               
          CALL      RSCCGK1 
HCCGKA10
          CALL      RKCCGK1 
          BRANCH    OVRCD,HCCGKA15
          PACK      KEY20,CCGKKEY,SP70                                          
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HCCGKA12
          CALL      HCCGKA60
          BRANCH    OVRCD,HCCGKA10
HCCGKA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY20,XITEMNO,ST1,ST2,ST3,ST4,ST5:                          
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCGKKEY 
          DISPLAY   *PSTDCOL02:STDROW,CCGKDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCGKA10 IF NOT EQUAL
HCCGKA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCGKA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCGKA30,HCCGKA35,HCCGKA40,HCCGKA45,HCCGKA04:
                              HCCGKA91,HCCGKA50
          GOTO      HCCGKA92
HCCGKA30
          CALL      RKCCGK1 
          BRANCH    OVRCD,HCCGKA85
          PACK      KEY20,CCGKKEY,SP70                                          
.                                                                               
.                                                                               
          CALL      RPCCGK1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCGKA10
HCCGKA35
          MOVE      ST1,KEY20
          CALL      RDCCGK1 
          CALL      RPCCGK1 
          BRANCH    OVRCD,HCCGKA85
          PACK      KEY20,CCGKKEY,SP70                                          
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCGKA36
          CALL      RPCCGK1 
          BRANCH    OVRCD,HCCGKA37
          PACK      KEY20,CCGKKEY,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCGKA36 IF NOT EQUAL
HCCGKA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCGKA10
HCCGKA40
          MOVE      ST1,KEY20
          CALL      RDCCGK1 
          CALL      RPCCGK1 
          BRANCH    OVRCD,HCCGKA85
          PACK      KEY20,CCGKKEY,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCGKA08
HCCGKA45
          CALL      RKCCGK1 
          BRANCH    OVRCD,HCCGKA85
          PACK      KEY20,CCGKKEY,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY20,Z70                                                   
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCGK1 
HCCGKA46
          CALL      RPCCGK1 
          BRANCH    OVRCD,HCCGKA47
          PACK      KEY20,CCGKKEY,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCGKA46 IF NOT EQUAL
HCCGKA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCGKA10
HCCGKA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCGKA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCGKA08
HCCGKA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCGKA08
HCCGKA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCGKA61 IF EQUAL
          MATCH     SCHFLD01,CCGKKEY 
          GOTO      HCCGKA72 IF NOT EQUAL
HCCGKA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCGKA62 IF EQUAL
          MATCH     SCHFLD02,CCGKDES 
          GOTO      HCCGKA72 IF NOT EQUAL
HCCGKA62
HCCGKA63
HCCGKA64
HCCGKA65
HCCGKA66
HCCGKA67
HCCGKA68
HCCGKA69
HCCGKA70
          GOTO      HCCGKA75
HCCGKA72  MOVE      ONE,OVRCD
HCCGKA75  RETURN
HCCGKA85
          LOAD      KEY20,XITEMNO,ST1,ST2,ST3,ST4,ST5:                          
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY20,CCGKKEY                                               
          CALL      RDCCGK1 
HCCGKA90
.                                                                               
          BEEP
          GOTO      HCCGKA20
HCCGKA91
          MOVE      SP70,CCGKDES 
          MOVE      SP70,CCGKSPA 
          MOVE      SP70,KEY20
          MOVE      SP70,CCGKKEY 
          GOTO      HCCGKA96
HCCGKA92
          MOVE      SP70,KEY20
          MOVE      SP70,CCGKKEY 
          LOAD      KEY20,CCITEM,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY20,CCGKKEY                                               
HCCGKA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCGKA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSGKMKY
.   Routine   :   KCCGKA00 Keyin G/L Key             
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCGKA00 TOPIC     "CCGKKEY "
          MOVE      UNDLN70,CCGKKEY 
          KEYIN     *PCCOL:CVERT,*DV,CCGKKEY:                                   
                    *PCCOL:CVERT,*V2LON,CCGKKEY:                                
                    *PCCOL:CVERT,*DV,CCGKKEY 
.
          ENDSET    CCGKKEY 
          APPEND    SP70,CCGKKEY 
          RESET     CCGKKEY 
.
          MATCH     SP70,CCGKKEY 
          GOTO      KCCGKA90 IF EQUAL
.
          CMATCH    EXITCHAR,CCGKKEY 
          GOTO      KCCGKA95 IF EQUAL
.
          CMATCH    QUEST,CCGKKEY 
          GOTO      KCCGKA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCCGKA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCGKKEY 
          APPEND    SP70,CCGKKEY 
          RESET     CCGKKEY 
.
          MATCH     SP70,CCGKKEY 
          GOTO      KCCGKA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CCGKKEY                                 
KCCGKA10
          PACK      KEY20,CCGKKEY                                               
          CALL      RACCGK1 
          BRANCH    OVRCD,KCCGKA80
          BRANCH    CKEYTYP,KCCGKA75
          CALL      RDCCGK1 
KCCGKA70
          MOVE      ZERO,EXIT
          GOTO      KCCGKA99
KCCGKA75
          MOVE      "G/L Key Already Exist - ",DISPMSG                                               
          CALL      MESSAGE1
          GOTO      KCCGKA00
KCCGKA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCGKA99,KCCGKA99
          MOVE      "G/L Key Does Not Exist - ",DISPMSG                                              
          CALL      MESSAGE1
          GOTO      KCCGKA00
KCCGKA90
          MOVE      ONE,EXIT
          GOTO      KCCGKA99
KCCGKA95
          MOVE      TWO,EXIT
KCCGKA99 TOPIC    PRGID
          RETURN
