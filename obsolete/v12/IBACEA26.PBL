.------------------------------------------------------------
. Print Default Mappings
.------------------------------------------------------------
REPT0000  DISPLAY   *P1:24,*EL,"Printing Report : ";
          MOVE      "64",SECTOR
          READ      CONTROLF,SECTOR;*59,CCCOCD1,CCCOCD2,CCCOCD3,CCCOCD4:
                                        CCCOCD5,CCCOIA1,CCCOIA2,CCCOIA3:
                                        CCCOIM1,CCCOIM2,CCCOIM3
          MOVE      "99",CLNO
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
.
          MOVE      SP70,LASTDTY
          MOVE      SP70,LASTSPC
          PACK      KEY11,SP70
          CALL      RDCEDE1
          IF        OVRCD=0
            CALL      RPCEDE1
          ENDIF
.
REPT1000  CALL      RKCEDE1
          BRANCH    OVRCD,REPT9000
.
          MATCH     LASTDTY,CEDEDTY
          IF        !@EQUAL
            CALL      HEAD0000
            MOVE      CEDEDTY,LASTDTY
          ENDIF
.
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          MATCH     LASTSPC,CEDESPC
          IF        !@EQUAL
            MOVE       CEDESPC,KEY3
            CALL       RDCESP1
            PRINT      CEDESPC,SP1,CESPDES;
            MOVE       CEDESPC,LASTSPC
          ENDIF
.
          MOVE       CEDEPSC,KEY7
          CALL       RDCEPS1
          MOVE       "99,999.99",PRIQTY
          MOVE       "99,999.99",SECQTY
          MOVE       "9999,999.99",PRICST
          MOVE       "9999,999.99",SECCST
          FEDIT      CEDEQTY1,PRIQTY
          FEDIT      CEDEQTY2,SECQTY
          ASSIGN     (CEPSST1+CEPSST2+CEPSST3+CEPSST4+CEPSST5)*CEDEQTY1,F12P2
          FEDIT      F12P2,PRICST
          ASSIGN     (CEPSST1+CEPSST2+CEPSST3+CEPSST4+CEPSST5)*CEDEQTY2,F12P2
          FEDIT      F12P2,SECCST
.
          PRINT     *47,CEDEPSC,SP1,CEPSDES,SP1:
                    PRIQTY,PRICST," |",SECQTY,SECCST
          ADD       ONE,CLNO
          GOTO      REPT1000
.
REPT9000  CALL      UND132
          PRINT     *45,"***** End of Report *****"
          RETURN
