.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSATYKY
.   Routine   :   HCCATA00 Allocation          
.   Return    :           
.-------------------------------------------------------------------------------
HCCATA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "05",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCATA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCATA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Code"               
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 5",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Allocation"         
.
          MOVE      STDCOL02,STDCOL03
          ADD       "35",STDCOL03
HCCATA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCATATY 
          PACK      KEY4,CCATATY,SP70                                           
.                                                                               
.                                                                               
          CALL      RSCCAT1 
HCCATA10
          CALL      RKCCAT1 
          BRANCH    OVRCD,HCCATA15
          PACK      KEY4,CCATATY,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HCCATA12
          CALL      HCCATA60
          BRANCH    OVRCD,HCCATA10
HCCATA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY4,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCATATY 
          DISPLAY   *PSTDCOL02:STDROW,CCATDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCATA10 IF NOT EQUAL
HCCATA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCATA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCATA30,HCCATA35,HCCATA40,HCCATA45,HCCATA04:
                              HCCATA91,HCCATA50
          GOTO      HCCATA92
HCCATA30
          CALL      RKCCAT1 
          BRANCH    OVRCD,HCCATA85
          PACK      KEY4,CCATATY,SP70                                           
.                                                                               
.                                                                               
          CALL      RPCCAT1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCATA10
HCCATA35
          MOVE      ST1,KEY4 
          CALL      RDCCAT1 
          CALL      RPCCAT1 
          BRANCH    OVRCD,HCCATA85
          PACK      KEY4,CCATATY,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCATA36
          CALL      RPCCAT1 
          BRANCH    OVRCD,HCCATA37
          PACK      KEY4,CCATATY,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCATA36 IF NOT EQUAL
HCCATA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCATA10
HCCATA40
          MOVE      ST1,KEY4 
          CALL      RDCCAT1 
          CALL      RPCCAT1 
          BRANCH    OVRCD,HCCATA85
          PACK      KEY4,CCATATY,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCATA08
HCCATA45
          CALL      RKCCAT1 
          BRANCH    OVRCD,HCCATA85
          PACK      KEY4,CCATATY,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY4,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCAT1 
HCCATA46
          CALL      RPCCAT1 
          BRANCH    OVRCD,HCCATA47
          PACK      KEY4,CCATATY,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCATA46 IF NOT EQUAL
HCCATA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCATA10
HCCATA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCATA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCATA08
HCCATA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCATA08
HCCATA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCATA61 IF EQUAL
          MATCH     SCHFLD01,CCATATY 
          GOTO      HCCATA72 IF NOT EQUAL
HCCATA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCATA62 IF EQUAL
          MATCH     SCHFLD02,CCATDES 
          GOTO      HCCATA72 IF NOT EQUAL
HCCATA62
HCCATA63
HCCATA64
HCCATA65
HCCATA66
HCCATA67
HCCATA68
HCCATA69
HCCATA70
          GOTO      HCCATA75
HCCATA72  MOVE      ONE,OVRCD
HCCATA75  RETURN
HCCATA85
          LOAD      KEY4,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY4,CCATATY                                                
          CALL      RDCCAT1 
HCCATA90
.                                                                               
          BEEP
          GOTO      HCCATA20
HCCATA91
          MOVE      SP70,CCATDES 
          MOVE      ZERO,CCATTQY 
          MOVE      SP70,CCATSPA 
          MOVE      SP70,KEY4 
          MOVE      SP70,CCATATY 
          GOTO      HCCATA96
HCCATA92
          MOVE      SP70,KEY4 
          MOVE      SP70,CCATATY 
          LOAD      KEY4,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY4,CCATATY                                                
HCCATA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCATA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSATYKY
.   Routine   :   KCCATA00 Keyin Allocation          
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCATA00 TOPIC     "CCATATY "
          MOVE      UNDLN70,CCATATY 
          KEYIN     *PCCOL:CVERT,*DV,CCATATY:                                   
                    *PCCOL:CVERT,*V2LON,CCATATY:                                
                    *PCCOL:CVERT,*DV,CCATATY 
.
          ENDSET    CCATATY 
          APPEND    SP70,CCATATY 
          RESET     CCATATY 
.
          MATCH     SP70,CCATATY 
          GOTO      KCCATA90 IF EQUAL
.
          CMATCH    EXITCHAR,CCATATY 
          GOTO      KCCATA95 IF EQUAL
.
          CMATCH    QUEST,CCATATY 
          GOTO      KCCATA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCCATA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCATATY 
          APPEND    SP70,CCATATY 
          RESET     CCATATY 
.
          MATCH     SP70,CCATATY 
          GOTO      KCCATA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CCATATY                                 
KCCATA10
          PACK      KEY4,CCATATY                                                
          CALL      RACCAT1 
          BRANCH    OVRCD,KCCATA80
          BRANCH    CKEYTYP,KCCATA75
          CALL      RDCCAT1 
KCCATA70
          MOVE      ZERO,EXIT
          GOTO      KCCATA99
KCCATA75
          MOVE      "Allocation Already Exist - ",DISPMSG                                            
          CALL      MESSAGE1
          GOTO      KCCATA00
KCCATA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCATA99,KCCATA99
          MOVE      "Allocation Does Not Exist - ",DISPMSG                                           
          CALL      MESSAGE1
          GOTO      KCCATA00
KCCATA90
          MOVE      ONE,EXIT
          GOTO      KCCATA99
KCCATA95
          MOVE      TWO,EXIT
KCCATA99 TOPIC    PRGID
          RETURN
