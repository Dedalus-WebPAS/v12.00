          ENDIF
          MOVE     ZERO,SECTOR
          WHILE    SECTOR<FMBN2SNO
            CALL     RPFMBN2 
            IF       OVRCD=1
              MOVE     FMBN2SNO,SECTOR
            ENDIF
            PACK     FMBN2MKY,FMBNBNK,FMBNTYP,FMBNREF,FMBNUNQ                                                
            MATCH    FMBN2EKY,FMBN2MKY
            IF       !@EQUAL
              MOVE     FMBN2SNO,SECTOR
