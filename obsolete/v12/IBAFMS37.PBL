          ENDIF
          WHILE    SECTOR<=FMBN2CNT
            CALL     RPFMBN2 
            IF       OVRCD=1
              MOVE     FMBN2SNO,SECTOR
            ENDIF
            PACK     FMBN2MKY,FMBNBNK,FMBNTYP,FMBNREF,FMBNUNQ                                                
            MATCH    FMBN2EKY,FMBN2MKY
            IF       !@EQUAL
              MOVE     FMBN2SNO,SECTOR
            ENDIF
            ADD      ONE,SECTOR
          DO
          CALL     DP01L000
          GOTO     PP01L999
PP01L115
          BEEP
          MOVE     ZERO,SECTOR
          WHILE    SECTOR<FMBN2SNO
            CALL     RKFMBN2 
            IF       OVRCD=1
              MOVE     FMBN2SNO,SECTOR
            ENDIF
            PACK     FMBN2MKY,FMBNBNK,FMBNTYP,FMBNREF,FMBNUNQ                                                
            MATCH    FMBN2EKY,FMBN2MKY
            IF       !@EQUAL
              MOVE     FMBN2SNO,SECTOR
            ENDIF
            ADD      ONE,SECTOR
          DO
          GOTO     PP01L999
PP01L120
PP01L999  RETURN
