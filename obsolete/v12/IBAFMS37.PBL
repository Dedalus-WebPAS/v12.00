.------------------------------------------------
. Print Payment Enquiry
.------------------------------------------------
PRNT0000  CLOCK     TIME,CTIMEIS
          MOVE      ZERO,CPAGENO
          MOVE      "60",CLNO
.
          PACK      KEY36,FMCHCHQ,ZERO,REFERENC,SP70    * go to start of recs
          CALL      RSFMBN2
.
PRNT1000  CALL      RKFMBN2            * get next record
          BRANCH    OVRCD,PRNT9999     * no more records ?
          CALL      VALB0000           * valid record for screen B
          BRANCH    OVRCD,PRNT9999,PRNT1000 * 0=ok,1=no more recs,2=get next rec
.
          COMPARE   "56",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          MOVE      FORMATP2,DIM20
          FEDIT     FMBNAMT,DIM20
          MOVE      FMTRDESC,KEY20
          PRINT     *1,FMBNLED,SLASH,FMBNACC,*17,FMTRCRED,*23,FMTRDOCR:
                    *39,KEY20,*61,DIM20
          GOTO      PRNT1000
.
PRNT9999  RETURN
.
HEAD0000  CALL      HEAD80
          MOVE      FORMATP2,DIM20
          FEDIT     KAMOUNT,DIM20
          PRINT     *N,*5,"Cheque Account       : ",FMCHCHQ,SP2,FMCHDES:
                    *N,*5,"Payment Number       : ",REFERENC
.
          UNPACK    TRNDATE,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
.
          PRINT     *N,*5,"Transaction Date     : ",CPCDATE:
                    *N,*5,"Status               : ",STATUS
.
          UNPACK    CANDATE,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
.
          PRINT     *5,"Presented/Cancel Date: ",CPCDATE:
                    *N,*5,"Amount               : ",DIM20:
                    *N,*5,"Creditor             : ",APMACRD,SP2,APMACN1:
                    *N,*N,*1,"Ledger/Bank":
                    *17,"Cred.":
                    *23,"Invoice No":
                    *39,"Transaction Description":
                    *74,"Amount":
                    *N,*1,"-----------":
                    *17,"-----":
                    *23,"----------":
                    *39,"-----------------------":
                    *74,"------"
          ADD       TEN1,CLNO
HEAD9999  RETURN
.
.
VALB0000  MATCH     FMCHCHQ,FMBNBNK
          GOTO      VALB9500 IF NOT EQUAL   * correct account ?
          MATCH     REFERENC,FMBNREF
          GOTO      VALB9500 IF NOT EQUAL   * correct cheque ?
          MATCH     SP70,FMBNLED
          GOTO      VALB9700 IF EQUAL       * ledger blank ?
.
          PACK      FILENAME,FMST,FMBNFYR,SP70
          REP       " 0",FILENAME
          MATCH     FILENAME,FILENAMB
          GOTO      VALB1000 IF EQUAL       * file already open ?
.
          MOVE      SP70,FILENAMB           * clear file currently opened
          MOVE      ZERO,OVRCD              * open file
          TRAP      OVERCOND IF IO
          OPEN      FMSTRNA1,FILENAME
          TRAPCLR   IO
          BRANCH    OVRCD,VALB9700          * file not found ?
          MOVE      FILENAME,FILENAMB        * file found, save filename
.
VALB1000  PACK      KEY10,FMBNBCH,FMBNLNO,SP70
          CALL      RDFMTR1
          BRANCH    OVRCD,VALB9700          * rec not found, skip this rec
.
VALB9000  MOVE      ZERO,OVRCD         * continue
          GOTO      VALB9999
.
VALB9500  MOVE      ONE,OVRCD          * no more records
          GOTO      VALB9999
.
VALB9700  MOVE      TWO,OVRCD          * get next record
.
VALB9999  RETURN
.
.
