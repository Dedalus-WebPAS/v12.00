.---------------------------------------------------------------------------
.   System    :   Web System Table                   
.   Include   :   WEBSPGKY
.   Routine   :   HWBSPA00 Program             
.   Return    :   WBSOOPT 
.-------------------------------------------------------------------------------
HWBSPA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HWBSPA04
          CALL      GSCRN000
          BRANCH    EXIT,HWBSPA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Program"            
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 9",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "40",STDCOL03
HWBSPA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,WBSPPRG 
          PACK      KEY8,WBSPPRG,SP70                                           
.                                                                               
.                                                                               
          CALL      RSWBSP1 
HWBSPA10
          CALL      RKWBSP1 
          BRANCH    OVRCD,HWBSPA15
          PACK      KEY8,WBSPPRG,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HWBSPA12
          CALL      HWBSPA60
          BRANCH    OVRCD,HWBSPA10
HWBSPA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY8,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,WBSPPRG 
          DISPLAY   *PSTDCOL02:STDROW,WBSPNAM 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HWBSPA10 IF NOT EQUAL
HWBSPA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HWBSPA20
          CALL      HSCRI000
          BRANCH    EXIT,HWBSPA30,HWBSPA35,HWBSPA40,HWBSPA45,HWBSPA04:
                              HWBSPA91,HWBSPA50
          GOTO      HWBSPA92
HWBSPA30
          CALL      RKWBSP1 
          BRANCH    OVRCD,HWBSPA85
          PACK      KEY8,WBSPPRG,SP70                                           
.                                                                               
.                                                                               
          CALL      RPWBSP1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HWBSPA10
HWBSPA35
          MOVE      ST1,KEY8 
          CALL      RDWBSP1 
          CALL      RPWBSP1 
          BRANCH    OVRCD,HWBSPA85
          PACK      KEY8,WBSPPRG,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HWBSPA36
          CALL      RPWBSP1 
          BRANCH    OVRCD,HWBSPA37
          PACK      KEY8,WBSPPRG,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HWBSPA36 IF NOT EQUAL
HWBSPA37
          MOVE      ZERO,XITEMNO
          GOTO      HWBSPA10
HWBSPA40
          MOVE      ST1,KEY8 
          CALL      RDWBSP1 
          CALL      RPWBSP1 
          BRANCH    OVRCD,HWBSPA85
          PACK      KEY8,WBSPPRG,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HWBSPA08
HWBSPA45
          CALL      RKWBSP1 
          BRANCH    OVRCD,HWBSPA85
          PACK      KEY8,WBSPPRG,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY8,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSWBSP1 
HWBSPA46
          CALL      RPWBSP1 
          BRANCH    OVRCD,HWBSPA47
          PACK      KEY8,WBSPPRG,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HWBSPA46 IF NOT EQUAL
HWBSPA47
          MOVE      ZERO,XITEMNO
          GOTO      HWBSPA10
HWBSPA50  COMPARE   ZERO,CSEARCH
          GOTO      HWBSPA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HWBSPA08
HWBSPA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HWBSPA08
HWBSPA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HWBSPA61 IF EQUAL
          MATCH     SCHFLD01,WBSPPRG 
          GOTO      HWBSPA72 IF NOT EQUAL
HWBSPA61
          MATCH     SP70,SCHFLD02
          GOTO      HWBSPA62 IF EQUAL
          MATCH     SCHFLD02,WBSPNAM 
          GOTO      HWBSPA72 IF NOT EQUAL
HWBSPA62
HWBSPA63
HWBSPA64
HWBSPA65
HWBSPA66
HWBSPA67
HWBSPA68
HWBSPA69
HWBSPA70
          GOTO      HWBSPA75
HWBSPA72  MOVE      ONE,OVRCD
HWBSPA75  RETURN
HWBSPA85
          LOAD      KEY8,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY8,WBSPPRG                                                
          CALL      RDWBSP1 
HWBSPA90
.                                                                               
          BEEP
          GOTO      HWBSPA20
HWBSPA91
          MOVE      SP70,WBSPNAM 
          MOVE      SP70,WBSPFIFO
          MOVE      SP70,WBSPOUTD
          MOVE      SP70,WBSPDEFT
          MOVE      SP70,WBSPUR1 
          MOVE      SP70,WBSPUR2 
          MOVE      SP70,WBSPUD1 
          MOVE      SP70,WBSPUD2 
          MOVE      SP70,WBSPUY1 
          MOVE      SP70,WBSPUY2 
          MOVE      SP70,WBSPSPA 
          MOVE      SP70,KEY8 
          MOVE      SP70,WBSPPRG 
          GOTO      HWBSPA96
HWBSPA92
          MOVE      SP70,KEY8 
          MOVE      SP70,WBSPPRG 
          LOAD      KEY8,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY8,WBSPPRG                                                
HWBSPA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HWBSPA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Web System Table                   
.   Include   :   WEBSPGKY
.   Routine   :   KWBSPA00 Keyin Program             
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KWBSPA00 TOPIC     "WBSPPRG "
          MOVE      UNDLN70,WBSPPRG 
          KEYIN     *PCCOL:CVERT,*DV,WBSPPRG:                                   
                    *PCCOL:CVERT,*V2LON,WBSPPRG:                                
                    *PCCOL:CVERT,*DV,WBSPPRG 
.
          ENDSET    WBSPPRG 
          APPEND    SP70,WBSPPRG 
          RESET     WBSPPRG 
.
          MATCH     SP70,WBSPPRG 
          GOTO      KWBSPA90 IF EQUAL
.
          CMATCH    EXITCHAR,WBSPPRG 
          GOTO      KWBSPA95 IF EQUAL
.
          CMATCH    QUEST,WBSPPRG 
          GOTO      KWBSPA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HWBSPA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    WBSPPRG 
          APPEND    SP70,WBSPPRG 
          RESET     WBSPPRG 
.
          MATCH     SP70,WBSPPRG 
          GOTO      KWBSPA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,WBSPPRG                                 
KWBSPA10
          PACK      KEY8,WBSPPRG                                                
          CALL      RAWBSP1 
          BRANCH    OVRCD,KWBSPA80
          BRANCH    CKEYTYP,KWBSPA75
          CALL      RDWBSP1 
KWBSPA70
          MOVE      ZERO,EXIT
          GOTO      KWBSPA99
KWBSPA75
          MOVE      "Program Already Exist - ",DISPMSG                                               
          CALL      MESSAGE1
          GOTO      KWBSPA00
KWBSPA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KWBSPA99,KWBSPA99
          MOVE      "Program Does Not Exist - ",DISPMSG                                              
          CALL      MESSAGE1
          GOTO      KWBSPA00
KWBSPA90
          MOVE      ONE,EXIT
          GOTO      KWBSPA99
KWBSPA95
          MOVE      TWO,EXIT
KWBSPA99 TOPIC    PRGID
          RETURN
