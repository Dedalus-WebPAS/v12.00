          BRANCH   OVRCD,LDRG9100
.
          MOVE     ZERO,OVRCD
          TRAP     OVERCOND IF IO
          OPEN     CCSXHDAF,"ccsxhdaf"
          TRAPCLR  IO
          BRANCH   OVRCD,LDRG9200
.
LDRG1000  READ     CCSDRGAF,SEQ;CCDGDRGC,CCDGDES,CCDGMDC,CCDGWAR,CCDGTYP:
                        CCDGSDR,CCDGSDT,CCDGLOW,CCDGHIG,CCDGSLOS,CCDGSWGT:
                        CCDGNAT,CCDGNLOS,CCDGSPA
          GOTO     LDRG1900 IF OVER
          MOVE     CCDGDRGC,KEY4
          CALL     RACCDG1
          IF       OVRCD=0
            CALL     UPCCDG1
          ELSE
            CALL     WRCCDG1
          ENDIF
          GOTO     LDRG1000
.
LDRG1900
.
LDRG2000  READ     CCSXHDAF,SEQ;CCXHSTY,CCXHKEY,CCXHDES,CCXHSUM,CCXHSPA
