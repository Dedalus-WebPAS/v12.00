          PACK     KEY13,CCXHSTY,CCXHKEY,SP70
          CALL     RDCCXH1
          IF       OVRCD=0
            MOVE     KEY35,CCXHDES
            CALL     UPCCXH1
          ELSE
            CALL     WRCCXH1
          ENDIF
          GOTO     LDRG2000
.
LDRG2900  MOVE     "Load Complete - ",DISPMSG
          BEEP
          CALL     MESSAGE1
          GOTO     LDRG9999
.
