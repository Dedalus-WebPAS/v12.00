.---------------------------------------------------------------------------
.   System    :   Financial Management System
.   Include   :   FMSNRBKY
.   Routine   :   HFMNBA00 Total Cost Centre
.   Return    :
.-------------------------------------------------------------------------------
HFMNBA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "30",HLEF
            MOVE      "80",HRIG
          ENDIF
HFMNBA04
          CALL      GSCRN000
          BRANCH    EXIT,HFMNBA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Total Cost Centre"
.
          MOVE      STDCOL01,STDCOL02
          ADD       "14",STDCOL02
          MOVE      STDCOL02,STDCOL03
          ADD       "42",STDCOL03
          DISPLAY   *PSTDCOL03:STDROW,*V2LON,*ULON,"Level"
.
          MOVE      STDCOL03,STDCOL04
          ADD       " 7",STDCOL04
          DISPLAY   *PSTDCOL04:STDROW,*V2LON,*ULON,"Print Code"
.
          MOVE      STDCOL04,STDCOL05
          ADD       "12",STDCOL05
HFMNBA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,FMNBTCOD
          PACK      KEY15,FMNBCODE,FMNBTCOD,SP70
          PACK      SAVKEY80,FMNBCODE,FMNBTCOD,SP70
          SETLPTR   SAVKEY80,3
          CALL      RSFMNB1
HFMNBA10
          CALL      RKFMNB1
          BRANCH    OVRCD,HFMNBA15
          PACK      KEY15,FMNBCODE,FMNBTCOD,SP70
          MATCH     SAVKEY80,KEY15
          GOTO      HFMNBA15 IF NOT EQUAL
          BRANCH    CSEARCH,HFMNBA12
          CALL      HFMNBA60
          BRANCH    OVRCD,HFMNBA10
HFMNBA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY15,XITEMNO,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,FMNBTCOD
          DISPLAY   *PSTDCOL02:STDROW,FMNBDESC
          DISPLAY   *PSTDCOL03:STDROW,FMNBTLEV
          DISPLAY   *PSTDCOL04:STDROW,FMNBPRTC
          COMPARE   MAXITEM,XITEMNO
          GOTO      HFMNBA10 IF NOT EQUAL
HFMNBA15
          UNPACK    SAVKEY80,FMNBCODE,FMNBTCOD
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HFMNBA20
          CALL      HSCRI000
          BRANCH    EXIT,HFMNBA30,HFMNBA35,HFMNBA40,HFMNBA45,HFMNBA04:
                              HFMNBA91,HFMNBA50
          GOTO      HFMNBA92
HFMNBA30
          CALL      RKFMNB1
          BRANCH    OVRCD,HFMNBA85
          PACK      KEY15,FMNBCODE,FMNBTCOD,SP70
          MATCH     SAVKEY80,KEY15
          GOTO      HFMNBA85 IF NOT EQUAL
          CALL      RPFMNB1
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMNBA10
HFMNBA35
          MOVE      ST1,KEY15
          CALL      RDFMNB1
          CALL      RPFMNB1
          BRANCH    OVRCD,HFMNBA85
          PACK      KEY15,FMNBCODE,FMNBTCOD,SP70
          MATCH     SAVKEY80,KEY15
          GOTO      HFMNBA85 IF NOT EQUAL
          MATCH     SP70,FMNBTCOD
          GOTO      HFMNBA85 IF EQUAL
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HFMNBA36
          CALL      RPFMNB1
          BRANCH    OVRCD,HFMNBA37
          PACK      KEY15,FMNBCODE,FMNBTCOD,SP70
          MATCH     SAVKEY80,KEY15
          GOTO      HFMNBA37 IF NOT EQUAL
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HFMNBA36 IF NOT EQUAL
HFMNBA37
          MOVE      ZERO,XITEMNO
          GOTO      HFMNBA10
HFMNBA40
          MOVE      ST1,KEY15
          CALL      RDFMNB1
          CALL      RPFMNB1
          BRANCH    OVRCD,HFMNBA85
          PACK      KEY15,FMNBCODE,FMNBTCOD,SP70
          MATCH     SAVKEY80,KEY15
          GOTO      HFMNBA85 IF NOT EQUAL
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMNBA08
HFMNBA45
          CALL      RKFMNB1
          BRANCH    OVRCD,HFMNBA85
          PACK      KEY15,FMNBCODE,FMNBTCOD,SP70
          MATCH     SAVKEY80,KEY15
          GOTO      HFMNBA85 IF NOT EQUAL
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY15,SAVKEY80,Z70
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSFMNB1
HFMNBA46
          CALL      RPFMNB1
          BRANCH    OVRCD,HFMNBA47
          PACK      KEY15,FMNBCODE,FMNBTCOD,SP70
          MATCH     SAVKEY80,KEY15
          GOTO      HFMNBA47 IF NOT EQUAL
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HFMNBA46 IF NOT EQUAL
HFMNBA47
          MOVE      ZERO,XITEMNO
          GOTO      HFMNBA10
HFMNBA50  COMPARE   ZERO,CSEARCH
          GOTO      HFMNBA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          KEYIN     *PSTDCOL03:STDROW,*V2LON,SCHFLD03
          RESET     SCHFLD03
          KEYIN     *PSTDCOL04:STDROW,*V2LON,SCHFLD04
          RESET     SCHFLD04
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMNBA08
HFMNBA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMNBA08
HFMNBA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HFMNBA61 IF EQUAL
          MATCH     SCHFLD01,FMNBTCOD
          GOTO      HFMNBA72 IF NOT EQUAL
HFMNBA61
          MATCH     SP70,SCHFLD02
          GOTO      HFMNBA62 IF EQUAL
          MATCH     SCHFLD02,FMNBDESC
          GOTO      HFMNBA72 IF NOT EQUAL
HFMNBA62
          MATCH     SP70,SCHFLD03
          GOTO      HFMNBA63 IF EQUAL
          MATCH     SCHFLD03,FMNBTLEV
          GOTO      HFMNBA72 IF NOT EQUAL
HFMNBA63
          MATCH     SP70,SCHFLD04
          GOTO      HFMNBA64 IF EQUAL
          MATCH     SCHFLD04,FMNBPRTC
          GOTO      HFMNBA72 IF NOT EQUAL
HFMNBA64
HFMNBA65
HFMNBA66
HFMNBA67
HFMNBA68
HFMNBA69
HFMNBA70
          GOTO      HFMNBA75
HFMNBA72  MOVE      ONE,OVRCD
HFMNBA75  RETURN
HFMNBA85
          LOAD      KEY15,XITEMNO,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY15,FMNBCODE,FMNBTCOD
          CALL      RDFMNB1
HFMNBA90
          UNPACK    SAVKEY80,FMNBCODE,FMNBTCOD
          BEEP
          GOTO      HFMNBA20
HFMNBA91
          MOVE      SP70,FMNBTLEV
          MOVE      SP70,FMNBDESC
          MOVE      SP70,FMNBPRTC
          MOVE      SP70,FMNBSPAR
          MOVE      SP70,KEY15
          MOVE      SP70,FMNBTCOD
          GOTO      HFMNBA96
HFMNBA92
          MOVE      SP70,KEY15
          MOVE      SP70,FMNBTCOD
          LOAD      KEY15,CCITEM,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY15,FMNBCODE,FMNBTCOD
HFMNBA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HFMNBA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Financial Management System
.   Include   :   FMSNRBKY
.   Routine   :   KFMNBA00 Keyin Total Cost Centre
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KFMNBA00
          MOVE      FMSUUD4A,CSTRLEN          * set up string length
          CALL      GSTR0000                  * get code
          PACK      FMNBTCOD,CSTRING,SP70
.
          ENDSET    FMNBTCOD
          APPEND    SP70,FMNBTCOD
          RESET     FMNBTCOD
.
          MATCH     SP70,FMNBTCOD
          GOTO      KFMNBA90 IF EQUAL
.
          CMATCH    EXITCHAR,FMNBTCOD
          GOTO      KFMNBA95 IF EQUAL
.
          CMATCH    QUEST,FMNBTCOD
          GOTO      KFMNBA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HFMNBA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    FMNBTCOD
          APPEND    SP70,FMNBTCOD
          RESET     FMNBTCOD
.
          MATCH     SP70,FMNBTCOD
          GOTO      KFMNBA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,FMNBTCOD
KFMNBA10
          PACK      KEY15,FMNBCODE,FMNBTCOD
          CALL      RAFMNB1
          BRANCH    OVRCD,KFMNBA80
          BRANCH    CKEYTYP,KFMNBA75
          CALL      RDFMNB1
KFMNBA70
          MOVE      ZERO,EXIT
          GOTO      KFMNBA99
KFMNBA75
          MOVE      "Total Cost Centre Already Exists - ",DISPMSG
          CALL      MESSAGE1
          GOTO      KFMNBA00
KFMNBA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KFMNBA99,KFMNBA99
          MOVE      "Total Cost Centre Does Not Exist - ",DISPMSG
          CALL      MESSAGE1
          GOTO      KFMNBA00
KFMNBA90
          MOVE      ONE,EXIT
          GOTO      KFMNBA99
KFMNBA95
          MOVE      TWO,EXIT
KFMNBA99
          RETURN
