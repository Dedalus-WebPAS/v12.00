ELEDG000  BRANCH   FLGIBAGL,ELEDG900
          MOVE     ZERO,CKEYTYP
          MOVE     SP70,SELLED
          CALL     KFMLAA00
          BRANCH   EXIT,ELEDG100,ELEDG999
          MOVE     FMLALEDG,SELLED
          GOTO     ELEDG999
.
ELEDG100  MOVE     SP70,SELLED
          MOVE     "All Ledgers ",FMLADESC
          GOTO     ELEDG999
.
ELEDG900  KEYIN    *PCCOL:CVERT,*V2LON,*RV,SELLED
          MOVE     SP70,FMLADESC
.
ELEDG999  RETURN
