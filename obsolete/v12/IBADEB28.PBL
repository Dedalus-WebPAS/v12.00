          CALL      PRTERR00
          MOVE      ONE,EXIT
          GOTO      CHKGL999
.
CHKGL990  MOVE      ZERO,EXIT
.
CHKGL999  RETURN
.------------------------------------------------------------
. Print Error
.------------------------------------------------------------
PRTERR00  COMPARE   "55",CLNO
          CALL      ERRHED00 IF NOT LESS
          ADD       ONE,ERRORS
.
          MOVE      "Invoice   ",DIM10
          MATCH     "2",DBFDDTY
          IF        @EQUAL
            MOVE      "Cr. Note  ",DIM10
          ENDIF
          MATCH     "3",DBFDDTY
          IF        @EQUAL
            MOVE      "Cash Rec. ",DIM10
          ENDIF
          MATCH     "4",DBFDDTY
          IF        @EQUAL
            MOVE      "Dr.Adjust.",DIM10
          ENDIF
          MATCH     "5",DBFDDTY
          IF        @EQUAL
            MOVE      "Cr.Adjust.",DIM10
          ENDIF
.
          PACK      KEY8,DBFDDEB,SP70
          CALL      RDDBDB1
.
          MOVE      DBFDITM,KEY8
          CALL      RDDBIT1
.
          PRINT     DBFDDEB,SP1,DIM10,SP1,DBFDDOC,"/",DBFDDLN,SP1,DBFDITM,SP1:
                    DBITDES,SP1,ERRMSG
          ADD       ONE,CLNO
          CALL      WRTEXB00
.
PRTERR99  RETURN
.------------------------------------------------------------
. Print Error Page Header
.------------------------------------------------------------
ERRHED00  CALL      HEAD132
          PRINT     *50,"Export Error Report"
          CALL      UND132
          PRINT     "Debtor   Tr. Ty.    Doc Ref.         Sales Item        ":
                    "                           Error Message"
          CALL      UND132
          ADD       "2",CLNO
          RETURN
.------------------------------------------------------------
. Print Error End of Report
.------------------------------------------------------------
ERREND00  COMPARE   ZERO,CPAGENO
          GOTO      ERREND99 IF EQUAL
          CALL      UND132
          PRINT     *50,"*** End of Error Report ***"
ERREND99  RETURN
.------------------------------------------------------------
. Print Debtor Name & Code Heading & Clear Totals
.------------------------------------------------------------
PRDEBT00  MOVE      DBFDDEB,KEY8
          CALL      RDDBDB1
          PRINT     DBDBNA1,"(",DBFDDEB,")"
          ADD       ONE,CLNO
          MOVE      DBFDDEB,LASTDEB
          MOVE      DBDBNA1,LASTDN1
          MOVE      ZERO,DTOTAMT
          MOVE      ZERO,DTOTTAX
          RETURN
.------------------------------------------------------------
. Print Report Detail Line
.------------------------------------------------------------
PRLINE00  COMPARE   "55",CLNO
          CALL      PRHEAD00 IF NOT LESS
          UNPACK    DBFHDDAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          MOVE      "Invoice   ",DIM10
          MATCH     "2",DBFDDTY
          IF        @EQUAL
            MOVE      "Cr. Note  ",DIM10
          ENDIF
          MATCH     "3",DBFDDTY
          IF        @EQUAL
            MOVE      "Cash Rec. ",DIM10
          ENDIF
          MATCH     "4",DBFDDTY
          IF        @EQUAL
            MOVE      "Dr.Adjust.",DIM10
          ENDIF
          MATCH     "5",DBFDDTY
          IF        @EQUAL
            MOVE      "Cr.Adjust.",DIM10
          ENDIF
.
          MATCH     "-",IBGISIGN
          IF        !@EQUAL
            MULT      SEQ,IBGIVALU
          ENDIF
          MOVE      "999,999,999.99",FTOTAL
          FEDIT     IBGIVALU,FTOTAL
          MOVE      DBFDITM,KEY8
          CALL      RDDBIT1
.
          PACK      KEY2,DBFDLED,SP70
          CALL      RDFMLA1
          PACK      DIM30,DBITDES,SP70
.
          PRINT     *5,DIM10,SP1,DBFDDOC,"/",DBFDDLN,SP1,CPCDATE,SP1:
                    DBFDGST,SP1,DIM30,"(",DBFDITM,")":
                    *91,DBFDLED,"/",IBGICRAA,SP1,IBGIACCT,FTOTAL
          ADD       ONE,CLNO
          CALL      WRTEXA00
.
PRLINE20  ADD         IBGIVALU,DTOTAMT
          ADD         IBGIVALU,RTOTAMT
PRLINE99  RETURN
.------------------------------------------------------------
. Print Debtors Total
.------------------------------------------------------------
DEBTOT00  MOVE        "999,999,999.99",FTOTAL
          FEDIT       DTOTAMT,FTOTAL
          CALL        UND132
          PRINT       "Total for ",DBDBNA1,"(",LASTDEB,")",*119,FTOTAL
          ADD         ONE,CLNO
          MOVE        ZERO,DTOTAMT
          CALL        UND132
          RETURN
.------------------------------------------------------------
. Print Report Total
.------------------------------------------------------------
PEND0000  COMPARE     ZERO,CPAGENO
          CALL        PRHEAD00 IF EQUAL
.
          MOVE        "999,999,999,999.99",FGTOTAL
          FEDIT       RTOTAMT,FGTOTAL
          CALL        UND132
          PRINT       "Report Total  ",*115,FGTOTAL
          CALL        UND132
          RETURN
.------------------------------------------------------------
. Print Report Header
.------------------------------------------------------------
PRHEAD00  CALL       HEAD132
          PRINT      "Export File          : ",EXPORTFN
          PRINT      "Documents Dates From : ",FROMDAT," to ",TODAT
          CALL       UND132
          PRINT      "Debtor Doc.Ty. Doc. Ref.        Doc.Date   GST    ":
                     "Sales  Item",*91,"Ledger/Dr Ac":
                     "ct. Cr. Acct.           Amount"
          CALL       UND132
          RETURN
.------------------------------------------------------------
. Open/Create Downlaod File
.------------------------------------------------------------
OPEN0000  STRIP     EXPORTFN
          ENDSET    EXPORTFN
          APPEND    SP70,EXPORTFN
          RESET     EXPORTFN
          MATCH     SP70,EXPORTFN
          GOTO      OPEN9000 IF EQUAL
.
          STRIP     EXPORTFN
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      IBAGINA1,EXPORTFN
          TRAPCLR   IO
          BRANCH    OVRCD,OPEN1000
.
OPEN0100  BEEP
          KEYIN     *P1:24,*EL,"Export File Already Exists. Overwrite (":
                    *V2LON,"Y",*HOFF,"/":
                    *V2LON,"N",*HOFF,") ? ",*V2LON,ANS
          REP       UPPLOW,ANS
          MOVE      ONE,EXIT
          MATCH     "N",ANS
          GOTO      OPEN9999 IF EQUAL
          MATCH     "Y",ANS
          GOTO      OPEN0100 IF NOT EQUAL
          CLOSE     IBAGINA1,DELETE
.
OPEN1000  MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          PREP      IBAGINA1,EXPORTFN
          TRAPCLR   IO
          BRANCH    OVRCD,OPEN9100
          MOVE      ZERO,EXIT
          GOTO      OPEN9999
.
OPEN9000  BEEP
          MOVE      "Export Filename MUST be Specified - ",DISPMSG
          CALL      MESSAGE1
          MOVE      ONE,EXIT
          GOTO      OPEN9999
.
OPEN9100  BEEP
          MOVE      "Unable to Create Export File - ",DISPMSG
          CALL      MESSAGE1
          MOVE      ONE,EXIT
          GOTO      OPEN9999
.
OPEN9999  RETURN
.------------------------------------------------------------
. File       : casetran
.
. Functions  : This file contains standard routines for
.              creating and transfering TAB delimited
.              export files to a PC using the unix script
.              keatran.
.
. Routines   : MAKEXP00  - Create the Export File and Headings
.              HDEXPA00  - Export File Normal Header
.              HDEXPB00  - Export File Error Header
.              WRTEXA00  - Write Export Normal details Record to File
.              WRTEXB00  - Write Export Error details Record to File
.              TRAN0000  - Transfer file to PC using script keatran
.
. Note       : The Variable EXPORTYN is used to determine if a
.              export file is to be created. 0=No, 1=Yes.
.
. Variables to be Declared
. ------------------------
. EXPORTFL  FILE      ASCII, FIXED=400     * Export File (Adjust Length to Suit)
. EXPORTFB  DIM       8                    * Export File Name
. TABDELM   EQU       0x09                 * Tab Delimiter
.------------------------------------------------------------
. Create Export File
.------------------------------------------------------------
MAKEXP00  COMPARE   ONE,EXPORTYN
          GOTO      MAKEXP99 IF NOT EQUAL
          MOVE      "EXPORT",KEY6
          PACK      EXPORTFB,KEY6,PORT
          REP       " 0",EXPORTFB
          PREP      EXPORTFL,EXPORTFB
.
MAKEXP99  RETURN
.------------------------------------------------------------
. Export File Normal Header
.------------------------------------------------------------
HDEXPA00  COMPARE   ONE,EXPORTYN
          GOTO      HDEXPA99 IF NOT EQUAL
.
          WRITE     EXPORTFL,SEQ;SP1,TABDELM
          WRITE     EXPORTFL,SEQ;SP1,TABDELM
          WRITE     EXPORTFL,SEQ;SP1,TABDELM
.
          WRITE     EXPORTFL,SEQ;"Debtor",TABDELM:
                                 "Debtor Name",TABDELM:
                                 "Document Type",TABDELM:
                                 "Document Reference",TABDELM:
                                 "Document Date",TABDELM:
                                 "GST Code",TABDELM:
                                 "Sales Item",TABDELM:
                                 "Sales Item Description",TABDELM:
                                 "Ledger",TABDELM:
                                 "Ledger Description",TABDELM:
                                 "Debit Account",TABDELM:
                                 "Credit Account",TABDELM:
                                 "Amount"
HDEXPA99  RETURN
.------------------------------------------------------------
. Export File Error Header
.------------------------------------------------------------
HDEXPB00  COMPARE   ONE,EXPORTYN
          GOTO      HDEXPB99 IF NOT EQUAL
          WRITE     EXPORTFL,SEQ;"Debtor",TABDELM:
                                 "Debtor Name",TABDELM:
                                 "Document Type",TABDELM:
                                 "Document Reference",TABDELM:
                                 "Sales Item",TABDELM:
                                 "Sales Item Description",TABDELM:
                                 "Error Message"
HDEXPB99  RETURN
.------------------------------------------------------------
. Write Normal Detail to Export File
.------------------------------------------------------------
WRTEXA00  COMPARE   ONE,EXPORTYN
          GOTO      WRTEXA99 IF NOT EQUAL
.
          WRITE     EXPORTFL,SEQ;LASTDEB,TABDELM:
                                 LASTDN1,TABDELM:
                                 DIM10,TABDELM:
                                 DBFDDOC,SLASH,DBFDDLN,TABDELM:
                                 CPCDATE,TABDELM:
                                 DBFDGST,TABDELM:
                                 DBFDITM,TABDELM:
                                 DBITDES,TABDELM:
                                 DBFDLED,TABDELM:
                                 FMLADESC,TABDELM:
                                 IBGICRAA,TABDELM:
                                 IBGIACCT,TABDELM:
                                 IBGIVALU
.
WRTEXA99  RETURN
.------------------------------------------------------------
. Write Error Detail to Export File
.------------------------------------------------------------
WRTEXB00  COMPARE   ONE,EXPORTYN
          GOTO      WRTEXB99 IF NOT EQUAL
.
          WRITE     EXPORTFL,SEQ;DBFDDEB,TABDELM:
                                 DBDBNA1,TABDELM:
                                 DIM10,TABDELM:
                                 DBFDDOC,SLASH,DBFDDLN,TABDELM:
                                 DBFDITM,TABDELM:
                                 DBITDES,TABDELM:
                                 ERRMSG
.
WRTEXB99  RETURN
.------------------------------------------------------------
. Execute Transfer Script eg softerm
.------------------------------------------------------------
TRAN0000  COMPARE   ONE,EXPORTYN
          GOTO      TRAN9999 IF NOT EQUAL
          MOVE      YES,DEFYESNO
          CLOSE     EXPORTFL
          DISPLAY   *P1:24,*EL,"Execute Transfer Now (":
                               *V2LON,"Y",*HOFF,"/":
                               *V2LON,"N",*HOFF,") ? "
          MOVE      "24",CVERT
          MOVE      "30",CCOL
          CALL      YORN0000
          COMPARE   ZERO,FYESNO
          GOTO      TRAN9999 IF EQUAL   *  No Transfer
.
          MOVE      ZERO,HLEF
          CALL      GETSCR00
.
          CLEAR     KEY80
          APPEND    "keatran ",KEY80
          APPEND    EXPORTFB,KEY80
          APPEND    ".txt",KEY80
          RESET     KEY80
          EXECUTE   KEY80,TASKID
.
          CALL      PUTSCR00
.
          DISPLAY   *P1:24,*EL,"Transfer Completed Ok (":
                               *V2LON,"Y",*HOFF,"/":
                               *V2LON,"N",*HOFF,") ? "
          MOVE      "24",CVERT
          MOVE      "31",CCOL
          CALL      YORN0000
          COMPARE   ZERO,FYESNO
          GOTO      TRAN0000 IF EQUAL   *  Not ok
.
          PREP      EXPORTFL,EXPORTFB
          CLOSE     EXPORTFL
.
TRAN9999  RETURN
.
.----------------------------------------
. Enter Service Date Range
.----------------------------------------
RDAT0000   MOVE     "    4",FLDID
           CALL     GETPOSIT
           MOVE     SP70,CDAY
           MOVE     CMM,CMON
           MOVE     CYY,CYEAR
           MOVE     CCC,CCENT
           CALL     KEYDATE
           BRANCH   OVRCD,RDAT0100
           MATCH    SP2,CDAY
           GOTO     RDAT0100 IF EQUAL
           PACK     STRDAT,CCENT,CYEAR,CMON,CDAY
           REP      " 0",STRDAT
           CALL     PACDATE
           MOVE     CPCDATE,FROMDAT
           GOTO     RDAT0140
RDAT0100   MOVE     "Start             ",FROMDAT
           MOVE     SP70,STRDAT
.
RDAT0140   MOVE     FROMDAT,VAR
           CALL     DISPITEM
.
RDAT0150   MOVE     "    5",FLDID
           CALL     GETPOSIT
           MOVE     SP70,CDAY
           MOVE     CMM,CMON
           MOVE     CYY,CYEAR
           MOVE     CCC,CCENT
           CALL     KEYDATE
           BRANCH   OVRCD,RDAT0200
           MATCH    SP2,CDAY
           GOTO     RDAT0200 IF EQUAL
           PACK     ENDDAT,CCENT,CYEAR,CMON,CDAY
           REP      " 0",ENDDAT
           CALL     PACDATE
           MOVE     CPCDATE,TODAT
           MATCH    STRDAT,ENDDAT
           GOTO     RDAT0999 IF EQUAL
           GOTO     RDAT0999 IF NOT LESS
           BEEP
           MOVE     "To Date Must Be > From Date - ",DISPMSG
           CALL     MESSAGE1
           GOTO     RDAT0000
.
RDAT0200   MOVE     "Finish             ",TODAT
           MOVE     "99999999",ENDDAT
.
RDAT0999   MOVE     "    4",FLDID
           CALL     GETPOSIT
           RETURN
.
