.------------------------------------------------------------
. Process General Ledger Transfer
.------------------------------------------------------------
PROC0000  DISPLAY   *P1:24,*EL,"Extracting Details ...."
          CALL      OPEN0000           * Create Export File
          BRANCH    EXIT,PROC9100
.
          CALL      MAKEXP00
          CALL      EXTR0000           * Extract Details & Print Error Report
.
          CALL      CHKERR00           * Check Errors Found and Enable Abort
          BRANCH    EXIT,PROC9000
.
          CALL      REPT0000           * Print Transfer Report & Update Flags
          CALL      TRAN0000
.
PROC9000  MOVE      ZERO,EXIT          * Exit Program
          GOTO      PROC9999
.
PROC9100  MOVE      FOUR,EXIT          * Redisplay Screen & Enable Changes
          GOTO      PROC9999
.
PROC9999  RETURN
.------------------------------------------------------------
. Extract Details to Export File & Print Any Errors
.------------------------------------------------------------
EXTR0000  MOVE      ZERO,ERRORS        * Number of Errors Printed
          MOVE      ZERO,CPAGENO       * Setup Report Details
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
          CALL      HDEXPB00
.
          MOVE      ZERO,SECTOR
          MOVE      SP70,LASTDEB
          MOVE      ZERO,FLAGDEB
          PACK      KEY25,ZERO,SP70
          CALL      RSDBFD5
EXTR1000  CALL      RKDBFD5
          BRANCH    OVRCD,EXTR9000
          MATCH     "0",DBFDGLS
          GOTO      EXTR9000 IF NOT EQUAL
.
          MATCH     "3",DBFDDTY        * NO Cash Transfer.. Done by Rec System
          IF        @EQUAL
            MOVE      ONE,DBFDGLS      * Flag to Avoid Read Next Time.
            CALL      UPDBFD5
            PACK      KEY25,ZERO,DBFDDEB,DBFDDTY,DBFDDOC,SP70
            CALL      RDDBFD5
            GOTO      EXTR1000
          ENDIF
.
          MATCH     SP70,SELLED
          IF        !@EQUAL
            MATCH     DBFDLED,SELLED
            GOTO      EXTR1000 IF NOT EQUAL
          ENDIF
.
EXTR1100  ADD       ONE,SECTOR
          IF        SECTOR%100=0
            DISPLAY   *P40:24,*V2LON,SECTOR
          ENDIF
          PACK      KEY21,DBFDDEB,DBFDDTY,DBFDDOC
          CALL      RDDBFH1
          BRANCH    OVRCD,EXTR1000
          MATCH     STRDAT,DBFHDDAT    * Doc. Date Range
          GOTO      EXTR1000 IF LESS
          MATCH     DBFHDDAT,ENDDAT
          GOTO      EXTR1000 IF LESS
.
          CALL      WRLINE00           * Write to export file
.
          GOTO      EXTR1000
.
EXTR9000  CALL      ERREND00           * Print End of Errors
          RETURN
.------------------------------------------------------------
. Check on Error Printed and Enable Abort
.------------------------------------------------------------
CHKERR00  MOVE      ZERO,EXIT
          COMPARE   ZERO,ERRORS
          GOTO      CHKERR99 IF EQUAL
          BEEP
          KEYIN     *P1:24,*EL,*V2LON,*DV,ERRORS,*HOFF:
                    " ERRORS Printed do you want to ":
                    "Continue with the Export (Y/N) ? ",*V2LON,ANS
          REP       UPPLOW,ANS
          MATCH     "Y",ANS
          GOTO      CHKERR99 IF EQUAL
          MATCH     "N",ANS
          GOTO      CHKERR00 IF NOT EQUAL
          CLOSE     IBAGINA1,DELETE
          MOVE      ONE,EXIT
CHKERR99  RETURN
.------------------------------------------------------------
. Print Transfer Report & Update GL Flag
.------------------------------------------------------------
REPT0000  DISPLAY   *P1:24,*EL,"Processing Export File ....";
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      IBAGINA1,EXPORTFN  * Reset to Start of File
          TRAPCLR   IO
.
          IF        OVRCD=1
            DISPLAY   *P1:24,*EL,*B,"No records found. ";
            CALL      HITENTER
            GOTO      REPT9999
          ENDIF
.
          MOVE      ZERO,CPAGENO       * Setup Report Details for Audit
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
          MOVE      SP70,LASTDEB
          MOVE      ZERO,RTOTAMT
          MOVE      ZERO,DTOTAMT
          MOVE      ZERO,SECTOR
          CALL      HDEXPA00
.
REPT1000  CALL      RKIBGI1
          BRANCH    OVRCD,REPT9000
          ADD       ONE,SECTOR
          IF        SECTOR%100=0
            DISPLAY   *P40:24,*V2LON,SECTOR
          ENDIF
          UNPACK    IBGIDESC,DBFDDTY,KEY1,DBFDDEB
          UNPACK    IBGIREF,DBFDDOC,DBFDDLN
          PACK      KEY24,DBFDDEB,DBFDDTY,DBFDDOC,DBFDDLN,SP70
          CALL      RDDBFD1
          IF        OVRCD=0
            MOVE      ONE,DBFDGLS
            CALL      UPDBFD1
          ENDIF
.
          PACK      KEY21,DBFDDEB,DBFDDTY,DBFDDOC,SP70
          CALL      RDDBFH1
.
          MATCH     DBFDDEB,LASTDEB
          GOTO      REPT1100 IF NOT EQUAL
          CALL      PRLINE00           * Print Report Line
.
          GOTO      REPT1000
.
. New Debtor
.
REPT1100  COMPARE   ZERO,CPAGENO       * First Page So No Totals to Print
          GOTO      REPT1200 IF EQUAL
          CALL      DEBTOT00
.
REPT1200  COMPARE   "55",CLNO
          CALL      PRHEAD00 IF NOT LESS
.
          CALL      PRDEBT00
          CALL      PRLINE00
          GOTO      REPT1000
.
REPT9000  IF        CPAGENO<>0
            CALL      DEBTOT00
          ENDIF
          CALL      PEND0000
          MOVE      "Process Complete - ",DISPMSG
          CALL      MESSAGE1
.
REPT9999  RETURN
.------------------------------------------------------------
. Write to Transfer File
.------------------------------------------------------------
WRLINE00  BRANCH    FLGIBAGL,WRLINE50
          CALL      CHKGL000             * Check IBA GL Details
          BRANCH    EXIT,WRLINE99
.
WRLINE50  MOVE      DBFDLED,IBGILEDG
          MOVE      DBFDIAC,IBGIACCT
          MOVE      DBFHDDAT,IBGIDATE
          MATCH     "1",DBFDDTY
          IF        @EQUAL
            MOVE      "-",IBGISIGN       * Invoice Cr Income Dr Control
          ELSE
            MATCH     "4",DBFDDTY
            IF        @EQUAL
              MOVE      "-",IBGISIGN       * Dr Adj. Cr Income Dr Control
            ELSE
              MOVE      " ",IBGISIGN       * Credit  Dr Income Cr Control
            ENDIF
          ENDIF
          MOVE      DBFDTOT,IBGIVALU     * Value of Transaction
.
          UNPACK    SP70,IBGIBASC        * default blank BAS Code
          PACK      KEY6,DBFDGST,SP70    * GST code
          CALL      RDIBGS1
          IF        OVRCD=0 & !(IBGSACTV=1)
            MOVE      IBGSBASC,IBGIBASC
          ENDIF
.
. Key to Debtors Details File (debfdtaf) Stored in Description & Reference
. to be used for update of flag after file is complete.  This way if we abort
. or crash during the first phase it is not a problem and the export may be
. simply run again.
.
          MOVE      DBFDDEB,KEY8
          CALL      RDDBDB1
          PACK      IBGIDESC,DBFDDTY,SP1,DBFDDEB,SP1,DBDBNA1,SP70
          PACK      IBGIREF,DBFDDOC,DBFDDLN,SP70
          MOVE      SP70,IBGIDISS
          MOVE      SP70,IBGIRESP
          MOVE      SP70,IBGICRAC
          MOVE      DBFDCAC,IBGICRAA
          MOVE      DBFDCAC,IBGICRAC
          CALL      WRIBGI1              * Write Transaction Amount
.
WRLINE60  COMPARE   ZERO,DBFDTAX
          GOTO      WRLINE99 IF EQUAL
          MOVE      DBFDTAX,IBGIVALU     * Value of Transaction
          MOVE      DBFDTAC,IBGIACCT
          MOVE      DBFDTCA,IBGICRAA
          MOVE      DBFDCAC,IBGICRAC
          MOVE      SP70,IBGIBASC
          CALL      WRIBGI1              * Write GST Transaction
.
WRLINE99  RETURN
