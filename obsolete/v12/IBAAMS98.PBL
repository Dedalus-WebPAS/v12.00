GETI1000  MOVE      "    3",AUDTSECT
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF FORMAT
          READ      CONTROLF,AUDTSECT;CFAMSRUN,CFAMYEAR,*18,CFAMIRUN:
                                      CFAMGL01,CFAMGL02,CFAMGL03,CFAMGL04:
                                      CFAMGL05,CFAMGL06,CFAMGL07,CFAMGL08:
                                      CFAMGL09,CFAMGL10,CFAMGL11,CFAMGL12:
                                      CFAMGL13:
                                      *53,CFAMPASS
          TRAPCLR   FORMAT
          IF        OVRCD=1
            MOVE      ZERO,CFAMSRUN
            MOVE      ZERO,CFAMIRUN
            MOVE      ZERO,CFAMGL01
            MOVE      ZERO,CFAMGL02
            MOVE      ZERO,CFAMGL03
            MOVE      ZERO,CFAMGL04
            MOVE      ZERO,CFAMGL05
            MOVE      ZERO,CFAMGL06
            MOVE      ZERO,CFAMGL07
            MOVE      ZERO,CFAMGL08
            MOVE      ZERO,CFAMGL09
            MOVE      ZERO,CFAMGL10
            MOVE      ZERO,CFAMGL11
            MOVE      ZERO,CFAMGL12
            MOVE      ZERO,CFAMGL13
            WRITAB    CONTROLF,AUDTSECT;CFAMSRUN,*18,CFAMIRUN:
                                      CFAMGL01,CFAMGL02,CFAMGL03,CFAMGL04:
                                      CFAMGL05,CFAMGL06,CFAMGL07,CFAMGL08:
                                      CFAMGL09,CFAMGL10,CFAMGL11,CFAMGL12:
                                      CFAMGL13
          ELSE
            GOTO    GETI1100
          ENDIF
.
          READ      CONTROLF,AUDTSECT;CFAMSRUN,CFAMYEAR,*18,CFAMIRUN:
                                      CFAMGL01,CFAMGL02,CFAMGL03,CFAMGL04:
                                      CFAMGL05,CFAMGL06,CFAMGL07,CFAMGL08:
                                      CFAMGL09,CFAMGL10,CFAMGL11,CFAMGL12:
                                      CFAMGL13:
                                      *53,CFAMPASS
.
GETI1100  PACK      TRANFLAG,CFAMGL01,CFAMGL02,CFAMGL03,CFAMGL04:
                             CFAMGL05,CFAMGL06,CFAMGL07,CFAMGL08:
                             CFAMGL09,CFAMGL10,CFAMGL11,CFAMGL12:
                             CFAMGL13
          CALL      PASS0000
          BRANCH    EXIT,GETI1999
.
          CALL      SCI1L000        * IBA Controlled Screen 1
.
GETI1999  RETURN
.
PASS0000  MOVE      SP70,KEY8
          KEYIN     *P1:24,*EL,*P20:24,"Enter Password : ",*EOFF,KEY8;
          PACK      KEY8,KEY8,SP30
          MATCH     KEY8,CFAMPASS
          GOTO      PASS9500 IF NOT EQUAL
.
          KEYIN     *EOFF,KEY8;
          SQUEEZE   KEY8
          MOVE      KEY8,AUDTSECT
.
          SUB       CDD,AUDTSECT
          SUB       CMM,AUDTSECT
          SUB       CYY,AUDTSECT
.
          COMPARE   ZERO,AUDTSECT
          GOTO      PASS9500 IF NOT EQUAL
.
PASS9000  MOVE      ZERO,EXIT
          GOTO      PASS9999
.
PASS9500  MOVE      ONE,EXIT
.
PASS9999  RETURN
