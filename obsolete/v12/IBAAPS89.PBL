EYEAR000  MOVE      ZERO,EXIT
          PACK      PURGDATE,CCC,CYY
          REP       " 0",PURGDATE
          MOVE      PURGDATE,F4
          SUB       TWO,F4
          MOVE      F4,PURGDATE
          REP       " 0",PURGDATE
.
          KEYIN     *PCCOL:CVERT,*V2LON,*JR,*RV,PURGDATE
          ENDSET    PURGDATE
          APPEND    SP70,PURGDATE
          RESET     PURGDATE
.
          MATCH     SP70,PURGDATE
          GOTO      EYEAR990 IF EQUAL
          PACK      KEY4,SP3,EXITCHAR
          MATCH     KEY4,PURGDATE
          GOTO      EYEAR990 IF EQUAL
.
          TYPE      PURGDATE
          GOTO      EYEAR050 IF EQUAL
.
EYEAR020  BEEP
          MOVE      "Invalid Financial Year - ",DISPMSG
          CALL      MESSAGE1
          GOTO      EYEAR000
.
EYEAR050  MOVE      PURGDATE,F4
          IF        F4<100
            SQUEEZE   PURGDATE
            MOVE      PURGDATE,F2
            PACK      PURGDATE,CCC,F2,SP70
            REP       " 0",PURGDATE
          ENDIF
.
          MOVE       "    4",FLDID
          MOVE       PURGDATE,VAR
          CALL       DISPITEM
.
          PACK       FMSTFILE,FMST,PURGDATE
          TRAP       OVERCOND IF IO
          OPEN       FMSTRNA1,FMSTFILE
          TRAPCLR    IO
.
          BRANCH     OVRCD,EYEAR020
          CLOSE      FMSTRNA1
          GOTO       EYEAR999
.
EYEAR990  MOVE      TWO,EXIT
.
EYEAR999  RETURN
