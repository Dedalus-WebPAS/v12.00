.----------------------------------------------------------------------
. PURG0000 - Purge Financial Year
.----------------------------------------------------------------------
PURG0000  MOVE      ZERO,CPAGENO
          MOVE      "99",CLNO
          CLOCK     TIME,CTIMEIS
          DISPLAY   *P1:24,*EL;
.
          PACK      FMSTFILE,FMST,PURGDATE,SP70
          OPEN      FMSTRNA1,FMSTFILE
.
          MOVE      ZERO,SECTOR
          PACK      KEY10,SP70
          CALL      RSFMTR1
.
PURG1000  CALL      RKFMTR1
          BRANCH    OVRCD,PURG9000
.
          ADD       ONE,SECTOR
          IF        SECTOR%1000=1
            DISPLAY   *P20:24,SECTOR,SLASH,FMTRBAT
          ENDIF
.
          PACK      KEY30,FMTRBAT,SP70
          CALL      RSFMBN4
          CALL      RKFMBN4
          BRANCH    OVRCD,PURG2000
.
          MATCH     FMTRBAT,FMBNBCH
          GOTO      PURG8000 IF EQUAL
.
PURG2000  PACK      KEY5,FMTRBAT,SP70
          CALL      RDFMBC1
          BRANCH    OVRCD,PURG8000
.
          PACK      KEY5,FMBCBAT,SP70
          CALL      RDAPBM1
          BRANCH    OVRCD,PURG4000
.
          PACK      KEY40,FMBCBAT,SP70
          CALL      RSAPIM1
          CALL      RKAPIM1
          BRANCH    OVRCD,PURG4000
.
          MATCH     APIMBCH,FMBCBAT
          GOTO      PURG4000 IF NOT EQUAL
.
          GOTO      PURG8000
.
PURG4000  PACK      KEY5,FMBCBAT
          CALL      DEFMBC1
.
          COMPARE   "56",CLNO
          CALL      PHED0000 IF NOT LESS
.
          UNPACK    FMBCPDAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          PRINT     *1,FMBCBAT,*10,FMBCTRT,*17,CPCDATE,*30,FMBCTOT
          ADD       ONE,CLNO
.
PURG8000  MOVE      FMTRBAT,F5
          ADD       ONE,F5
          MOVE      F5,FMTRBAT
          REP       " 0",FMTRBAT
          PACK      KEY10,FMTRBAT,SP70
          CALL      RSFMTR1
          GOTO      PURG1000
.
PURG9000  PRINT     *N,*N,*1,"***** End of Report *****"
.
PURG9999  RETURN
.
