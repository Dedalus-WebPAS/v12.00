.----------------------------------------
. List Allocation Wieghting by Department
.----------------------------------------
REPTA000  MOVE      "Printing : ",DISPMSG
          CALL      MESSAGE2
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
          PACK      LASTPRT,SP70,SP70
.
          PACK      KEY14,CCHCHCD,STRDPT,SP70
          CALL      RSCCAW1
REPTA100  CALL      RKCCAW1
          BRANCH    OVRCD,REPTA999
.
          MATCH     SP70,CCHCHCD
          GOTO      REPTA200 IF EQUAL
          MATCH     CCAWDPT,ENDDPT
          GOTO      REPTA999 IF LESS
.
REPTA200  MATCH     STRATY,CCAWATY
          GOTO      REPTA100 IF LESS
          MATCH     CCAWATY,ENDATY
          GOTO      REPTA100 IF LESS
.
          COMPARE   "60",CLNO
          CALL      HEADA000 IF NOT LESS
.
          MOVE      CCAWATY,KEY4
          CALL      RDCCAT1
          IF        OVRCD=1
            MOVE      "Not on File",CCATDES
          ENDIF
.
          PACK      KEY10,CCAWHCD,CCAWDPT
          CALL      RDCCDP1
          IF        OVRCD=1
            MOVE      "Not on File",CCDPDES
          ENDIF
.
          PACK      NEXTPRT,CCAWHCD,CCAWDPT
          MATCH     LASTPRT,NEXTPRT
          IF        !@EQUAL
            PACK      LASTPRT,CCAWHCD,CCAWDPT
            PRINT     *7,CCAWHCD,"/",CCAWDPT,SP1,CCDPDES
            ADD       ONE,CLNO
          ENDIF
          PRINT     *55,CCAWATY,SP1,CCATDES,SP1,CCAWQTY
          ADD       ONE,CLNO
          GOTO      REPTA100
.
REPTA999  CALL      UND132
          PRINT     *40,"*** End of Report ***"
          CALL      MESSAGE3
          MOVE      "Report Completed - ",DISPMSG
          CALL      MESSAGE1
          RETURN
.
HEADA000  CALL      HEAD132
          DISPLAY   *P12:24,*V2LON,CPAGENO
          MATCH     SP70,CCHCHCD
          IF        !@EQUAL
            PRINT      "Hospital         : ",CCHCHCD,SP1,CCHCDES
          ENDIF
          PACK      KEY16,STRDPT,ENDDPT
          MATCH     "        ~~~~~~~~",KEY8
          IF        !@EQUAL
            PRINT      "Department From  : ",FROMDPT," to ",TODPT
          ENDIF
          PACK      KEY8,STRATY,ENDATY
          MATCH     "    ~~~~",KEY8
          IF        !@EQUAL
            PRINT      "Allocation From  : ",FROMATY," to ",TOATY
          ENDIF
          CALL      UND132
          PRINT     "      Hospital Department                             ":
                    "Allocation Type                                  ":
                    "Weighting"
          CALL      UND132
          MOVE      "8",CLNO
          RETURN
.------------------------------------------------
. List Allpocation Weighting by Allocation Type
.------------------------------------------------
REPTB000  MOVE      "Printing : ",DISPMSG
          CALL      MESSAGE2
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
          PACK      LASTPRT,SP70,SP70
          PACK      KEY14,SP70
          CALL      RSCCAW2
REPTB100  CALL      RKCCAW2
          BRANCH    OVRCD,REPTB999
.
          MATCH     STRATY,CCAWATY
          GOTO      REPTB100 IF LESS
          MATCH     CCAWATY,ENDATY
          GOTO      REPTB100 IF LESS
.
          MATCH     SP70,CCHCHCD
          GOTO      REPTB200 IF EQUAL
          MATCH     CCAWHCD,CCHCHCD
          GOTO      REPTB100 IF NOT EQUAL
.
          MATCH     STRDPT,CCAWDPT
          GOTO      REPTB100 IF LESS
          MATCH     CCAWDPT,ENDDPT
          GOTO      REPTB100 IF LESS
.
REPTB200  COMPARE   "60",CLNO
          CALL      HEADB000 IF NOT LESS
.
          MOVE      CCAWATY,KEY4
          CALL      RDCCAT1
          IF        OVRCD=1
            MOVE      "Not on File",CCATDES
          ENDIF
.
          PACK      KEY10,CCAWHCD,CCAWDPT
          CALL      RDCCDP1
          IF        OVRCD=1
            MOVE      "Not on File",CCDPDES
          ENDIF
          PACK      NEXTPRT,CCAWATY
          MATCH     LASTPRT,NEXTPRT
          IF        !@EQUAL
            MATCH     SP70,LASTPRT
            IF        !@EQUAL
              PRINT     *86,"---------------------------"
              PRINT     *86,"Total     ",ALLTOTAL
              CALL      HEADB000
            ENDIF
            MOVE      ZERO,ALLTOTAL
            PACK      LASTPRT,CCAWATY
            PRINT     *7,CCAWATY,SP1,CCATDES
            ADD       ONE,CLNO
          ENDIF
          ADD       CCAWQTY,ALLTOTAL
          PRINT     *48,CCAWHCD,"/",CCAWDPT,SP1,CCDPDES,SP1,CCAWQTY
          ADD       ONE,CLNO
          GOTO      REPTB100
.
REPTB999  PRINT     *86,"---------------------------"
          PRINT     *86,"Total     ",ALLTOTAL
          CALL      UND132
          PRINT     *40,"*** End of Report ***"
          CALL      MESSAGE3
          MOVE      "Report Completed - ",DISPMSG
          CALL      MESSAGE1
          RETURN
.
HEADB000  CALL      HEAD132
          DISPLAY   *P12:24,*V2LON,CPAGENO
          MATCH     SP70,CCHCHCD
          IF        !@EQUAL
            PRINT      "Hospital         : ",CCHCHCD,SP1,CCHCDES
          ENDIF
          PACK      KEY16,STRDPT,ENDDPT
          MATCH     "        ~~~~~~~~",KEY8
          IF        !@EQUAL
            PRINT      "Department From  : ",FROMDPT," to ",TODPT
          ENDIF
          PACK      KEY8,STRATY,ENDATY
          MATCH     "    ~~~~",KEY8
          IF        !@EQUAL
            PRINT      "Allocation From  : ",FROMATY," to ",TOATY
          ENDIF
          CALL      UND132
          PRINT     "      Allocation Type                          Hospital":
                    " Department                                     ":
                    "Weighting"
          CALL      UND132
          MOVE      "8",CLNO
          RETURN
