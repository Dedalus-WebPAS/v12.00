.----------------------------------------
. List Patient Service Codes
.----------------------------------------
LPRVI000
          MOVE      "Printing : ",DISPMSG
          CALL      MESSAGE2
          MOVE      SCSLITM,FIELDNO
          SUB       TWO,FIELDNO
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
          PACK      LASTPRT,SP70,SP70
.
          PACK      KEY18,CCHCHCD,STRDPT,SP70
          CALL      RSCCPD1
LPRVI100  CALL      RKCCPD1
          BRANCH    OVRCD,LPRVI999
          MATCH     CCPDHCD,CCHCHCD
          GOTO      LPRVI9999 IF NOT EQUAL
.
          MATCH     CCPDDPT,ENDDPT
          GOTO      LPRVI9999 IF LESS
.
          IF        PRINTSUS=0
            MATCH     "1",CCPDSUS
            GOTO      LPRVI100 IF EQUAL
          ENDIF
          PACK      NEXTPRT,CCPDHCD,CCPDDPT
          MATCH     NEXTPRT,LASTPRT
          IF        !@EQUAL
            PACK      LASTPRT,CCPDHCD,CCPDDPT
            CALL      LPRVIHED
          ENDIF
.
          COMPARE   "60",CLNO
          CALL      LPRVIHED IF NOT LESS
          ADD       TWO,CLNO
          PRINT     *N,CCPDPCD,SP1,CCPDDES,*54,"Indirect",*94,CCPDRVI
.
          PACK      KEY22,CCPDHCD,CCPDDPT,CCPDPCD,SP70
          PACK      KEY18,CCPDHCD,CCPDDPT,CCPDPCD,SP70
          CALL      RSCCPV1
LPRVI200  CALL      RKCCPV1
          BRANCH    OVRCD,LPRVI100
          PACK      KEY22,CCPVHCD,CCPVDPT,CCPVPCD,SP70
          MATCH     KEY18,KEY22
          GOTO      LPRVI100 IF NOT EQUAL
          MOVE      CCPVCTY,KEY4
          CALL      RDCCCT1
          COMPARE   "60",CLNO
          CALL      HEDRVI00 IF NOT LESS
          PRINT     *49,CCPVCTY,SP1,CCCTDES,SP5,CCPVRVI
          ADD       ONE,CLNO
          GOTO      LPRVI200
.
LPRVI999  CALL      UND132
          PRINT     *40,"*** End of Report ***"
          CALL      MESSAGE3
          MOVE      "Report Completed - ",DISPMSG
          CALL      MESSAGE1
          RETURN
.
HEDRVI00
          CALL      LPRVIHED
          ADD       ONE,CLNO
          PRINT     CCPDPCD,SP1,CCPDDES," - Continued"
          RETURN
.
LPRVIHED  CALL      HEAD132
          DISPLAY   *P12:24,*V2LON,CPAGENO
          PACK      KEY10,CCPDHCD,CCPDDPT
          CALL      RDCCDP1
          IF        OVRCD=1
            MOVE      "Not on File",CCDPDES
          ENDIF
.
          PRINT     "Department : ",CCPDHCD,"/",CCPDDPT,SP1,CCDPDES
          CALL      UND132
          PRINT     "Patient Service",*49,"Cost Type",*99,"Relative Value"
          CALL      UND132
          MOVE      "8",CLNO
          RETURN
