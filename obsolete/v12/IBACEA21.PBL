.------------------------------------------------------------
. Print Service Provider Listing
.------------------------------------------------------------
REPT0000  DISPLAY   *P1:24,*EL,"Printing Report : ";
          MOVE      "99",CLNO
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
          PACK      KEY3,STRSPC,SP70
          CALL      RDCESP1
          IF        OVRCD=0
            CALL      RPCESP1
          ENDIF
REPT1000  CALL      RKCESP1
          BRANCH    OVRCD,REPT9000
          MATCH     CESPSPC,ENDSPC
          GOTO      REPT9000 IF LESS
.
          DISPLAY   *P19:24,*V2LON,CESPSPC
.
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
          ADD       ONE,CLNO
          PRINT     "| ",CESPSPC,"  | ",CESPDES,*60,"| ",CESPUR1,*80,"|"
          GOTO      REPT1000
.
REPT9000  CALL      UND80
          PRINT     *25,"***** End of Report *****"
          RETURN
.------------------------------------------------------------
. Page Header
.------------------------------------------------------------
HEAD0000  CALL      HEAD80
          PRINT     "Service Privider Master List From : ",FROMSPC
          PRINT     "                             To   : ",TOSPC
          CALL      UND80
          PRINT     "| Code | Description",*60,"| Ledger/Account",*80,"|"
          CALL      UND80
          RETURN
