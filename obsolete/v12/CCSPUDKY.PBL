.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSPUDKY
.   Routine   :   HCCPUA00 Patient Service Class
.   Return    :   CCPUCOD 
.-------------------------------------------------------------------------------
HCCPUA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCPUA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCPUA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Class"              
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 6",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HCCPUA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCPUCOD 
          PACK      KEY5,CCPUCLS,CCPUCOD,SP70                                   
          PACK      SAVKEY80,CCPUCLS,CCPUCOD,SP70                               
          SETLPTR   SAVKEY80,1                                                  
          CALL      RSCCPU1 
HCCPUA10
          CALL      RKCCPU1 
          BRANCH    OVRCD,HCCPUA15
          PACK      KEY5,CCPUCLS,CCPUCOD,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HCCPUA15 IF NOT EQUAL                                       
          BRANCH    CSEARCH,HCCPUA12
          CALL      HCCPUA60
          BRANCH    OVRCD,HCCPUA10
HCCPUA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY5,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCPUCOD 
          DISPLAY   *PSTDCOL02:STDROW,CCPUDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCPUA10 IF NOT EQUAL
HCCPUA15
          UNPACK    SAVKEY80,CCPUCLS,CCPUCOD                                    
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCPUA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCPUA30,HCCPUA35,HCCPUA40,HCCPUA45,HCCPUA04:
                              HCCPUA91,HCCPUA50
          GOTO      HCCPUA92
HCCPUA30
          CALL      RKCCPU1 
          BRANCH    OVRCD,HCCPUA85
          PACK      KEY5,CCPUCLS,CCPUCOD,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HCCPUA85 IF NOT EQUAL                                       
          CALL      RPCCPU1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPUA10
HCCPUA35
          MOVE      ST1,KEY5 
          CALL      RDCCPU1 
          CALL      RPCCPU1 
          BRANCH    OVRCD,HCCPUA85
          PACK      KEY5,CCPUCLS,CCPUCOD,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HCCPUA85 IF NOT EQUAL                                       
          MATCH     SP70,CCPUCOD                                                
          GOTO      HCCPUA85 IF EQUAL                                           
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCPUA36
          CALL      RPCCPU1 
          BRANCH    OVRCD,HCCPUA37
          PACK      KEY5,CCPUCLS,CCPUCOD,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HCCPUA37 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCPUA36 IF NOT EQUAL
HCCPUA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCPUA10
HCCPUA40
          MOVE      ST1,KEY5 
          CALL      RDCCPU1 
          CALL      RPCCPU1 
          BRANCH    OVRCD,HCCPUA85
          PACK      KEY5,CCPUCLS,CCPUCOD,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HCCPUA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPUA08
HCCPUA45
          CALL      RKCCPU1 
          BRANCH    OVRCD,HCCPUA85
          PACK      KEY5,CCPUCLS,CCPUCOD,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HCCPUA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY5,SAVKEY80,Z70                                           
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCPU1 
HCCPUA46
          CALL      RPCCPU1 
          BRANCH    OVRCD,HCCPUA47
          PACK      KEY5,CCPUCLS,CCPUCOD,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HCCPUA47 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCPUA46 IF NOT EQUAL
HCCPUA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCPUA10
HCCPUA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCPUA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPUA08
HCCPUA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPUA08
HCCPUA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCPUA61 IF EQUAL
          MATCH     SCHFLD01,CCPUCOD 
          GOTO      HCCPUA72 IF NOT EQUAL
HCCPUA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCPUA62 IF EQUAL
          MATCH     SCHFLD02,CCPUDES 
          GOTO      HCCPUA72 IF NOT EQUAL
HCCPUA62
HCCPUA63
HCCPUA64
HCCPUA65
HCCPUA66
HCCPUA67
HCCPUA68
HCCPUA69
HCCPUA70
          GOTO      HCCPUA75
HCCPUA72  MOVE      ONE,OVRCD
HCCPUA75  RETURN
HCCPUA85
          LOAD      KEY5,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY5,CCPUCLS,CCPUCOD                                        
          CALL      RDCCPU1 
HCCPUA90
          UNPACK    SAVKEY80,CCPUCLS,CCPUCOD                                    
          BEEP
          GOTO      HCCPUA20
HCCPUA91
          MOVE      SP70,CCPUDES 
          MOVE      SP70,CCPUSPA 
          MOVE      SP70,KEY5 
          MOVE      SP70,CCPUCOD 
          GOTO      HCCPUA96
HCCPUA92
          MOVE      SP70,KEY5 
          MOVE      SP70,CCPUCOD 
          LOAD      KEY5,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY5,CCPUCLS,CCPUCOD                                        
HCCPUA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCPUA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSPUDKY
.   Routine   :   KCCPUA00 Keyin Patient Service Class
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCPUA00 TOPIC     "CCPUCOD "
          MOVE      UNDLN70,CCPUCOD 
          KEYIN     *PCCOL:CVERT,*DV,CCPUCOD:                                   
                    *PCCOL:CVERT,*V2LON,CCPUCOD:                                
                    *PCCOL:CVERT,*DV,CCPUCOD 
.
          ENDSET    CCPUCOD 
          APPEND    SP70,CCPUCOD 
          RESET     CCPUCOD 
.
          MATCH     SP70,CCPUCOD 
          GOTO      KCCPUA90 IF EQUAL
.
          CMATCH    EXITCHAR,CCPUCOD 
          GOTO      KCCPUA95 IF EQUAL
.
          CMATCH    QUEST,CCPUCOD 
          GOTO      KCCPUA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCCPUA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCPUCOD 
          APPEND    SP70,CCPUCOD 
          RESET     CCPUCOD 
.
          MATCH     SP70,CCPUCOD 
          GOTO      KCCPUA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CCPUCOD                                 
KCCPUA10
          PACK      KEY5,CCPUCLS,CCPUCOD                                        
          CALL      RACCPU1 
          BRANCH    OVRCD,KCCPUA80
          BRANCH    CKEYTYP,KCCPUA75
          CALL      RDCCPU1 
KCCPUA70
          MOVE      ZERO,EXIT
          GOTO      KCCPUA99
KCCPUA75
          MOVE      "Patient Service Class Already Exist - ",DISPMSG                                  
          CALL      MESSAGE1
          GOTO      KCCPUA00
KCCPUA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCPUA99,KCCPUA99
          MOVE      "Patient Service Class Does Not Exist - ",DISPMSG                                 
          CALL      MESSAGE1
          GOTO      KCCPUA00
KCCPUA90
          MOVE      ONE,EXIT
          GOTO      KCCPUA99
KCCPUA95
          MOVE      TWO,EXIT
KCCPUA99 TOPIC    PRGID
          RETURN
