.-------------------------------------------------------------
. Locking Standard Routine to Post Changes.
.-------------------------------------------------------------
POST0000  PACK     KEY3,CESPSPC       * Setup Key
          CALL     RACESP1            * Check for record on file
          IF       OVRCD=0
            CALL     UPCESP1          * Update Changes
            CALL     UUCESP1          * Unlock
          ELSE
            CALL     WRCESP1          * Write New Record
          ENDIF
          RETURN                      * Return to program.
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Cancel Changes.
.
.-------------------------------------------------------------
CANC0000  PACK     KEY3,CESPSPC        * Setup Key for file.
          CALL     RACESP1
          IF       OVRCD=0
            CALL     UUCESP1           * Unlock.
          ENDIF
          RETURN
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Delete Record.
.-------------------------------------------------------------
DELT0000  MOVE     SP70,DISPMSG        * Clear Line 24 Display MeCEage
          CALL     MESSAGE2            * Save and Clear Line 24
          CALL     DELQST              * Delete (Y/N/C) CEXIT 1/2/3
          CALL     MESSAGE3            * Restore Line 24
          BRANCH   CEXIT,DELT1000,DELT9000,DELT9100
.
DELT1000  PACK     KEY3,CESPSPC        * Setup Key to File
          CALL     RACESP1             * Read Record
          IF       OVRCD=0
            CALL     DECESP1           * Delete Record
          ENDIF
          MOVE     TWO,EXIT            * Restart Screen
          GOTO     DELT9999
DELT9000  MOVE     ONE,EXIT            * Return to Prompt on "No"
          GOTO     DELT9999
DELT9100  MOVE     TWO,EXIT            * Restart Screen on "Cancel"
          GOTO     DELT9999
DELT9999  RETURN
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Enter Key Field
.-------------------------------------------------------------
EKEYF000  MOVE      TWO,CKEYTYP             * Record Does not have to be on file
.                                           * This allows add and change.
          CALL      KCESPA00                * Call Standard Keyin Routine with ?
          BRANCH    EXIT,EKEYF999,EKEYF999  * Exit 1 Null Entry 2 Exitchar
          COMPARE   ONE,OVRCD               * New Record so no Lock
          GOTO      EKEYF999 IF EQUAL       * Return
.
EKEYF100  PACK      KEY3,CESPSPC            * Setup Key to File
          CALL      RLCESP1                 * Read and Lock Record
          BRANCH    OVRCD,EKEYF999,EKEYF200 * OVRCD=1 Not on File 2 Locked
          MOVE      ZERO,EXIT               * Setup Exit Field
          MOVE      ZERO,OVRCD              * Setup Ovrcd Field
          TRAP      OVERCOND IF IO
          OPEN      FMSAMAA1,"fmsamaaf"
          TRAPCLR   IO
          IF        OVRCD=0
            UNPACK    CESPUR1,KEY2,KEY1,KEY12
            PACK      KEY14,KEY2,KEY12
            CALL      RDFMAM1
            IF        OVRCD=1
              MOVE      SP70,FMAMDESC
            ENDIF
            CLOSE     FMSAMAA1
          ENDIF
.
          MOVE      ZERO,OVRCD              * Setup Ovrcd Field
          GOTO      EKEYF999
.
EKEYF200  MOVE      SP70,DISPMSG            * Clear Line 24 MESSage Display
          CALL      MESSAGE2                * Clear Line 24 and Save
          KEYIN     *P1:24,*EL,"Record Locked - (":
                    *V2LON,"R",*HOFF,")etry or (":
                    *V2LON,"E",*HOFF,")xit ? ",ANS
          CALL      MESSAGE3                * Restore Line 24
          REP       UPPLOW,ANS              * Convert to Upper Case
          MATCH     ANSR,ANS                * Re-try
          GOTO      EKEYF100 IF EQUAL       * Return to locking
          MATCH     ANSE,ANS                * Exit
          GOTO      EKEYF200 IF NOT EQUAL   * Must be a valid response
          GOTO      EKEYF000                * Return to Keyin
.
EKEYF999  RETURN                            * End of Routine
ELEDA000  KEYIN    *PCCOL:CVERT,*V2LON,*JR,*ZF,KEY2,"/",KEY12
          ENDSET   KEY2
          APPEND   SP70,KEY2
          RESET    KEY2
          ENDSET   KEY12
          APPEND   SP70,KEY12
          RESET    KEY12
          PACK     CESPUR1,KEY2,SLASH,KEY12,SP70
          PACK     KEY14,KEY2,KEY12
.
          MOVE     ZERO,OVRCD              * Setup Ovrcd Field
          TRAP     OVERCOND IF IO
          OPEN     FMSAMAA1,"fmsamaaf"
          TRAPCLR  IO
          BRANCH   OVRCD,ELEDA999
.
          UNPACK   CESPUR1,KEY2,KEY1,KEY12
          PACK     KEY14,KEY2,KEY12
          CALL     RDFMAM1
          BRANCH   OVRCD,ELEDA800
          CLOSE    FMSAMAA1
.
          GOTO     ELEDA999
.
ELEDA800  MOVE     "Invalid Ledger/Account. - ",DISPMSG
