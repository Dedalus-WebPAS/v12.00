. *----------------------------------------------------------------------
. * Include Name  :   CCSCECTM.PBL
. *
. * Function      :   Display routine for CCSCECFD fields.
. *
. * Mods          :   
. *      V9.03.00  26.02.2004  Sylvek Litewka  CAR 46508
. *                Created include.
. *----------------------------------------------------------------------
CCEC0000  BRANCH    FLDITMNO,CCEC0100,CCEC0200,CCEC0300,CCEC0400,CCEC0500:
                             CCEC0600,CCEC0700,CCEC0800,CCEC0900,CCEC1000:
                             CCEC1100
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      CCEC9999
.
CCEC0100  WRITE     HTMLFILE;CCECHCD;       * Hospital
          GOTO      CCEC9999
.
CCEC0200  WRITE     HTMLFILE;CCECEPS;       * Episode
          GOTO      CCEC9999
.
CCEC0300  WRITE     HTMLFILE;CCECLV1;       * Level 1
          GOTO      CCEC9999
.
CCEC0400  WRITE     HTMLFILE;CCECLV2;       * Level 2
          GOTO      CCEC9999
.
CCEC0500  WRITE     HTMLFILE;CCECLV3;       * Level 3
          GOTO      CCEC9999
.
CCEC0600  WRITE     HTMLFILE;CCECFTC;       * Comp. From Total Cost (Income)
          GOTO      CCEC9999
.
CCEC0700  WRITE     HTMLFILE;CCECTTC;       * Comp. To Total Cost
          GOTO      CCEC9999
.
CCEC0800  WRITE     HTMLFILE;CCECLOS;       * Length of Stay
          GOTO      CCEC9999
.
CCEC0900  MATCH     "0",CCECEXC             * Exclusion Flag (0=No)
          IF        @EQUAL
            WRITE     HTMLFILE;"No";
          ELSE
            WRITE     HTMLFILE;"Yes";
          ENDIF
          GOTO      CCEC9999
.
CCEC1000  MOVE      ZERO,BDTOTF             * Income/Model per Bed Day
          IF        CCECFTC>0
            ASSIGN    CCECFTC/CCECLOS,BDTOTF
          ENDIF
.
          WRITE     HTMLFILE;BDTOTF;
          GOTO      CCEC9999
.
CCEC1100  MOVE      ZERO,BDTOTT             * Cost per Bad Day
          IF        CCECTTC>0
            ASSIGN    CCECTTC/CCECLOS,BDTOTT
          ENDIF
.
          WRITE     HTMLFILE;BDTOTT;
          GOTO      CCEC9999
.
CCEC9999  RETURN
