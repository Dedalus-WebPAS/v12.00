          CALL      MESSAGE4
          BRANCH    EXIT,SAVE9000
          PACK      KEY40,APIMBCH,APIMCRD,APIMREF,APIMDOC,SP70
          CALL      RAAPIM1
          IF        OVRCD=0
            CALL      DEAPIM1
          ENDIF
          GOTO      SAVE9100
.
SAVE6000  ASSIGN    APIMTOT+APIMGST-APIMDIS,XYZTOTAL
          MOVE      APIMREF,LASTINV
.
          COMPARE   ZERO,PAYASGO
          GOTO      SAVE9100 IF EQUAL
.
          MOVE      ZERO,ORGAPAMT
          MOVE      ZERO,PAYAPAMT
          PACK      KEY40,APIMBCH,APIMCRD,APIMREF,APIMDOC,SP70
          PACK      KEY51,APIMBCH,APIMCRD,APIMREF,APIMDOC,SP70
          MOVE      ZERO,F3
          CALL      RSAPIN1
.
SAVE6500  CALL      RKAPIN1
          BRANCH    OVRCD,SAVE8000
          PACK      KEY51,APINBCH,APINCRD,APINREF,APINDOC,SP70
          MATCH     KEY40,KEY51
          GOTO      SAVE8000 IF NOT EQUAL
.
          PACK      SKEY51,APINBCH,APINCRD,APINREF,APINDOC,APINORD,APINLIN,SP70
.
          MOVE      APINREF,SAVEREF
          STRIP     APINREF
          PACK      KEY15,ANSP,ANSA,ANSY,ANSG,HYPHEN,APINREF,SP70
          MOVE      KEY15,APINREF
          MOVE      APINREF,APINDOC
          ASSIGN    (APINAMT/HUNDRED)*IBGEAMNT,PAYASTOT
          ASSIGN    (PAYASTOT*SEQ),APINAMT
          MOVE      ZERO,APINGST
          MOVE      ZERO,F3
          MOVE      ANSP,APINPAYG
.
SAVE6600  ADD       ONE,F3
          MOVE      F3,APINLIN
          PACK      KEY51,APINBCH,APINCRD,APINREF,APINDOC,APINORD,APINLIN,SP70
          CALL      RAAPIN1
          IF        OVRCD=1
            CALL      WRAPIN1
          ELSE
            GOTO      SAVE6600
          ENDIF
.
          MOVE      PAYASTOT,APINAMT
          ADD       PAYASTOT,PAYAPAMT
          PACK      KEY15,APINCRD,SP1,SAVEREF,SP70
          MOVE      KEY15,APINREF
          MOVE      APINREF,APINDOC
          MOVE      FMCOPGCR,APINCRD
          MOVE      PAYASTOT,APINAMT
          MOVE      ANSZ,APINPAYG
          MOVE      SP3,APINBASC
          IF        BASFLAG=1
            MOVE      FMCOPBAS,APINBASC
          ENDIF
.
SAVE7000  ADD       ONE,F3
          MOVE      F3,APINLIN
          PACK      KEY51,APINBCH,APINCRD,APINREF,APINDOC,APINORD,APINLIN,SP70
          CALL      RAAPIN1
          IF        OVRCD=1
            CALL      WRAPIN1
          ELSE
            GOTO      SAVE7000
          ENDIF
.
          MOVE      SKEY51,KEY51
          CALL      RSAPIN1
          GOTO      SAVE6500
.
SAVE8000  PACK      KEY40,APIMBCH,APIMCRD,APIMREF,APIMDOC,SP70
          MOVE      KEY40,SKEY40
          CALL      RDAPIM1
          BRANCH    OVRCD,SAVE9100
.
          MOVE      APIMTOT,XYZTOTAL
          MOVE      ZERO,APIMGST
          MOVE      APIMREF,LASTINV
          CALL      UPAPIM1
.
          MOVE      APIMREF,SAVEREF
          STRIP     APIMREF
          PACK      KEY15,ANSP,ANSA,ANSY,ANSG,HYPHEN,APIMREF,SP70
          MOVE      KEY15,APIMREF
          MOVE      APIMREF,APIMDOC
          PACK      KEY40,APIMBCH,APIMCRD,APIMREF,APIMDOC,SP70
          CALL      RAAPIM1
          IF        OVRCD=1
            ASSIGN    (PAYAPAMT*SEQ),APIMTOT
            MOVE      ZERO,APIMDIS
            MOVE      ZERO,APIMGST
            MOVE      ANSP,APIMPAYG
            CALL      WRAPIM1
          ENDIF
.
          PACK      KEY15,APIMCRD,SP1,SAVEREF,SP70
          MOVE      KEY15,APIMREF
          MOVE      APIMREF,APIMDOC
          MOVE      FMCOPGCR,APIMCRD
.
          PACK      KEY40,APIMBCH,APIMCRD,APIMREF,APIMDOC,SP70
          CALL      RAAPIM1
          IF        OVRCD=1
            MOVE      PAYAPAMT,APIMTOT
            MOVE      ZERO,APIMDIS
            MOVE      ZERO,APIMGST
            MOVE      ANSZ,APIMPAYG
            CALL      WRAPIM1
          ENDIF
          GOTO      SAVE9100
.
SAVE9000  MOVE      TWO,EXIT   * Restart Screen
          GOTO      SAVE9999
.
SAVE9100  MOVE      ZERO,EXIT  * Exit Screen
          GOTO      SAVE9999
.
SAVE9200  MOVE      ONE,EXIT   * Return to Prompt
          GOTO      SAVE9999
.
SAVE9999  RETURN
