          GOTO      CHTOT100
.
CHTOT120  KEYIN     *P30:18,*V2LON,*RV,APIMDIS:
                    *P30:18,*DV,APIMDIS
          GOTO      CHTOT100
.
CHTOT130  KEYIN     *P30:19,*V2LON,*RV,APIMGST:
                    *P30:19,*DV,APIMGST
          GOTO      CHTOT100
.
CHTOT900  PACK      KEY40,APIMBCH,APIMCRD,APIMREF,APIMDOC,SP70
          CALL      RAAPIM1
          IF        OVRCD=0
            CALL      UPAPIM1
          ELSE
            CALL      WRAPIM1
          ENDIF
          GOTO      CHTOT999
.
CHTOT950  PACK      KEY40,APIMBCH,APIMCRD,APIMREF,APIMDOC,SP70
          CALL      RDAPIM1
.
