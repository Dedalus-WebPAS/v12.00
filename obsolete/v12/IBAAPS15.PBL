.----------------------------------------------------------------------
. List Batch - Summary
.----------------------------------------------------------------------
LSTBCH00
          CLOSE     FMSAMAA1
          CLOSE     FMSAMAA2
          CLOSE     FMSAMAA3
          CLOSE     FMSAMAA4
          OPEN      IBACODE1,"ibacodef"
...       OPEN      IBAPRNT1,"ibaprntf"                               *D-239574
          CALL      OPNPRINT
          MOVE      ZERO,CPAGENO
          MOVE      "88",CLNO
          CLOCK     TIME,CTIMEIS
          PACK      KEY40,APBMBCH,SP70
          MOVE      SP70,KEY5
          CALL      RSAPIM1
LSTBCH10
          CALL      RKAPIM1
          BRANCH    OVRCD,LSTBCH90
          MATCH     APIMBCH,APBMBCH
          GOTO      LSTBCH90 IF NOT EQUAL
          MATCH     APIMCRD,KEY5
          IF        @EQUAL
