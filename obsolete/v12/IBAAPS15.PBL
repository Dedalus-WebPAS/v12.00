.----------------------------------------------------------------------
. Enter Purchase Order Number
.----------------------------------------------------------------------
KIORD000  IF         PAYASGO=1
.           MOVE       "Cannot Enter Order For PAYG Creditor",DISPMSG
.           CALL       MESSAGE1
            MOVE       ONE,EXIT
            GOTO       KIORD800
          ENDIF
.
          COMPARE    ZERO,HACCNT
          GOTO       KIORD010 IF EQUAL
          CALL       APISCLS1
.
KIORD010  MOVE       APIMCRD,APCICRD
          MOVE       TWO,CKEYTYP
          CALL       KAPCIB00         * New Keyin Consolidated Orders
          BRANCH     EXIT,KIORD800,KIORD999
.
.---- check if order already on file ----
.
          COMPARE    ZERO,HACCNT
          GOTO       KIORD900 IF EQUAL
.
          MOVE       APCIISC,KEY1
          CALL       RDAPIS1          * Read Inventory Directory Information
          IF         OVRCD=1
            MOVE       "Order Not on Inventory Receivals. ",DISPMSG
            CALL       MESSAGE4
            BRANCH     EXIT,KIORD010
            MOVE       ZERO,EXIT
            GOTO       KIORD900
          ENDIF
.
          CALL       APISOPN1
.
          PACK       SIORNAME,SP70
          PACK       KEY7,APCIORD
          IF         INCOSTYP=1
            CALL       RDSIPA1
            IF         OVRCD=0
              MOVE       SIPAORI,KEY3
              CALL       RDSIOR1
            ENDIF
          ENDIF
          PACK       APINORD,APCIISC,APCIORD
          PACK       ORDERNO,APCIISC,APCIORD
          PACK       KEY48,APIMBCH,APIMCRD,APIMREF,APIMDOC,APINORD,SP70
          PACK       KEY51,APIMBCH,APIMCRD,APIMREF,APIMDOC,APINORD,SP70
          CALL       RSAPIN1
          CALL       RKAPIN1
          IF         OVRCD=0
            PACK       KEY51,APINBCH,APINCRD,APINREF,APINDOC,APINORD,SP70
            MATCH      KEY48,KEY51
            GOTO       KIORD980 IF EQUAL
          ENDIF
.
          PACK       KEY8,APCIISC,APCIORD   * does order exist ?
          CALL       RDAPCI1
          IF         OVRCD=1
            MOVE       "Order Not on Inventory Receivals. ",DISPMSG
            CALL       MESSAGE4
            BRANCH     EXIT,KIORD010
            MOVE       ZERO,EXIT
            GOTO       KIORD900
          ENDIF
.
          CALL       CHECKORD         * Validate Ledger Details
          BRANCH     EXIT,KIORD010
.
          PACK       ORDERNO,APCIISC,APCIORD
          PACK       APINORD,APCIISC,APCIORD
.
          MOVE       ZERO,F3
.
          MOVE      "16",HTOP
          MOVE      "1",HLEF
          MOVE      "80",HRIG
          MOVE      "24",HBOT
          CALL      GETSCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HRIG
          MOVE      ZERO,HBOT
.
          PACK       KEY14,APCIISC,APCIORD,SP70
          CALL       RSAPOD1
.
KIORD100  CALL       RKAPOD1
          BRANCH     OVRCD,KIORD400
          MATCH      APCIISC,APODISC            * System Code
          GOTO       KIORD400 IF NOT EQUAL
          MATCH      APCIORD,APODORD            * Order Number
          GOTO       KIORD400 IF NOT EQUAL
.
          PACK       KEY51,APBMBCH,APIMCRD,SP70
          CALL       RSAPIN1
.
KIORD150  CALL       RKAPIN1
          BRANCH     OVRCD,KIORD170
.
          MATCH      APBMBCH,APINBCH
          GOTO       KIORD170 IF NOT EQUAL
          MATCH      APIMCRD,APINCRD
          GOTO       KIORD170 IF NOT EQUAL
          MATCH      ORDERNO,APINORD
          GOTO       KIORD170 IF NOT EQUAL
          MATCH      APODCAT,APINCAT
          GOTO       KIORD150 IF NOT EQUAL
.
          CLEAR      DISPMSG
          APPEND     "WARNING: ",DISPMSG
          APPEND     APODDES,DISPMSG
          APPEND     " Exists",DISPMSG
          RESET      DISPMSG
          CALL       MESSAGE1
.
KIORD170  MOVE       APBMBCH,APINBCH
          MOVE       APIMCRD,APINCRD
          MOVE       APIMREF,APINREF
          MOVE       APIMDOC,APINDOC
          PACK       APINORD,APCIISC,APCIORD
          MOVE       ZERO,APINUPD
          MOVE       APODUCT,APINUCT
          MOVE       APODQTY,APINQTY
          MOVE       APODAMT,APINAMT
          SUB        APODGSTA,APINAMT
          MOVE       APODGSTA,APINGST
          MOVE       APODLED,APINLED
          MOVE       APODACC,APINACC
          MOVE       APODDIS,APINDIS
          MOVE       APODRES,APINRES
          MOVE       APODCAT,APINCAT
          MOVE       APODDES,APINDES
          MOVE       APODGST,APINCGS
          MOVE       SP3,APINBASC
          PACK       KEY6,APINCGS,SP70
          CALL       RDIBGS1
          IF         OVRCD<>1
            IF         BASFLAG=1
