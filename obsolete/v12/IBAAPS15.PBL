            CALL      DISPITEM
          ENDIF
          MOVE     "   61",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      GSTALLOC,VAR
            CALL      DISPITEM
          ENDIF
.
          MOVE     "02",SCRID
          MOVE      "   24",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      ORDTOTAL,VAR
            CALL      DISPITEM
          ENDIF
          RETURN
