          MOVE      ZERO,OVRCD
          PACK      KEY5,SCPSROW,SCPSCOL
          READ      APIN1FL1,KEY5;SCPSROW,SCPSCOL,APIN1MKY
          GOTO      OVERCOND IF OVER
          RETURN
