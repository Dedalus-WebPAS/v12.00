CHKTER00  MATCH    SP70,IBAITER
          IF       @EQUAL
            MOVE     APMAPPD,IBAITER
          ENDIF
          MOVE     IBAITER,KEY2
          CALL     RDAPPP1
          BRANCH   OVRCD,CHKTER10
          MOVE     ZERO,EXIT
          GOTO     CHKTER99
.
CHKTER10  CALL     DISIMP00
          BEEP
          MOVE     "Terms Code Invalid - ",DISPMSG
          CALL     MESSAGE1
          DISPLAY  *P1:24,*EL,"Terms Code           : "
          MOVE     ZERO,CKEYTYP
          MOVE     "24",CVERT
          MOVE     "24",CCOL
          CALL     KAPPPA00
          BRANCH   EXIT,CHKTER90,CHKTER90
          MOVE     APPPTER,IBAITER
          MOVE     ZERO,EXIT
          GOTO     CHKTER99
.
CHKTER90  MOVE     ONE,EXIT
CHKTER99  RETURN
.------------------------------------------------------------
. Check Dissection
.------------------------------------------------------------
CHKDIS00  MATCH    SP70,IBAIDIS
          IF       @EQUAL
            MOVE     APBMDIS,IBAIDIS
          ENDIF
          MOVE     IBAIDIS,KEY5
          CALL     RDFMDS1
          BRANCH   OVRCD,CHKDIS10
          MOVE     ZERO,EXIT
          GOTO     CHKDIS99
.
CHKDIS10  CALL     DISIMP00
          BEEP
          MOVE     "Dissection Invalid - ",DISPMSG
          CALL     MESSAGE1
          DISPLAY  *P1:24,*EL,"Dissection Code      : "
          MOVE     ZERO,CKEYTYP
          MOVE     "24",CVERT
          MOVE     "24",CCOL
          CALL     KFMDSA00
          BRANCH   EXIT,CHKDIS90,CHKDIS90
          MOVE     FMDSCODE,IBAIDIS
          MOVE     ZERO,EXIT
          GOTO     CHKDIS99
.
CHKDIS90  MOVE     ONE,EXIT
CHKDIS99  RETURN
.------------------------------------------------------------
. Check Responsibility
.------------------------------------------------------------
CHKRES00  MATCH    SP70,IBAIRES
          IF       @EQUAL
            MOVE     APBMRES,IBAIRES
          ENDIF
          MOVE     IBAIRES,KEY4
          CALL     RDFMRS1
          BRANCH   OVRCD,CHKRES10
          MOVE     ZERO,EXIT
          GOTO     CHKRES99
.
CHKRES10  CALL     DISIMP00
          BEEP
          MOVE     "Responsibility Invalid - ",DISPMSG
          CALL     MESSAGE1
          DISPLAY  *P1:24,*EL,"Responsibility Code  : "
          MOVE     ZERO,CKEYTYP
          MOVE     "24",CVERT
          MOVE     "24",CCOL
          CALL     KFMRSA00
          BRANCH   EXIT,CHKRES90,CHKRES90
          MOVE     FMRSCODE,IBAIRES
          MOVE     ZERO,EXIT
          GOTO     CHKRES99
.
CHKRES90  MOVE     ONE,EXIT
CHKRES99  RETURN
.------------------------------------------------------------
. Check Invoice Date
.------------------------------------------------------------
CHKIDT00  UNPACK   IBAIIDT,CCENT,CYEAR,CMON,CDAY
          CALL     VALDAT00
          BRANCH   EXIT,CHKIDT10
          MOVE     ZERO,EXIT
          GOTO     CHKIDT99
.
CHKIDT10  CALL     DISIMP00
          BEEP
          MOVE     "Invalid Invoice Date - ",DISPMSG
          CALL     MESSAGE1
          DISPLAY  *P1:24,*EL,"Invoice Date         : "
          MOVE     ZERO,CKEYTYP
          MOVE     "24",CVERT
          MOVE     "24",CCOL
          CALL     EINDT000
          BRANCH   EXIT,CHKIDT90,CHKIDT90
          MOVE     ZERO,EXIT
          GOTO     CHKIDT99
CHKIDT90  MOVE     ONE,EXIT
CHKIDT99  RETURN
.------------------------------------------------------------
. Display Import Record
.------------------------------------------------------------
DISIMP00 UNPACK   IBAIIDT,CCENT,CYEAR,CMON,CDAY
         CALL     PACDATE
         MOVE     CPCDATE,KEY10
         UNPACK   IBAIPDT,CCENT,CYEAR,CMON,CDAY
         CALL     PACDATE
         PACK     KEY6,IBAIGSTC,SP70
         CALL     RDIBGS1
.
         DISPLAY    *P1:3,*EF:
                   *P1:6,"                  ":
                      *V2LON,*ULON,"  Import Record Error  ",*HOFF:
                 *P1:8,"  Record Count   : ",*V2LON,SECTOR,*HOFF:
                 *P1:10,"  Creditor Code  : ",*V2LON,IBAICRD,*HOFF,SP1,APMACN1:
                 *P1:11,"  Reference      : ",*V2LON,IBAIREF,*HOFF:
                 *P1:12,"  Invoice Date   : ",*V2LON,KEY10,*HOFF:
                 *P1:13,"  Terms Code     : ",*V2LON,IBAITER,*HOFF,SP1,APPPDES:
                 *P1:14,"  Payable Date   : ",*V2LON,CPCDATE,*HOFF:
                 *P1:15,"  Remittance 1   : ",*V2LON,IBAICM1,*HOFF:
                 *P1:16,"  Remittance 2   : ",*V2LON,IBAICM2,*HOFF:
                 *P1:17,"  Comment        : ",*V2LON,IBAIICM,*HOFF:
                 *P1:18,"  Ledger/Account : ",*V2LON,IBAILED:
                                               "/",IBAIACC,*HOFF,SP1,FMAMDESC:
                 *P1:19,"  Dissection     : ",*V2LON,IBAIDIS,*HOFF,SP1,FMDSDESC:
                 *P1:20,"  Responsibility : ",*V2LON,IBAIRES,*HOFF,SP1,FMRSDESC:
                 *P1:21,"  Invoice Amount : ",*V2LON,IBAIAMT,*HOFF:
                 *P1:22,"  GST Amount     : ",*V2LON,IBAIGST,*HOFF:
                 *P1:23,"  GST Code       : ",*V2LON,IBAIGSTC,*HOFF:
                                              SP1,IBGSDESC
          RETURN
.------------------------------------------------------------
. Check for Valid Date
.------------------------------------------------------------
VALDAT00  MOVE      CMON,F2
          IF        ((F2<1)|(F2>12))
            GOTO       VALDAT90
          ENDIF
          BRANCH    F2,VALDAT31,VALDAT29,VALDAT31,VALDAT30,VALDAT31,VALDAT30:
                       VALDAT31,VALDAT31,VALDAT30,VALDAT31,VALDAT30,VALDAT31
.
VALDAT29  MOVE      CDAY,F2
          IF        ((F2<1)|(F2>29))
            GOTO       VALDAT90
          ENDIF
          MOVE      CYEAR,IYEAR
          MOVE      IYEAR,CLYEAR
          DIVIDE    FOUR,CLYEAR
          MULT      FOUR,CLYEAR
.
          COMPARE   IYEAR,CLYEAR
          GOTO      VALDAT50 IF EQUAL
          IF        (F2>28)
            GOTO       VALDAT90
          ENDIF
          GOTO       VALDAT50
.
VALDAT30  MOVE      CDAY,F2
          IF        ((F2<1)|(F2>30))
            GOTO       VALDAT90
          ENDIF

          GOTO      VALDAT50
.
VALDAT31  MOVE      CDAY,F2
          IF        ((F2<1)|(F2>31))
            GOTO       VALDAT90
          ENDIF
.
VALDAT50  MATCH     "19",CCENT            * Valid for 1900 to 2099
          GOTO      VALDAT60 IF EQUAL
          MATCH     "20",CCENT
          GOTO      VALDAT90 IF NOT EQUAL
.
VALDAT60  MOVE      ZERO,EXIT
          GOTO      VALDAT99
.
VALDAT90  MOVE      ONE,EXIT
.
VALDAT99  RETURN
.
.------------------------------------------------------------
. LOAD Import File Contents
.------------------------------------------------------------
LOAD0000  MOVE     ZERO,SECTOR
          CLOSE    IBAAINV1
          OPEN     IBAAINV1,VERIFDFN      * Position at Start
.
LOAD1000  READ     IBAAINV1,SEQ;IBAICRD,IBAIREF,IBAIIDT,IBAITER,IBAIPDT:
                                IBAICM1,IBAICM2,IBAIICM,IBAILED,IBAIACC:
                                IBAIDIS,IBAIRES,IBAIAMT,IBAIGST,IBAIGSTC
          GOTO     LOAD9999 IF OVER
.
          MOVE     IBAICRD,KEY5
          CALL     RDAPMA1
          PACK     KEY14,IBAILED,IBAIACC,SP70
          CALL     RDFMAM1
          MOVE     IBAIDIS,KEY5
          CALL     RDFMDS1
          MOVE     IBAIRES,KEY4
          CALL     RDFMRS1
          MOVE     IBAITER,KEY2
          CALL     RDAPPP1
          PACK     KEY6,IBAIGSTC
          CALL     RDIBGS1
.
          CALL     WRHED000          * Write IMS Header
          CALL     WRDET000          * Write INP Details
.
          GOTO     LOAD1000
.
LOAD9999  RETURN
.------------------------------------------------------------
. Write Header Record
.------------------------------------------------------------
WRHED000  MOVE     IBAICRD,APIMCRD
          MOVE     IBAIREF,APIMREF
          MOVE     IBAIREF,APIMDOC
          MOVE     ZERO,APIMTYP    * Type (0=inv,1=Crd,2=Pay)
          MOVE     ZERO,APIMUPD    * Updated 0=No
          MOVE     ZERO,APIMOUT    * Outstanding 0=No
          MOVE     IBAITER,APIMTER
          MOVE     IBAIIDT,APIMIDT
          MOVE     IBAIPDT,APIMPDT
          MOVE     APIMIDT,APIMDDT
          PACK     APIMTDT,CCC,CYY,CMM,CDD
          MOVE     IBAIAMT,APIMTOT
          MOVE     ZERO,APIMDIS
          MOVE     IBAIGST,APIMGST
          MOVE     ZERO,APIMPAY    * Paid=0
          MOVE     IBAICM1,APIMCM1
          MOVE     IBAICM2,APIMCM2
          MOVE     IBAIICM,APIMICM
          REP      " 0",APIMIDT
          REP      " 0",APIMPDT
          REP      " 0",APIMTDT
          MOVE     SP1,APIMPAYG
          PACK     KEY40,APBMBCH,APIMCRD,APIMREF,APIMDOC
          UNPACK   KEY40,APIMBCH,APIMCRD,APIMREF,APIMDOC
          CALL     RDAPIM1
          IF       OVRCD=0
            ADD      IBAIAMT,APIMTOT
            ADD      IBAIGST,APIMGST
            CALL     UPAPIM1
          ELSE
            CALL     WRAPIM1
          ENDIF
          RETURN
.------------------------------------------------------------
. Write Detail Record
.------------------------------------------------------------
WRDET000  MOVE      SP70,APINORD       * Must be an add
          MOVE      ZERO,F3
WRDET100  ADD       ONE,F3
          PACK      KEY51,APIMBCH,APIMCRD,APIMREF,APIMDOC,APINORD,F3
          CALL      RAAPIN1
          COMPARE   ZERO,OVRCD
          GOTO      WRDET100 IF EQUAL
.
WRDET500  MOVE      F3,APINLIN
          PACK      KEY51,APIMBCH,APIMCRD,APIMREF,APIMDOC,APINORD,APINLIN
          UNPACK    KEY51,APINBCH,APINCRD,APINREF,APINDOC,APINORD,APINLIN
          MOVE      ZERO,APINUPD
          MOVE      IBAILED,APINLED
          MOVE      IBAIACC,APINACC
          MOVE      IBAIDIS,APINDIS
          MOVE      IBAIRES,APINRES
          MOVE      SP70,APINCAT
          MOVE      IBAIICM,APINDES
          MOVE      ZERO,APINQTY
          MOVE      ZERO,APINUCT
          MOVE      ZERO,APINPAY
          MOVE      IBAIAMT,APINAMT
          MOVE      IBAIGST,APINGST
          MOVE      SP6,APINCGS
          MOVE      SP3,APINBASC
.
          COMPARE   ZERO,APINGST
          GOTO      WRDET600 IF EQUAL
.
          MATCH     SP6,IBAIGSTC
          IF        @EQUAL
            MOVE      FMCOINGS,APINCGS
          ELSE
            PACK      KEY6,IBAIGSTC,SP70
            CALL      RDIBGS1
            IF        OVRCD=1
              MOVE      FMCOINGS,APINCGS
            ELSE
              MOVE      IBAIGSTC,APINCGS
            ENDIF
          ENDIF
.
          MOVE      SP3,APINBASC
          PACK      KEY6,APINCGS,SP70
          CALL      RDIBGS1
          BRANCH    OVRCD,WRDET600
.
          IF        BASFLAG=1
            MOVE      IBGSBASC,APINBASC
          ENDIF
.
WRDET600  MOVE      SP1,APINPAYG
          MOVE      SP70,APINSPA
          CALL      RAAPIN1
          IF        OVRCD=1
            CALL      WRAPIN1
          ENDIF
.
          RETURN
.
.----------------------------------------------------------------------
. Save Header Details
.----------------------------------------------------------------------
SAVHED00  MATCH     "New  ",APBMBCH
          GOTO      SAVHED20 IF NOT EQUAL
          MOVE      SP70,APBMUDT
.
SAVHED10  CALL      RDFMCOCC
          MOVE      FMBCCBAT,APBMBCH
          MATCH     "00000",APBMBCH
          GOTO      PHED0010 IF EQUAL
          PACK      KEY5,APBMBCH
          CALL      RAAPBM1
          COMPARE   ZERO,OVRCD
          GOTO      SAVHED10 IF EQUAL
.
SAVHED20  MOVE     APBMBCH,APIMBCH
          PACK     KEY5,APBMBCH
          MOVE     SP70,APBMUDT
          MOVE     "IN",APBMTYP
          MOVE     SP70,APBMCHQ
          CALL     RAAPBM1
          IF       OVRCD=0
            CALL     UPAPBM1
          ELSE
            CALL     WRAPBM1
            CALL     RLAPBM1
          ENDIF
.
SAVHED99  RETURN
.
UPDHED00  MOVE      ZERO,BATCHCAL
          PACK      KEY40,APBMBCH,SP70
          CALL      RSAPIM1
UPDHED10  CALL      RKAPIM1
          BRANCH    OVRCD,UPDHED20
          MATCH     APBMBCH,APIMBCH
          GOTO      UPDHED20 IF NOT EQUAL
          ASSIGN    APIMTOT+APIMGST-APIMDIS+BATCHCAL,BATCHCAL
          GOTO      UPDHED10
.
UPDHED20  RETURN
.----------------------------------------------------------------------
. Save Header Details
.----------------------------------------------------------------------
DISPLAST  MOVE      "   30",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      LASTCRED,VAR
            CALL      DISPITEM
          ENDIF
          MOVE      "   31",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      LASTINV,VAR
            CALL      DISPITEM
          ENDIF
          MOVE      "   32",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            IF        LASTTOT=0
              MOVE      SP70,VAR
            ELSE
              MOVE      LASTTOT,VAR
            ENDIF
            CALL      DISPITEM
          ENDIF
          MOVE      "   34",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      APBMTOT,VAR
            CALL      DISPITEM
          ENDIF
          MOVE      "   35",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            IF        BATCHCAL=0
              MOVE      SP70,VAR
            ELSE
              MOVE      BATCHCAL,VAR
            ENDIF
            CALL      DISPITEM
          ENDIF
.
          MOVE      "   36",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      APBMBCH,VAR
            CALL      DISPITEM
          ENDIF
.
          RETURN
.----------------------------------------------------------------------
. Calculate Allocation Total
.----------------------------------------------------------------------
HEDTOT00  COMPARE   TWO,IBCNUGST
          GOTO      HEDTOT20 IF EQUAL
.
          ASSIGN    APIMTOT-APIMDIS+APIMGST,INVTOTAL
          MOVE      INVTOTAL,DOCTOTAL
          ASSIGN    APIMTOT-APIMDIS,ALLTOTAL
          GOTO      HEDTOT80
.
HEDTOT20  IF        PAYASGO=1
            ASSIGN    APIMTOT-APIMDIS-PAYASTOT,INVTOTAL
            ASSIGN    APIMTOT-APIMDIS,ALLTOTAL
            MOVE      ALLTOTAL,DOCTOTAL
          ELSE
            ASSIGN    APIMTOT-APIMDIS,INVTOTAL
            MOVE      INVTOTAL,DOCTOTAL
            ASSIGN    APIMTOT-APIMDIS-APIMGST,ALLTOTAL
          ENDIF
.
HEDTOT80  MOVE      "   28",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      INVTOTAL,VAR
            CALL      DISPITEM
          ENDIF
          MOVE      "   29",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      ALLTOTAL,VAR
            CALL      DISPITEM
          ENDIF
          MOVE      "   60",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      DOCTOTAL,VAR
            CALL      DISPITEM
          ENDIF
          MOVE      "   61",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      DOCTOTAL,VAR
            CALL      DISPITEM
          ENDIF
          RETURN
.----------------------------------------------------------------------
. Delete Invoice Details
.----------------------------------------------------------------------
DAPIM000  MOVE     "Delete Invoice. ",DISPMSG
          CALL     MESSAGE4
          BRANCH   EXIT,DAPIM900
          PACK     KEY40,APBMBCH,APIMCRD,APIMREF,APIMDOC
          UNPACK   KEY40,APIMBCH,APIMCRD,APIMREF,APIMDOC
          CALL     RDAPIM1                                      * record exists?
          IF       OVRCD=0
            CALL     DEAPIM1                                    * delete record
            ASSIGN   BATCHCAL-APIMTOT-APINGST+APIMDIS,BATCHCAL  * update total
          ENDIF
.
          PACK     KEY51,APIMBCH,APIMCRD,APIMREF,APIMDOC,SP70
          CALL     RSAPIN1
.
DAPIM100  CALL     RKAPIN1
          BRANCH   OVRCD,DAPIM199
          MATCH    APIMBCH,APINBCH
          GOTO     DAPIM199 IF NOT EQUAL
          MATCH    APIMCRD,APINCRD
          GOTO     DAPIM199 IF NOT EQUAL
          MATCH    APIMREF,APINREF
          GOTO     DAPIM199 IF NOT EQUAL
          MATCH    APIMDOC,APINDOC
          GOTO     DAPIM199 IF NOT EQUAL
.
          PACK     KEY51,APINBCH,APINCRD,APINREF,APINDOC,APINORD,APINLIN
          CALL     DEAPIN1
          CALL     RSAPIN1
          GOTO     DAPIM100
.
DAPIM199  MOVE     "2",EXIT
          GOTO     DAPIM999
.
DAPIM900  MOVE     "1",EXIT
.
DAPIM999  RETURN
.----------------------------------------------------------------------
.     INVOICE ENTRY
.     -------------
. Save Header Details and Input Details
.----------------------------------------------------------------------
WAPIM000  IF       IBCNUGST=2
            ASSIGN   (APIMTOT-APIMGST),APIMTOT
          ENDIF
.
          IF       !(PAIDOUT=0)
            IF       (APIMTOT+APIMGST-APIMDIS)>PAIDOUT
              CLEAR    DISPMSG
              APPEND   "Must Be Less Than Outstanding Payment = ",DISPMSG
              APPEND   PAIDOUT,DISPMSG
              APPEND   " - ",DISPMSG
              RESET    DISPMSG
              CALL     MESSAGE1
              MOVE     ONE,EXIT
              IF       IBCNUGST=2
                 ASSIGN   (APIMTOT+APIMGST),APIMTOT
               ENDIF
              GOTO     WAPIM999
            ENDIF
          ENDIF
.
          MOVE     ZERO,APIMTYP    * Type (0=inv,1=Crd,2=Pay)
          MOVE     ZERO,APIMUPD    * Updated 0=No
          MOVE     ZERO,APIMOUT    * Outstanding 0=No
          MOVE     ZERO,APIMPAY    * Paid=0
          MOVE     APIMIDT,APIMDDT
          PACK     APIMTDT,CCC,CYY,CMM,CDD
          REP      " 0",APIMTDT
          MOVE     SP1,APIMPAYG
          IF       PAYASGO=1
            MOVE     ANSP,APIMPAYG
          ENDIF
          IF       NOGST=1
            MOVE     ANSN,APIMPAYG
          ENDIF
.
          PACK     KEY40,APBMBCH,APIMCRD,APIMREF,APIMDOC
          UNPACK   KEY40,APIMBCH,APIMCRD,APIMREF,APIMDOC
          CALL     RAAPIM1
          IF       OVRCD=0
            CALL     UPAPIM1
          ELSE
            CALL     WRAPIM1
          ENDIF
.
          MOVE       ZERO,TOTALLOC
          MOVE       ZERO,GSTALLOC
          PACK       KEY40,APBMBCH,APIMCRD,APIMREF,APIMDOC,SP70
          PACK       KEY51,APBMBCH,APIMCRD,APIMREF,APIMDOC,SP70
          CALL       RSAPIN1
.
WAPIM100  CALL       RKAPIN1
          BRANCH     OVRCD,WAPIM200
          PACK       KEY51,APINBCH,APINCRD,APINREF,APINDOC,SP70
          MATCH      KEY40,KEY51
          GOTO       WAPIM200 IF NOT EQUAL
          ADD        APINAMT,TOTALLOC
          ADD        APINGST,GSTALLOC
          GOTO       WAPIM100
.
. Display Invoice Header Details Screen 06
.
WAPIM200  MOVE     "06",SCRID
          CALL     SBACK000 * Display Background
          CALL     DF06L000 * Display Fields
.
. Enter Details Screen 02
.
          PACK     APIMKEY,APIMBCH,APIMCRD,APIMREF,APIMDOC
          CALL     SC02L000
.
          SUB      SAVETOT,BATCHCAL
          PACK     KEY40,APIMBCH,APIMCRD,APIMREF,APIMDOC
          CALL     RAAPIM1
          IF       OVRCD=0
            MOVE     APMACN1,LASTCRED
            MOVE     XYZTOTAL,LASTTOT
            ADD      LASTTOT,BATCHCAL
          ENDIF
          MOVE     TWO,EXIT
.
WAPIM999  RETURN
.
.-------------------------------------------------------------------------
. Enter Creditor Code
. Code to Default to Last Entered Commented Out as per S.A.H.B. 18/05/92
.-------------------------------------------------------------------------
KAPMA000  MOVE     ZERO,PAYASGO
          MOVE     SP1,APIMPAYG
          ENDSET   APMACRD
          APPEND   SP70,APMACRD
          RESET    APMACRD
          MATCH    SP70,APMACRD
          IF       @EQUAL | FMDEFCRD=0
            OPEN     APSMXRA1,"apsmxraf"
            OPEN     APSMXRA2,"apsmxraf"
            CALL     KAPMAA00
            CLOSE    APSMXRA1
            CLOSE    APSMXRA2
            LOAD     ACCEPT,EXIT,ZERO,ZERO  * clear accept if quiting screen
            BRANCH   EXIT,KAPMA999,KAPMA999
            IF       APMAHIN=1
              MOVE     "Creditor On Hold for Invoice Input - ",DISPMSG
              CALL     MESSAGE1
              MOVE     SP70,APMACRD
              GOTO     KAPMA000
            ENDIF
            MOVE     APMACRD,APIMCRD
          ELSE
            MOVE     APMACRD,APIMCRD
            MOVE     APIMCRD,KEY5
            CALL     RDAPMA1
            IF       OVRCD=1
              MOVE     SP70,APIMCRD
              MOVE     SP70,APMACRD
              GOTO     KAPMA000
            ENDIF
            MOVE     ZERO,EXIT
          ENDIF
.
          MOVE     ZERO,NOGST
          MOVE     ZERO,HASABN                                         *I-14452
.
          COMPARE  TWO,IBCNUGST
          GOTO     KAPMA999 IF NOT EQUAL
.
          MATCH    SP11,APMAABN
          GOTO     KAPMA900 IF NOT EQUAL
.
          MOVE     SP70,DISPMSG
          CALL     MESSAGE2
.
          KEYIN    *P1:24,*EL,*B,*V2LON,"WARNING: ",*HOFF:
                   "No ABN Number. Enter ABN : ",*V2LON,APMAABN;
.
          ENDSET   APMAABN
          APPEND   SP70,APMAABN
          RESET    APMAABN
          MATCH    SP11,APMAABN
          IF       @EQUAL
            CALL     MESSAGE3
            GOTO     KAPMA300
          ENDIF
.
          DISPLAY   *P49:24,*EL,"GST Registered ? "
          MOVE      SIXTY6,CCOL
          MOVE      TWENTY4,CVERT
          MOVE      "Y",DEFYESNO
          CALL      YORN0000
          CALL      MESSAGE3
          MOVE      FYESNO,APMAGREG
          CALL      UPAPMA1
          PACK      KEY5,APMACRD,SP70
          CALL      RDAPMA1
          GOTO      KAPMA900
.
KAPMA300  PACK      KEY6,FMCOPAYG,SP70
          CALL      RDIBGS1
          IF        OVRCD=1
            CLEAR     DISPMSG
            APPEND    "Invalid Pay As You Go Tax Code (",DISPMSG
            APPEND    FMCOPAYG,DISPMSG
            APPEND    ") - ",DISPMSG
            RESET     DISPMSG
            CALL      MESSAGE1
            GOTO      KAPMA999
          ENDIF
.
          PACK      KEY5,FMCOPGCR,SP70
          CALL      RDAPMA1
          IF        OVRCD=1
            CLEAR     DISPMSG
            APPEND    "Invalid Pay As You Go Creditor Code (",DISPMSG
            APPEND    FMCOPGCR,DISPMSG
            APPEND    ") - ",DISPMSG
            RESET     DISPMSG
            CALL      MESSAGE1
            GOTO      KAPMA999
          ENDIF
.
          PACK      KEY5,APIMCRD,SP70
          CALL      RDAPMA1
.
          PACK     KEY8,CCC,CYY,CMM,CDD,SP70
          REP      " 0",KEY8
.
          PACK     KEY14,IBGSCODE,KEY8,SP70
          CALL     RDIBGE1
          IF       OVRCD=1
            CALL     RPIBGE1
            BRANCH   OVRCD,KAPMA600
            MATCH    IBGSCODE,IBGECODE
            GOTO     KAPMA600 IF NOT EQUAL
          ENDIF
          GOTO      KAPMA700
.
KAPMA600  CLEAR     DISPMSG
          APPEND    "Invalid Pay As You Go Tax Code (",DISPMSG
          APPEND    FMCOPAYG,DISPMSG
          APPEND    ") - ",DISPMSG
          RESET     DISPMSG
          CALL      MESSAGE1
          GOTO      KAPMA000
.
KAPMA700  COMPARE  ZERO,IBGEAMNT
          GOTO     KAPMA800 IF NOT EQUAL
.
          CLEAR     DISPMSG
          APPEND    "Invalid Pay As You Go Tax Rate (",DISPMSG
          APPEND    IBGEAMNT,DISPMSG
          APPEND    ") For Code ",DISPMSG
          APPEND    FMCOPAYG,DISPMSG
          APPEND    " - ",DISPMSG
          RESET     DISPMSG
          CALL      MESSAGE1
          GOTO      KAPMA000
.
KAPMA800  MOVE     SP70,DISPMSG
          CALL     MESSAGE2
          KEYIN    *P1:24,*EL,*B,"Do You Want to Deduct ",*V2LON,*DV,IBGEAMNT:
                   *HOFF," ? ",*V2LON,ANS
          CALL     MESSAGE3
          REP      UPPLOW,ANS
          CMATCH   "Y",ANS
          GOTO     KAPMA870 IF NOT EQUAL
.
KAPMA850  MOVE     ONE,PAYASGO
          MOVE     "P",APIMPAYG
          GOTO     KAPMA999
.
KAPMA870  MOVE     ONE,NOGST
          MOVE     "N",APIMPAYG
          GOTO     KAPMA999
.
KAPMA900  IF       APMAGREG=0
.           MOVE     ONE,NOGST
            MOVE     "N",APIMPAYG
            MOVE     ONE,HASABN                                        *I-14452
          ENDIF
.
KAPMA999  RETURN
.----------------------------------------------------------------------
. Enter Invoice Reference
.----------------------------------------------------------------------
KAPIM000  MOVE     ZERO,SAVETOT
          MOVE     ZERO,PAIDOUT
          MOVE     APMACRD,APIMCRD
          CALL     KAPIMA00
          BRANCH   EXIT,KAPIM900,KAPIM900
.
          MOVE     APIMREF,APIMDOC
          PACK     KEY35,APIMCRD,APIMREF,APIMDOC,SP70
          PACK     KEY40,APIMCRD,APIMREF,APIMDOC,SP70
          CALL     RSAPIM3
.
KAPIM100  CALL     RKAPIM3
          BRANCH   OVRCD,KAPIM200
          PACK     KEY40,APIMCRD,APIMREF,APIMDOC,SP70
          MATCH    KEY35,KEY40
          GOTO     KAPIM200 IF NOT EQUAL
.
          MATCH    "0",APIMTYP           * Invoice ?
          GOTO     KAPIM100 IF NOT EQUAL
.
          MATCH    APBMBCH,APIMBCH
          GOTO     KAPIM100 IF EQUAL
          UNPACK   APIMIDT,CCENT,CYEAR,CMON,CDAY
          CALL     PACDATE
          CLEAR    DISPMSG
          APPEND   "Duplicate Reference in Batch ",DISPMSG
          APPEND   APIMBCH,DISPMSG
          APPEND   " Dated ",DISPMSG
          APPEND   CPCDATE,DISPMSG
          APPEND   " - ",DISPMSG
          MATCH    "1",FMCODUPI        * Disallow Duplicate Inv. Ref's ?
          IF       @EQUAL
            CALL     MESSAGE1
            GOTO     KAPIM000
          ELSE
            RESET    DISPMSG
            MOVE     "N",DEFYESNO
            CALL     MESSAGE4
            MOVE     " ",DEFYESNO
            BRANCH   EXIT,KAPIM000
            MOVE     ZERO,EXIT
          ENDIF
.
KAPIM200  UNPACK   KEY35,APIMCRD,APIMREF,APIMDOC
          PACK     KEY40,APBMBCH,APIMCRD,APIMREF,APIMDOC,SP70
          UNPACK   KEY40,APIMBCH,APIMCRD,APIMREF,APIMDOC
          MOVE     ZERO,APIMTYP
          MOVE     ZERO,APIMUPD
          MOVE     SP70,APIMTER
          MOVE     SP70,APIMPDT
          MOVE     SP70,APIMTDT
          MOVE     ZERO,APIMTOT
          MOVE     ZERO,APIMDIS
          MOVE     ZERO,APIMGST
          MOVE     SP70,APIMCM1
          MOVE     SP70,APIMCM2
          MOVE     ZERO,APIMDIS
          MOVE     ZERO,APIMPAY
          MOVE     SP70,APIMSPA
          PACK     KEY40,APBMBCH,APIMCRD,APIMREF,SP70
          PACK     KEY25,APIMBCH,APIMCRD,APIMREF,SP70
          CALL     RSAPIM1
          CALL     RKAPIM1
          BRANCH   OVRCD,KAPIM800
          PACK     KEY40,APIMBCH,APIMCRD,APIMREF,SP70
          MATCH    KEY25,KEY40
          GOTO     KAPIM800 IF NOT EQUAL
          IF       IBCNUGST=2
            ASSIGN   APIMTOT+APIMGST,APIMTOT
          ENDIF
          ASSIGN   APIMTOT+APIMGST-APIMDIS,SAVETOT
          MOVE     APIMTER,KEY2
          CALL     RDAPPP1
          IF       OVRCD=1
            MOVE     ZERO,OVRCD
            MOVE     SP70,APPPDES
          ENDIF
          GOTO     KAPIM999
.
. New Entry
.
KAPIM800  UNPACK   KEY25,APIMBCH,APIMCRD,APIMREF
          CALL     SUSP0000        * Check for Payments in Suspense
          BRANCH   EXIT,KAPIM900   * Abort Selected
          MOVE     "   17",FLDID
          CALL     GETPOSIT
          MOVE     ZERO,EXIT
          MOVE     ONE,ACCEPT
          MOVE     ONE,OVRCD
.
          MOVE     SP70,APIMCM1        * clear comments
          MOVE     SP70,APIMCM2
          GOTO     KAPIM999
.
. Exit No Entry
.
KAPIM900  MOVE     ONE,EXIT
          MOVE     SP70,APMACRD
.
KAPIM999  RETURN
.
SUSP0000  PACK     KEY25,APIMBCH,APIMCRD,APIMREF
          PACK     LINKSAV,APIMBCH,APIMCRD,APIMREF
          PACK     KEY40,APIMCRD,SP70
          CALL     RSAPIM3
          CALL     RKAPIM3
          BRANCH   OVRCD,SUSP9000
          MATCH    APIMCRD,APMACRD
          GOTO     SUSP9000 IF NOT EQUAL
          MATCH    SP70,APIMREF
          GOTO     SUSP9000 IF NOT EQUAL
.
. Payments in Suspence Records Exist
.--------------------------------------------------
          MOVE      ZERO,HLEF
          CALL      GETSCR00
.
          MOVE      ZERO,NONCHQ  * clear no cheque flag
          CALL      SCSPL000     * Display Payments in Suspence
.
          UNPACK    LINKSAV,APIMBCH,APIMCRD,APIMREF
          LOAD      APIMDOC,NONCHQ,APIMREF  * NONCHQ=1 if no cheque selected
.
          CALL      PUTSCR00
          MOVE      "01",SCRID
          MOVE      ZERO,PRCOL
          MOVE      ZERO,PRVERT
          MOVE      "    1",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE     SCPSCOL,PRCOL
            MOVE     SCPSROW,PRVERT
          ENDIF
          MOVE      ZERO,EXIT
          GOTO      SUSP9999
.
. No Payments in Suspence Records Exist
.--------------------------------------------------
SUSP9000  UNPACK    LINKSAV,APIMBCH,APIMCRD,APIMREF
          MOVE      APIMREF,APIMDOC
          MOVE      ZERO,EXIT
          GOTO      SUSP9999
.
SUSP9999  RETURN
.
.----------------------------------------------------------------------
. Enter Invoice Date
.----------------------------------------------------------------------
EINDT000
          MOVE      CCC,CCENT
          MOVE      CYY,CYEAR
          MOVE      CMM,CMON
          MOVE      SP70,CDAY
          CALL      KEYDATE
          BRANCH    OVRCD,EINDT900
          BRANCH    CQUEST,EINDT000
          MATCH     SP70,CDAY
          GOTO      EINDT900 IF EQUAL
.
          PACK      APIMIDT,CCENT,CYEAR,CMON,CDAY
          PACK      KEY8,CCC,CYY,CMM,CDD
          REP       " 0",APIMIDT
          REP       " 0",KEY8
          MATCH     KEY8,APIMIDT
          GOTO      EINDT800 IF EQUAL
          MATCH     KEY8,APIMIDT
          IF        !@LESS
            MOVE      "Date in the Future. ",DISPMSG
            CALL      MESSAGE4   * Message Continue (Y/N) EXIT=1 No
            BRANCH    EXIT,EINDT000
            GOTO      EINDT800
          ENDIF
.
          UNPACK    APIMIDT,KEY4,KEY2
          MOVE      KEY4,TSTCCYY1
          MOVE      KEY2,TSTMM1
.
          UNPACK    KEY8,KEY4,KEY2
          MOVE      KEY4,TSTCCYY2
          MOVE      KEY2,TSTMM2
          IF        TSTCCYY2-TSTCCYY1>1
            MOVE      "Date Over 1 Year. ",DISPMSG
            CALL      MESSAGE4   * Message Continue (Y/N) EXIT=1 No
            BRANCH    EXIT,EINDT000
            GOTO      EINDT800
          ENDIF
          IF        TSTCCYY2=TSTCCYY1
            IF        TSTMM2-TSTMM1>6
              MOVE      "Date Over 6 Months. ",DISPMSG
              CALL      MESSAGE4   * Message Continue (Y/N) EXIT=1 No
              BRANCH    EXIT,EINDT000
              GOTO      EINDT800
            ENDIF
          ELSE
            IF        TSTMM2-TSTMM1+12>6
              MOVE      "Date Over 6 Months. ",DISPMSG
              CALL      MESSAGE4   * Message Continue (Y/N) EXIT=1 No
              BRANCH    EXIT,EINDT000
              GOTO      EINDT800
            ENDIF
          ENDIF
EINDT800
          MOVE      ZERO,EXIT
          GOTO      EINDT999
.
EINDT900  MOVE      TWO,EXIT
EINDT999
          RETURN
.----------------------------------------------------------------------
. Enter Date to Pay Invoice APIMPDT
.----------------------------------------------------------------------
EPYDT000
          CALL      PAYDAT00         * Determine Payment Date
          UNPACK    APIMPDT,CCENT,CYEAR,CMON,CDAY
          CALL      KEYDATE
          BRANCH    OVRCD,EPYDT000
          BRANCH    CQUEST,EPYDT000
.
          PACK      APIMPDT,CCENT,CYEAR,CMON,CDAY
          PACK      KEY8,CCC,CYY,CMM,CDD
          REP       " 0",APIMPDT
          REP       " 0",KEY8
.
          MATCH     KEY8,APIMPDT
          IF        @LESS
            MOVE      "Date in the Past. ",DISPMSG
            CALL      MESSAGE4   * Message Continue (Y/N) EXIT=1 No
            BRANCH    EXIT,EPYDT000
            GOTO      EPYDT800
          ENDIF
.
          UNPACK    APIMPDT,KEY4,KEY2
          MOVE      KEY4,TSTCCYY1
          MOVE      KEY2,TSTMM1
.
          UNPACK    KEY8,KEY4,KEY2
          MOVE      KEY4,TSTCCYY2
          MOVE      KEY2,TSTMM2
          IF        TSTCCYY1-TSTCCYY2>1
            MOVE      "Date Over 1 Year. ",DISPMSG
            CALL      MESSAGE4   * Message Continue (Y/N) EXIT=1 No
            BRANCH    EXIT,EPYDT000
            GOTO      EPYDT800
          ENDIF
          IF        TSTCCYY2=TSTCCYY1
            IF        TSTMM1-TSTMM2>6
              MOVE      "Date Over 6 Months. ",DISPMSG
              CALL      MESSAGE4   * Message Continue (Y/N) EXIT=1 No
              BRANCH    EXIT,EPYDT000
              GOTO      EPYDT800
            ENDIF
          ELSE
            IF        TSTMM1-TSTMM2+12>6
              MOVE      "Date Over 6 Months. ",DISPMSG
              CALL      MESSAGE4   * Message Continue (Y/N) EXIT=1 No
              BRANCH    EXIT,EPYDT000
              GOTO      EPYDT800
            ENDIF
          ENDIF
EPYDT800
          MOVE      ZERO,EXIT
          GOTO      EPYDT999
.
EPYDT900  MOVE      TWO,EXIT
EPYDT999
          RETURN
.------------------------------------------------------
. Determine Payment Date from Terms Code
.------------------------------------------------------
PAYDAT00
          MATCH     SP70,APIMTER
.         GOTO      PAYDAT99 IF EQUAL
          GOTO      PAYDAT10 IF EQUAL
.
          IF        APPPDA1=9999
            MOVE      "99990101",APIMPDT
            GOTO      PAYDAT99
          ENDIF
.
          BRANCH    APPPTYP,PAYDAT10,PAYDAT20,PAYDAT50,PAYDAT50
          GOTO      PAYDAT30
.
PAYDAT10  UNPACK    APBMDAT,CCENT,CYEAR,CMON,CDAY * Batch Entered Date
          GOTO      PAYDAT40
.
PAYDAT20  UNPACK    APBMADT,CCENT,CYEAR,CMON,CDAY * Batch Period Date
          GOTO      PAYDAT40
.
PAYDAT30  UNPACK    APIMIDT,CCENT,CYEAR,CMON,CDAY * Invoice Date
.
PAYDAT40  MOVE      CCENT,CC
          MOVE      CYEAR,YY
          MOVE      CMON,MM
          MOVE      CDAY,DD
          CALL      DMYCON
          ADD       APPPDA1,JULDAY                * Add First Level of Terms
.         SUB       ONE,JULDAY
          MOVE      JULDAY,JWKDAY
          MOVE      JULYR,JWKYER
          MOVE      JULCC,JWKCC
          CALL      JULCON
          GOTO      PAYDAT90
.
. Fixed Day of Month
.
PAYDAT50  UNPACK    APIMIDT,CCENT,CYEAR,CMON,CDAY * Invoice Date
          PACK      DIM4,CCENT,CYEAR
          REP       " 0",DIM4
          MOVE      CMON,MM
          MOVE      APPPDA1,DD
          IF        APPPTYP=4
            ADD       ONE,MM
            IF        MM>12
              MOVE      ONE,MM
              MOVE      DIM4,FORM4
              ADD       ONE,FORM4
              MOVE      FORM4,DIM4
            ENDIF
          ENDIF
          IF        APPPTYP<>4 & CDD>APPPDA1
            ADD       ONE,MM
            IF        MM>12
              MOVE      ONE,MM
              MOVE      DIM4,FORM4
              ADD       ONE,FORM4
              MOVE      FORM4,DIM4
            ENDIF
          ENDIF
          UNPACK    DIM4,CCENT,CYEAR
          MOVE      CCENT,CC
          MOVE      CYEAR,YY
          CALL      DMLEAPYR                  * determine if a leap year
          BRANCH    MM,PAYDAT61,PAYDAT62,PAYDAT61,PAYDAT60,PAYDAT61,PAYDAT60:
                       PAYDAT61,PAYDAT61,PAYDAT60,PAYDAT61,PAYDAT60,PAYDAT61
PAYDAT60
          IF        DD>30
            MOVE      "30",DD
          ENDIF
          GOTO      PAYDAT90
PAYDAT61
          IF        DD>31
            MOVE      "31",DD
          ENDIF
          GOTO      PAYDAT90
PAYDAT62
          MOVE      "28",F2
          ADD       LEAPYR,F2
          IF        DD>F2
            MOVE      F2,DD
          ENDIF
.
PAYDAT90  PACK      APIMPDT,CC,YY,MM,DD,SP70
          REP       " 0",APIMPDT
.
PAYDAT99  RETURN
.----------------------------------------------------------------------
. Enter Terms with Creditor Default
.----------------------------------------------------------------------
ETERM000
          IF        ACCEPT=1
            MOVE      APMAPPD,APIMTER
          ENDIF
          MATCH     SP70,APIMTER
          IF        @EQUAL
            MOVE      SP70,APPPTER
            MOVE      SP70,APPPDES
            MOVE      ZERO,APPPDA1
            MOVE      ZERO,APPPPE1
            MOVE      ZERO,APPPDA2
            MOVE      ZERO,APPPPE2
            MOVE      ZERO,APPPDA3
            MOVE      ZERO,APPPPE3
          ELSE
            MOVE      APIMTER,KEY2
            CALL      RDAPPP1
            IF        OVRCD=0
               MOVE      "   22",FLDID
               CALL      GETPOSIT
               IF        EXIT=0
                 MOVE      APPPDES,VAR
                 CALL      DISPITEM
               ENDIF
               MOVE      "   21",FLDID
               CALL      GETPOSIT
               IF        EXIT=0
                 MOVE      APPPTER,VAR
                 CALL      DISPITEM
               ENDIF
            ENDIF
          ENDIF
.
          MOVE      ZERO,CKEYTYP
          CALL      KAPPPZ00
          MOVE      APPPTER,APIMTER
          BRANCH    EXIT,ETERM999,ETERM999
.
          COMPARE   ONE,ACCEPT
          GOTO      ETERM999 IF EQUAL
          CALL      PAYDAT00
          MOVE      "   19",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            UNPACK    APIMPDT,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
            MOVE      CPCDATE,VAR
            CALL      DISPITEM
          ENDIF
          COMPARE     ZERO,APPPPE1
          IF          @EQUAL
            IF          !(APIMDIS=0)
              MOVE        ZERO,APIMDIS
              MOVE      "   24",FLDID
              CALL      GETPOSIT
              IF        EXIT=0
                MOVE      APIMDIS,VAR
                CALL      DISPITEM
              ENDIF
              CALL      ALLTOT00
            ENDIF
          ELSE
            IF        !((APPPPE1*APIMTOT)/100.0=APIMDIS)
              ASSIGN    (APPPPE1*APIMTOT)/100.0,APIMDIS
              MOVE      "   24",FLDID
              CALL      GETPOSIT
              IF        EXIT=0
                MOVE      APIMDIS,VAR
                CALL      DISPITEM
              ENDIF
              CALL      ALLTOT00
            ENDIF
          ENDIF
          MOVE      "   21",FLDID
          CALL      GETPOSIT
ETERM999
          RETURN
.
.---------------------------------------------------------------------------
.   System    :   Accounts Payable System
.   Include   :   APSPPDKY
.   Routine   :   KAPPPZ00 Keyin Terms
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KAPPPZ00 TOPIC     "APPPTER "
          KEYIN     *PCCOL:CVERT,*RV,APPPTER:
                    *PCCOL:CVERT,*DV,APPPTER
.
          ENDSET    APPPTER
          APPEND    SP70,APPPTER
          RESET     APPPTER
.
          MATCH     SP70,APPPTER
          GOTO      KAPPPZ90 IF EQUAL
.
          CMATCH    EXITCHAR,APPPTER
          GOTO      KAPPPZ95 IF EQUAL
.
          CMATCH    QUEST,APPPTER
          GOTO      KAPPPZ10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HAPPPA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    APPPTER
          APPEND    SP70,APPPTER
          RESET     APPPTER
.
          MATCH     SP70,APPPTER
          GOTO      KAPPPZ00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,APPPTER
KAPPPZ10
          PACK      KEY2,APPPTER
          CALL      RAAPPP1
          BRANCH    OVRCD,KAPPPZ80
          BRANCH    CKEYTYP,KAPPPZ75
          CALL      RDAPPP1
KAPPPZ70
          MOVE      ZERO,EXIT
          GOTO      KAPPPZ99
KAPPPZ75
          MOVE      "Terms Already Exist - ",DISPMSG
          CALL      MESSAGE1
          GOTO      KAPPPZ00
KAPPPZ80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KAPPPZ99,KAPPPZ99
          MOVE      "Terms Does Not Exist - ",DISPMSG
          CALL      MESSAGE1
          GOTO      KAPPPZ00
KAPPPZ90
          MOVE      ONE,EXIT
          GOTO      KAPPPZ99
KAPPPZ95
          MOVE      TWO,EXIT
KAPPPZ99  TOPIC    PRGID
          RETURN
.
.----------------------------------------------------------------------
. Enter Invoice Amount
.----------------------------------------------------------------------
EIAMT000  MOVE      SP70,VAR
          CALL      DISPITEM
          BRANCH    ACCEPT,EIAMT800  * Insert Mode
.
          KEYIN     *PCCOL:CVERT,*DV,SP20,*PCCOL:CVERT,*V2LON,*RV,APIMTOT
          MOVE      APIMTOT,VAR
          CALL      DISPITEM
.
EIAMT200  MOVE      "   24",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            CALL      EDISC000
            MOVE      APIMDIS,VAR
            CALL      DISPITEM
          ENDIF
.
.          MOVE      "   25",FLDID
.          CALL      GETPOSIT
.          IF        EXIT=0
.            CALL      EGST0000
.            MOVE      APIMGST,VAR
.            CALL      DISPITEM
.          ENDIF
.
          MOVE      "   23",FLDID
          CALL      GETPOSIT
          GOTO      EIAMT999
.
EIAMT800  MOVE      ZERO,APIMTOT
          MOVE      ZERO,APIMDIS
          MOVE      ZERO,APIMGST
          KEYIN     *PCCOL:CVERT,*DV,SP20,*PCCOL:CVERT,*V2LON,*RV,APIMTOT
          GOTO      EIAMT999
.
EIAMT999  RETURN
.----------------------------------------------------------------------
. Enter Discount
.----------------------------------------------------------------------
EDISC000  MOVE      SP70,VAR
          CALL      DISPITEM
.
          IF          ACCEPT=1
            IF          APPPPE1=0
               MOVE       ZERO,APIMDIS
            ELSE
               ASSIGN      (APPPPE1*APIMTOT)/100.0,APIMDIS
            ENDIF
            GOTO        EDISC800
          ENDIF
.
          KEYIN       *PCCOL:CVERT,*DV,SP19:
                      *PCCOL:CVERT,*V2LON,*RV,APIMDIS
          MOVE      APIMDIS,VAR
          CALL      DISPITEM
.
          MOVE      ZERO,APIMGST
          MOVE      "   25",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            CALL      EGST0000
            MOVE      APIMGST,VAR
            CALL      DISPITEM
          ENDIF
          MOVE      "   24",FLDID
          CALL      GETPOSIT
          GOTO      EDISC999
.
EDISC800  MOVE      ZERO,APIMGST
          KEYIN     *PCCOL:CVERT,*DV,SP19:
                    *PCCOL:CVERT,*V2LON,*RV,APIMDIS
          GOTO      EDISC999
.
EDISC999  RETURN
.----------------------------------------------------------------------
. Enter GST
.----------------------------------------------------------------------
EGST0000  BRANCH    CHGTOT,EGST5000
.
          MOVE      SP70,VAR
          CALL      DISPITEM
.
EGST0010  IF        NOGST=1
            MOVE      ZERO,APIMGST
            GOTO      EGST9999
          ENDIF
.
          COMPARE   ZERO,APIMGST
          GOTO      EGST5000 IF NOT EQUAL
.
          COMPARE   ZERO,APIMTOT
          GOTO      EGST5000 IF EQUAL
.
          MATCH     SP6,FMCOINGS
          GOTO      EGST5000 IF EQUAL
.
          IF        PAYASGO=1
            MOVE      FMCOPAYG,KEY6
          ELSE
            IF        HASABN=1
              MOVE      FMCOXBAS,KEY6                                  *C-14452
            ELSE                                                       *C-14452
              MOVE      FMCOINGS,KEY6                                  *C-14452
            ENDIF                                                      *C-14452
          ENDIF
.
          PACK      KEY14,KEY6,APIMIDT,SP70
          CALL      RDIBGE1
          COMPARE   ZERO,OVRCD
          GOTO      EGST3000 IF EQUAL
.
          CALL      RPIBGE1
          BRANCH    OVRCD,EGST5000
.
          MATCH     KEY6,IBGECODE
          GOTO      EGST5000 IF NOT EQUAL
.
EGST3000  ASSIGN    APIMTOT-APIMDIS,F12P2
.
          COMPARE     TWO,IBCNUGST
          GOTO        EGST4000 IF NOT EQUAL
.
          IF        PAYASGO=1
            ASSIGN    (F12P2/HUNDRED)*IBGEAMNT,PAYASTOT
            MOVE      ZERO,APIMGST
            GOTO      EGST9999
          ELSE
            IF        NOGST=1
              MOVE      ZERO,APIMGST
            ELSE
              ASSIGN    (F12P2-(F12P2/(ONE+(IBGEAMNT/HUNDRED)))),APIMGST
            ENDIF
          ENDIF
          GOTO      EGST5000
.
EGST4000  ASSIGN    (F12P2/HUNDRED)*IBGEAMNT,APIMGST
.
EGST5000  IF        ACCEPT=1
            KEYIN     *PCCOL:CVERT,*DV,SP19,*PCCOL:CVERT,*V2LON,*RV,APIMGST
          ELSE
            KEYIN     *PCCOL:CVERT,*DV,SP19,*PCCOL:CVERT,*V2LON,*RV,APIMGST
            BRANCH    CHGTOT,EGST9999
            MOVE      APIMGST,VAR
            CALL      DISPITEM
          ENDIF
.
EGST9999  RETURN
.--------------------------------------------------
. Keyin Invoice Description
.--------------------------------------------------
KICM0000  IF        ACCEPT=1
            MOVE      SP70,APIMICM
          ENDIF
KICM0005
          KEYIN     *PCCOL:CVERT,*RV,*V2LON,APIMICM
          MOVE      ZERO,EXIT
.
          ENDSET    APIMICM
          APPEND    SP70,APIMICM
          RESET     APIMICM
.
          MATCH     SP70,APIMICM
          GOTO      KICM9999 IF EQUAL
.
          CMATCH    QUEST,APIMICM
          GOTO      KICM0090 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HFMTDA00
          MOVE      SCCITEM,CCITEM
.
          MOVE      FMTDCODE,APIMICM
          ENDSET    APIMICM
          APPEND    SP70,APIMICM
          RESET     APIMICM
.
          MATCH     SP70,APIMICM
          GOTO      KICM0000 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,APIMICM
KICM0090
          MOVE      ZERO,EXIT
          PACK      KEY4,APIMICM,SP70
          PACK      KEY50,KEY4,SP70
          MATCH     KEY50,APIMICM
          IF        @EQUAL
            CALL      RDFMTD1
            BRANCH    OVRCD,KICM9999
            MOVE      FMTDDESC,APIMICM
            MATCH     KEY50,APIMICM
            GOTO      KICM0005 IF NOT EQUAL
          ENDIF
.
KICM9999  RETURN
.
.
