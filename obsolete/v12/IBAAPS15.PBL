.----------------------------------------------------------------------
. Keyin File Name
. Note : EXIT (0=Ok, 2=Exitchar)
.----------------------------------------------------------------------
KFILE000  KEYIN     *P1:24,*EL,"Enter Import File Name : ",*V2LON,IMPORTFN
          PACK      IMPORTFN,IMPORTFN,SP70
          MATCH     SP70,IMPORTFN
          GOTO      KFILE9500 IF EQUAL
          MATCH     EXITCHAR,IMPORTFN
          GOTO      KFILE9500 IF EQUAL
.
KFILE100  STRIP     IMPORTFN
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      IBAAINI1,IMPORTFN
          TRAPCLR   IO
          COMPARE   ZERO,OVRCD
          GOTO      KFILE900 IF EQUAL
.
          REP       "./",IMPORTFN
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      IBAAINI1,IMPORTFN
          TRAPCLR   IO
          COMPARE   ZERO,OVRCD
          GOTO      KFILE900 IF EQUAL
.
          MOVE      SP70,DISPMSG
          CALL      MESSAGE2
KFILE500  MOVE      "Y",ANS
          KEYIN     *P1:24,*EL,"File Does Not Exist. ":
                    "Execute Transfer (Y/N) ? ",*V2LON,*RV,ANS
          CALL      MESSAGE3
          REP       UPPLOW,ANS
          MATCH     ANSN,ANS
          GOTO      KFILE000 IF EQUAL
          MATCH     ANSY,ANS
          GOTO      KFILE500 IF NOT EQUAL
          REP       "/.",IMPORTFN
          CLEAR     KEY80
          APPEND    "ibaaps15.us1 ",KEY80
          APPEND    IMPORTFN,KEY80
          RESET     KEY80
          MOVE      ZERO,HLEF
          CALL      GETSCR00
          EXECUTE   KEY80,TASKID
          CALL      PUTSCR00
          GOTO      KFILE100
.
KFILE900  MOVE      ZERO,EXIT
          MOVE      "import",KEY6
          PACK      VERIFDFN,KEY6,PORT
          REP       " 0",VERIFDFN
          PREP      IBAAINV1,VERIFDFN
.
          GOTO      KFILE9999
KFILE950  MOVE      ONE,EXIT           * exit
KFILE999  RETURN
.
.------------------------------------------------------------
. Verify Import File Contents
.------------------------------------------------------------
VERI0000  MOVE     ZERO,SECTOR
.
VERI1000  MOVE     ZERO,OVRCD
          TRAP     OVERCOND IF FORMAT
          ADD      ONE,SECTOR
          READ     IBAAINI1,SEQ;IBAICRD,IBAIREF,IBAIIDT,IBAITER,IBAIPDT:
                                IBAICM1,IBAICM2,IBAIICM,IBAILED,IBAIACC:
                                IBAIDIS,IBAIRES,IBAIAMT,IBAIGST,IBAIGSTC
          GOTO     VERI9990 IF OVER
          TRAPCLR  FORMAT
          BRANCH   OVRCD,VERI9100
.
          MOVE     SP70,APMACN1
          MOVE     SP70,APPPDES
          MOVE     SP70,FMAMDESC
          MOVE     SP70,FMDSDESC
          MOVE     SP70,FMRSDESC
          MOVE     SP70,IBGSDESC
.
          CALL     CHKCRD00
          BRANCH   EXIT,VERI9900
.
          CALL     CHBREF00                 * verify blank reference
          BRANCH   EXIT,VERI9900            * srf 634525, 646107
.
          CALL     CHKREF00
          BRANCH   EXIT,VERI9900
.
          CALL     CHKACC00
          BRANCH   EXIT,VERI9900
.
          CALL     CHKTER00
          BRANCH   EXIT,VERI9900
.
          CALL     CHKRES00
          BRANCH   EXIT,VERI9900
.
          CALL     CHKDIS00
          BRANCH   EXIT,VERI9900
.
          CALL     CHKIDT00
          BRANCH   EXIT,VERI9900
.
          MATCH    SP70,IBAIPDT
          IF       @EQUAL
            MOVE     IBAITER,APIMTER
            MOVE     IBAIIDT,APIMIDT
            CALL     PAYDAT00
            MOVE     APIMPDT,IBAIPDT
          ELSE
            UNPACK   IBAIPDT,CCENT,CYEAR,CMON,CDAY
            CALL     VALDAT00
            IF       EXIT=1
              MOVE     IBAITER,APIMTER
              MOVE     IBAIIDT,APIMIDT
              CALL     PAYDAT00
              MOVE     APIMPDT,IBAIPDT
            ENDIF
          ENDIF
.
          WRITE    IBAAINV1,SEQ;IBAICRD,IBAIREF,IBAIIDT,IBAITER,IBAIPDT:
                                IBAICM1,IBAICM2,IBAIICM,IBAILED,IBAIACC:
                                IBAIDIS,IBAIRES,IBAIAMT,IBAIGST,IBAIGSTC
.
          GOTO     VERI1000
.
VERI9100  CALL     DISIMP00
          BEEP
          MOVE     "ERROR : Import File Format Error - ",DISPMSG
          CALL     MESSAGE1
          GOTO     VERI9900
.
VERI9900  MOVE     ONE,EXIT               * Abort Load
          GOTO     VERI9999
.
VERI9990  MOVE     ZERO,EXIT              * Continue Load
.
VERI9999  RETURN
.------------------------------------------------------------
. Check Creditor Code
.------------------------------------------------------------
CHKCRD00  MOVE     ZERO,EXIT
          MOVE     IBAICRD,KEY5
          CALL     RDAPMA1
          BRANCH   OVRCD,CHKCRD10
          IF       IBCNUGST=2
            MATCH    SP11,APMAABN
            IF       @EQUAL
              BEEP
              MOVE     "ERROR : Creditor Must Have ABN - ",DISPMSG
              CALL     MESSAGE1
              GOTO     CHKCRD10
            ENDIF
            IF       APMAGREG=0
              COMPARE  ZERO,IBAIGST
              GOTO     CHKCRD99 IF EQUAL
              BEEP
              MOVE     "ERROR : Creditor Should Not Have GST - ",DISPMSG
              CALL     MESSAGE1
              GOTO     CHKCRD10
            ENDIF
          ENDIF
          GOTO     CHKCRD99
.
CHKCRD10  CALL     DISIMP00
          BEEP
          MOVE     "ERROR : Invalid Creditor Code - ",DISPMSG
          CALL     MESSAGE1
          DISPLAY  *P1:24,*EL,"Creditor Code        : "
          MOVE     "24",CVERT
          MOVE     "24",CCOL
          MOVE     ZERO,CKEYTYP
          CALL     KAPMAA00
          BRANCH   EXIT,CHKCRD90,CHKCRD90
          MOVE     APMACRD,IBAICRD
          IF       IBCNUGST=2
            MATCH    SP11,APMAABN
            IF       @EQUAL
              BEEP
              MOVE     "ERROR : Creditor Must Have ABN - ",DISPMSG
              CALL     MESSAGE1
              GOTO     CHKCRD90
            ENDIF
            IF       APMAGREG=0
              COMPARE  ZERO,IBAIGST
              GOTO     CHKCRD99 IF EQUAL
              BEEP
              MOVE     "ERROR : Creditor Should Not Have GST - ",DISPMSG
              CALL     MESSAGE1
              GOTO     CHKCRD90
            ENDIF
          ENDIF
          GOTO     CHKCRD99
CHKCRD90  MOVE     ONE,EXIT
CHKCRD99  RETURN
.------------------------------------------------------------
. Check for Blank Reference
.------------------------------------------------------------
CHBREF00  MATCH     IBAIREF,SP70
          GOTO      CHBREF90 IF NOT EQUAL
.
          CALL      DISIMP00
.
          UNPACK    APIMIDT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
CHBREF10  CLEAR     DISPMSG
          MOVE      "Blank Reference found - ",DISPMSG
.
          CALL      MESSAGE1
          KEYIN     *P1:24,*EL,"Invoice Reference    : ",IBAIREF
.
          ENDSET    IBAIREF
          APPEND    SP70,IBAIREF
          RESET     IBAIREF
.
          MATCH     SP70,IBAIREF
          GOTO      CHBREF10 IF EQUAL
.
          CMATCH    EXITCHAR,IBAIREF
          GOTO      CHBREF90 IF NOT EQUAL
.
          PACK      IBAIREF,SP70,SP70
          MOVE      ONE,EXIT
          GOTO      CHBREF99
.
CHBREF90  MOVE      ZERO,EXIT
CHBREF99  RETURN
.------------------------------------------------------------
. Check for Duplicate Reference
.------------------------------------------------------------
CHKREF00  PACK     KEY35,IBAICRD,IBAIREF,IBAIREF,SP70
          PACK     KEY40,IBAICRD,IBAIREF,IBAIREF,SP70
          CALL     RSAPIM3
CHKREF10  CALL     RKAPIM3
          BRANCH   OVRCD,CHKREF90
          PACK     KEY40,APIMCRD,APIMREF,APIMDOC,SP70
          MATCH    KEY35,KEY40
          GOTO     CHKREF90 IF NOT EQUAL
.
          MATCH    "0",APIMTYP           * Invoice ?
          GOTO     CHKREF10 IF NOT EQUAL
.
          MATCH    APBMBCH,APIMBCH
          GOTO     CHKREF10 IF EQUAL
.
          CALL     DISIMP00
.
          UNPACK   APIMIDT,CCENT,CYEAR,CMON,CDAY
          CALL     PACDATE
          CLEAR    DISPMSG
          APPEND   "Duplicate Reference in Batch ",DISPMSG
          APPEND   APIMBCH,DISPMSG
          APPEND   " Dated ",DISPMSG
          APPEND   CPCDATE,DISPMSG
          APPEND   " - ",DISPMSG
          MATCH    "1",FMCODUPI        * Disallow Duplicate Inv. Ref's ?
          IF       @EQUAL
            CALL     MESSAGE1
            MOVE     ONE,EXIT
            GOTO     CHKREF99
          ELSE
            RESET    DISPMSG
            MOVE     "N",DEFYESNO
            CALL     MESSAGE4
            MOVE     " ",DEFYESNO
            BRANCH   EXIT,CHKREF99
            MOVE     ZERO,EXIT
          ENDIF
.
CHKREF90  MOVE     ZERO,EXIT
CHKREF99  RETURN
.------------------------------------------------------------
. Check Ledger Account
.------------------------------------------------------------
CHKACC00  PACK     KEY14,IBAILED,IBAIACC
          CALL     RDFMAM1
          BRANCH   OVRCD,CHKACC10
          MOVE     ZERO,EXIT
          GOTO     CHKACC99
.
CHKACC10  CALL     DISIMP00
          BEEP
          MOVE     "ERROR : Invalid Ledger/Account - ",DISPMSG
          CALL     MESSAGE1
          DISPLAY  *P1:24,*EL,"Ledger/Account       : "
          MOVE     "24",CVERT
          MOVE     "24",CCOL
          MOVE     ZERO,CKEYTYP
          CALL     KFMLAA00
          BRANCH   EXIT,CHKACC90,CHKACC90
          MOVE     FMLALEDG,IBAILED
          MOVE     FMLALEDG,FMAMLEDG
          DISPLAY  *P1:24,*EL,"Ledger/Account       : ",*V2LON,IBAILED,*HOFF,"/"
          MOVE     "27",CCOL
          MOVE     "24",CVERT
          MOVE     ONE,CACCTIND
          CALL     EACCT000
          BRANCH   EXIT,CHKACC90,CHKACC90
          MOVE     FMAMACCT,IBAIACC
          MOVE     ZERO,EXIT
          GOTO     CHKACC99
.
CHKACC90  MOVE     ONE,EXIT
.
CHKACC99  RETURN
.------------------------------------------------------------
. Check Terms
