          PACK      KEY5,SCPSROW,SCPSCOL
          READ      APIN1FL1,KEY5;ANS
          IF        @OVER
            WRITE     APIN1FL1,KEY5;SCPSROW,SCPSCOL,APIN1MKY
          ELSE
            UPDATE    APIN1FL1;SCPSROW,SCPSCOL,APIN1MKY
          ENDIF
          RETURN
