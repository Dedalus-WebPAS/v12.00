.**********************************************************************
. Check if proposed payments in progress for invoices (Exit program if so)
. (nb : CKPPY000 + CKPPU000 is for other inputs)
.**********************************************************************
CKPPI000  OPEN      CONTROLF,"controlf"
          READ      CONTROLF,ZERO;*107,IBCNUBAS
          MOVE      IBCNUBAS,BASFLAG
          IF        BASFLAG=1
            MOVE      "BAS",BASDESC
          ENDIF
.
          MOVE      SP70,LASTISYS      * Clear Last System Flag
          MOVE      "9999999999999999999999999999999999999999",KEY40
          CALL      RDAPIM1            * check if proposed payments active
          BRANCH    OVRCD,CKPPI999
.
          CALL      RDFMCO50
          PACK      FMCOTACC,FMCOTACC,SP70
          REP       " 0",FMCOTACC
.
          MATCH     "3",FMCOTACC       * full access allowed ?
          GOTO      CKPPI999 IF EQUAL
.
          MATCH     "2",FMCOTACC       * invoice access allowed ?
          GOTO      CKPPI999 IF EQUAL
.
          MATCH     "1",FMCOTACC       * invoice access allowed if DOF ?
          GOTO      CKPPI999 IF EQUAL
.
          MOVE      "ACCESS DENIED - Proposed Payments In Progress - ",DISPMSG
          CALL      MESSAGE1
.
          MATCH     "1426",PASSCODE    * sneaky delete of payments file
          GOTO      CKPPI900 IF NOT EQUAL
          MATCH     "*",ANS
          GOTO      CKPPI900 IF NOT EQUAL
.
          MOVE      "HELLO IBA - Delete Proposed Payments File ? ",DISPMSG
          CALL      MESSAGE4           * does iba want to delete auto pay file ?
          BRANCH    EXIT,CKPPI999
.
          CALL      DELAP000           * delete file if possible
          GOTO      CKPPI000
.
CKPPI900  CLOCK      FORK,KEY2         * exit program
          MATCH      " 0",KEY2
          IF         @EQUAL
            CHAIN       PGM
          ELSE
            STOP
          ENDIF
.
CKPPI999  READ      CONTROLF,SIXTY3;*85,INCOSTYP
          READ      CONTROLF,FIFTY1;*242,FMDEFCRD
          RETURN
