.----------------------------------------------------------------------
. Delete Item From Invoice Order Details
.----------------------------------------------------------------------
.
.--- save prompt ----
.
DRANG000  MOVE      SP70,DISPMSG
          CALL      MESSAGE2
.
.---- count number of valid items ----
.
DRANG050  MOVE      ZERO,ITEMTOT
          PACK      KEY5,SP70
          READ      APIN1FL1,KEY5;;
.
DRANG100  READKS    APIN1FL1;SCPSROW,SCPSCOL,APIN1MKY
          GOTO      DRANG200 IF OVER
          MATCH     SP70,APIN1MKY
          GOTO      DRANG100 IF EQUAL
.
          ADD       ONE,ITEMTOT
          GOTO      DRANG100
.
.---- get range ----
.
DRANG200  IF        ITEMTOT=0
            DISPLAY   *P1:24,*B,*EF,"No Items on Screen - ";
            CALL      HITENTER
            GOTO      DRANG999
          ENDIF
.
DRANG300  KEYIN     *P1:24,*EF,"Delete From Item : ":
                    *P20:24,*V2LON,ITEMFROM:
                    *P20:24,*DV,ITEMFROM;
.
          IF        ITEMFROM=0
            GOTO      DRANG999
          ENDIF
          IF        ITEMFROM<0 | ITEMFROM>ITEMTOT
            DISPLAY   *P1:24,*B,*EF,"Illegal Item - ";
            CALL      HITENTER
            GOTO      DRANG300
          ENDIF
.
          KEYIN     *P30:24,*EF,"To Item : ":
                    *P40:24,*V2LON,ITEMTO:
                    *P40:24,*DV,ITEMTO;
.
          IF        ITEMFROM>ITEMTO | ITEMTO>ITEMTOT
            DISPLAY   *P1:24,*B,*EF,"Illegal Range - ";
            CALL      HITENTER
            GOTO      DRANG300
          ENDIF
.
          MOVE      "N",ANS            * confirm delete range
          KEYIN     *P50:24,*EF,"Ok to Delete ? ":
                    *P65:24,*V2LON,*RV,ANS;
          PACK      ANS,ANS,SP70
          REP       UPPLOW,ANS
          MATCH     "Y",ANS
          GOTO      DRANG300 IF NOT EQUAL
.
.---- delete items ----
.
          PACK      KEY51,SP70
          PACK      KEY5,Z70
          READ      APIN1FL1,KEY5;;
.
DRANG400  READKP    APIN1FL1;SCPSROW,SCPSCOL,APIN1MKY
          GOTO      DRANG500 IF OVER
          MATCH     SP70,APIN1MKY
          GOTO      DRANG400 IF EQUAL
.
          PACK      KEY51,APIN1MKY,SP70
          IF        ITEMTOT>=ITEMFROM & ITEMTO>=ITEMTOT
            CALL      RDAPIN1
            CALL      DEAPIN1
          ENDIF
          SUB       ONE,ITEMTOT
          GOTO      DRANG400
.
.---- display screen ----
.
DRANG500  CALL      RSAPIN1
          CALL      RPAPIN1
          CALL      DP02L000
          CALL      UPDTOT00
          GOTO      DRANG050
.
.--- redisplay prompt ----
.
DRANG999  CALL      MESSAGE3
          RETURN
