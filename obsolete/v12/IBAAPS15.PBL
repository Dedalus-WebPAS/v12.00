          MOVE      ZERO,OVRCD
          PACK      KEY5,SP70
          READ      APIN1FL1,KEY5;;
          READKS    APIN1FL1;SCPSROW,SCPSCOL,APIN1MKY
          GOTO      OVERCOND IF OVER
          MATCH     SP70,APIN1MKY
          GOTO      OVERCOND IF EQUAL
          RETURN
