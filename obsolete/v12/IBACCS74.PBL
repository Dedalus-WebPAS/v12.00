ERUNN000  KEYIN     *PCCOL:CVERT,*V2LON,*MASK,F4
          IF        F4=0
            MOVE      ONE,EXIT
            GOTO      ERUNN999
          ENDIF
          MOVE      F4,CCUPRUN
          MOVE      F4,KEY4
          PACK      KEY12,KEY4,SP70
          CALL      RSCCUP1
          CALL      RKCCUP1
          BRANCH    OVRCD,ERUNN010
          MATCH     KEY4,CCUPRUN
          GOTO      ERUNN010 IF NOT EQUAL
          MOVE      ZERO,EXIT
          GOTO      ERUNN999
.
ERUNN010  MOVE      "No Records for Run. - ",DISPMSG
          BEEP
          CALL      MESSAGE1
          GOTO      ERUNN000
.
ERUNN999  RETURN
.------------------------------------------------------------
. Keyin Error Run Number
.------------------------------------------------------------
ERECN000  KEYIN     *PCCOL:CVERT,*V2LON,*MASK,F8
          IF        F8=0
            MOVE      ONE,EXIT
            GOTO      ERECN999
          ENDIF
.
          COMPARE   "99999999",F8          * All 9's to add next record
          GOTO      ERECN100 IF EQUAL
.
          PACK      KEY12,CCUPRUN,F8,SP70
          CALL      RDCCUP1
          IF        OVRCD=1
            MOVE      "Error Number Not Found - ",DISPMSG
            BEEP
            CALL      MESSAGE1
            GOTO      ERECN000
          ENDIF
          PACK      KEY2,CCUPHCD
          CALL      RDCCHC1
          PACK      KEY10,CCUPHCD,CCUPDPT
          CALL      RDCCDP1
          PACK      KEY18,CCUPHCD,CCUPDPT,CCUPPCD
          CALL      RDCCPD1
.
          REP       " 0",CCUPDAT
          PACK      KEY14,CCUPDAT,SP70
          CALL      RSCCCA3
          CALL      RKCCCA3
          IF        OVRCD=1
            BEEP
            MOVE      "WARNING: Period Not Found for Service Date. - ",DISPMSG
            CALL      MESSAGE1
            GOTO      ERECN090
          ENDIF
          IF        CCCASTAT=1
            BEEP
            MOVE      "WARNING: Period Locked Service Date. - ",DISPMSG
            CALL      MESSAGE1
            GOTO      ERECN090
          ENDIF
.
          PACK      KEY24,CCUPHCD,CCUPDPT,CCCAYEAR,CCCAPER,CCUPPCD
          CALL      RDCCPS1
          IF        OVRCD=0
            SUB       CCUPQTY,CCPSQTY
            SUB       CCUPCHG,CCPSCHG
            CALL      UPCCPS1
          ELSE
            BEEP
            MOVE      "WARNING: Sumary Record Not Found. - ",DISPMSG
            CALL      MESSAGE1
            GOTO      ERECN090
          ENDIF
.
ERECN090  MOVE      ZERO,EXIT
          MOVE      ZERO,OVRCD
          GOTO      ERECN999
.
ERECN100  MOVE      CCUPRUN,KEY4
          PACK      KEY12,KEY4,Z70
          CALL      RSCCUP1
          CALL      RPCCUP1
          BRANCH    OVRCD,ERECN110
          MATCH     KEY4,CCUPRUN
          GOTO      ERECN110 IF NOT EQUAL
          MOVE      CCUPREC,F8
          GOTO      ERECN120
.
ERECN110  MOVE      KEY4,CCUPRUN
          MOVE      ZERO,F8
.
ERECN120  ADD       ONE,F8
          MOVE      SP70,CCHCDES
          MOVE      SP70,CCDPDES
          MOVE      SP70,CCPDDES
          MOVE      F8,CCUPREC
          MOVE      ZERO,EXIT
          PACK      KEY12,CCUPRUN,F8,SP70
          CALL      RDCCUP1
          BRANCH    OVRCD,ERECN999
          GOTO      ERECN120
.
ERECN999  RETURN
.
.------------------------------------------------------------
. Keyin Department
.------------------------------------------------------------
EDEPT000  MOVE      CCUPHCD,CCDPHCD
          MOVE      ZERO,CKEYTYP
          CALL      KCCDPA00
          BRANCH    EXIT,EDEPT999,EDEPT999
          MOVE      CCDPDPT,CCUPDPT
          MOVE      ZERO,EXIT
EDEPT999  RETURN
.
.------------------------------------------------------------
. Keyin Patient Service
.------------------------------------------------------------
EPROC000  MOVE      CCUPHCD,CCPDHCD
          MOVE      CCUPDPT,CCPDDPT
          MOVE      ZERO,CKEYTYP
          CALL      KCCPDA00
          BRANCH    EXIT,EPROC999,EPROC999
          MOVE      CCPDPCD,CCUPPCD
          MOVE      ZERO,EXIT
EPROC999  RETURN
.
.------------------------------------------------------------
. Cancel Changes (Update Summary)
.------------------------------------------------------------
CANC0000  PACK      KEY12,CCUPRUN,CCUPREC,SP70
          CALL      RDCCUP1
          BRANCH    OVRCD,CANC9999
          REP       " 0",CCUPDAT
          PACK      KEY14,CCUPDAT,SP70
          CALL      RSCCCA3
          CALL      RKCCCA3
          BRANCH    OVRCD,CANC9999
          PACK      KEY24,CCUPHCD,CCUPDPT,CCCAYEAR,CCCAPER,CCUPPCD
          CALL      RDCCPS1
          IF        OVRCD=0
            ADD       CCUPQTY,CCPSQTY
            ADD       CCUPCHG,CCPSCHG
            CALL      UPCCPS1
          ENDIF
.
CANC9999  RETURN
.
.------------------------------------------------------------
. Cancel Changes (Update Summary)
.------------------------------------------------------------
POST0000  PACK      KEY12,CCUPRUN,CCUPREC,SP70
          CALL      RACCUP1
          IF        OVRCD=0
            CALL      UPCCUP1
          ELSE
            CALL      WRCCUP1
          ENDIF
.
          REP       " 0",CCUPDAT
          PACK      KEY14,CCUPDAT,SP70
          CALL      RSCCCA3
          CALL      RKCCCA3
          BRANCH    OVRCD,POST9999
          PACK      KEY24,CCUPHCD,CCUPDPT,CCCAYEAR,CCCAPER,CCUPPCD
          CALL      RDCCPS1
          IF        OVRCD=0
            ADD       CCUPQTY,CCPSQTY
            ADD       CCUPCHG,CCPSCHG
            CALL      UPCCPS1
          ELSE
            UNPACK    KEY24,CCPSHCD,CCPSDPT,CCPSYEAR,CCPSPER,CCPSPCD
            MOVE      CCUPQTY,CCPSQTY
            MOVE      CCUPCHG,CCPSCHG
            MOVE      ZERO,CCPSBQT
            MOVE      ZERO,CCPSCST
            MOVE      ZERO,CCPSBCT
            MOVE      ZERO,CCPSFBU
            MOVE      ZERO,CCPSSCT
            MOVE      SP70,CCPSSPA
            CALL      WRCCPS1
          ENDIF
.
POST9999  RETURN
.
.-------------------------------------------------------------
. Routine to Delete Record.
.-------------------------------------------------------------
DELT0000  MOVE     SP70,DISPMSG        * Clear Line 24 Display Message
          CALL     MESSAGE2            * Save and Clear Line 24
          CALL     DELQST              * Delete (Y/N/C) CEXIT 1/2/3
          CALL     MESSAGE3            * Restore Line 24
          BRANCH   CEXIT,DELT1000,DELT9000,DELT9100
.
DELT1000  PACK     KEY12,CCUPRUN,CCUPREC  * Setup Key to File
          CALL     RACCUP1             * Read Record
          IF       OVRCD=0
            CALL     DECCUP1           * Delete Record
          ENDIF
          MOVE     TWO,EXIT            * Restart Screen
          GOTO     DELT9999
DELT9000  MOVE     ONE,EXIT            * Return to Prompt on "No"
          GOTO     DELT9999
DELT9100  MOVE     TWO,EXIT            * Restart Screen on "Cancel"
          GOTO     DELT9999
DELT9999  RETURN
.
