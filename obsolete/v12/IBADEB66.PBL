.--------------------------------------------------------
. Print Debtor Details
.--------------------------------------------------------
PLIN0000  IF        REPTACT=1
            COMPARE   ONE,DBDBACT
            GOTO      PLIN9999 IF NOT EQUAL
          ENDIF
          COMPARE   "55",CLNO
          CALL      PHED0000 IF NOT LESS
          MOVE      NO,YESNO
          LOAD      YESNO,DBDBACT,YES
          MOVE      ANSN,DIM1B
          LOAD      DIM1B,DBDBBAD,ANSY
          MOVE      "999,999,999",KEY11
          MOVE      DBDBCLM,F12
          FEDIT     F12,KEY11
          PRINT     DBDBDEB,*10,DBDBNA1,*46,DBDBAD1,*82,DBDBPC:
                    *91,"BH:",DBDBPH1,*115,YESNO,*120,DIM1B,*122,KEY11:
                    *N,*10,DBDBNA2,*46,DBDBAD2:
                    *91,"AH:",DBDBPH2:
                    *N,*10,"Sort Key: ",DBDBSORT,*46,DBDBAD3:
                    *91,"FX:",DBDBPH3:
                    *N,*46,DBDBAD4
          ADD       FOUR,CLNO
.
PLIN9999  RETURN
