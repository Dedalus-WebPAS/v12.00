          CALL      RDDBDB3
          IF        OVRCD=0
            CALL      RPDBDB3
          ENDIF
REPT3100  CALL      RKDBDB3
          BRANCH    OVRCD,REPT9000
          MATCH     DBDBSORT,ENDSKY
          GOTO      REPT9000 IF LESS
          MATCH     STRDEB,DBDBDEB    * Debtors Code Range
          GOTO      REPT3100 IF LESS
          MATCH     DBDBDEB,ENDDEB
          GOTO      REPT3100 IF LESS
.
          IF        REPTBAD=1
            COMPARE   ONE,DBDBBAD
            GOTO      REPT3100 IF NOT EQUAL
          ENDIF
.
