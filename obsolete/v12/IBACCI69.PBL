.**********************************************************************
.*                               SET0000                              *
.*                   Set up initial variables values                  *
.**********************************************************************
.
.         Get the current date
.
SET0000   CALL      IBACLOCK 
          MOVE      CCC,CCENT
          MOVE      CYY,CYEAR
          MOVE      CMM,CMON
          MOVE      CDD,CDAY
.
          MOVE      CURCENT,CCENT
.
          CALL      CTPF0000                * create failure temp file
          CALL      CTPS0000                * create success temp file
.
.         Process the date in question
.
          MOVE      CCENT,CC
          MOVE      CYEAR,YY
          MOVE      CMON,MM
          MOVE      CDAY,DD
          CALL      DMYCON                   * convert to julian form
          SUB       CCCNDELY,JULDAY            for yesterdays date
.
. ---- this is needed to fool the end of year boundary ----
.
          COMPARE   ONE,JULDAY
          GOTO      SET5000 IF NOT LESS
.
.         Back dated into previous year
.
          SUB       ONE,JULYR
.
.         Check if this puts us into the previous century
.
          COMPARE   ZERO,JULYR
          GOTO      SET0500 IF NOT LESS
.
          MOVE      CCENT,FORM2
          SUB       ONE,FORM2
          MOVE      FORM2,CCENT
.
.         JULDAY =  0 : 31st December
.         JULDAY = -1 : 30th December
.         JULDAY = -2 : 29th December
.         JULDAY = -3 : 28th December
.         JULDAY = -4 : 27th December
.         JULDAY = -5 : 26th December
.         JULDAY = -6 : 25th December
.         JULDAY = -7 : 24th December
.         JULDAY = -8 : 23rd December
.         JULDAY = -9 : 22nd December
.
SET0500   ADD       THIRTY1,JULDAY
          MOVE      JULDAY,DD           * Day in December
.
          PACK      REPDATE,CCENT,JULYR,TEN2,DD
          REP       " 0",REPDATE
          GOTO      SET6000
.
.         Calculated date is still in the same year. Use JULCON to convert the
.         date back to DD/MM/YY
.
SET5000   MOVE      JULDAY,JWKDAY
          MOVE      JULYR,JWKYER
          MOVE      JULCC,JWKCC
          CALL      JULCON         
          PACK      REPDATE,CC,YY,MM,DD
          REP       " 0",REPDATE
.
.         Valid date. Get the period details, and then proceed
.
SET6000   MOVE      REPDATE,REPDIM8  
          MOVE      REPDATE,CPTDATE
          CALL      GPERD                    * calculate fiscal periods
          BRANCH    OVRCD,SET9000
          MOVE      CPTDATE,CCUTDATE
          MOVE      DRGYR,CCUTFSYR
          MOVE      DRGNUM,CCUTFSPR
          MOVE      FALSE,EXIT
.
.         see if the date is after the system paramter date
.
          MATCH     REPDATE,CCCNCEDT
          IF        @LESS
            MOVE      REPDATE,CCCNCEDT
            WRITAB    CONTROLF,SEVENTY1;*168,CCCNCEDT
          ENDIF
          GOTO      SET9999
.
.------ date generated is not in the date range file ------
.
SET9000   MOVE      TRUE,EXIT
.
SET9999   RETURN
.
+
.*******************************************************************************
.         Get the default charging claim code for multi hospital
.*******************************************************************************
DCLM0000  OPEN      CONTROLF,"controlf"
          READ      CONTROLF,ZERO;*43,IBCNMHOS
          IF        IBCNMHOS=1
            OPEN      PATHSPA1,"pathspaf"
            MOVE      PMVXMHOS,KEY3
            CALL      RDPTHSP1
            IF        OVRCD=0
              MOVE      PTHSDCLM,PTCNDCLM     * default claim code charging
            ENDIF
          ENDIF
DCLM9999  RETURN
