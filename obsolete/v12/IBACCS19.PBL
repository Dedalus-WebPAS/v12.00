          MOVE     SP70,VAR
          CALL     DISPITEM
          MOVE     ZERO,CKEYTYP
          CALL     KCCPRA00
          BRANCH   EXIT,EANNQ350,EANNQ200
          ASSIGN   (ANNDIR*CCPRP01)/100.0,AM01
          ASSIGN   (ANNDIR*CCPRP02)/100.0,AM02
          ASSIGN   (ANNDIR*CCPRP03)/100.0,AM03
          ASSIGN   (ANNDIR*CCPRP04)/100.0,AM04
          ASSIGN   (ANNDIR*CCPRP05)/100.0,AM05
          ASSIGN   (ANNDIR*CCPRP06)/100.0,AM06
          ASSIGN   (ANNDIR*CCPRP07)/100.0,AM07
          ASSIGN   (ANNDIR*CCPRP08)/100.0,AM08
          ASSIGN   (ANNDIR*CCPRP09)/100.0,AM09
          ASSIGN   (ANNDIR*CCPRP10)/100.0,AM10
          ASSIGN   (ANNDIR*CCPRP11)/100.0,AM11
          ASSIGN   (ANNDIR*CCPRP12)/100.0,AM12
          ASSIGN   (ANNDIR*CCPRP13)/100.0,AM13
          ASSIGN   ANNDIR-AM01-AM02-AM03-AM03-AM04-AM05-AM06,F12P2
          ASSIGN   F12P2-AM07-AM08-AM09-AM10-AM11-AM12-AM13,F12P2
          COMPARE  ZERO,F12P2
          GOTO     EANNQ400 IF EQUAL
          MOVE     "14",SECTOR
EANNQ310  SUB      ONE,SECTOR
          LOAD     F12P2A,SECTOR,AM01,AM02,AM03,AM04,AM05,AM06:
                                 AM07,AM08,AM09,AM10,AM11,AM12,AM13
          COMPARE  ONE,SECTOR
          GOTO     EANNQ320 IF EQUAL
          COMPARE  ZERO,F12P2
          GOTO     EANNQ310 IF EQUAL
EANNQ320  ADD      F12P2,F12P2A
          STORE    F12P2A,SECTOR,AM01,AM02,AM03,AM04,AM05,AM06:
                                 AM07,AM08,AM09,AM10,AM11,AM12,AM13
          GOTO     EANNQ400
.
EANNQ350  ASSIGN   (ANNDIR/NUMPER),F12P2A
          ASSIGN   ANNDIR-(F12P2A*NUMPER)+F12P2A,F12P2
          MOVE     SP70,CCPRPRO
EANNQ400
          MOVE     ONE,KEYFLAG
.
          MOVE     ZERO,SECTOR
          PACK     KEY6,SAVEYEAR,SP70
          CALL     RDCCCA1
