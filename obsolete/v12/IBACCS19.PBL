          MOVE     CCCAPER,KEY2
          PACK     KEY24,CCHCHCD,CCDPDPT,CCCTCTY,CCCAYEAR,CCCAPER
          UNPACK   KEY24,CCMBHCD,CCMBDPT,CCMBCTY,CCMBYEAR,CCMBPER
          CALL     RDCCMB2
          IF       OVRCD=1
            MOVE      ZERO,CCMBDIR
            MOVE      ZERO,CCMBIND
            MOVE      ZERO,CCMBFLX
            MOVE      SP70,CCMBSPA
          ENDIF
          MATCH     SP70,CCPRPRO
          IF        @EQUAL
            ASSIGN    ANNDIR/NUMPER,CCMBDIR
            IF        KEYFLAG=1
              MOVE      ZERO,KEYFLAG
              MOVE      F12P2,CCMBDIR
            ENDIF
          ELSE
            ADD       ONE,SECTOR
            LOAD      CCMBDIR,SECTOR,AM01,AM02,AM03,AM04,AM05,AM06:
                                     AM07,AM08,AM09,AM10,AM11,AM12,AM13
          ENDIF
          MOVE      SP70,SCTMROW
          MOVE      SP70,SCTMCOL
          CALL      TWCCMB22
          GOTO      EANNQ600
.
