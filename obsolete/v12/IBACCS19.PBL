PROC0000  PACK      KEY20,CCHCHCD,STRDPT,SP70
          DISPLAY   *P1:24,*EL,"Processing : "
          MOVE      ZERO,SECTOR
          CALL      RSCCMB1
PROC1000  CALL      RKCCMB1
          BRANCH    OVRCD,PROC9000
          MATCH     CCHCHCD,CCMBHCD
          GOTO      PROC9000 IF NOT EQUAL
          MATCH     CCMBDPT,ENDDPT     * Department Range
          GOTO      PROC9000 IF LESS
.
          MATCH     CCMBYEAR,CCCAYEAR
          GOTO      PROC1000 IF NOT EQUAL
.
          MATCH     STRTYP,CCMBCTY     * Cost Type Range
          GOTO      PROC1000 IF LESS
          MATCH     CCMBCTY,ENDTYP
          GOTO      PROC1000 IF LESS
.
          MATCH     STRPER,CCMBPER     * Period Range
          GOTO      PROC1000 IF LESS
          MATCH     CCMBPER,ENDPER
          GOTO      PROC1000 IF LESS
.
          ADD       ONE,SECTOR
          IF        (SECTOR%100=0)
            DISPLAY   *P15:24,*V2LON,*EL,SECTOR
          ENDIF
.
          COMPARE   ZERO,ADJPER
          GOTO      PROC1100 IF EQUAL
.
          ASSIGN    CCMBDIR+((CCMBDIR*ADJPER)/100.0),CCMBDIR
.
PROC1100  ADD       ADJAMT,CCMBDIR
          CALL      UPCCMB1
          GOTO      PROC1000
.
PROC9000  DISPLAY   *P1:24,*EL,"Cost Model Update Complete. - ";
          BEEP
          CALL      HITENTER
          RETURN
