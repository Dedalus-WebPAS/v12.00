.----------------------------------------
. List General Ledger Keys
.----------------------------------------
LCCGI000  MOVE      "Printing : ",DISPMSG
          CALL      MESSAGE2
          MOVE      SCSLITM,FIELDNO
          SUB       TWO,FIELDNO
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
          BRANCH    FIELDNO,LCCGI010,LCCGI020
          PACK      LASTPRT,SP70,SP70
.
LCCGI010  PACK      KEY23,SP70
          CALL      RSCCGI1
          GOTO      LCCGI100
LCCGI020  PACK      KEY37,SP70
          CALL      RSCCGI2
.
LCCGI100  BRANCH    FIELDNO,LCCGI110,LCCGI120
.
LCCGI110  CALL      RKCCGI1
          GOTO      LCCGI150
.
LCCGI120  CALL      RKCCGI2
.
LCCGI150
          BRANCH    OVRCD,LCCGI999
          COMPARE   "60",CLNO
          CALL      LCCGIHED IF NOT LESS
.
          MOVE      CCGIKEY,KEY20
          CALL      RDCCGK1
          IF        OVRCD=1
            MOVE      "Not on File",CCGKDES
          ENDIF
.
          MOVE      CCGICTY,KEY4
          CALL      RDCCCT1
          IF        OVRCD=1
            MOVE      "Not on File",CCCTDES
          ENDIF
.
          PACK      KEY10,CCGIHCD,CCGIDPT
          CALL      RDCCDP1
          IF        OVRCD=1
            MOVE      "Not on File",CCDPDES
          ENDIF
.
          DISPLAY   *P12:24,CCGIKEY
          IF        FIELDNO=1
            PACK      NEXTPRT,CCGIKEY
            MATCH     LASTPRT,NEXTPRT
            IF        !@EQUAL
              PACK      LASTPRT,CCGIKEY
              PRINT     *1,CCGIKEY,*22,CCGKDES
              ADD       ONE,CLNO
            ENDIF
            PRINT     *61,CCGIHCD,*67,CCGIDPT,*78,CCGICTY,*83,CCCTDES:
                      *119,CCGIPER
          ELSE
            PACK      NEXTPRT,CCGIHCD,CCGIDPT
            MATCH     LASTPRT,NEXTPRT
            IF        !@EQUAL
              PACK      LASTPRT,CCGIHCD,CCGIDPT
              PRINT     *1,CCGIHCD,"/",CCGIDPT,SP1,CCDPDES
              ADD       ONE,CLNO
            ENDIF
            PRINT     *23,CCGICTY,SP1,CCCTDES,SP1,CCGIKEY,CCGKDES,SP1,CCGIPER
          ENDIF
          ADD       ONE,CLNO
          GOTO      LCCGI100
.
LCCGI999  CALL      UND132
          PRINT     *40,"*** End of Report ***"
          CALL      MESSAGE3
          MOVE      "Report Completed - ",DISPMSG
          CALL      MESSAGE1
          RETURN
.
LCCGIHED  CALL      HEAD132
          IF        FIELDNO=1
            PRINT     "General Ledger Allocation by Interface Key"
            CALL      UND132
            PRINT     "Interface Key        Description                  ":
                      "       Hospital Department Cost Type              ":
                      "                   Percentage"
            CALL      UND132
          ELSE
            PRINT     "General Ledger Allocation by Hospital/Department"
            CALL      UND132
            PRINT     "Hospital/Department   Cost Type                   ":
                      "             Interface Key                        ":
                      "                    Percentage"
            CALL      UND132
          ENDIF
          MOVE      "8",CLNO
          RETURN
