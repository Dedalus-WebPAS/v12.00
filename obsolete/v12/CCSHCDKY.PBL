.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSHCDKY
.   Routine   :   HCCHCA00 Hospital            
.   Return    :   CCGPGNO 
.-------------------------------------------------------------------------------
HCCHCA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "15",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCHCA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCHCA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Code"               
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 5",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Hospital"           
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HCCHCA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCHCHCD 
          PACK      KEY2,CCHCHCD,SP70                                           
.                                                                               
.                                                                               
          CALL      RSCCHC1 
HCCHCA10
          CALL      RKCCHC1 
          BRANCH    OVRCD,HCCHCA15
          PACK      KEY2,CCHCHCD,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HCCHCA12
          CALL      HCCHCA60
          BRANCH    OVRCD,HCCHCA10
HCCHCA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY2,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCHCHCD 
          DISPLAY   *PSTDCOL02:STDROW,CCHCDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCHCA10 IF NOT EQUAL
HCCHCA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCHCA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCHCA30,HCCHCA35,HCCHCA40,HCCHCA45,HCCHCA04:
                              HCCHCA91,HCCHCA50
          GOTO      HCCHCA92
HCCHCA30
          CALL      RKCCHC1 
          BRANCH    OVRCD,HCCHCA85
          PACK      KEY2,CCHCHCD,SP70                                           
.                                                                               
.                                                                               
          CALL      RPCCHC1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCHCA10
HCCHCA35
          MOVE      ST1,KEY2 
          CALL      RDCCHC1 
          CALL      RPCCHC1 
          BRANCH    OVRCD,HCCHCA85
          PACK      KEY2,CCHCHCD,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCHCA36
          CALL      RPCCHC1 
          BRANCH    OVRCD,HCCHCA37
          PACK      KEY2,CCHCHCD,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCHCA36 IF NOT EQUAL
HCCHCA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCHCA10
HCCHCA40
          MOVE      ST1,KEY2 
          CALL      RDCCHC1 
          CALL      RPCCHC1 
          BRANCH    OVRCD,HCCHCA85
          PACK      KEY2,CCHCHCD,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCHCA08
HCCHCA45
          CALL      RKCCHC1 
          BRANCH    OVRCD,HCCHCA85
          PACK      KEY2,CCHCHCD,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY2,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCHC1 
HCCHCA46
          CALL      RPCCHC1 
          BRANCH    OVRCD,HCCHCA47
          PACK      KEY2,CCHCHCD,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCHCA46 IF NOT EQUAL
HCCHCA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCHCA10
HCCHCA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCHCA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCHCA08
HCCHCA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCHCA08
HCCHCA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCHCA61 IF EQUAL
          MATCH     SCHFLD01,CCHCHCD 
          GOTO      HCCHCA72 IF NOT EQUAL
HCCHCA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCHCA62 IF EQUAL
          MATCH     SCHFLD02,CCHCDES 
          GOTO      HCCHCA72 IF NOT EQUAL
HCCHCA62
HCCHCA63
HCCHCA64
HCCHCA65
HCCHCA66
HCCHCA67
HCCHCA68
HCCHCA69
HCCHCA70
          GOTO      HCCHCA75
HCCHCA72  MOVE      ONE,OVRCD
HCCHCA75  RETURN
HCCHCA85
          LOAD      KEY2,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY2,CCHCHCD                                                
          CALL      RDCCHC1 
HCCHCA90
.                                                                               
          BEEP
          GOTO      HCCHCA20
HCCHCA91
          MOVE      SP70,CCHCDES 
          MOVE      SP70,CCHCSPA 
          MOVE      SP70,KEY2 
          MOVE      SP70,CCHCHCD 
          GOTO      HCCHCA96
HCCHCA92
          MOVE      SP70,KEY2 
          MOVE      SP70,CCHCHCD 
          LOAD      KEY2,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY2,CCHCHCD                                                
HCCHCA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCHCA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSHCDKY
.   Routine   :   KCCHCA00 Keyin Hospital            
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCHCA00 TOPIC     "CCHCHCD "
          MOVE      UNDLN70,CCHCHCD 
          KEYIN     *PCCOL:CVERT,*DV,CCHCHCD:                                   
                    *PCCOL:CVERT,*V2LON,CCHCHCD:                                
                    *PCCOL:CVERT,*DV,CCHCHCD 
.
          ENDSET    CCHCHCD 
          APPEND    SP70,CCHCHCD 
          RESET     CCHCHCD 
.
          MATCH     SP70,CCHCHCD 
          GOTO      KCCHCA90 IF EQUAL
.
          CMATCH    EXITCHAR,CCHCHCD 
          GOTO      KCCHCA95 IF EQUAL
.
          CMATCH    QUEST,CCHCHCD 
          GOTO      KCCHCA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCCHCA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCHCHCD 
          APPEND    SP70,CCHCHCD 
          RESET     CCHCHCD 
.
          MATCH     SP70,CCHCHCD 
          GOTO      KCCHCA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CCHCHCD                                 
KCCHCA10
          PACK      KEY2,CCHCHCD                                                
          CALL      RACCHC1 
          BRANCH    OVRCD,KCCHCA80
          BRANCH    CKEYTYP,KCCHCA75
          CALL      RDCCHC1 
KCCHCA70
          MOVE      ZERO,EXIT
          GOTO      KCCHCA99
KCCHCA75
          MOVE      "Hospital Already Exist - ",DISPMSG                                              
          CALL      MESSAGE1
          GOTO      KCCHCA00
KCCHCA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCHCA99,KCCHCA99
          MOVE      "Hospital Does Not Exist - ",DISPMSG                                             
          CALL      MESSAGE1
          GOTO      KCCHCA00
KCCHCA90
          MOVE      ONE,EXIT
          GOTO      KCCHCA99
KCCHCA95
          MOVE      TWO,EXIT
KCCHCA99 TOPIC    PRGID
          RETURN
