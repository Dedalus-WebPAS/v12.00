.---------------------------------------------------------------------------
.   System    :   NZ National Health System          
.   Include   :   NHIDMCKY
.   Routine   :   KNHDMA00 Keyin Domicile            
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KNHDMA00 TOPIC     "NHDMDOM "
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,NHDMDOM
.
          ENDSET    NHDMDOM 
          APPEND    SP70,NHDMDOM 
          RESET     NHDMDOM 
.
          MATCH     SP70,NHDMDOM 
          GOTO      KNHDMA90 IF EQUAL
.
          CMATCH    EXITCHAR,NHDMDOM 
          GOTO      KNHDMA95 IF EQUAL
.
          CMATCH    QUEST,NHDMDOM 
          GOTO      KNHDMA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HNHDMA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    NHDMDOM 
          APPEND    SP70,NHDMDOM 
          RESET     NHDMDOM 
.
          MATCH     SP70,NHDMDOM 
          GOTO      KNHDMA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,NHDMDOM                                 
KNHDMA10
          PACK      KEY4,NHDMDOM                                                
          CALL      RANHDM1 
          BRANCH    OVRCD,KNHDMA80
          BRANCH    CKEYTYP,KNHDMA75
          CALL      RDNHDM1 
KNHDMA70
          MOVE      ZERO,EXIT
          GOTO      KNHDMA99
KNHDMA75
          MOVE      "Domicile Already Exist - ",DISPMSG                                              
          CALL      MESSAGE1
          GOTO      KNHDMA00
KNHDMA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KNHDMA99,KNHDMA99
          MOVE      "Domicile Does Not Exist - ",DISPMSG                                             
          CALL      MESSAGE1
          GOTO      KNHDMA00
KNHDMA90
          MOVE      ONE,EXIT
          GOTO      KNHDMA99
KNHDMA95
          MOVE      TWO,EXIT
KNHDMA99 TOPIC    PRGID
          RETURN
