.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSPGPKY
.   Routine   :   HCCPGA00 Group Code          
.   Return    :   CCPDPCD 
.-------------------------------------------------------------------------------
HCCPGA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCPGA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCPGA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Group"              
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 6",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HCCPGA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCPGGCD 
          PACK      KEY14,CCPGHCD,CCPGDPT,CCPGGCD,SP70                          
          PACK      SAVKEY80,CCPGHCD,CCPGDPT,CCPGGCD,SP70                       
          SETLPTR   SAVKEY80,10                                                 
          CALL      RSCCPG1 
HCCPGA10
          CALL      RKCCPG1 
          BRANCH    OVRCD,HCCPGA15
          PACK      KEY14,CCPGHCD,CCPGDPT,CCPGGCD,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCPGA15 IF NOT EQUAL                                       
          BRANCH    CSEARCH,HCCPGA12
          CALL      HCCPGA60
          BRANCH    OVRCD,HCCPGA10
HCCPGA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY14,XITEMNO,ST1,ST2,ST3,ST4,ST5:                          
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCPGGCD 
          DISPLAY   *PSTDCOL02:STDROW,CCPGDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCPGA10 IF NOT EQUAL
HCCPGA15
          UNPACK    SAVKEY80,CCPGHCD,CCPGDPT,CCPGGCD                            
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCPGA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCPGA30,HCCPGA35,HCCPGA40,HCCPGA45,HCCPGA04:
                              HCCPGA91,HCCPGA50
          GOTO      HCCPGA92
HCCPGA30
          CALL      RKCCPG1 
          BRANCH    OVRCD,HCCPGA85
          PACK      KEY14,CCPGHCD,CCPGDPT,CCPGGCD,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCPGA85 IF NOT EQUAL                                       
          CALL      RPCCPG1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPGA10
HCCPGA35
          MOVE      ST1,KEY14
          CALL      RDCCPG1 
          CALL      RPCCPG1 
          BRANCH    OVRCD,HCCPGA85
          PACK      KEY14,CCPGHCD,CCPGDPT,CCPGGCD,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCPGA85 IF NOT EQUAL                                       
          MATCH     SP70,CCPGGCD                                                
          GOTO      HCCPGA85 IF EQUAL                                           
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCPGA36
          CALL      RPCCPG1 
          BRANCH    OVRCD,HCCPGA37
          PACK      KEY14,CCPGHCD,CCPGDPT,CCPGGCD,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCPGA37 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCPGA36 IF NOT EQUAL
HCCPGA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCPGA10
HCCPGA40
          MOVE      ST1,KEY14
          CALL      RDCCPG1 
          CALL      RPCCPG1 
          BRANCH    OVRCD,HCCPGA85
          PACK      KEY14,CCPGHCD,CCPGDPT,CCPGGCD,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCPGA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPGA08
HCCPGA45
          CALL      RKCCPG1 
          BRANCH    OVRCD,HCCPGA85
          PACK      KEY14,CCPGHCD,CCPGDPT,CCPGGCD,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCPGA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY14,SAVKEY80,Z70                                          
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCPG1 
HCCPGA46
          CALL      RPCCPG1 
          BRANCH    OVRCD,HCCPGA47
          PACK      KEY14,CCPGHCD,CCPGDPT,CCPGGCD,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCPGA47 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCPGA46 IF NOT EQUAL
HCCPGA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCPGA10
HCCPGA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCPGA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPGA08
HCCPGA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPGA08
HCCPGA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCPGA61 IF EQUAL
          MATCH     SCHFLD01,CCPGGCD 
          GOTO      HCCPGA72 IF NOT EQUAL
HCCPGA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCPGA62 IF EQUAL
          MATCH     SCHFLD02,CCPGDES 
          GOTO      HCCPGA72 IF NOT EQUAL
HCCPGA62
HCCPGA63
HCCPGA64
HCCPGA65
HCCPGA66
HCCPGA67
HCCPGA68
HCCPGA69
HCCPGA70
          GOTO      HCCPGA75
HCCPGA72  MOVE      ONE,OVRCD
HCCPGA75  RETURN
HCCPGA85
          LOAD      KEY14,XITEMNO,ST1,ST2,ST3,ST4,ST5:                          
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY14,CCPGHCD,CCPGDPT,CCPGGCD                               
          CALL      RDCCPG1 
HCCPGA90
          UNPACK    SAVKEY80,CCPGHCD,CCPGDPT,CCPGGCD                            
          BEEP
          GOTO      HCCPGA20
HCCPGA91
          MOVE      SP70,CCPGDES 
          MOVE      ZERO,CCPGDSC 
          MOVE      SP70,CCPGSPA 
          MOVE      SP70,KEY14
          MOVE      SP70,CCPGGCD 
          GOTO      HCCPGA96
HCCPGA92
          MOVE      SP70,KEY14
          MOVE      SP70,CCPGGCD 
          LOAD      KEY14,CCITEM,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY14,CCPGHCD,CCPGDPT,CCPGGCD                               
HCCPGA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCPGA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSPGPKY
.   Routine   :   KCCPGA00 Keyin Group               
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCPGA00 TOPIC     "CCPGGCD "
          MOVE      UNDLN70,CCPGGCD 
          KEYIN     *PCCOL:CVERT,*DV,CCPGGCD:                                   
                    *PCCOL:CVERT,*V2LON,CCPGGCD:                                
                    *PCCOL:CVERT,*DV,CCPGGCD 
.
          ENDSET    CCPGGCD 
          APPEND    SP70,CCPGGCD 
          RESET     CCPGGCD 
.
          MATCH     SP70,CCPGGCD 
          GOTO      KCCPGA90 IF EQUAL
.
          CMATCH    EXITCHAR,CCPGGCD 
          GOTO      KCCPGA95 IF EQUAL
.
          CMATCH    QUEST,CCPGGCD 
          GOTO      KCCPGA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCCPGA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCPGGCD 
          APPEND    SP70,CCPGGCD 
          RESET     CCPGGCD 
.
          MATCH     SP70,CCPGGCD 
          GOTO      KCCPGA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CCPGGCD                                 
KCCPGA10
          PACK      KEY14,CCPGHCD,CCPGDPT,CCPGGCD                               
          CALL      RACCPG1 
          BRANCH    OVRCD,KCCPGA80
          BRANCH    CKEYTYP,KCCPGA75
          CALL      RDCCPG1 
KCCPGA70
          MOVE      ZERO,EXIT
          GOTO      KCCPGA99
KCCPGA75
          MOVE      "Group Already Exist - ",DISPMSG                                                 
          CALL      MESSAGE1
          GOTO      KCCPGA00
KCCPGA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCPGA99,KCCPGA99
          MOVE      "Group Does Not Exist - ",DISPMSG                                                
          CALL      MESSAGE1
          GOTO      KCCPGA00
KCCPGA90
          MOVE      ONE,EXIT
          GOTO      KCCPGA99
KCCPGA95
          MOVE      TWO,EXIT
KCCPGA99 TOPIC    PRGID
          RETURN
