.------------------------------------------------------------
. Setup Report Defaults
.------------------------------------------------------------
REPDEF00
          MOVE      SP70,STRPSC
          MOVE      Z70,ENDPSC
          MOVE      "Start                  ",FROMPSC
          MOVE      "Finish                 ",TOPSC
          MOVE      "Standard               ",DESCFMT
          MOVE      "0",REPTFMT
          RETURN
.------------------------------------------------------------
. Print Service Provider Listing
.------------------------------------------------------------
REPT0000  DISPLAY   *P1:24,*EL,"Printing Report : ";
          MOVE      "64",SECTOR
          READ      CONTROLF,SECTOR;*59,CCCOCD1,CCCOCD2,CCCOCD3,CCCOCD4:
                                        CCCOCD5,CCCOIA1,CCCOIA2,CCCOIA3:
                                        CCCOIM1,CCCOIM2,CCCOIM3
          MOVE      "99",CLNO
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
          PACK      KEY3,STRPSC,SP70
          CALL      RDCEPS1
          IF        OVRCD=0
            CALL      RPCEPS1
          ENDIF
REPT1000  CALL      RKCEPS1
          BRANCH    OVRCD,REPT9000
          MATCH     CEPSPSC,ENDPSC
          GOTO      REPT9000 IF LESS
.
          DISPLAY   *P19:24,*V2LON,CEPSPSC
.
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
          ADD       ONE,CLNO
.
          MOVE      CEPSTYP,F1
          MOVE      "999,999.99",STD01
          MOVE      "999,999.99",STD02
          MOVE      "999,999.99",STD03
          MOVE      "999,999.99",STD04
          MOVE      "999,999.99",STD05
          MOVE      "999,999.99",STDCST
          FEDIT     CEPSST1,STD01
          FEDIT     CEPSST2,STD02
          FEDIT     CEPSST3,STD03
          FEDIT     CEPSST4,STD04
          FEDIT     CEPSST5,STD05
          ASSIGN    CEPSST1+CEPSST2+CEPSST3+CEPSST4+CEPSST5,F12P2
          FEDIT     F12P2,STDCST
          MOVE      "999,999.99",DEFQTY
          FEDIT     CEPSQTY,DEFQTY
.
          LOAD      TYPDESC,F1,THEATRE,CCCOIA1,CCCOIA2,CCCOIA3,ACCOTHER:
                               CCCOIM1,CCCOIM2,CCCOIM3,MISOTHER
.
          IF        REPTFMT=0
            PRINT     CEPSPSC,SP1,CEPSDES,DEFQTY,SP1,CEPSQDES,SP2,CEPSTYP:
                      SP1,TYPDESC:
                      " | ",STD01,SP1,STD02,SP1,STD03,SP1,STD04,SP1,STD05," |"
          ELSE
            PRINT     "| ",CEPSPSC,SP1,CEPSDES,*51:
                      " | ",STD01,SP1,STD02,SP1,STD03,SP1,STD04,SP1,STD05," | ":
                      STDCST," per ",CEPSQDES," |"
          ENDIF
          GOTO      REPT1000
.
REPT9000  CALL      UND132
          PRINT     *45,"***** End of Report *****"
          RETURN
.------------------------------------------------------------
. Page Header
.------------------------------------------------------------
HEAD0000  CALL      HEAD132
          PRINT     "Patient Service Master List From : ",FROMPSC
          PRINT     "                            To   : ",TOPSC
          PRINT     "Report Format                    : ",REPTFMT,SP1,DESCFMT
          CALL      UND132
          IF        REPTFMT=0
            PRINT     "Patient Service                               ":
                      "   Quantity    Income       |              ":
                      "   Standard Cost Components               |"
            PRINT     "                                              ":
                      "Default Type   Component    | ",CCCOCD1,SP1,CCCOCD2,SP1:
                      CCCOCD3,SP1,CCCOCD4,SP1,CCCOCD5,*132,"|"
          ELSE
            PRINT     "| Patient Service                             ":
                      "     |                   Standard Cost Compone":
                      "nts             | Total Standard Cost  |"
            PRINT     "|                                                  | ":
                      CCCOCD1,SP1,CCCOCD2,SP1,CCCOCD3,SP1,CCCOCD4,SP1,CCCOCD5:
                       " |                      |"
          ENDIF
          CALL      UND132
          ADD       "5",CLNO
          RETURN
