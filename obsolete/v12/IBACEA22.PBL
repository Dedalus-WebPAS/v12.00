. Enter a Range of Patient Service Codes
.------------------------------------------------------------
RPSC0000   MOVE     "    5",FLDID
           CALL     GETPOSIT
           CALL     KCEPSA00
           BRANCH   EXIT,RPSC0100,RPSC0999
           MOVE     CEPSPSC,STRPSC
           PACK     FROMPSC,CEPSPSC,SP1,CEPSDES
           GOTO     RPSC0140
RPSC0100   MOVE     "Start             ",FROMPSC
           MOVE     SP70,STRPSC
.
RPSC0140   MOVE     FROMPSC,VAR
           CALL     DISPITEM
.
RPSC0150   MOVE     "    6",FLDID
           CALL     GETPOSIT
           CALL     KCEPSA00
           BRANCH   EXIT,RPSC0200,RPSC0999
           MOVE     CEPSPSC,ENDPSC
           PACK     TOPSC,CEPSPSC,SP1,CEPSDES
           MATCH    STRPSC,ENDPSC
           GOTO     RPSC0999 IF EQUAL
           GOTO     RPSC0999 IF NOT LESS
           BEEP
           MOVE     "To Patient Serv. Must Be > From Patient Serv. - ",DISPMSG
           CALL     MESSAGE1
           GOTO     RPSC0000
.
RPSC0200   MOVE     "Finish             ",TOPSC
           MOVE     "ZZ",ENDPSC
.
RPSC0999   MOVE     "    1",FLDID
           CALL     GETPOSIT
           RETURN
