                            ITMDET06,ITMDET07,ITMDET08,ITMDET09,ITMDET10:
                            ITMDET11,ITMDET12,ITMDET13,ITMDET14,ITMDET15
          APPEND    "New Stock Item",TRANDESC
          GOTO      ITMDET99
ITMDET01  APPEND    "Stock Adj.",TRANDESC
          GOTO      ITMDET20
ITMDET02  APPEND    "Stocktake Adj.",TRANDESC
          GOTO      ITMDET20
