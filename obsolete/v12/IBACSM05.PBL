.-------------------------------------------------------------
. File : /users/bga/std/auditkey
.
. Audit & Locking Standard Routine to Enter Key Field
.
. where   xx = Key Length
.         ss = System 2 Character Code
.         ff = File 2 Character Code
.       yyyy = Key Field IDs
.-------------------------------------------------------------
EKEYF000  MOVE      "   10",FLDID
          CALL      GETPOSIT
          CALL      KCSIAA00                * Call Standard Keyin Routine with ?
          BRANCH    EXIT,EKEYF999,EKEYF999  * Exit 1 Null Entry 2 Exitchar
          COMPARE   ONE,OVRCD               * New Record so no Lock
          GOTO      EKEYF999 IF EQUAL       * Return
.
EKEYF100  PACK      KEY7,CSIACAT,SP70       * Setup Key to File
          CALL      RLCSIA1                 * Read and Lock Record
          BRANCH    OVRCD,EKEYF999,EKEYF200 * OVRCD=1 Not on File 2 Locked
.
          MOVE      ZERO,HLEF
          CALL      GETSCR00
          CALL      CHALL000                * Check All Databases
          IF        ERRCOUNT<>0
            BEEP
            MOVE       "Deletion Aborted - ",DISPMSG
            CALL       MESSAGE1
            CALL       PUTSCR00
            GOTO       EKEYF000
          ENDIF
          CALL      PUTSCR00
.
          MOVE      ZERO,EXIT               * Setup Exit Field
          MOVE      ZERO,OVRCD              * Setup Ovrcd Field
          GOTO      EKEYF999
.
EKEYF200  MOVE      SP70,DISPMSG            * Clear Line 24 Message Display
          CALL      MESSAGE2                * Clear Line 24 and Save
          KEYIN     *P1:24,*EL,"Record Locked - (":
                    *V2LON,"R",*HOFF,")etry or e(":
                    *V2LON,"X",*HOFF,")it ? ",ANS
          CALL      MESSAGE3                * Restore Line 24
          REP       UPPLOW,ANS              * Convert to Upper Case
          MATCH     ANSR,ANS                * Re-try
          GOTO      EKEYF100 IF EQUAL       * Return to locking
          MATCH     ANSE,ANS                * Exit
          GOTO      EKEYF000 IF EQUAL       * Must be a valid response
          MATCH     ANSE,ANS                * Exit
          GOTO      EKEYF200 IF NOT EQUAL   * Must be a valid response
          GOTO      EKEYF000                * Return to Keyin
.
EKEYF999  RETURN                            * End of Routine
.------------------------------------------------------------
. Check All Databases
.------------------------------------------------------------
CHALL000  MOVE     "5",CVERT
          DISPLAY  *P1:5,*EF,*V2LON,*ULON,"Database            ",*HOFF,SP1:
                         *V2LON,*ULON,"   Date   ",*HOFF,SP1:
                         *V2LON,*ULON,"Transaction Details                   "
.
          MOVE     ZERO,ERRCOUNT
          MOVE     ZERO,ENDFLAG
          PACK     KEY3,SP70
          CALL     RSIBDB1
CHALL100  CALL     RKIBDB1                * Update Each Database
          BRANCH   OVRCD,CHALL900
          DISPLAY  *P1:23,*EL,"Database ",IBDBDBS,SP1,IBDBDES
.
          CALL     OPNFIL00               * Open Database Files
          BRANCH   EXIT,CHALL990
.
          MOVE     CSIACAT,KEY7
          CALL     RDSIIA1                * Check on Database
          BRANCH   OVRCD,CHALL100
.
          CALL     CHKSOH00               * Check SOH & SOO
          BRANCH   ENDFLAG,CHALL000,CHALL900
.
          CALL     CHKBOD00               * Check Back Orders
          BRANCH   ENDFLAG,CHALL000,CHALL900
.
          IF       SIIANON=0
            CALL     CHKHIS00             * Item History
            BRANCH   ENDFLAG,CHALL000,CHALL900
          ENDIF
          CALL     CLSFIL00
          GOTO     CHALL100
CHALL900  MOVE     ZERO,EXIT
          GOTO     CHALL999
CHALL990  MOVE     ONE,EXIT
CHALL999  DISPLAY  *P1:23,*EL
          RETURN
.------------------------------------------------------------
. Check SOH Prior to Delete
.------------------------------------------------------------
CHKSOH00  PACK      KEY12,SIIACAT,SP70
          CALL      RSSIIE1
CHKSOH10  CALL      RKSIIE1
          BRANCH    OVRCD,CHKSOH90
          MATCH     SIIACAT,SIIECAT
          GOTO      CHKSOH90 IF NOT EQUAL
.
          IF        SIIESOH>0
            CLEAR     TRANDESC
            APPEND    "Stock in Warehouse ",TRANDESC
            APPEND    SIIEWAR,TRANDESC
            RESET     TRANDESC
            PACK      TRANDATE,CCC,CYY,CMM,CDD
            REP       " 0",TRANDATE
            CALL      ERRLIN00
            GOTO      CHKSOH95
          ENDIF
.
          IF        SIIESOO>0
            CLEAR     TRANDESC
            APPEND    "Stock on Order for WareHouse ",TRANDESC
            APPEND    SIIEWAR,TRANDESC
            RESET     TRANDESC
            PACK      TRANDATE,CCC,CYY,CMM,CDD
            REP       " 0",TRANDATE
            CALL      ERRLIN00
            GOTO      CHKSOH95
          ENDIF
.
          GOTO      CHKSOH10
.
CHKSOH90  MOVE      ZERO,EXIT
          GOTO      CHKSOH99
.
CHKSOH95  MOVE      ONE,EXIT
.
CHKSOH99  RETURN
.------------------------------------------------------------
. Check for Back Orders
.------------------------------------------------------------
CHKBOD00  PACK      KEY25,SIIACAT,SP70
CHKBOD10  CALL      RSSIBA3
          CALL      RKSIBA3
          BRANCH    OVRCD,CHKBOD90
          PACK      KEY25,SIBACAT,SIBAWAR,SIBACST,SIBAREQ,SP70
          MATCH     SIIACAT,KEY25
          GOTO      CHKBOD90 IF NOT EQUAL
          IF        SIBAQTY>0
            CLEAR     TRANDESC
            APPEND    "Back Orders for Warehouse ",TRANDESC
            APPEND    SIBAWAR,TRANDESC
            RESET     TRANDESC
            PACK      TRANDATE,CCC,CYY,CMM,CDD
            REP       " 0",TRANDATE
            CALL      ERRLIN00
            GOTO      CHKBOD95
          ENDIF
          CALL      DESIBA3
          GOTO      CHKBOD10
.
CHKBOD90  MOVE      ZERO,EXIT
          GOTO      CHKBOD99
CHKBOD95  MOVE      ONE,EXIT
CHKBOD99  RETURN
.------------------------------------------------------------
. Check Item History File
.------------------------------------------------------------
CHKHIS00  PACK      SIPEDATE,CCC,CYY,CMM,CDD,SP70
          REP       " 0",SIPEDATE      * get last financial year
          CALL      RSIPE000
          IF        OVRCD=1
            MOVE      "Cannot find a valid period",TRANDESC
            PACK      TRANDATE,CCC,CYY,CMM,CDD
            REP       " 0",TRANDATE
            CALL      ERRLIN00
            GOTO      CHKHIS95
          ENDIF
          MOVE      SIPEYEAR,SIPEYF4
          SUB       ONE,SIPEYF4
          MOVE      SIPEYF4,SIPEYEAR
.
          PACK      KEY5,SP70         * check item history
          CALL      RSSIWA1
CHKHIS10  CALL      RKSIWA1
          BRANCH    OVRCD,CHKHIS90
          PACK      KEY28,SIWAWAR,SIIACAT,Z70
          PACK      KEY13,SIWAWAR,SIIACAT
          CALL      RSSIIT1
CHKHIS20  CALL      RPSIIT1
          BRANCH    OVRCD,CHKHIS10
          PACK      KEY28,SIITWAR,SIITCAT,SIITDAT,SIITTIM,SP70
          MATCH     KEY13,KEY28
          GOTO      CHKHIS10 IF NOT EQUAL
.
          MOVE      SIITPER,KEY4
          MATCH     SIPEYEAR,KEY4
          IF          !@LESS
            COMPARE    ZERO,SIITTYP
            GOTO       CHKHIS20 IF EQUAL
            BRANCH     SIITTYP,CHKHIS20,CHKHIS20,CHKHIS20
            MOVE       SIITDAT,TRANDATE
            CALL       PACDATE
            CLEAR      TRANDESC
            APPEND     "Item history - ",TRANDESC
            CALL       ITMDET00
            RESET      TRANDESC
            CALL       ERRLIN00
            GOTO       CHKHIS95
          ENDIF
          GOTO      CHKHIS10
CHKHIS90  MOVE      ZERO,EXIT
          GOTO      CHKHIS99
CHKHIS95  MOVE      ONE,EXIT
CHKHIS99  RETURN
.
