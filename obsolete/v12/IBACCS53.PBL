.----------------------------------------
. Departmental Cost Model Analysis
.----------------------------------------
REPT0000  MOVE      "Printing : ",DISPMSG
          MOVE      ZERO,TOTALDIR
          MOVE      ZERO,TOTALIND
          MOVE      ZERO,GRANDDIR
          MOVE      ZERO,GRANDIND
          CALL      MESSAGE2
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
.
          MOVE      CCCAYEAR,KEY4
          PACK      KEY6,KEY4,FPERIOD
          CALL      RDCCCA1
          MATCH     "  ",FPERIOD
          IF        @EQUAL
            CALL      RKCCCA1
          ENDIF
          MOVE      CCCADES,FROMPNAM
          MOVE      KEY4,CCCAYEAR
.
          PACK      KEY6,KEY4,LPERIOD
          CALL      RDCCCA1
          MATCH     "ZZ",LPERIOD
          IF        @EQUAL
            CALL      RPCCCA1
          ENDIF
          MOVE      CCCADES,TOPNAM
          MOVE      KEY4,CCCAYEAR
.
          PACK      KEY10,CCHCHCD,FDEPART
          CALL      RDCCDP1
          IF        OVRCD=0
            CALL      RPCCDP1
          ENDIF
.
REPT1000  CALL      RKCCDP1
          BRANCH    OVRCD,REPT9000
          MATCH     CCHCHCD,CCDPHCD
          GOTO      REPT9000 IF NOT EQUAL
          MATCH     CCDPDPT,LDEPART
          GOTO      REPT9000 IF LESS
          IF        DIRDONLY=1
            MATCH     "0",CCDPDTY
            GOTO      REPT1000 IF NOT EQUAL
          ENDIF
.
          DISPLAY   *P12:24,CCDPDPT
.
          MOVE      ZERO,TOTALDIR
          MOVE      ZERO,TOTALIND
          BRANCH    SUMREPT,REPT3000
.
REPT2000  MOVE      ZERO,FIRSTIND
          PACK      KEY4,SP70
          CALL      RSCCCT1
REPT2100  CALL      RKCCCT1
          BRANCH    OVRCD,REPT4000
.
          MOVE      ZERO,DIRECT
          MOVE      ZERO,INDIRT
          MOVE      ONE,KEYFLAG
          PACK      KEY18,CCDPHCD,CCDPDPT,CCCTCTY,CCCAYEAR,SP70
          PACK      KEY20,CCDPHCD,CCDPDPT,CCCTCTY,CCCAYEAR,SP70
          CALL      RSCCMB2
REPT2200  CALL      RKCCMB2
          BRANCH    OVRCD,REPT2300
          PACK      KEY20,CCMBHCD,CCMBDPT,CCMBCTY,CCMBYEAR,CCMBPER,SP70
          MATCH     KEY18,KEY20
          GOTO      REPT2300 IF NOT EQUAL
          MATCH     CCMBPER,LPERIOD
          GOTO      REPT2300 IF LESS
          MATCH     FPERIOD,CCMBPER
          GOTO      REPT2200 IF LESS
.
          MOVE      ZERO,KEYFLAG
          ADD       CCMBDIR,TOTALDIR
          ADD       CCMBIND,TOTALIND
          ADD       CCMBDIR,DIRECT
          ADD       CCMBIND,INDIRT
          GOTO      REPT2200
.
REPT2300  BRANCH    KEYFLAG,REPT2100
          IF        DIRECT=0&INDIRT=0
            GOTO      REPT2100
          ENDIF
.
          ADD       ONE,CLNO
          COMPARE   "60",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          IF        FIRSTIND=0
            PRINT     CCDPDPT,SP1,CCDPDES;
            MOVE      ONE,FIRSTIND
          ENDIF
.
          MOVE      CCFED1,FTOTAL1
          MOVE      CCFED1,FTOTAL2
          MOVE      CCFED1,FTOTAL3
          FEDIT     DIRECT,FTOTAL1
          FEDIT     INDIRT,FTOTAL2
          ASSIGN    DIRECT+INDIRT,TOTAL
          FEDIT     TOTAL,FTOTAL3
.
          MATCH     "*",FTOTAL1
          IF        @EQUAL
            MOVE      DIRECT,F10P2
            MOVE      F10P2,FTOTAL1
          ENDIF
.
          MATCH     "*",FTOTAL2
          IF        @EQUAL
            MOVE      INDIRT,F10P2
            MOVE      F10P2,FTOTAL2
          ENDIF
.
          MATCH     "*",FTOTAL3
          IF        @EQUAL
            MOVE      TOTAL,F10P2
            MOVE      F10P2,FTOTAL3
          ENDIF
          IF        INCINDIR=0
            PRINT    *46,CCCTCTY,SP1,CCCTDES,*89,FTOTAL1
          ELSE
            PRINT    *46,CCCTCTY,SP1,CCCTDES,*89,FTOTAL1,*104,FTOTAL2:
                     *119,FTOTAL3
          ENDIF
          GOTO       REPT2100
.
. Summary Report Only
.
REPT3000
          PACK      KEY16,CCDPHCD,CCDPDPT,CCCAYEAR,LPERIOD,SP70
          PACK      KEY20,CCDPHCD,CCDPDPT,CCCAYEAR,FPERIOD,SP70
          CALL      RSCCMB1
REPT3100  CALL      RKCCMB1
          BRANCH    OVRCD,REPT4000
          PACK      KEY20,CCMBHCD,CCMBDPT,CCMBYEAR,CCMBPER,SP70
          MATCH     KEY20,KEY16
          GOTO      REPT3200 IF EQUAL
          GOTO      REPT4000 IF LESS
REPT3200  ADD       CCMBDIR,TOTALDIR
          ADD       CCMBIND,TOTALIND
          GOTO      REPT3100
.
.
. Print Department Totals
.
REPT4000  MOVE      CCFED1,FTOTAL1
          MOVE      CCFED1,FTOTAL2
          MOVE      CCFED1,FTOTAL3
          FEDIT     TOTALDIR,FTOTAL1
          FEDIT     TOTALIND,FTOTAL2
          ASSIGN    TOTALDIR+TOTALIND,TOTAL
          FEDIT     TOTAL,FTOTAL3
.
          MATCH     "*",FTOTAL1
          IF        @EQUAL
            MOVE      TOTALDIR,F10P2
            MOVE      F10P2,FTOTAL1
          ENDIF
.
          MATCH     "*",FTOTAL2
          IF        @EQUAL
            MOVE      TOTALIND,F10P2
            MOVE      F10P2,FTOTAL2
          ENDIF
.
          MATCH     "*",FTOTAL3
          IF        @EQUAL
            MOVE      TOTAL,F10P2
            MOVE      F10P2,FTOTAL3
          ENDIF
.
          BRANCH    SUMREPT,REPT4800
          ADD       THREE,CLNO
          COMPARE   "60",CLNO
          CALL      HEAD0000 IF NOT LESS
          IF        FIRSTIND=0
            PRINT     CCDPDPT,SP1,CCDPDES
          ENDIF
          IF        INCINDIR=0
            PRINT    *89,"-------------"
            PRINT    *89,FTOTAL1
            PRINT    *89,"-------------"
          ELSE
            PRINT    *89,"-------------",*104,"-------------":
                    *119,"-------------"
            PRINT    *89,FTOTAL1,*104,FTOTAL2,*119,FTOTAL3
            PRINT    *89,"-------------",*104,"-------------":
                    *119,"-------------"
          ENDIF
          ADD       TOTALDIR,GRANDDIR
          ADD       TOTALIND,GRANDIND
          GOTO      REPT1000
REPT4800
          ADD       ONE,CLNO
          COMPARE   "60",CLNO
          CALL      HEAD0000 IF NOT LESS
          IF        INCINDIR=0
            PRINT     CCDPDPT,SP1,CCDPDES,*49,FTOTAL1
          ELSE
            MOVE      CCDPDES,KEY30
            PRINT     CCDPDPT,SP1,KEY30,FTOTAL1,SP1,FTOTAL2,SP1,FTOTAL3
          ENDIF
          ADD       TOTALDIR,GRANDDIR
          ADD       TOTALIND,GRANDIND
          GOTO      REPT1000
.
REPT9000  MOVE      CCFED1,FTOTAL1
          MOVE      CCFED1,FTOTAL2
          MOVE      CCFED1,FTOTAL3
          FEDIT     GRANDDIR,FTOTAL1
          FEDIT     GRANDIND,FTOTAL2
          ASSIGN    GRANDDIR+GRANDIND,TOTAL
          FEDIT     TOTAL,FTOTAL3
.
          MATCH     "*",FTOTAL1
          IF        @EQUAL
            MOVE      GRANDDIR,F10P2
            MOVE      F10P2,FTOTAL1
          ENDIF
.
          MATCH     "*",FTOTAL2
          IF        @EQUAL
            MOVE      GRANDIND,F10P2
            MOVE      F10P2,FTOTAL2
          ENDIF
.
          MATCH     "*",FTOTAL3
          IF        @EQUAL
            MOVE      TOTAL,F10P2
            MOVE      F10P2,FTOTAL3
          ENDIF
.
          BRANCH    SUMREPT,REPT9100
          CALL      UND132
          IF        INCINDIR=0
            PRINT    *70,"Overall Total",*89,FTOTAL1
            PRINT    *89,"-------------"
          ELSE
            PRINT    *70,"Overall Total",*89,FTOTAL1,*104,FTOTAL2,*119,FTOTAL3
            PRINT    *89,"-------------",*104,"-------------":
                    *119,"-------------"
          ENDIF
          CALL      UND132
          PRINT     *45,"*** End of Report ***"
          GOTO      REPT9200
.
REPT9100  CALL      UND80
          IF        INCINDIR=0
            PRINT     *15,"Overall Total",*49,FTOTAL1
          ELSE
            PRINT     *15,"Overall Total",*40,FTOTAL1,SP1,FTOTAL2,SP1,FTOTAL3
          ENDIF
          CALL      UND80
          PRINT     *25,"*** End of Report ***"
.
REPT9200  CALL      MESSAGE3
          MOVE      "Report Completed - ",DISPMSG
          CALL      MESSAGE1
          RETURN
.
HEAD0000  MOVE      ZERO,FIRSTIND
          BRANCH    SUMREPT,HEAD1000
          CALL      HEAD132
          MOVE      "8",CLNO
          PRINT     "Cost Model       : ",CCMACOM,SP1,CCMADES
          PRINT     "Hospital         : ",CCHCHCD,SP3,CCHCDES:
                    " Year : ",CCCAYEAR
          IF        CPAGENO=1
            PRINT     "Department From  : ",FROMDEPT
            PRINT     "           To    : ",TODEPT
            PRINT     "Period     From  : ",FROMPNAM," to ",TOPNAM
            ADD       "3",CLNO
          ENDIF
          CALL      UND132
          IF        INCINDIR=0
            PRINT     "Department                                   ":
                      "Cost Type                                    ":
                      "Direct Cost"
          ELSE
            PRINT     "Department                                   ":
                      "Cost Type                                    ":
                      "Direct Cost  Indirect Cost     Total Cost"
          ENDIF
          CALL      UND132
          GOTO      HEAD9999
.
HEAD1000  CALL      HEAD80
          MOVE      "8",CLNO
          PRINT     "Hospital         : ",CCHCHCD,SP1,CCHCDES:
                    " Year : ",CCCAYEAR
          IF        CPAGENO=1
            PRINT     "Department From  : ",FROMDEPT
            PRINT     "           To    : ",TODEPT
            PRINT     "Period     From  : ",FROMPNAM," to ",TOPNAM
            ADD       "3",CLNO
          ENDIF
          CALL      UND80
          IF        INCINDIR=0
            PRINT     "Department                                        Direc":
                      "t Cost"
          ELSE
            PRINT     "Department                               Direct Cost":
                      " Indirect Cost    Total Cost"
          ENDIF
          CALL      UND80
HEAD9999  RETURN
