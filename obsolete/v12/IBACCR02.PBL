. Print Report Heading
.------------------------------------------------------------
HEAD0000  CALL      HEAD132
          MOVE      "9",CLNO
          IF        CPAGENO=1
            PRINT     "Pat. Serv. From  : ",FROMPROC:
                      " to : ",TOPROC
            ADD       "1",CLNO
          ENDIF
          PRINT     "Costing Model    : ",CCMACOM,SP5,CCMADES
          PRINT     "Hospital         : ",CCHCHCD,SP7,CCHCDES:
                    " Year   : ",CCCAYEAR
          PRINT     "Department       : ",CCDPDPT,SP1,CCDPDES
          PRINT     "Patient Service  : ",CCPDPCD,SP1,CCPDDES
          CALL      UND132
          PRINT     "| Period         |                Volume               |":
                    "               Unit Cost             |":
                    "               Total Cost            |"
          PRINT     "|                |       Budget       Actual     % Var |":
                    "       Budget       Actual     % Var |":
                    "       Budget       Actual     % Var |"
          CALL      UND132
HEAD9999  RETURN
