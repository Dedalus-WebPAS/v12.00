. Print Department Details
.------------------------------------------------------------
PDPT0000  MOVE      ZERO,ACTTOTAL
          MOVE      ZERO,BUDTOTAL
          PACK      KEY18,CCDPHCD,CCDPDPT,FPROCED,SP70
          CALL      RDCCPD1
          IF        OVRCD=0
            CALL      RPCCPD1
          ENDIF
PDPT1000  CALL      RKCCPD1
          BRANCH    OVRCD,PDPT9000
          PACK      KEY10,CCDPHCD,CCDPDPT,SP70
          PACK      KEY18,CCPDHCD,CCPDDPT,CCPDPCD
          MATCH     KEY10,KEY18
          GOTO      PDPT9000 IF NOT EQUAL
          MATCH     CCPDPCD,LPROCED
          GOTO      PDPT9000 IF LESS
.
          CALL      HEAD0000          * Page Per Pat. Serv.
.
          CALL      PPCD0000          * Print Pat. Serv. Code Details
.
          GOTO      PDPT1000
.
. Print Department Totals ( If Any )
.
PDPT9000  RETURN
.------------------------------------------------------------
. Print Pat. Serv. Code Details
.------------------------------------------------------------
PPCD0000  MOVE      ZERO,TOTALCST
          MOVE      ZERO,TOTALBUD
          MOVE      ZERO,TOTALBQT
          MOVE      ZERO,TOTALQTY
          MOVE      ONE,KEYFLAG
          PACK      KEY6,PROYEAR,SP70
          CALL      RSCCCA1
PPCD1000  CALL      RKCCCA1
          BRANCH    OVRCD,PPCD9000
          MATCH     PROYEAR,CCCAYEAR
          GOTO      PPCD9000 IF NOT EQUAL
.
          PACK      KEY24,CCDPHCD,CCDPDPT,CCPDPCD,CCCAYEAR,CCCAPER
          CALL      RDCCME2           * Determine Model Total(Budget)
          IF        OVRCD=1
            MOVE      ZERO,CCMEQTY
            MOVE      ZERO,CCMECST
          ENDIF
.
          PACK      KEY24,CCDPHCD,CCDPDPT,CCPDPCD,CCCAYEAR,CCCAPER
          CALL      RDCCPS2
          IF        OVRCD=1
            MOVE      ZERO,CCPSQTY
            MOVE      ZERO,CCPSCST
          ENDIF
.
          MOVE      CCFED1,FTOTAL1
          MOVE      CCFED1,FTOTAL2
          MOVE      "999,999.99",F10TOT1
          MOVE      CCFED1,FTOTAL3
          MOVE      CCFED1,FTOTAL4
          MOVE      "999,999.99",F10TOT2
          MOVE      CCFED1,FTOTAL5
          MOVE      CCFED1,FTOTAL6
          MOVE      "999,999.99",F10TOT3
.
          MOVE      CCMEQTY,F12P2
          FEDIT     F12P2,FTOTAL1
          MOVE      CCPSQTY,F12P2
          FEDIT     F12P2,FTOTAL2
          ASSIGN    ((CCMEQTY-CCPSQTY)/CCMEQTY)*100.0,F12P2
          FEDIT     F12P2,F10TOT1
          IF        CCMEQTY=0
            MOVE      SP70,F10TOT1
          ENDIF
.
          MOVE      CCMECST,F12P2
          FEDIT     F12P2,FTOTAL3
          MOVE      CCPSCST,F12P2
          FEDIT     F12P2,FTOTAL4
          ASSIGN    ((CCMECST-CCPSCST)/CCMECST)*100.0,F12P2
          FEDIT     F12P2,F10TOT2
          IF        CCMECST=0
            MOVE      SP70,F10TOT2
          ENDIF
.
          ASSIGN    CCMECST*CCMEQTY,BUDGET
          FEDIT     BUDGET,FTOTAL5
          ASSIGN    CCPSCST*CCPSQTY,ACTUAL
          FEDIT     ACTUAL,FTOTAL6
          ASSIGN    ((BUDGET-ACTUAL)/BUDGET)*100.0,F12P2
          FEDIT     F12P2,F10TOT3
          IF        BUDGET=0
            MOVE      SP70,F10TOT3
          ENDIF
.
          ADD       ACTUAL,TOTALCST
          ADD       BUDGET,TOTALBUD
          ADD       CCMEQTY,TOTALBQT
          ADD       CCPSQTY,TOTALQTY
          PRINT     "| ",CCCADES,"|",FTOTAL1,FTOTAL2,F10TOT1," |":
                                     FTOTAL3,FTOTAL4,F10TOT2," |":
                                     FTOTAL5,FTOTAL6,F10TOT3," |"
          GOTO      PPCD1000
.
PPCD9000
          MOVE      CCFED1,FTOTAL1
          MOVE      CCFED1,FTOTAL2
          MOVE      "999,999.99",F10TOT1
          MOVE      CCFED1,FTOTAL3
          MOVE      CCFED1,FTOTAL4
          MOVE      "999,999.99",F10TOT2
          MOVE      CCFED1,FTOTAL5
          MOVE      CCFED1,FTOTAL6
          MOVE      "999,999.99",F10TOT3
.
          MOVE      TOTALBQT,F12P2
          FEDIT     F12P2,FTOTAL1
.
          MOVE      TOTALQTY,F12P2
          FEDIT     F12P2,FTOTAL2
.
          ASSIGN    ((TOTALBQT-TOTALQTY)/TOTALBQT)*100.0,F12P2
          FEDIT     F12P2,F10TOT1
          IF        TOTALBQT=0
            MOVE      SP70,F10TOT1
          ENDIF
.
          ASSIGN    TOTALBUD/TOTALBQT,BUDGET
          FEDIT     BUDGET,FTOTAL3
.
          ASSIGN    TOTALCST/TOTALQTY,ACTUAL
          FEDIT     ACTUAL,FTOTAL4
.
          ASSIGN    ((BUDGET-ACTUAL)/BUDGET)*100.0,F12P2
          FEDIT     F12P2,F10TOT2
          IF        BUDGET=0
            MOVE      SP70,F10TOT2
          ENDIF
.
          MOVE      TOTALBUD,F12P2
          FEDIT     F12P2,FTOTAL5
.
          MOVE      TOTALCST,F12P2
          FEDIT     F12P2,FTOTAL6
.
          ASSIGN    ((TOTALBUD-TOTALCST)/TOTALBUD)*100.0,F12P2
          FEDIT     F12P2,F10TOT3
          IF        TOTALBUD=0
            MOVE      SP70,F10TOT3
          ENDIF
.
          CALL      UND132
          PRINT     "| Total          |",FTOTAL1,FTOTAL2,F10TOT1," |":
                                         FTOTAL3,FTOTAL4,F10TOT2," |":
                                         FTOTAL5,FTOTAL6,F10TOT3," |"
.
PPCD9999  RETURN
