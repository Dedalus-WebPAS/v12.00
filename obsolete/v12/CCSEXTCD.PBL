. Temp file I/O for Cost Allocation Extract
RSEXTM1   RESET     KEY16
          READ      EXTTMPA1,KEY16;;
          RETURN
.
RAEXTM1   RESET     KEY16
          MOVE      ZERO,OVRCD
          READ      EXTTMPA1,KEY16;ANS
          GOTO      OVERCOND IF OVER
          RETURN
.
RDEXTM1   RESET     KEY16
          MOVE      ZERO,OVRCD
          READ      EXTTMPA1,KEY16;EXTMDPT,EXTMCTY,EXTMATY,EXTMTWG:
                                  EXTMWGT,EXTMTOT,EXTMAMT,EXTMCNT
          GOTO      OVERCOND IF OVER
          RETURN
.
RKEXTM1   MOVE      ZERO,OVRCD
          READKS    EXTTMPA1;EXTMDPT,EXTMCTY,EXTMATY,EXTMTWG:
                                  EXTMWGT,EXTMTOT,EXTMAMT,EXTMCNT
          GOTO      OVERCOND IF OVER
          RETURN
.
RPEXTM1   MOVE      ZERO,OVRCD
          READKP    EXTTMPA1;EXTMDPT,EXTMCTY,EXTMATY,EXTMTWG:
                                  EXTMWGT,EXTMTOT,EXTMAMT,EXTMCNT
          GOTO      OVERCOND IF OVER
          RETURN
.
UPEXTM1   UPDATE    EXTTMPA1;EXTMDPT,EXTMCTY,EXTMATY,EXTMTWG:
                                  EXTMWGT,EXTMTOT,EXTMAMT,EXTMCNT
          RETURN
.
WREXTM1   RESET     KEY16
          WRITE     EXTTMPA1,KEY16;EXTMDPT,EXTMCTY,EXTMATY,EXTMTWG:
                                  EXTMWGT,EXTMTOT,EXTMAMT,EXTMCNT
          RETURN
.
DEEXTM1   RESET     KEY16
          DELETE    EXTTMPA1,KEY16
          RETURN
.
