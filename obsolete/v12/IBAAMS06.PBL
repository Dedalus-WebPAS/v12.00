EDEP0100  MOVE      "Prime Cost       ",BASDESC
          GOTO      EDEP0900
.
EDEP0200  MOVE      "Diminishing Value",BASDESC
          GOTO      EDEP0900
.
EDEP0300  MOVE      "Original Cost    ",BASDESC
          GOTO      EDEP0900
.
EDEP0900  BRANCH    AMDEFRQ,EDEP1100,EDEP1200,EDEP1300,EDEP1400,EDEP1500
EDEP1100  MOVE      "Monthly     ",FRQDESC
          GOTO      EDEP1900
.
EDEP1200  MOVE      "13 Periods  ",FRQDESC
          GOTO      EDEP1900
.
EDEP1300  MOVE      "Quarterly   ",FRQDESC
          GOTO      EDEP1900
.
EDEP1400  MOVE      "Half Yearly ",FRQDESC
          GOTO      EDEP1900
.
EDEP1500  MOVE      "Yearly      ",FRQDESC
          GOTO      EDEP1900
EDEP1900
          MOVE      ZERO,EXIT               * Setup Exit Field
          MOVE      ZERO,OVRCD              * Setup Ovrcd Field
          GOTO      EDEP9999
.
.
EDEP9999  RETURN                            * End of Routine
.
EBAS0000  MOVE      SP70,DISPMSG
          CALL      MESSAGE2
          DISPLAY   *P1:24,"Dep'n Based on : ",*V2LON,"1. ",*HOFF:
                    "Prime Cost, ",*V2LON,"2. ",*HOFF,"Diminishing Value, ":
                    *V2LON,"3. ",*HOFF,"Original Cost"
EBAS0100  MOVE      ONE,AMDEBAS
          KEYIN     *PCCOL:CVERT,*V2LON,*MASK,*RV,AMDEBAS
          BRANCH    AMDEBAS,EBAS1000,EBAS2000,EBAS3000
          BEEP
          GOTO      EBAS0100
EBAS1000  MOVE      "Prime Cost       ",BASDESC
          GOTO      EBAS9000
.
EBAS2000  MOVE      "Diminishing Value",BASDESC
          GOTO      EBAS9000
.
EBAS3000  MOVE      "Original Cost    ",BASDESC
          GOTO      EBAS9000
.
EBAS9000  CALL      MESSAGE3
          RETURN
EFRQ0000  MOVE      SP70,DISPMSG
          CALL      MESSAGE2
          DISPLAY   *P1:24,"Frequency : ":
                    *V2LON,"1. ",*HOFF,"Monthly, ":
                    *V2LON,"2. ",*HOFF,"13 Periods, ":
                    *V2LON,"3. ",*HOFF,"Quarterly, ":
                    *V2LON,"4. ",*HOFF,"Half Yearly, ":
                    *V2LON,"5. ",*HOFF,"Yearly"
EFRQ0100  MOVE      ONE,AMDEFRQ
          KEYIN     *PCCOL:CVERT,*V2LON,*MASK,*RV,AMDEFRQ
          BRANCH    AMDEFRQ,EFRQ1000,EFRQ2000,EFRQ3000,EFRQ4000,EFRQ5000
          BEEP
          GOTO      EFRQ0100
EFRQ1000  MOVE      "Monthly     ",FRQDESC
          GOTO      EFRQ9000
.
EFRQ2000  MOVE      "13 Periods  ",FRQDESC
          GOTO      EFRQ9000
.
EFRQ3000  MOVE      "Quarterly   ",FRQDESC
          GOTO      EFRQ9000
.
EFRQ4000  MOVE      "Half Yearly ",FRQDESC
          GOTO      EFRQ9000
.
EFRQ5000  MOVE      "Yearly      ",FRQDESC
          GOTO      EFRQ9000
.
EFRQ9000  CALL      MESSAGE3
          RETURN
POST0000  MOVE    AMDEDEP,KEY4
          CALL    RAAMDE1
          IF      OVRCD=0
            CALL    UPAMDE1
          ELSE
            CALL    WRAMDE1
          ENDIF
          RETURN
.----------------------------------------------
. Delete
.----------------------------------------------
DELT0000  MOVE     SP70,DISPMSG
          CALL     MESSAGE2
          CALL     DELQST
          CALL     MESSAGE3
          BRANCH   CEXIT,DELT1000,DELT9000,DELT9100
.
DELT1000  MOVE     AMDEDEP,KEY4
          CALL     RAAMDE1
          IF       OVRCD=0
            CALL     DEAMDE1
          ENDIF
          MOVE     TWO,EXIT
          GOTO     DELT9999
.
DELT9000  MOVE     ONE,EXIT
          GOTO     DELT9999
DELT9100  MOVE     TWO,EXIT
          GOTO     DELT9999
DELT9999  RETURN
.
