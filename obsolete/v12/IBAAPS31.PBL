.----------------------------------------------------------------------
. Print Report
.----------------------------------------------------------------------
REPT0000  CALL      OPEN0000
          BRANCH    EXIT,REPT9999
.
          MOVE      ZERO,TOTMONTH
          MOVE      ZERO,TOTAL
.
          CLOCK     TIME,CTIMEIS                 * get current time
          MOVE      "999",CLNO                   * init last line number
.
          DISPLAY   *P1:24,*EL,"Printing   : "
          CALL      MAKEXP00                     * Create Export File
.
          PACK      SAVEBASC,Z70                 * Save BAS Code
          MOVE      ONE,ACCTPRIN
.
          MATCH     SP70,STRBAS
          IF        @EQUAL
            PACK      KEY3,SP70
            CALL      RSIBBA1
REPT0050    CALL      RKIBBA1
            BRANCH    OVRCD,REPT9999
.
            COMPARE   ONE,IBBAACTV
            GOTO      REPT0050 IF EQUAL
.
            PACK      STRBAS,IBBABASC
          ENDIF
.
          PACK      KEY23,KYINLEDG,STRBAS,SP70
          CALL      RSFMBS2
REPT0100  CALL      RKFMBS2
          BRANCH    OVRCD,REPT9500
.
          MATCH     KYINLEDG,FMBSLEDG
          GOTO      REPT9500 IF NOT EQUAL
.
          MATCH     FMBSBASC,ENDBAS
          GOTO      REPT9500 IF LESS
.
          MATCH     FROMDATE,FMBSPDAT
          GOTO      REPT0100 IF LESS
.
          MATCH     FMBSPDAT,TODATE
          GOTO      REPT0100 IF LESS
.
          MOVE      "Invalid BAS Code              ",IBBADESC
          PACK      KEY3,FMBSBASC,SP70
          CALL      RDIBBA1
.
          IF        OVRCD=0 & IBBAACTV=1
            GOTO      REPT0100
          ENDIF
.
          PACK      KEY10,FMBSBAT,FMBSUNIQ,SP70
          CALL      RDFMTR1
          BRANCH    OVRCD,REPT0100
.
          PACK      KEY14,FMTRLEDG,FMTRACCT
          CALL      RDFMAM1
          BRANCH    OVRCD,REPT0100
.
          MOVE      "5",DIM1
          MATCH     FMAMTYPE,DIM1
          GOTO      REPT0100 IF LESS
.
.---- display the account being processed ---
.
          DISPLAY   *P14:24,*V2LON,FMTRLEDG,SLASH,FMTRACCT
.
          MATCH     SAVEBASC,FMBSBASC
          GOTO      REPT0200 IF EQUAL
.
          CALL      PRPT0000
.
REPT0200  COMPARE   "56",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          CALL      PLIN0000                     * print the report line
          GOTO      REPT0100
.
REPT9500  IF        CPAGENO=0
            CALL      HEAD0000
          ENDIF
.
          BRANCH    ACCTPRIN,REPT9900
.
          COMPARE   "56",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          MOVE      FORMATT,OUTAMTT
          FEDIT     TOTMONTH,OUTAMTT     * format total
          CALL      UND132
          PRINT     *15,SAVEBASC,SP2,SAVEBASD:
                    *53,"     Total ":
                    *67,OUTAMTT
          ADD       ONE,CLNO
.
          MOVE      FORMATT,OUTAMTT
          FEDIT     TOTAL,OUTAMTT
          CALL      UND132
          PRINT     *52,"Grand Total ",*67,OUTAMTT
          CALL      TRAN0000
.
REPT9900  CALL      UND132
          PRINT     *1,"** End of Report **"
          MOVE      ZERO,CPAGENO
          DISPLAY   *P1:24,*EL
          GOTO      REPT9999
.
REPT9999  RETURN
.----------------------------------------------------------------------
. print a line on the report
.----------------------------------------------------------------------
PLIN0000  CALL      FAMT0000
          UNPACK    FMTRPDAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          MOVE      FORMAT,OUTAMT
          FEDIT     FMTRAMT,OUTAMT
          ADD       FMTRAMT,TOTMONTH
          ADD       FMTRAMT,TOTAL
.
          MOVE      SP8,CHQNO
          MOVE      FMTRORD,ORDNO
          MATCH     "PY",FMTRTYPE
          GOTO      PLIN0500 IF NOT EQUAL
          MOVE      SP8,ORDNO
          MOVE      FMTRORD,CHQNO
.
PLIN0500  PRINT     *1,FMTRBAT,*8,CPCDATE,*19,FMTRDOCR,*35,FMTRTYPE:
                    *40,ORDNO,*49,CHQNO,*58,FMTRDISS,*65,FMTRRESP,*70,OUTAMT;
.
          MOVE      FMBSBASC,SAVEBASC
          MOVE      IBBADESC,SAVEBASD
.
          MOVE      SP70,APMACRD
          MOVE      SP70,APMACN1
.
          MATCH     SP70,FMTRDESC
          GOTO      PLIN1000 IF EQUAL
.
          PRINT     *91,FMTRDESC
          ADD       ONE,CLNO
.
          GOTO      PLIN9000
.
PLIN1000  MATCH     "00000",FMTRCRED
          GOTO      PLIN8000 IF EQUAL
.
          MATCH     SP5,FMTRCRED
          GOTO      PLIN8000 IF EQUAL
.
          PACK      KEY5,FMTRCRED,SP70
          CALL      RDAPMA1
          BRANCH    OVRCD,PLIN8000
.
          PRINT     *91,FMTRCRED,SP1,APMACN1
          ADD       ONE,CLNO
.
          GOTO      PLIN9000
.
PLIN8000  MATCH     SP70,FMTRDESC
          GOTO      PLIN9000 IF NOT EQUAL
.
          PRINT     *91,FMTRDESC
          ADD       ONE,CLNO
.
          GOTO      PLIN9000
.
PLIN9000  CALL      WRTEXP00
.
PLIN9999  RETURN
