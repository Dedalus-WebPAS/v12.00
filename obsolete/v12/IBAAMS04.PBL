          KEYIN     *PCCOL:CVERT,*V2LON,*MASK,*RV,VAR
          MOVE      VAR,AMCDUR1 
          SFORMAT   VAR,127
          ENDSET    AMCDUR1 
          APPEND    SP70,AMCDUR1 
          RESET     AMCDUR1 
          CMATCH    EXITCHAR,AMCDUR1 
          GOTO      SF02L990 IF EQUAL
          IF        SCPSMAN=1
            MATCH     SP70,AMCDUR1 
            IF        @EQUAL
              MOVE      "Mandatory Field - ",DISPMSG
              BEEP
              CALL      MESSAGE1
              GOTO      SF02L105
            ENDIF
          ENDIF
          CALL      CLRDAT00
          MOVE      AMCDUR1,VAR      
          CALL      DISPITEM
          GOTO     SF02L001
.
SF02L106
          CALL     SELECTED
          STRIP     AMCDUR2 
          SFORMAT   VAR,SCPSLEN
          MOVE      AMCDUR2,VAR
          KEYIN     *PCCOL:CVERT,*V2LON,*MASK,*RV,VAR
          MOVE      VAR,AMCDUR2 
          SFORMAT   VAR,127
          ENDSET    AMCDUR2 
          APPEND    SP70,AMCDUR2 
          RESET     AMCDUR2 
          CMATCH    EXITCHAR,AMCDUR2 
          GOTO      SF02L990 IF EQUAL
          IF        SCPSMAN=1
            MATCH     SP70,AMCDUR2 
            IF        @EQUAL
              MOVE      "Mandatory Field - ",DISPMSG
              BEEP
              CALL      MESSAGE1
              GOTO      SF02L106
            ENDIF
          ENDIF
          MOVE      AMCDUR2,VAR      
          CALL      DISPITEM
          GOTO     SF02L001
.
SF02L107
          CALL     SELECTED
          CALL      YORN0000
          MOVE       FYESNO,AMCDUY1 
          MOVE      NO,YESNO
          MOVE      ZERO,FORM2
          MOVE      AMCDUY1,FORM2    
          LOAD      YESNO,FORM2,YES
          MOVE      YESNO,VAR
          CALL      DISPITEM
          GOTO     SF02L001
.
SF02L108
          CALL     SELECTED
          CALL      YORN0000
          MOVE       FYESNO,AMCDUY2 
          MOVE      NO,YESNO
          MOVE      ZERO,FORM2
          MOVE      AMCDUY2,FORM2    
          LOAD      YESNO,FORM2,YES
          MOVE      YESNO,VAR
          CALL      DISPITEM
          GOTO     SF02L001
SF02L990  MOVE      ONE,EXIT * Exit Char Restart
SF02L999  RETURN
