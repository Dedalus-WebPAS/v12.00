.==============================================================================
.==============================================================================
. N.B.  Copy of DEBDBTKY with Global Change of DBDB to DBLN for Linked Debtor
.       Keyin in IBADEB05.
.==============================================================================
.==============================================================================
.
.-------------------------------------------------------------------------------
.   System    :   Accounts Receiveable               
.   Include   :   DEBDBTKY
.   Routine   :   KDBLNA00 Keyin Debtor              
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KDBLNA00 TOPIC     "DBLNDEB "
          MOVE      UNDLN70,DBLNDEB 
          KEYIN     *PCCOL:CVERT,*DV,DBLNDEB:                                   
                    *PCCOL:CVERT,*V2LON,DBLNDEB:                                
                    *PCCOL:CVERT,*DV,DBLNDEB 
.
          ENDSET    DBLNDEB 
          APPEND    SP70,DBLNDEB 
          RESET     DBLNDEB 
.
          MATCH     SP70,DBLNDEB 
          GOTO      KDBLNA90 IF EQUAL
.
          CMATCH    EXITCHAR,DBLNDEB 
          GOTO      KDBLNA95 IF EQUAL
.
          CMATCH    QUEST,DBLNDEB 
          GOTO      KDBLNA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HDBLNA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    DBLNDEB 
          APPEND    SP70,DBLNDEB 
          RESET     DBLNDEB 
.
          MATCH     SP70,DBLNDEB 
          GOTO      KDBLNA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,DBLNDEB                                 
KDBLNA10
          PACK      KEY8,DBLNDEB                                                
          CALL      RADBLN1 
          BRANCH    OVRCD,KDBLNA80
          BRANCH    CKEYTYP,KDBLNA75
          CALL      RDDBLN1 
KDBLNA70
          MOVE      ZERO,EXIT
          GOTO      KDBLNA99
KDBLNA75
          MOVE      "Debtor Already Exist - ",DISPMSG                                                
          CALL      MESSAGE1
          GOTO      KDBLNA00
KDBLNA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KDBLNA99,KDBLNA99
          MOVE      "Debtor Does Not Exist - ",DISPMSG                                               
          CALL      MESSAGE1
          GOTO      KDBLNA00
KDBLNA90
          MOVE      ONE,EXIT
          GOTO      KDBLNA99
KDBLNA95
          MOVE      TWO,EXIT
KDBLNA99 TOPIC    PRGID
          RETURN
