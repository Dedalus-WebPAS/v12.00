.----------------------------------------------------------
. Transfer from MHG to Mercy Private
.----------------------------------------------------------
TRAN0000  MOVE      ZERO,RECNUM
.
          CLEAR     TRANDESC
          APPEND    "trn",TRANDESC
          APPEND    FMBCBAT,TRANDESC
          RESET     TRANDESC
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      IBAGINA1,TRANDESC
          TRAPCLR   IO
          COMPARE   ZERO,OVRCD
          GOTO      TRAN9100 IF EQUAL
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          PREP      IBAGINA1,TRANDESC
          TRAPCLR   IO
          BRANCH    OVRCD,TRAN9200
.
          MOVE      ZERO,SECTOR
          MOVE      "MP",DIM2
          PACK      KEY39,FMBCBAT,DIM2,SP70
          CALL      RSSIRB3
.
TRAN1000  CALL      RKSIRB3
          BRANCH    OVRCD,TRAN3000
.
          MATCH     FMBCBAT,SIRBBCH
          GOTO      TRAN3000 IF NOT EQUAL
.
          ADD       ONE,RECNUM
          IF        (RECNUM%1000) = 0
            DISPLAY    *P1:24,*EL,"Requisitions : ",*V2LON,RECNUM
          ENDIF
          IF        RECNUM=1
            PACK      KEY5,FMBCBAT,SP70
            CALL      RAAPTF1
            IF        OVRCD=1
              MOVE      FMBCBAT,APTFBAT
              MOVE      FMBCBAT,APTFTRAN
              PACK      APTFDATE,PEREDAT,SP70
              REP       " 0",APTFDATE
              PACK      APTFSPAR,SP70
              CALL      WRAPTF1
            ENDIF
          ENDIF
.
          MATCH     DIM2,SIRBLED
          GOTO      TRAN3000 IF NOT EQUAL
.
          MOVE      "01",IBGILEDG
.
          PACK      SIIADES,SP70
          MOVE      SP5,SIIASUB
          PACK      KEY7,SIRBCAT,SP70
          CALL      RDSIIA1
.
          RESET     SIRBACC,6
          CMATCH    "-",SIRBACC
          IF        @EQUAL
            RESET     SIRBACC
            UNPACK    SIRBACC,D3,D2,D1,D6
            PACK      SIRBACC,D3,D1,SIIASUB,SP70
          ENDIF
          RESET     SIRBACC
          MOVE      SIRBACC,IBGIACCT
.
          MOVE      PEREDAT,IBGIDATE
          MOVE      SP1,IBGISIGN
          MOVE      SIRBAMT,IBGIVALU
          MOVE      SIIADES,IBGIDESC
          PACK      IBGIREF,SIRBCAT,SP70
          MOVE      SP5,IBGIDISS
          MOVE      SP4,IBGIRESP
          MOVE      SP20,IBGICRAC
          MOVE      SP20,IBGICRAA
          MOVE      SP3,IBGIBASC
.
          ADD       ONE,SECTOR
          CALL      WRIBGI1
          GOTO      TRAN1000
.
TRAN3000  MOVE      ZERO,RECNUM
          MOVE      "IN",D2
          PACK      KEY15,D2,PERSDAT,SP70
          CALL      RSAPBM4
.
TRAN3100  CALL      RKAPBM4
          BRANCH    OVRCD,TRAN5000
.
          MATCH     "IN",APBMTYP
          GOTO      TRAN5000 IF NOT EQUAL
.
          ADD       ONE,RECNUM
          IF        (RECNUM%1000) = 0
            DISPLAY    *P1:24,*EL,"Invoices : ",*V2LON,RECNUM
          ENDIF
.
          MATCH     APBMADT,PEREDAT
          GOTO      TRAN5000 IF LESS
.
          PACK      KEY5,APBMBCH,SP70
          CALL      RDAPTF1
          IF        OVRCD=1
            MOVE      APBMBCH,APTFBAT
            MOVE      FMBCBAT,APTFTRAN
            PACK      APTFDATE,PEREDAT,SP70
            REP       " 0",APTFDATE
            PACK      APTFSPAR,SP70
            CALL      WRAPTF1
          ELSE
            GOTO      TRAN3100
          ENDIF
.
          PACK      KEY51,APBMBCH,SP70
          CALL      RSAPIN1
.
TRAN3500  CALL      RKAPIN1
          BRANCH    OVRCD,TRAN3100
.
          MATCH     APBMBCH,APINBCH
          GOTO      TRAN3100 IF NOT EQUAL
.
          MATCH     DIM2,APINLED
          GOTO      TRAN3500 IF NOT EQUAL
.
          UNPACK    APINCAT,KEY3,DIM4
          UNPACK    APINORD,KEY1,KEY7
          PACK      KEY10,KEY7,KEY3,SP70
          CALL      RDSIPC1
          BRANCH    OVRCD,TRAN3900
.
          PACK      KEY7,SIPCCAT,SP70
          CALL      RDSIIA1
          BRANCH    OVRCD,TRAN3700
.
          COMPARE   ONE,SIIANON
          GOTO      TRAN3900 IF NOT EQUAL
.
TRAN3700  MOVE      "01",IBGILEDG
.
          RESET     APINACC,6
          CMATCH    "-",APINACC
          IF        @EQUAL
            RESET     APINACC
            UNPACK    APINACC,D3,D2,D1,D6
            PACK      APINACC,D3,D1,SIPCPRD,SP70
          ENDIF
          RESET     APINACC
          MOVE      APINACC,IBGIACCT
.
          MOVE      PEREDAT,IBGIDATE
          MOVE      SP1,IBGISIGN
          MOVE      APINAMT,IBGIVALU
          PACK      IBGIDESC,SIPCPD,SP70
          PACK      IBGIREF,SIPCCAT,SP70
          MOVE      SP5,IBGIDISS
          MOVE      SP4,IBGIRESP
          MOVE      SP20,IBGICRAC
          MOVE      SP20,IBGICRAA
          MOVE      SP3,IBGIBASC
.
          ADD       ONE,SECTOR
          CALL      WRIBGI1
.
TRAN3900  GOTO      TRAN3500
.
.
TRAN5000  MOVE      ZERO,RECNUM
          MOVE      "CR",D2
          PACK      KEY15,D2,PERSDAT,SP70
          CALL      RSAPBM4
.
TRAN5100  CALL      RKAPBM4
          BRANCH    OVRCD,TRAN9000
.
          MATCH     "CR",APBMTYP
          GOTO      TRAN9000 IF NOT EQUAL
.
          ADD       ONE,RECNUM
          IF        (RECNUM%1000) = 0
            DISPLAY    *P1:24,*EL,"Credit Notes : ",*V2LON,RECNUM
          ENDIF
.
          MATCH     APBMADT,PEREDAT
          GOTO      TRAN9000 IF LESS
.
          PACK      KEY5,APBMBCH,SP70
          CALL      RDAPTF1
          IF        OVRCD=1
            MOVE      APBMBCH,APTFBAT
            MOVE      FMBCBAT,APTFTRAN
            PACK      APTFDATE,PEREDAT,SP70
            REP       " 0",APTFDATE
            PACK      APTFSPAR,SP70
            CALL      WRAPTF1
          ELSE
            GOTO      TRAN5100
          ENDIF
.
          PACK      KEY51,APBMBCH,SP70
          CALL      RSAPIN1
.
TRAN5500  CALL      RKAPIN1
          BRANCH    OVRCD,TRAN5100
.
          MATCH     APBMBCH,APINBCH
          GOTO      TRAN5100 IF NOT EQUAL
.
          MATCH     DIM2,APINLED
          GOTO      TRAN5500 IF NOT EQUAL
.
          UNPACK    APINCAT,KEY3,DIM4
          UNPACK    APINORD,KEY1,KEY7
          PACK      KEY10,KEY7,KEY3,SP70
          CALL      RDSIPC1
          BRANCH    OVRCD,TRAN5900
.
          PACK      KEY7,SIPCCAT,SP70
          CALL      RDSIIA1
          BRANCH    OVRCD,TRAN5700
.
          COMPARE   ONE,SIIANON
          GOTO      TRAN5900 IF NOT EQUAL
.
TRAN5700  MOVE      "01",IBGILEDG
.
          RESET     APINACC,6
          CMATCH    "-",APINACC
          IF        @EQUAL
            RESET     APINACC
            UNPACK    APINACC,D3,D2,D1,D6
            PACK      APINACC,D3,D1,SIPCPRD,SP70
          ENDIF
          RESET     APINACC
          MOVE      APINACC,IBGIACCT
.
          MOVE      PEREDAT,IBGIDATE
          MOVE      SP1,IBGISIGN
          MOVE      APINAMT,IBGIVALU
          MULT      SEQ,IBGIVALU
          PACK      IBGIDESC,SIPCPD,SP70
          PACK      IBGIREF,SIPCCAT,SP70
          MOVE      SP5,IBGIDISS
          MOVE      SP4,IBGIRESP
          MOVE      SP20,IBGICRAC
          MOVE      SP20,IBGICRAA
          MOVE      SP3,IBGIBASC
.
          ADD       ONE,SECTOR
          CALL      WRIBGI1
.
TRAN5900  GOTO      TRAN5500
.
TRAN9000  COMPARE   ZERO,SECTOR
          GOTO      TRAN9300 IF EQUAL
.
          CALL      GETSCR00
          DISPLAY   *P1:24,*EL;
          CLEAR     KEY80
          APPEND    "ibaaps97.us1 ",KEY80
          APPEND    TRANDESC,KEY80
          APPEND    ".txt",KEY80
          RESET     KEY80
          EXECUTE   KEY80,TASKID
          CALL      PUTSCR00
          DISPLAY   *P1:24,*EL,"Interface Complete - ";
          CALL      HITENTER
          GOTO      TRAN9999
.
TRAN9100  MOVE      "Transfer File Already Exists - ",DISPMSG
          CALL      MESSAGE1
          GOTO      TRAN9999
.
TRAN9200  MOVE      "Unable to Create Interface File - ",DISPMSG
          CALL      MESSAGE1
          GOTO      TRAN9999
.
TRAN9300  MOVE      "No Entries to be Transferred - ",DISPMSG
          CALL      MESSAGE1
          GOTO      TRAN9999
.
TRAN9999  RETURN
.
EBAT0000  GETSCR    1,ONE,ONE,EIGHTY,TWENTY4
.
EBAT1000  CALL      KBCFFM00
          BRANCH    EXIT,EBAT9500,EBAT9500
.
          PACK      KEY5,FMBCBAT,SP70
          CALL      RDAPTF1
          BRANCH    OVRCD,EBAT1100
.
          MOVE      "Batch Already Transferred - ",DISPMSG
          CALL      MESSAGE1
          GOTO      EBAT1000
.
EBAT1100  MATCH     "JA",FMBCTRT
          IF        !@EQUAL
            MOVE      "Invalid Batch Type - ",DISPMSG
            CALL      MESSAGE1
            GOTO      EBAT1000
          ENDIF
.
          MOVE      FMBCLED,KEY2
          MATCH     SP2,FMBCLED
          IF        @EQUAL
            CALL      RSFMLA1
            CALL      RKFMLA1
          ELSE
            CALL      RDFMLA1
          ENDIF
.
          UNPACK      FMBCPDAT,CCENT,CYEAR,CMON,CDAY
          PACK        WORKDATE,CCENT,CYEAR,CMON,CDAY
          CALL        CFYR0000
          IF        EXIT=0
            UNPACK    PERSDAT,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
            MOVE      CPCDATE,STRDATE
            UNPACK    PEREDAT,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
            MOVE      CPCDATE,ENDDATE
          ENDIF
.
EBAT9000  MOVE      ZERO,EXIT
          GOTO      EBAT9999
.
EBAT9500  MOVE      TWO,EXIT
          GOTO      EBAT9999
.
EBAT9999  RETURN
.
