.------------------------------------------------------------
. Check Disposal year and period
.------------------------------------------------------------
DCHK0000  MOVE     ZERO,EXIT
          MOVE     AMMADYR,DISF4
          MOVE     FINYEAR,CURF4
          IF       DISF4<CURF4
            GOTO     DCHK9000       * If Disposed of in a prior year
          ENDIF
          MOVE     AMMADPR,PERF2
          IF       PERF2>ENDPER
            GOTO     DCHK9999
          ENDIF
.
          MOVE     ONE,DISPOSAL
.
          GOTO     DCHK9999
.
DCHK9000  MOVE     ONE,EXIT
.
DCHK9999  RETURN
.
.------------------------------------------------------------
. Check Acquisition year and period
.------------------------------------------------------------
ACHK0000  MOVE     ZERO,EXIT
          MOVE     AMMAAYR,DISF4
          MOVE     FINYEAR,CURF4
          MOVE     AMMAAPR,PERF2
.
. If Acquired in a Future then Ignore
.-------------------------------------
          IF       (DISF4>CURF4)
            GOTO     ACHK9000
          ENDIF
.
          IF       (DISF4=CURF4)
            IF       (PERF2>ENDPER)
              GOTO     ACHK9000
            ENDIF
          ENDIF
          GOTO     ACHK9999
.
ACHK9000  MOVE     ONE,EXIT
.
ACHK9999  RETURN
