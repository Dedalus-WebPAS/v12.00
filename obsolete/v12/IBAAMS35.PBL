. Print Report
.--------------
. Report Sequences  0. Register/Asset
.                   1. Asset Type
.                   2. Department
.                   3. Building/Location
.                   4. Depreciation Type
.-------------------------------------------------------------------------------
REPT0000  DISPLAY   *P1:24,*EL,"Printing Report : ";
          MOVE      ZERO,CPAGENO          * reset page no.
          CLOCK     TIME,CTIMEIS          * get current time
          MOVE      "99",CLNO             * init last line number
          MOVE      ZERO,TOTALPRT         * No Total Print
          MOVE      ZERO,ATOTAL1
          MOVE      ZERO,ATOTAL2
          MOVE      ZERO,ATOTAL3
          MOVE      ZERO,ATOTAL4
          MOVE      ZERO,ATOTAL5
          MOVE      ZERO,ATOTAL6
          MOVE      ZERO,BTOTAL1
          MOVE      ZERO,BTOTAL2
          MOVE      ZERO,BTOTAL3
          MOVE      ZERO,BTOTAL4
          MOVE      ZERO,BTOTAL5
          MOVE      ZERO,BTOTAL6
          MOVE      ZERO,CTOTAL1
          MOVE      ZERO,CTOTAL2
          MOVE      ZERO,CTOTAL3
          MOVE      ZERO,CTOTAL4
          MOVE      ZERO,CTOTAL5
          MOVE      ZERO,GRAND1
          MOVE      ZERO,GRAND2
          MOVE      ZERO,GRAND3
          MOVE      ZERO,GRAND4
          MOVE      ZERO,GRAND5
          MOVE      ZERO,GRAND6
.
          MOVE      ZERO,USINGUC1
          PACK      UC1HEAD,SP70
          MOVE      "A1   ",KEY5
          CALL      RDAMCD1
          BRANCH    OVRCD,REPT0050
.
          MATCH     SP70,AMCDDES
          GOTO      REPT0050 IF EQUAL
.
          MOVE      AMCDDES,UC1HEAD
          MOVE      ONE,USINGUC1
.
REPT0050  BRANCH    REPTSEQ,REPT0100,REPT0200,REPT0300,REPT0400
          CALL      SEQA0000              * Register/Asset
          GOTO      REPT9000
.
REPT0100  CALL      SEQB0000              * Asset Type
          GOTO      REPT9000
.
REPT0200  CALL      SEQC0000              * Department
          GOTO      REPT9000
.
REPT0300  CALL      SEQD0000              * Building/Location
          GOTO      REPT9000
.
REPT0400  CALL      SEQE0000              * Depreciation Type
          GOTO      REPT9000
.
REPT9000  COMPARE   ZERO,CPAGENO
          CALL      HEAD0000 IF EQUAL
          MOVE      "999,999,999,999.99",FTOTAL1
          MOVE      "999,999,999,999.99",FTOTAL2
          MOVE      "999,999,999,999",KEY15
          MOVE      "999,999,999,999.99",FTOTAL4
          MOVE      "999,999,999,999.99",FTOTAL5
          MOVE      GRAND1,F12
          FEDIT     F12,KEY15
          FEDIT     GRAND3,FTOTAL1
          FEDIT     GRAND4,FTOTAL2
          MOVE      GRAND5,F12P2
          FEDIT     F12P2,FTOTAL4
          MOVE      GRAND6,F12P2
          FEDIT     F12P2,FTOTAL5
          CALL      UND132
          IF         !(STRPER=ENDPER)
            MOVE       SP70,FTOTAL1
            MOVE       SP70,FTOTAL2
          ENDIF
          PRINT     " Report Total : ",*23,KEY15:
                    *53,FTOTAL5,*79,FTOTAL1,FTOTAL2,FTOTAL4
          CALL      UND132
          PRINT     *45,"*** End of Report ***"
.
REPT9999  RETURN
.------------------------------------------------------------
. Print Asset Line
.------------------------------------------------------------
PLINE000  COMPARE  "55",CLNO
          CALL     HEAD0000 IF NOT LESS
          ADD      ONE,CLNO
.
          PACK     KEY18,AMMAREG,AMMAASS,FINYEAR
          CALL     RDAMDS1
          COMPARE  ONE,OVRCD
          CALL     CLRDEP00 IF EQUAL
.
          MOVE     ZERO,SECTOR
          MOVE     ZERO,CURRDEPN
          MOVE     ZERO,YTDDEPN
PLINE100  ADD      ONE,SECTOR
          IF       DEPNTYPE=0
            LOAD     F12P2,SECTOR,AMDSA01,AMDSA02,AMDSA03,AMDSA04,AMDSA05:
                                  AMDSA06,AMDSA07,AMDSA08,AMDSA09,AMDSA10:
                                  AMDSA11,AMDSA12,AMDSA13
          ELSE
            LOAD     F12P2,SECTOR,AMDST01,AMDST02,AMDST03,AMDST04,AMDST05:
                                  AMDST06,AMDST07,AMDST08,AMDST09,AMDST10:
                                  AMDST11,AMDST12,AMDST13
          ENDIF
          IF       SECTOR>=STRPER
            ADD      F12P2,CURRDEPN
          ELSE
            ADD      F12P2,YTDDEPN
          ENDIF
.
          COMPARE  ENDPER,SECTOR
          GOTO     PLINE100 IF NOT EQUAL
.
          IF       DEPNTYPE=0
            MOVE     AMMAADT,KEY4
            MOVE     AMDSAAD,ACCDEPN
          ELSE
            MOVE     AMMATDT,KEY4
            MOVE     AMDSTAD,ACCDEPN
          ENDIF
          CALL     RDAMDE1
          MOVE     AMMAAYR,PYEARF4
          MOVE     FINYEAR,CYEARF4
          ASSIGN   CYEARF4-PYEARF4+1,ASSETAGE
          IF       ((ASSETAGE>0)&(ASSETAGE<10))
            LOAD      F4P2,ASSETAGE,AMDERT01,AMDERT02,AMDERT03,AMDERT04:
                                        AMDERT05,AMDERT06,AMDERT07,AMDERT08:
                                        AMDERT09,AMDERT10
            IF        F4P2=0
              MOVE      AMDERAT,F4P2
            ENDIF
          ELSE
            MOVE      AMDERAT,F4P2
          ENDIF
.
          MOVE     "999,999,999,999.99",FTOTAL1
          MOVE     "999,999,999,999.99",FTOTAL2
          MOVE     "999,999,999,999",KEY15
          MOVE     "999,999,999,999.99",FTOTAL4
          MOVE     "999,999,999,999.99",FTOTAL5
.
          ASSIGN   AMMACST+AMDSRVT-ACCDEPN-YTDDEPN,F12P2
          MOVE     F12P2,F12
.
. Confirmed with ACC 01/10/95 that if asset disposed and report for
. Multiple periods the show the depreciation as zero.
.
          IF       DISPOSAL=1 & !(STRPER=ENDPER)
            MOVE     ZERO,CURRDEPN
          ENDIF
          IF       AMDEBAS=2
            FEDIT    CURRDEPN,FTOTAL2
            ASSIGN   ATOTAL4+CURRDEPN,ATOTAL4
            ASSIGN   BTOTAL4+CURRDEPN,BTOTAL4
            ASSIGN   CTOTAL4+CURRDEPN,CTOTAL4
            ASSIGN   GRAND4+CURRDEPN,GRAND4
            MOVE     SP70,FTOTAL1
          ELSE
            FEDIT    CURRDEPN,FTOTAL1
            ASSIGN   ATOTAL3+CURRDEPN,ATOTAL3
            ASSIGN   BTOTAL3+CURRDEPN,BTOTAL3
            ASSIGN   CTOTAL3+CURRDEPN,CTOTAL3
            ASSIGN   GRAND3+CURRDEPN,GRAND3
            MOVE     SP70,FTOTAL2
          ENDIF
.
          MATCH    AMMADDT,SP70
          IF       !@EQUAL
            UNPACK   AMMADDT,KEY2,DIM2,KEY2
            MOVE     KEY2,FORM2
            LOAD     KEY3,FORM2,M1,M2,M3,M4,M5,M6,M7,M8,M9,M10,M11,M12
            MOVE     "999,999,999.99",KEY14
            FEDIT    AMMADVA,KEY14
            PACK     KEY9,DISPSL,SP1,KEY3,SP1,DIM2
          ELSE
            UNPACK   AMMAAQD,KEY2,DIM2,KEY2
            MOVE     KEY2,FORM2
            LOAD     KEY3,FORM2,M1,M2,M3,M4,M5,M6,M7,M8,M9,M10,M11,M12
            MOVE     SP70,KEY14
            PACK     KEY9,ADDITN,SP1,KEY3,SP1,DIM2
          ENDIF
.
          MOVE     "999,999,999,999.99",KEY18
          IF       DISPOSAL=0
            MOVE     AMMACST,F12
            FEDIT    F12,KEY15
            ASSIGN   AMMACST+AMDSRVT-ACCDEPN-CURRDEPN-YTDDEPN,F12P2
            FEDIT    F12P2,FTOTAL4
            ASSIGN   ACCDEPN+YTDDEPN+CURRDEPN,F12P2
            FEDIT    F12P2,KEY18
          ELSE
            MOVE     ZERO,F12
            MOVE     ZERO,F12P2
            FEDIT    F12,KEY15
            FEDIT    F12P2,FTOTAL4
            MOVE     ZERO,F12P2
            FEDIT    F12P2,KEY18
          ENDIF
          IF       !(STRPER=ENDPER)
            MOVE     SP70,FTOTAL1
            MOVE     SP70,FTOTAL2
          ENDIF
.
          MOVE     AMMADES,KEY8
          MOVE     AMMAPRI,F2P2
          MOVE     F4P2,F3P2
          PRINT    *001,AMMAASS:
                   *014,KEY8:
                   *023,KEY15:
                   *039,KEY9:
                   *049,AMMATYP:
                   *053,KEY18:
                   *072,F3P2:
                   *079,FTOTAL1:
                   *097,FTOTAL2:
                   *115,FTOTAL4        * 132
.
          IF       DISPOSAL=0
            ASSIGN   ATOTAL1+AMMACST,ATOTAL1
            ASSIGN   BTOTAL1+AMMACST,BTOTAL1
            ASSIGN   CTOTAL1+AMMACST,CTOTAL1
            ASSIGN   GRAND1+AMMACST,GRAND1
            ASSIGN   ATOTAL5+AMMACST+AMDSRVT-ACCDEPN-CURRDEPN-YTDDEPN,ATOTAL5
            ASSIGN   BTOTAL5+AMMACST+AMDSRVT-ACCDEPN-CURRDEPN-YTDDEPN,BTOTAL5
            ASSIGN   BTOTAL6+F12P2,BTOTAL6
            ASSIGN   ATOTAL6+F12P2,ATOTAL6
            ASSIGN   CTOTAL5+AMMACST+AMDSRVT-ACCDEPN-CURRDEPN-YTDDEPN,CTOTAL5
            ASSIGN   GRAND5+AMMACST+AMDSRVT-ACCDEPN-CURRDEPN-YTDDEPN,GRAND5
          ENDIF
          ASSIGN   GRAND6+F12P2,GRAND6
.
          RETURN
