.---------------------------------------------------------------------------
.   System    :   Casemix Expense Analysis           
.   Include   :   CEAPSCKY
.   Routine   :   HCEPSA00 Patient Service     
.   Return    :           
.-------------------------------------------------------------------------------
HCEPSA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HCEPSA04
          CALL      GSCRN000
          BRANCH    EXIT,HCEPSA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Pat. Serv."         
.
          MOVE      STDCOL01,STDCOL02
          ADD       "11",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HCEPSA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CEPSPSC 
          PACK      KEY7,CEPSPSC,SP70                                           
.                                                                               
.                                                                               
          CALL      RSCEPS1 
HCEPSA10
          CALL      RKCEPS1 
          BRANCH    OVRCD,HCEPSA15
          PACK      KEY7,CEPSPSC,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HCEPSA12
          CALL      HCEPSA60
          BRANCH    OVRCD,HCEPSA10
HCEPSA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY7,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CEPSPSC 
          DISPLAY   *PSTDCOL02:STDROW,CEPSDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCEPSA10 IF NOT EQUAL
HCEPSA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCEPSA20
          CALL      HSCRI000
          BRANCH    EXIT,HCEPSA30,HCEPSA35,HCEPSA40,HCEPSA45,HCEPSA04:
                              HCEPSA91,HCEPSA50
          GOTO      HCEPSA92
HCEPSA30
          CALL      RKCEPS1 
          BRANCH    OVRCD,HCEPSA85
          PACK      KEY7,CEPSPSC,SP70                                           
.                                                                               
.                                                                               
          CALL      RPCEPS1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCEPSA10
HCEPSA35
          MOVE      ST1,KEY7 
          CALL      RDCEPS1 
          CALL      RPCEPS1 
          BRANCH    OVRCD,HCEPSA85
          PACK      KEY7,CEPSPSC,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCEPSA36
          CALL      RPCEPS1 
          BRANCH    OVRCD,HCEPSA37
          PACK      KEY7,CEPSPSC,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCEPSA36 IF NOT EQUAL
HCEPSA37
          MOVE      ZERO,XITEMNO
          GOTO      HCEPSA10
HCEPSA40
          MOVE      ST1,KEY7 
          CALL      RDCEPS1 
          CALL      RPCEPS1 
          BRANCH    OVRCD,HCEPSA85
          PACK      KEY7,CEPSPSC,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCEPSA08
HCEPSA45
          CALL      RKCEPS1 
          BRANCH    OVRCD,HCEPSA85
          PACK      KEY7,CEPSPSC,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY7,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCEPS1 
HCEPSA46
          CALL      RPCEPS1 
          BRANCH    OVRCD,HCEPSA47
          PACK      KEY7,CEPSPSC,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCEPSA46 IF NOT EQUAL
HCEPSA47
          MOVE      ZERO,XITEMNO
          GOTO      HCEPSA10
HCEPSA50  COMPARE   ZERO,CSEARCH
          GOTO      HCEPSA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCEPSA08
HCEPSA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCEPSA08
HCEPSA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCEPSA61 IF EQUAL
          MATCH     SCHFLD01,CEPSPSC 
          GOTO      HCEPSA72 IF NOT EQUAL
HCEPSA61
          MATCH     SP70,SCHFLD02
          GOTO      HCEPSA62 IF EQUAL
          MATCH     SCHFLD02,CEPSDES 
          GOTO      HCEPSA72 IF NOT EQUAL
HCEPSA62
HCEPSA63
HCEPSA64
HCEPSA65
HCEPSA66
HCEPSA67
HCEPSA68
HCEPSA69
HCEPSA70
          GOTO      HCEPSA75
HCEPSA72  MOVE      ONE,OVRCD
HCEPSA75  RETURN
HCEPSA85
          LOAD      KEY7,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY7,CEPSPSC                                                
          CALL      RDCEPS1 
HCEPSA90
.                                                                               
          BEEP
          GOTO      HCEPSA20
HCEPSA91
          MOVE      SP70,CEPSDES 
          MOVE      SP70,CEPSTYP 
          MOVE      ZERO,CEPSQTY 
          MOVE      SP70,CEPSQDES
          MOVE      ZERO,CEPSST1 
          MOVE      ZERO,CEPSST2 
          MOVE      ZERO,CEPSST3 
          MOVE      ZERO,CEPSST4 
          MOVE      ZERO,CEPSST5 
          MOVE      SP70,CEPSUR1 
          MOVE      SP70,CEPSUR2 
          MOVE      SP70,CEPSUD1 
          MOVE      SP70,CEPSUD2 
          MOVE      SP70,CEPSACT 
          MOVE      SP70,CEPSDRG 
          MOVE      SP70,CEPSVSC 
          MOVE      SP70,CEPSUY1 
          MOVE      SP70,CEPSUY2 
          MOVE      SP70,CEPSSPA 
          MOVE      SP70,KEY7 
          MOVE      SP70,CEPSPSC 
          GOTO      HCEPSA96
HCEPSA92
          MOVE      SP70,KEY7 
          MOVE      SP70,CEPSPSC 
          LOAD      KEY7,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY7,CEPSPSC                                                
HCEPSA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCEPSA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Casemix Expense Analysis           
.   Include   :   CEAPSCKY
.   Routine   :   KCEPSA00 Keyin Patient Service Code
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCEPSA00 TOPIC     "CEPSPSC "
          MOVE      UNDLN70,CEPSPSC 
          KEYIN     *PCCOL:CVERT,*DV,CEPSPSC:                                   
                    *PCCOL:CVERT,*V2LON,CEPSPSC:                                
                    *PCCOL:CVERT,*DV,CEPSPSC 
.
          ENDSET    CEPSPSC 
          APPEND    SP70,CEPSPSC 
          RESET     CEPSPSC 
.
          MATCH     SP70,CEPSPSC 
          GOTO      KCEPSA90 IF EQUAL
.
          CMATCH    EXITCHAR,CEPSPSC 
          GOTO      KCEPSA95 IF EQUAL
.
          CMATCH    QUEST,CEPSPSC 
          GOTO      KCEPSA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCEPSA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CEPSPSC 
          APPEND    SP70,CEPSPSC 
          RESET     CEPSPSC 
.
          MATCH     SP70,CEPSPSC 
          GOTO      KCEPSA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CEPSPSC                                 
KCEPSA10
          PACK      KEY7,CEPSPSC                                                
          CALL      RACEPS1 
          BRANCH    OVRCD,KCEPSA80
          BRANCH    CKEYTYP,KCEPSA75
          CALL      RDCEPS1 
KCEPSA70
          MOVE      ZERO,EXIT
          GOTO      KCEPSA99
KCEPSA75
          MOVE      "Patient Service Code Already Exist - ",DISPMSG                                  
          CALL      MESSAGE1
          GOTO      KCEPSA00
KCEPSA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCEPSA99,KCEPSA99
          MOVE      "Patient Service Code Does Not Exist - ",DISPMSG                                 
          CALL      MESSAGE1
          GOTO      KCEPSA00
KCEPSA90
          MOVE      ONE,EXIT
          GOTO      KCEPSA99
KCEPSA95
          MOVE      TWO,EXIT
KCEPSA99 TOPIC    PRGID
          RETURN
