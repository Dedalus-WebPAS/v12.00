.---------------------------------------------------------------------------
.   System    :   Asset Management System            
.   Include   :   AMSLOCKY
.   Routine   :   HAMLOA00 Asset Location      
.   Return    :           
.-------------------------------------------------------------------------------
HAMLOA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HAMLOA04
          CALL      GSCRN000
          BRANCH    EXIT,HAMLOA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Location"           
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 9",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HAMLOA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,AMLOLOC 
          PACK      KEY9,AMLOBLD,AMLOLOC,SP70                                   
          PACK      SAVKEY80,AMLOBLD,AMLOLOC,SP70                               
          SETLPTR   SAVKEY80,3                                                  
          CALL      RSAMLO1 
HAMLOA10
          CALL      RKAMLO1 
          BRANCH    OVRCD,HAMLOA15
          PACK      KEY9,AMLOBLD,AMLOLOC,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HAMLOA15 IF NOT EQUAL                                       
          BRANCH    CSEARCH,HAMLOA12
          CALL      HAMLOA60
          BRANCH    OVRCD,HAMLOA10
HAMLOA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY9,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,AMLOLOC 
          DISPLAY   *PSTDCOL02:STDROW,AMLODES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HAMLOA10 IF NOT EQUAL
HAMLOA15
          UNPACK    SAVKEY80,AMLOBLD,AMLOLOC                                    
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HAMLOA20
          CALL      HSCRI000
          BRANCH    EXIT,HAMLOA30,HAMLOA35,HAMLOA40,HAMLOA45,HAMLOA04:
                              HAMLOA91,HAMLOA50
          GOTO      HAMLOA92
HAMLOA30
          CALL      RKAMLO1 
          BRANCH    OVRCD,HAMLOA85
          PACK      KEY9,AMLOBLD,AMLOLOC,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HAMLOA85 IF NOT EQUAL                                       
          CALL      RPAMLO1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HAMLOA10
HAMLOA35
          MOVE      ST1,KEY9 
          CALL      RDAMLO1 
          CALL      RPAMLO1 
          BRANCH    OVRCD,HAMLOA85
          PACK      KEY9,AMLOBLD,AMLOLOC,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HAMLOA85 IF NOT EQUAL                                       
          MATCH     SP70,AMLOLOC                                                
          GOTO      HAMLOA85 IF EQUAL                                           
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HAMLOA36
          CALL      RPAMLO1 
          BRANCH    OVRCD,HAMLOA37
          PACK      KEY9,AMLOBLD,AMLOLOC,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HAMLOA37 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HAMLOA36 IF NOT EQUAL
HAMLOA37
          MOVE      ZERO,XITEMNO
          GOTO      HAMLOA10
HAMLOA40
          MOVE      ST1,KEY9 
          CALL      RDAMLO1 
          CALL      RPAMLO1 
          BRANCH    OVRCD,HAMLOA85
          PACK      KEY9,AMLOBLD,AMLOLOC,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HAMLOA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HAMLOA08
HAMLOA45
          CALL      RKAMLO1 
          BRANCH    OVRCD,HAMLOA85
          PACK      KEY9,AMLOBLD,AMLOLOC,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HAMLOA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY9,SAVKEY80,Z70                                           
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSAMLO1 
HAMLOA46
          CALL      RPAMLO1 
          BRANCH    OVRCD,HAMLOA47
          PACK      KEY9,AMLOBLD,AMLOLOC,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HAMLOA47 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HAMLOA46 IF NOT EQUAL
HAMLOA47
          MOVE      ZERO,XITEMNO
          GOTO      HAMLOA10
HAMLOA50  COMPARE   ZERO,CSEARCH
          GOTO      HAMLOA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HAMLOA08
HAMLOA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HAMLOA08
HAMLOA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HAMLOA61 IF EQUAL
          MATCH     SCHFLD01,AMLOLOC 
          GOTO      HAMLOA72 IF NOT EQUAL
HAMLOA61
          MATCH     SP70,SCHFLD02
          GOTO      HAMLOA62 IF EQUAL
          MATCH     SCHFLD02,AMLODES 
          GOTO      HAMLOA72 IF NOT EQUAL
HAMLOA62
HAMLOA63
HAMLOA64
HAMLOA65
HAMLOA66
HAMLOA67
HAMLOA68
HAMLOA69
HAMLOA70
          GOTO      HAMLOA75
HAMLOA72  MOVE      ONE,OVRCD
HAMLOA75  RETURN
HAMLOA85
          LOAD      KEY9,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY9,AMLOBLD,AMLOLOC                                        
          CALL      RDAMLO1 
HAMLOA90
          UNPACK    SAVKEY80,AMLOBLD,AMLOLOC                                    
          BEEP
          GOTO      HAMLOA20
HAMLOA91
          MOVE      SP70,AMLODES 
          MOVE      SP70,AMLOUR1 
          MOVE      SP70,AMLOUR2 
          MOVE      SP70,AMLOUD1 
          MOVE      SP70,AMLOUD2 
          MOVE      SP70,AMLOUY1 
          MOVE      SP70,AMLOUY2 
          MOVE      SP70,AMLOSPA 
          MOVE      SP70,KEY9 
          MOVE      SP70,AMLOLOC 
          GOTO      HAMLOA96
HAMLOA92
          MOVE      SP70,KEY9 
          MOVE      SP70,AMLOLOC 
          LOAD      KEY9,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY9,AMLOBLD,AMLOLOC                                        
HAMLOA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HAMLOA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Asset Management System            
.   Include   :   AMSLOCKY
.   Routine   :   KAMLOA00 Keyin Asset Location      
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KAMLOA00 TOPIC     "AMLOLOC "
          MOVE      UNDLN70,AMLOLOC 
          KEYIN     *PCCOL:CVERT,*DV,AMLOLOC:                                   
                    *PCCOL:CVERT,*V2LON,AMLOLOC:                                
                    *PCCOL:CVERT,*DV,AMLOLOC 
.
          ENDSET    AMLOLOC 
          APPEND    SP70,AMLOLOC 
          RESET     AMLOLOC 
.
          MATCH     SP70,AMLOLOC 
          GOTO      KAMLOA90 IF EQUAL
.
          CMATCH    EXITCHAR,AMLOLOC 
          GOTO      KAMLOA95 IF EQUAL
.
          CMATCH    QUEST,AMLOLOC 
          GOTO      KAMLOA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HAMLOA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    AMLOLOC 
          APPEND    SP70,AMLOLOC 
          RESET     AMLOLOC 
.
          MATCH     SP70,AMLOLOC 
          GOTO      KAMLOA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,AMLOLOC                                 
KAMLOA10
          PACK      KEY9,AMLOBLD,AMLOLOC                                        
          CALL      RAAMLO1 
          BRANCH    OVRCD,KAMLOA80
          BRANCH    CKEYTYP,KAMLOA75
          CALL      RDAMLO1 
KAMLOA70
          MOVE      ZERO,EXIT
          GOTO      KAMLOA99
KAMLOA75
          MOVE      "Asset Location Already Exist - ",DISPMSG                                        
          CALL      MESSAGE1
          GOTO      KAMLOA00
KAMLOA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KAMLOA99,KAMLOA99
          MOVE      "Asset Location Does Not Exist - ",DISPMSG                                       
          CALL      MESSAGE1
          GOTO      KAMLOA00
KAMLOA90
          MOVE      ONE,EXIT
          GOTO      KAMLOA99
KAMLOA95
          MOVE      TWO,EXIT
KAMLOA99 TOPIC    PRGID
          RETURN
