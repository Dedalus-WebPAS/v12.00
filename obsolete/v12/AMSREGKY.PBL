.---------------------------------------------------------------------------
.   System    :   Asset Management System            
.   Include   :   AMSREGKY
.   Routine   :   HAMREA00 Register Help       
.   Return    :           
.-------------------------------------------------------------------------------
HAMREA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HAMREA04
          CALL      GSCRN000
          BRANCH    EXIT,HAMREA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Register"           
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 9",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HAMREA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,AMREREG 
          PACK      KEY2,AMREREG,SP70                                           
.                                                                               
.                                                                               
          CALL      RSAMRE1 
HAMREA10
          CALL      RKAMRE1 
          BRANCH    OVRCD,HAMREA15
          PACK      KEY2,AMREREG,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HAMREA12
          CALL      HAMREA60
          BRANCH    OVRCD,HAMREA10
HAMREA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY2,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,AMREREG 
          DISPLAY   *PSTDCOL02:STDROW,AMREDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HAMREA10 IF NOT EQUAL
HAMREA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HAMREA20
          CALL      HSCRI000
          BRANCH    EXIT,HAMREA30,HAMREA35,HAMREA40,HAMREA45,HAMREA04:
                              HAMREA91,HAMREA50
          GOTO      HAMREA92
HAMREA30
          CALL      RKAMRE1 
          BRANCH    OVRCD,HAMREA85
          PACK      KEY2,AMREREG,SP70                                           
.                                                                               
.                                                                               
          CALL      RPAMRE1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HAMREA10
HAMREA35
          MOVE      ST1,KEY2 
          CALL      RDAMRE1 
          CALL      RPAMRE1 
          BRANCH    OVRCD,HAMREA85
          PACK      KEY2,AMREREG,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HAMREA36
          CALL      RPAMRE1 
          BRANCH    OVRCD,HAMREA37
          PACK      KEY2,AMREREG,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HAMREA36 IF NOT EQUAL
HAMREA37
          MOVE      ZERO,XITEMNO
          GOTO      HAMREA10
HAMREA40
          MOVE      ST1,KEY2 
          CALL      RDAMRE1 
          CALL      RPAMRE1 
          BRANCH    OVRCD,HAMREA85
          PACK      KEY2,AMREREG,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HAMREA08
HAMREA45
          CALL      RKAMRE1 
          BRANCH    OVRCD,HAMREA85
          PACK      KEY2,AMREREG,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY2,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSAMRE1 
HAMREA46
          CALL      RPAMRE1 
          BRANCH    OVRCD,HAMREA47
          PACK      KEY2,AMREREG,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HAMREA46 IF NOT EQUAL
HAMREA47
          MOVE      ZERO,XITEMNO
          GOTO      HAMREA10
HAMREA50  COMPARE   ZERO,CSEARCH
          GOTO      HAMREA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HAMREA08
HAMREA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HAMREA08
HAMREA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HAMREA61 IF EQUAL
          MATCH     SCHFLD01,AMREREG 
          GOTO      HAMREA72 IF NOT EQUAL
HAMREA61
          MATCH     SP70,SCHFLD02
          GOTO      HAMREA62 IF EQUAL
          MATCH     SCHFLD02,AMREDES 
          GOTO      HAMREA72 IF NOT EQUAL
HAMREA62
HAMREA63
HAMREA64
HAMREA65
HAMREA66
HAMREA67
HAMREA68
HAMREA69
HAMREA70
          GOTO      HAMREA75
HAMREA72  MOVE      ONE,OVRCD
HAMREA75  RETURN
HAMREA85
          LOAD      KEY2,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY2,AMREREG                                                
          CALL      RDAMRE1 
HAMREA90
.                                                                               
          BEEP
          GOTO      HAMREA20
HAMREA91
          MOVE      SP70,AMREDES 
          MOVE      SP70,AMREUR1 
          MOVE      SP70,AMREUR2 
          MOVE      SP70,AMREUD1 
          MOVE      SP70,AMREUD2 
          MOVE      SP70,AMREUY1 
          MOVE      SP70,AMREUY2 
          MOVE      SP70,AMRESPA 
          MOVE      SP70,KEY2 
          MOVE      SP70,AMREREG 
          GOTO      HAMREA96
HAMREA92
          MOVE      SP70,KEY2 
          MOVE      SP70,AMREREG 
          LOAD      KEY2,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY2,AMREREG                                                
HAMREA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HAMREA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Asset Management System            
.   Include   :   AMSREGKY
.   Routine   :   KAMREA00 Keyin Register            
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KAMREA00 TOPIC     "AMREREG "
          MOVE      UNDLN70,AMREREG 
          KEYIN     *PCCOL:CVERT,*DV,AMREREG:                                   
                    *PCCOL:CVERT,*V2LON,AMREREG:                                
                    *PCCOL:CVERT,*DV,AMREREG 
.
          ENDSET    AMREREG 
          APPEND    SP70,AMREREG 
          RESET     AMREREG 
.
          MATCH     SP70,AMREREG 
          GOTO      KAMREA90 IF EQUAL
.
          CMATCH    EXITCHAR,AMREREG 
          GOTO      KAMREA95 IF EQUAL
.
          CMATCH    QUEST,AMREREG 
          GOTO      KAMREA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HAMREA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    AMREREG 
          APPEND    SP70,AMREREG 
          RESET     AMREREG 
.
          MATCH     SP70,AMREREG 
          GOTO      KAMREA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,AMREREG                                 
KAMREA10
          PACK      KEY2,AMREREG                                                
          CALL      RAAMRE1 
          BRANCH    OVRCD,KAMREA80
          BRANCH    CKEYTYP,KAMREA75
          CALL      RDAMRE1 
KAMREA70
          MOVE      ZERO,EXIT
          GOTO      KAMREA99
KAMREA75
          MOVE      "Register Already Exist - ",DISPMSG                                              
          CALL      MESSAGE1
          GOTO      KAMREA00
KAMREA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KAMREA99,KAMREA99
          MOVE      "Register Does Not Exist - ",DISPMSG                                             
          CALL      MESSAGE1
          GOTO      KAMREA00
KAMREA90
          MOVE      ONE,EXIT
          GOTO      KAMREA99
KAMREA95
          MOVE      TWO,EXIT
KAMREA99 TOPIC    PRGID
          RETURN
