.**********************************************************************
. Print routine (modified copy of APSBCHPR)
.   Requires : APSBCHDF
.              APSMASFD & IO
.**********************************************************************
PRNT0000
          CALL      IBACLOCK
          MOVE      ZERO,TOTDEBS       * initialise total debits
          MOVE      SP70,LASTNAME
          CALL      HEAD0000           * print header
.
          PACK      KEY5,SP70
          CALL      RSAPMA1
.
PRNT1000  CALL      RKAPMA1
          BRANCH    OVRCD,PRNT7000
.
          PACK      KEY40,APMACRD,SP70
          CALL      RSAPIM3
.
PRNT2000  CALL      RKAPIM3
          BRANCH    OVRCD,PRNT1000
.
          MATCH     APMACRD,APIMCRD
          GOTO      PRNT1000 IF NOT EQUAL
          MATCH     SP70,APIMREF
          GOTO      PRNT1000 IF NOT EQUAL
          MATCH     "2",APIMTYP
          GOTO      PRNT2000 IF NOT EQUAL
.
          CALL      APBP0000           * print batch
          GOTO      PRNT2000
.
PRNT7000
          MOVE      FORMAT2,OUTFORM2
          FEDIT     TOTDEBS,OUTFORM2
          PRINT     SP1
          CALL      UND132
          PRINT     *100,"Total : ",*113,OUTFORM2
          CALL      UND132
.
PRNT9999  RETURN
.
.*********************************************************************
.*                          APBP0000                                 *
.*                     Print a single reference                      *
.*********************************************************************
APBP0000
          CLEAR     DISPMSG
          APPEND    "Printing Creditor : ",DISPMSG
          APPEND    APIMCRD,DISPMSG
          CALL      MESSAGE2
          PACK      KEY5,APIMBCH,SP70
          CALL      RDAPBM1
.
          CALL      PLIN0000             * print the report line
.
APBP9999  RETURN
.**********************************************************************
.*                         HEAD0000                                   *
.*                  display the page heading                          *
.**********************************************************************
.
HEAD0000  MOVE      ONE,CNOUNDLN
          MOVE      "~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~",LASTNAME
          CALL      HEAD132
.
          MOVE      "Cheque Number  ",REFDESC1
          MOVE      "Batch   Date   ",REFDESC2
.
          CALL      UND132
.
          PRINT     *1,"Creditor Code",*17,"Creditor Name",*50,REFDESC1:
                    *67,REFDESC2:
                       *85,"    Paid Amount":
                      *101,"       Invoiced":
                      *117,"    Outstanding"
.
          CALL      UND132
.
          ADD       ONE,CLNO
.
HEAD9999  RETURN
.
.**********************************************************************
.*                         PLIN0000                                   *
.*                 Print the report lines                             *
.**********************************************************************
.
PLIN0000  COMPARE   "55",CLNO            * test for a full page
          CALL      UND132 IF NOT LESS
          COMPARE   "55",CLNO            * test for a full page
          CALL      HEAD0000 IF NOT LESS
.
.----- format the amount for printing ----
.
          CLEAR     DIM28
          PACK      KEY14,APINLED,APINACC
          UNPACK    SP70,FMAMDESC
          UNPACK    KEY14,FMAMLEDG,FMAMACCT
          CALL      RDFMAM1
          MOVE      FMAMDESC,DIM28
.
          MATCH     "IN",APBMTYP
          GOTO      PLIN9100 IF EQUAL
          MATCH     "CR",APBMTYP
          GOTO      PLIN9100 IF EQUAL
          MATCH     "PY",APBMTYP
          GOTO      PLIN9100 IF EQUAL
          GOTO      PLIN9200
.
.---- print supplier if desired ----
.
PLIN9100
          MATCH     APMACN1,LASTNAME
          GOTO      PLIN9200 IF EQUAL       * same as last creditor ?
.
          PACK      LASTNAME,APMACN1,SP70
          PRINT     *N,APMACRD,*17,APMACN1;
          ADD       ONE,CLNO
.
PLIN9200
          UNPACK    APBMADT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          PRINT     *50,APIMDOC,*67,APIMBCH,SP1,CPCDATE;
          ADD       ONE,CLNO
.
          MOVE      FORMAT,OUTFORM
          FEDIT     APIMPAY,OUTFORM
          PRINT     *85,OUTFORM;
.
          MOVE      ZERO,APUMAIN
          PACK      KEY25,APIMBCH,APIMCRD,APIMDOC
          CALL      RDAPUM1
          BRANCH    OVRCD,PLIN9300
          MOVE      FORMAT,OUTFORM
          FEDIT     APUMAIN,OUTFORM
          PRINT     *101,OUTFORM;
.
PLIN9300  ASSIGN    APIMPAY-APUMAIN,APIMTOT
          ADD       APIMTOT,TOTDEBS        * add to debits
          MOVE      FORMAT,OUTFORM
          FEDIT     APIMTOT,OUTFORM
          PRINT     *117,OUTFORM
.
PLIN9999  RETURN
