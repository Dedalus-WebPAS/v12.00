          KEYIN     *PCCOL:CVERT,*V2LON,*NOMASK,*RV,CEPSST4 
          IF        SCPSMAN=1
            COMPARE   ZERO,CEPSST4 
            IF        @EQUAL
              MOVE      "Mandatory Field - ",DISPMSG
              BEEP
              CALL      MESSAGE1
              GOTO      SF00L120
            ENDIF
          ENDIF
          MOVE      CEPSST4,VAR      
          CALL      DISPITEM
          GOTO     SF00L001
.
SF00L121
          CALL     SELECTED
          MOVE      SP70,VAR
          CALL      DISPITEM
          KEYIN     *PCCOL:CVERT,*V2LON,*NOMASK,*RV,CEPSST5 
          IF        SCPSMAN=1
            COMPARE   ZERO,CEPSST5 
            IF        @EQUAL
              MOVE      "Mandatory Field - ",DISPMSG
              BEEP
              CALL      MESSAGE1
              GOTO      SF00L121
            ENDIF
          ENDIF
          MOVE      CEPSST5,VAR      
          CALL      DISPITEM
          GOTO     SF00L001
SF00L990  MOVE      ONE,EXIT * Exit Char Restart
SF00L999  RETURN
