.-------------------------------
. Get Deparetment Name
.-------------------------------
GETDEP00  MOVE     DBSEDEP,KEY8
          MOVE     SP70,DBDEDES
          CALL     RDDBDE1
          RETURN

.----------------------------------------------------------------------------
. Save Changes for User ID
.----------------------------------------------------------------------------
POST0000   CALL      POSTB000
.
POST9999   RETURN
.--------------------------------------------
. Save Batch Screen
.
.  1. Delete all Related Batch Items
.  2. Loop Through & add Posted Batch Items
.--------------------------------------------
POSTB000  PACK      KEY12,SECODE,SP70
          CALL      RSDBSE1
POSTB100  CALL      RKDBSE1
          BRANCH    OVRCD,POSTB200
          MATCH     DBSEUSR,SECODE
          GOTO      POSTB200 IF NOT EQUAL
          PACK      KEY12,DBSEUSR,DBSEDEP
          CALL      DEDBSE1
          MOVE      FOUR,AUDTTYPE     * Delete Audit Type
          CALL      WADBSE00          * Write Delete Audit
          GOTO      POSTB100
.
POSTB200  MOVE      SP70,KEY5
          CALL      TRDBSE11
          COMPARE   ZERO,OVRCD
          GOTO      POSTB310 IF EQUAL
.
POSTB300  CALL      TKDBSE11
          BRANCH    OVRCD,POSTB999
POSTB310  PACK      KEY12,SECODE,DBSEDEP
          UNPACK    KEY12,DBSEUSR,DBSEDEP
          CALL      RADBSE1
          IF        OVRCD=0
            CALL      UPDBSE1
          ELSE
            CALL      WRDBSE1
            MOVE      ONE,AUDTTYPE     * New Record Audit Type
            CALL      WADBSE00         * Write Audit Record
          ENDIF
          GOTO      POSTB300
.
POSTB999  RETURN
.
.-------------------------------
. Add All Departments to a User
.-------------------------------
ADDA0000  MOVE     "Add All Departments. ",DISPMSG
          CALL     MESSAGE4            * Continue Y/N
          BRANCH   EXIT,ADDA9100
.
          DISPLAY  *P1:24,*EL,"Adding ... "
          PACK     KEY8,SP70
          CALL     RSDBDE1
ADDA1000  CALL     RKDBDE1
          BRANCH   OVRCD,ADDA9000
          COMPARE  ZERO,DBDEACT
          GOTO     ADDA1000 IF EQUAL
          DISPLAY  *P20:24,DBDEDEP
          PACK     KEY12,SECODE,DBDEDEP,SP70
          CALL     RDDBSE1
          COMPARE  ZERO,OVRCD
          GOTO     ADDA1000 IF EQUAL
.
          UNPACK   KEY12,DBSEUSR,DBSEDEP
          CALL     WRDBSE1          * Write Record
          MOVE     ONE,AUDTTYPE     * New Record Audit Type
          CALL     WADBSE00         * Write Audit Record
          GOTO     ADDA1000
.
ADDA9000  MOVE     "Complete - ",DISPMSG
          BEEP
          CALL     MESSAGE1
          MOVE     TWO,EXIT
          GOTO     ADDA9999
.
ADDA9100  MOVE     ONE,EXIT            * Return to Prompt on "No"
          GOTO     ADDA9999
.
ADDA9999  RETURN
.-------------------------------
. Add All Departments to a User
.-------------------------------
DELA0000  MOVE     SP70,DISPMSG        * Clear Line 24 Display Message
          CALL     MESSAGE2            * Save and Clear Line 24
          CALL     DELQST              * Delete (Y/N/C) CEXIT 1/2/3
          CALL     MESSAGE3            * Restore Line 24
          BRANCH   CEXIT,DELA0100,DELA9100,DELA9200
.
DELA0100  DISPLAY  *P1:24,*EL,"Deleting ... "
          PACK     KEY12,SECODE,SP70
          CALL     RSDBSE1
DELA1000  CALL     RKDBSE1
          BRANCH   OVRCD,DELA9000
          MATCH    DBSEUSR,SECODE
          GOTO     DELA9000 IF NOT EQUAL
          DISPLAY  *P20:24,DBSEDEP
          PACK     KEY12,DBSEUSR,DBSEDEP,SP70
          CALL     DEDBSE1
          MOVE     FOUR,AUDTTYPE     * Delete Audit Type
          CALL     WADBSE00          * Write Delete Audit
          GOTO     DELA1000
.
DELA9000  MOVE     "Complete - ",DISPMSG
          BEEP
          CALL     MESSAGE1
          MOVE     "2",EXIT
          GOTO     DELA9999
.
DELA9100  MOVE     ONE,EXIT            * Return to Prompt on "No"
          GOTO     DELA9999
.
DELA9200  MOVE     TWO,EXIT            * Restart Screen on "Cancel"
          GOTO     DELA9999
.
DELA9999  RETURN
.
