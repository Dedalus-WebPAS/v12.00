. Print Department Details by Cost Type
.---------------------------------------------------------------------------
PCTS0000  PACK      KEY4,SP70
          CALL      RSCCCT1
PCTS1000  CALL      RKCCCT1
          BRANCH    OVRCD,PCTS9999
.
          CALL      CTYTOT00           * Calculate Cost Type Total for Period
          COMPARE   ZERO,TOTALCTY      * If Zero for Cost Type Ignore
          GOTO      PCTS1000 IF EQUAL
.
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          MOVE      SP70,FTOTAL1
          MOVE      SP70,FTOTAL2
          MOVE      SP70,FTOTAL3
          MOVE      CCFED1,FTOTAL4
          MOVE      TOTALCTY,F12P2
          FEDIT     F12P2,FTOTAL4
.
          ADD       ONE,CLNO
          PRINT     "| ",CCCTCTY,SP1,CCCTDES:
                     *72,"|",FTOTAL1," |",FTOTAL2," |",FTOTAL3," |",FTOTAL4," |"
          CALL      UND132
.
          MOVE      ZERO,TOTALCTY
          MOVE      ZERO,TOTALRVI
.
          CALL      PPCD0000      * Print Patient Service within this cost type
.
          ADD       TOTALCTY,TOTALDPT
          MOVE      "9999,999.9999",FTOTAL1
          MOVE      SP70,FTOTAL2
          MOVE      SP70,FTOTAL3
          MOVE      CCFED1,FTOTAL4
.
          MOVE      TOTALRVI,F12P4
          FEDIT     F12P4,FTOTAL1
.
          MOVE      TOTALCTY,F12P2
          FEDIT     F12P2,FTOTAL4
          CALL      UND132
          ADD       ONE,CLNO
          PRINT     "| ",*20,"Total":
                     *72,"|",FTOTAL1," |",FTOTAL2," |",FTOTAL3," |",FTOTAL4," |"
          CALL      UND132
          GOTO      PCTS1000
.
PCTS9999  RETURN
