PPCD1100  CALL      RKCCPS2
          BRANCH    OVRCD,PPCD2000
          PACK      KEY24,CCPSHCD,CCPSDPT,CCPSPCD,CCPSYEAR,CCPSPER,SP70
          MATCH     KEY22,KEY24
          GOTO      PPCD2000 IF NOT EQUAL
          MATCH     CCPSPER,LPERIOD
          GOTO      PPCD2000 IF LESS
          MATCH     FPERIOD,CCPSPER
          GOTO      PPCD1100 IF LESS
.
          ADD       CCPSQTY,QTYTOTAL
          ADD       ONE,PERIODNO
.
          PACK      KEY28,CCPSHCD,CCPSDPT,CCPSYEAR,CCPSPER,CCPSPCD,CCCTCTY
          CALL      RDCCPC1
          BRANCH    OVRCD,PPCD1100
.
          ADD       CCPCRVI,RVITOTAL
          ADD       CCPCCST,CSTTOTAL
          ASSIGN    CTYTOTAL+(CCPSQTY*CCPCCST),CTYTOTAL
          GOTO      PPCD1100
.
PPCD2000  COMPARE   ZERO,PERIODNO
          GOTO      PPCD1000 IF EQUAL
          COMPARE   ZERO,CTYTOTAL
          GOTO      PPCD1000 IF EQUAL
.
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          ADD       CTYTOTAL,TOTALCTY
          ASSIGN    TOTALRVI+(RVITOTAL/PERIODNO),TOTALRVI
.
          MOVE      "9999,999.9999",FTOTAL1
          MOVE      "99,999,999.99",FTOTAL2
          MOVE      "9999,999.9999",FTOTAL3
          MOVE      "99,999,999.99",FTOTAL4
.
          ASSIGN    (RVITOTAL/PERIODNO),F12P4    * Average Rvi
          FEDIT     F12P4,FTOTAL1
.
          MOVE      QTYTOTAL,F12P2
          FEDIT     F12P2,FTOTAL2
.
          ASSIGN    (CSTTOTAL/PERIODNO),F12P4    * Average Cost
          FEDIT     F12P4,FTOTAL3
.
          MOVE      CTYTOTAL,F12P2
          FEDIT     F12P2,FTOTAL4
.
          ADD       ONE,CLNO
          PRINT     "|",*20,CCPDSEQ,SP2,CCPDPCD,CCPDDES:
                     *72,"|",FTOTAL1," |",FTOTAL2," |",FTOTAL3," |",FTOTAL4," |"
          GOTO      PPCD1000
PPCD9999  RETURN
.
HEAD0000  CALL      HEAD132
          ASSIGN    DPTDIR+DPTIND,F12P2
          PRINT        "Sequence      : ",REPTSEQ,SP1,SEQDESC:
                    *N,"Hospital      : ",CCHCHCD,SP7,CCHCDES:
                    "                Year          : ",CCCAYEAR:
                    *N,"Department    : ",CCDPDPT,SP1,CCDPDES:
                    "                Period        : ",FROMPNAM," to ",TOPNAM:
                    *N,"Direct Cost   : ",DPTDIR:
                       "       Indirect Cost : ",DPTIND:
                       "       Total Cost    : ",F12P2
          CALL      UND132
          IF        REPTSEQ=1
            PRINT     "| Cost Type                                   ":
                      "                         |              |     ":
                      "         |              |              |"
            PRINT     "|                  Seq. Patient Service       ":
                      "                         |       R.V.I. |     ":
                      "  Volume |    Unit Cost |   Total Cost |"
          ELSE
            PRINT     "| Seq. Patient Service                       |":
                      "              |              |                ":
                      " Unit Cost              |              |"
            PRINT     "|                   Cost Type                |":
                      "       Volume |  Avg. R.V.I. |        Fixed   ":
                      "  Variable        Total |   Total Cost |"
          ENDIF
          CALL      UND132
          MOVE      "9",CLNO
          RETURN
.--------------------------------------------------------
. Determine the department level costs for period range
.--------------------------------------------------------
DPTCST00  PACK      KEY12,CCDPHCD,CCDPDPT,CCCAYEAR,SP70
          PACK      KEY16,CCDPHCD,CCDPDPT,CCCAYEAR,SP70
          MOVE      ZERO,DPTDIR
          MOVE      ZERO,DPTIND
          CALL      RSCCSD1
DPTCST10  CALL      RKCCSD1
          BRANCH    OVRCD,DPTCST99
          PACK      KEY16,CCSDHCD,CCSDDPT,CCSDYEAR,SP70
          MATCH     KEY12,KEY16
          GOTO      DPTCST99 IF NOT EQUAL
.
          MATCH     CCSDPER,LPERIOD
          GOTO      DPTCST99 IF LESS
          MATCH     FPERIOD,CCSDPER
          GOTO      DPTCST10 IF LESS
.
          ADD       CCSDDIR,DPTDIR
          ADD       CCSDIND,DPTIND
          GOTO      DPTCST10
.
DPTCST99  RETURN
.--------------------------------------------------------
. Determine the Cost Type level costs for period range
.--------------------------------------------------------
CTYTOT00  MOVE      ZERO,TOTALCTY
          PACK      KEY20,CCDPHCD,CCDPDPT,CCCTCTY,CCCAYEAR,SP70
          PACK      KEY18,CCDPHCD,CCDPDPT,CCCTCTY,CCCAYEAR,SP70
          CALL      RSCCDC2
CTYTOT10  CALL      RKCCDC2          * Costs by Cost Type
          BRANCH    OVRCD,CTYTOT99
          PACK      KEY20,CCDCHCD,CCDCDPT,CCDCCTY,CCDCYEAR,CCDCPER,SP70
          MATCH     KEY18,KEY20
          GOTO      CTYTOT99 IF NOT EQUAL
          MATCH     CCDCPER,LPERIOD
          GOTO      CTYTOT99 IF LESS
          MATCH     FPERIOD,CCDCPER
          GOTO      CTYTOT10 IF LESS
.
          ASSIGN    TOTALCTY+CCDCDIR+CCDCIND,TOTALCTY
          GOTO      CTYTOT10
.
CTYTOT99  RETURN
.---------------------------------------------------------------------------
. Print Department Details by Patient Service
.---------------------------------------------------------------------------
PPCS0000  PACK      KEY21,CCDPHCD,CCDPDPT,SP70
          CALL      RSCCPD4
PPCS1000  CALL      RKCCPD4
          BRANCH    OVRCD,PPCS9999
          PACK      KEY21,CCPDHCD,CCPDDPT,SP70
          PACK      KEY10,CCDPHCD,CCDPDPT,SP70
          MATCH     KEY10,KEY21
          GOTO      PPCS9999 IF NOT EQUAL
.
          BRANCH    PRTSUSP,PPCS1100
          MATCH     "1",CCPDSUS
          GOTO      PPCS1000 IF EQUAL
.
PPCS1100  COMPARE   "50",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          CALL      PCDTOT00
.
          MOVE      CCFED1,FTOTAL1
          MOVE      SP70,FTOTAL2
          MOVE      SP70,FTOTAL3
          MOVE      SP70,FTOTAL4
          MOVE      "9999,999.9999",FTOTAL5
          MOVE      CCFED1,FTOTAL6
.
          MOVE      TOTALQTY,F12P2
          FEDIT     F12P2,FTOTAL1
.
          ASSIGN    TOTALCST/TOTALQTY,F12P4
          FEDIT     F12P4,FTOTAL5
          IF        TOTALQTY=0
            MOVE      SP70,FTOTAL5
          ENDIF
.
          MOVE      TOTALCST,F12P2
          FEDIT     F12P2,FTOTAL6
          ADD       F12P2,TOTALDPT
.
          ADD       ONE,CLNO
          PRINT     "| ",CCPDSEQ,SP2,CCPDPCD,SP1,CCPDDES:
                    *46,"|",FTOTAL1," |",FTOTAL2," |",FTOTAL3:
                    FTOTAL4,FTOTAL5," |",FTOTAL6," |"
          CALL      UND132
.
          CALL      PCTY0000      * Print Cost Types within this Patient Service
.
          MOVE      "9999,999.9999",FTOTAL2
          MOVE      "9999,999.9999",FTOTAL3
          MOVE      "9999,999.9999",FTOTAL4
          MOVE      "9999,999.9999",FTOTAL5
          MOVE      CCFED1,FTOTAL6
.
          MOVE      TOTALRVI,F12P4
          FEDIT     F12P4,FTOTAL2
.
          MOVE      TOTALFIX,F12P4
          FEDIT     F12P4,FTOTAL3
.
          MOVE      TOTALVAR,F12P4
          FEDIT     F12P4,FTOTAL4
.
          ASSIGN    TOTALVAR+TOTALFIX,F12P4
          FEDIT     F12P4,FTOTAL5
.
          ASSIGN    F12P4*TOTALQTY,F12P2
          FEDIT     F12P2,FTOTAL6
.
          CALL      UND132
          ADD       ONE,CLNO
          PRINT     "|",*21,"Total",*46,"|",FTOTAL1," |",FTOTAL2," |":
                    FTOTAL3,FTOTAL4,FTOTAL5," |",FTOTAL6," |"
          CALL      UND132
          GOTO      PPCS1000
.
PPCS9999  RETURN
.------------------------------------------------------------
. Print Cost Type Analysis
.------------------------------------------------------------
PCTY0000  MOVE      ZERO,TOTALRVI
          MOVE      ZERO,TOTALFIX
          MOVE      ZERO,TOTALVAR
          MOVE      SP70,KEY4
          CALL      RSCCEX1
.
PCTY1000  CALL      RKCCEX1
          BRANCH    OVRCD,PCTY9999
.
          COMPARE   ZERO,CCEXCST
          GOTO      PCTY1000 IF EQUAL
.
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          MOVE      "9999,999.9999",FTOTAL2
          MOVE      "9999,999.9999",FTOTAL3
          MOVE      "9999,999.9999",FTOTAL4
          MOVE      "9999,999.9999",FTOTAL5
          MOVE      CCFED1,FTOTAL6
.
          ASSIGN    CCEXRVI/CCEXNUM,F12P4
          ADD       F12P4,TOTALRVI
          FEDIT     F12P4,FTOTAL2
.
          MOVE      CCEXCTY,KEY4
          CALL      RDCCCT1
          IF        OVRCD=1
            MOVE      "Invalid Cost Type",CCCTDES
            MOVE      "1",CCCTFIX
          ENDIF
.
          MOVE      CCEXCST,F12P2
          FEDIT     F12P2,FTOTAL6
.
          ASSIGN    CCEXCST/TOTALQTY,F12P4
          FEDIT     F12P4,FTOTAL5
          MATCH     "1",CCCTFIX
          IF        @EQUAL
            FEDIT     F12P4,FTOTAL3
            MOVE      SP70,FTOTAL4
            ADD       F12P4,TOTALFIX
          ELSE
            MOVE      SP70,FTOTAL3
            FEDIT     F12P4,FTOTAL4
            ADD       F12P4,TOTALVAR
          ENDIF
          ADD       ONE,CLNO
          PRINT     "|",*21,CCEXCTY,SP1,CCCTDES,"|",FTOTAL2," |":
                    FTOTAL3,FTOTAL4,FTOTAL5," |",FTOTAL6," |"
          GOTO      PCTY1000
.
PCTY9999  RETURN
.--------------------------------------------------------
. Determine the Cost Type level costs for period range
.--------------------------------------------------------
PCDTOT00  CALL      CLEXT000
.
          MOVE      ZERO,TOTALQTY
          MOVE      ZERO,TOTALCST
          PACK      KEY24,CCDPHCD,CCDPDPT,CCPDPCD,CCCAYEAR,SP70
          PACK      KEY22,CCDPHCD,CCDPDPT,CCPDPCD,CCCAYEAR,SP70
          CALL      RSCCPS2
.
PCDTOT10  CALL      RKCCPS2          * Costs by Cost Type
          BRANCH    OVRCD,PCDTOT99
          PACK      KEY24,CCPSHCD,CCPSDPT,CCPSPCD,CCPSYEAR,CCPSPER,SP70
          MATCH     KEY22,KEY24
          GOTO      PCDTOT99 IF NOT EQUAL
          MATCH     CCPSPER,LPERIOD
          GOTO      PCDTOT99 IF LESS
          MATCH     FPERIOD,CCPSPER
          GOTO      PCDTOT10 IF LESS
          ASSIGN    TOTALCST+(CCPSCST*CCPSQTY),TOTALCST
          ASSIGN    TOTALQTY+CCPSQTY,TOTALQTY
.
          PACK      KEY28,CCPSHCD,CCPSDPT,CCPSYEAR,CCPSPER,CCPSPCD,SP70
          PACK      KEY24,CCPSHCD,CCPSDPT,CCPSYEAR,CCPSPER,CCPSPCD,SP70
          CALL      RSCCPC1
PCDTOT20  CALL      RKCCPC1
          BRANCH    OVRCD,PCDTOT10
          PACK      KEY28,CCPCHCD,CCPCDPT,CCPCYEAR,CCPCPER,CCPCPCD,SP70
          MATCH     KEY24,KEY28
          GOTO      PCDTOT10 IF NOT EQUAL
          CALL      EXTRA000
          GOTO      PCDTOT20
.
PCDTOT99  RETURN
.------------------------------------------------------------
. Clear Extract
.------------------------------------------------------------
CLEXT000  PACK      KEY4,SP70  * Clear Extract
          CALL      RSCCEX1
          CALL      RKCCEX1
          BRANCH    OVRCD,CLEXT999
          PACK      KEY4,CCEXCTY
          CALL      DECCEX1
          GOTO      CLEXT000
CLEXT999  RETURN
.------------------------------------------------------------
. Extract Cost Type Analysis
.------------------------------------------------------------
. CCSEXTA1   IFILE   SQL, FIXED=72, KEY="U1-4"
.
. CCEXCTY    DIM     4     4     1
. CCEXNUM    FORM    2     2     40
. CCEXCST    FORM    12.6  10    42
. CCEXRVI    FORM    12.6  10    62
.------------------------------------------------------------
EXTRA000  COMPARE   ZERO,CCPCCST
          GOTO      EXTRA999 IF EQUAL
.
          PACK      KEY4,CCPCCTY
          CALL      RDCCEX1
          IF        OVRCD=0
            ADD       ONE,CCEXNUM
            ADD       CCPCRVI,CCEXRVI
            ASSIGN    CCEXCST+(CCPCCST*CCPSQTY),CCEXCST
            CALL      UPCCEX1
          ELSE
            UNPACK    KEY4,CCEXCTY
            MOVE      ONE,CCEXNUM
            MOVE      CCPCRVI,CCEXRVI
            ASSIGN    CCPCCST*CCPSQTY,CCEXCST
            CALL      WRCCEX1
          ENDIF
.
EXTRA999  RETURN
