.--------------------------------------------------
. Patient Service Cost Allocation Report
.--------------------------------------------------
REPT0000  DISPLAY   *P1:24,*EL,"Printing : "
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
          CLEAR     FILENAME
          APPEND    "CCST56",FILENAME
          APPEND    PORT,FILENAME
          RESET     FILENAME
          REP       " 0",FILENAME
          MOVE      " 72 U1-4",KEY10
          PACK      COMMAND,ISBUILD,FILENAME,KEY10
          EXECUTE   COMMAND,TASKID
          OPEN      CCSEXTA1,FILENAME
.
          MOVE      CCCAYEAR,KEY4
          PACK      KEY6,KEY4,FPERIOD
          CALL      RDCCCA1
          MATCH     "  ",FPERIOD
          IF        @EQUAL
            CALL      RKCCCA1
          ENDIF
          MOVE      CCCADES,FROMPNAM
          MOVE      KEY4,CCCAYEAR
.
          PACK      KEY6,KEY4,LPERIOD
          CALL      RDCCCA1
          MATCH     "ZZ",LPERIOD
          IF        @EQUAL
            CALL      RPCCCA1
          ENDIF
          MOVE      CCCADES,TOPNAM
          MOVE      KEY4,CCCAYEAR
.
          PACK      KEY10,CCHCHCD,FDEPART,SP70
          CALL      RDCCDP1
          IF        OVRCD=0
            CALL      RPCCDP1
          ENDIF
REPT1000  CALL      RKCCDP1
          BRANCH    OVRCD,REPT9999
          MATCH     CCDPHCD,CCHCHCD
          GOTO      REPT9999 IF NOT EQUAL
          MATCH     CCDPDPT,LDEPART
          GOTO      REPT9999 IF LESS
.
          MATCH     "1",CCDPDTY
          GOTO      REPT1000 IF EQUAL
.
          CALL      DPTCST00         * Calculate Department Costs
.
          DISPLAY   *P12:24,CCDPHCD,CCDPDPT
.
          CALL      HEAD0000         * Page Break on Department
.
          MOVE      ZERO,TOTALDPT
          IF        REPTSEQ=1
            CALL      PCTS0000         * Cost Type Sequence
            MOVE      SP70,FTOTAL1
            MOVE      SP70,FTOTAL2
            MOVE      SP70,FTOTAL3
            MOVE      CCFED1,FTOTAL4
            MOVE      TOTALDPT,F12P2
            FEDIT     F12P2,FTOTAL4
            ADD       ONE,CLNO
            PRINT     "| ",*20,"Department Total",*72,"|",FTOTAL1," |":
                      FTOTAL2," |",FTOTAL3," |",FTOTAL4," |"
          ELSE
            CALL      PPCS0000         * Patient Service Sequence
            MOVE      SP70,FTOTAL1
            MOVE      SP70,FTOTAL2
            MOVE      SP70,FTOTAL3
            MOVE      SP70,FTOTAL4
            MOVE      SP70,FTOTAL5
            MOVE      CCFED1,FTOTAL6
            MOVE      TOTALDPT,F12P2
            FEDIT     F12P2,FTOTAL6
            ADD       ONE,CLNO
            CALL      UND132
            PRINT     "|",*21,"Department Total",*46,"|",FTOTAL1," |":
                      FTOTAL2," |",FTOTAL3,FTOTAL4,FTOTAL5," |",FTOTAL6," |"
          ENDIF
.
          GOTO      REPT1000
.
REPT9999  CALL      UND132
          PRINT     *50,"*** End of Report ***"
          MOVE      "Report Completed - ",DISPMSG
          CALL      MESSAGE1
          CLOSE     CCSEXTA1,DELETE
          RETURN
