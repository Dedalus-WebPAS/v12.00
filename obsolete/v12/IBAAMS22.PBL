          READ      AMLE3FL1,KEY5;ANS
          IF        @OVER
            WRITE     AMLE3FL1,KEY5;SCPSROW,SCPSCOL,AMLE3MKY
          ELSE
            UPDATE    AMLE3FL1;SCPSROW,SCPSCOL,AMLE3MKY
          ENDIF
          RETURN
