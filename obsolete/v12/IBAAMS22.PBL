          MOVE      ZERO,OVRCD
          PACK      KEY5,SCPSROW,SCPSCOL
          READ      AMLE3FL1,KEY5;SCPSROW,SCPSCOL,AMLE3MKY
          GOTO      OVERCOND IF OVER
