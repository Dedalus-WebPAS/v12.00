UPTAMLE4
          PACK      KEY5,SCPSROW,SCPSCOL
          READ      AMLE4FL1,KEY5;ANS
          IF        @OVER
            WRITE     AMLE4FL1,KEY5;SCPSROW,SCPSCOL,AMLE4MKY
          ELSE
            UPDATE    AMLE4FL1;SCPSROW,SCPSCOL,AMLE4MKY
          ENDIF
          RETURN
RDTAMLE4
          MOVE      ZERO,OVRCD
          PACK      KEY5,SCPSROW,SCPSCOL
          READ      AMLE4FL1,KEY5;SCPSROW,SCPSCOL,AMLE4MKY
          GOTO      OVERCOND IF OVER
          RETURN
RFTAMLE4
          MOVE      ZERO,OVRCD
          PACK      KEY5,SP70
          READ      AMLE4FL1,KEY5;;
          READKS    AMLE4FL1;SCPSROW,SCPSCOL,AMLE4MKY
          GOTO      OVERCOND IF OVER
          MATCH     SP70,AMLE4MKY
          GOTO      OVERCOND IF EQUAL
          RETURN
