. Patient Service Usage Model Analysis
.----------------------------------------
REPT0000  CALL      EXTR0000      * Extract from the Patient Service Details
.
          DISPLAY   *P1:24,*EL,"Printing Location : "
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
          MOVE      ONE,FIRSTIND
.
          MOVE      CCCAYEAR,KEY4
          PACK      KEY6,KEY4,FPERIOD
          CALL      RDCCCA1
          MATCH     "  ",FPERIOD
          IF        @EQUAL
            CALL      RKCCCA1
          ENDIF
          MOVE      CCCADES,FROMPNAM
          MOVE      KEY4,CCCAYEAR
.
          PACK      KEY6,KEY4,LPERIOD
          CALL      RDCCCA1
          MATCH     "ZZ",LPERIOD
          IF        @EQUAL
            CALL      RPCCCA1
          ENDIF
          MOVE      CCCADES,TOPNAM
          MOVE      KEY4,CCCAYEAR
.
          MOVE      ZERO,TOTALCST
          MOVE      ZERO,TOTALBUD
          MOVE      ZERO,TOTALQTY
          MOVE      ZERO,TOTALNPT
          MOVE      ZERO,GRANDCST
          MOVE      ZERO,GRANDBUD
          MOVE      ZERO,GRANDQTY
          MOVE      ZERO,GRANDNPT
          MOVE      ZERO,REPTCST
          MOVE      ZERO,REPTBUD
          MOVE      ZERO,REPTQTY
          MOVE      ZERO,REPTNPT
          MOVE      "ZZZZZZZZ",LASTPRT
          MOVE      "ZZZZZZZZ",LASTDEPT
          PACK      KEY16,SP70
          CALL      RSCCEX2
.
REPT1000  CALL      RKCCEX2
          BRANCH    OVRCD,REPT9000
.
          MATCH     LASTPRT,CCEXSER
          IF        !@EQUAL
            COMPARE   ZERO,FIRSTIND
            CALL      SETOT000 IF EQUAL
            MOVE      ONE,FIRSTIND
            MOVE      "ZZZZZZZZ",LASTDEPT
            DISPLAY   *P26:24,CCEXSER
            MOVE      CCEXSER,LASTPRT
            CALL      HEAD0000
          ENDIF
.
          MATCH     CCEXDPT,LASTDEPT
          IF        !@EQUAL
            COMPARE   ZERO,FIRSTIND
            CALL      DPTOT000 IF EQUAL
            MOVE      ZERO,FIRSTIND
            MOVE      CCEXDPT,LASTDEPT
            PACK      KEY5,TWO,CCEXDPT
            CALL      RDCCPU1
            IF        OVRCD=1
              MOVE      SP70,CCPUDES
            ENDIF
            PRINT     "| ",CCEXDPT,SP1,CCPUDES;
          ELSE
            PRINT     "| ";
          ENDIF
.
          PACK      KEY5,ONE,CCEXPCT
          CALL      RDCCPU1
          IF        OVRCD=1
            MOVE      SP70,CCPUDES
          ENDIF
          MATCH     SP70,CCEXPCT
          IF        @EQUAL
            MOVE      SP70,CCPUDES
          ENDIF
          MOVE      CCPUDES,KEY23
.
          MOVE      "99,999,999",F10TOT1
          MOVE      "99,999,999",F10TOT2
          MOVE      CCFED1,FTOTAL1
          MOVE      CCFED1,FTOTAL2
          MOVE      CCFED1,FTOTAL3
          MOVE      CCFED1,FTOTAL4
.
          MOVE      CCEXQTY,F12
          FEDIT     F12,F10TOT1
          MOVE      CCEXNPT,F12
          FEDIT     F12,F10TOT2
.
          MOVE      CCEXBUD,F12P2
          FEDIT     F12P2,FTOTAL1
          ASSIGN    CCEXBUD/CCEXNPT,F12P2
          FEDIT     F12P2,FTOTAL2
          IF        CCEXNPT=0
            MOVE      SP70,FTOTAL2
          ENDIF
.
          MOVE      CCEXCST,F12P2
          FEDIT     F12P2,FTOTAL3
          ASSIGN    CCEXCST/CCEXNPT,F12P2
          FEDIT     F12P2,FTOTAL4
          IF        CCEXNPT=0
            MOVE      SP70,FTOTAL4
          ENDIF
.
          PRINT     *44,CCEXPCT,"  | ",F10TOT1," | ",F10TOT2:
                    " |",FTOTAL1,FTOTAL2," |",FTOTAL3,FTOTAL4," |"
          ADD       CCEXBUD,TOTALBUD
          ADD       CCEXCST,TOTALCST
          ADD       CCEXQTY,TOTALQTY
          ADD       CCEXNPT,TOTALNPT
          ADD       CCEXCST,GRANDCST
          ADD       CCEXBUD,GRANDBUD
          ADD       CCEXQTY,GRANDQTY
          ADD       CCEXNPT,GRANDNPT
          ADD       CCEXCST,REPTCST
          ADD       CCEXBUD,REPTBUD
          ADD       CCEXQTY,REPTQTY
          ADD       CCEXNPT,REPTNPT
          GOTO      REPT1000
.
REPT9000  CALL      SETOT000
          CALL      UND132
          CALL      RPTOT000
          CALL      UND132
          PRINT     *25,"*** End of Report ***"
          CALL      MESSAGE3
          MOVE      "Report Completed - ",DISPMSG
          CALL      MESSAGE1
          CLOSE     CCSEXTA2
          CLOSE     CCSEXTA1,DELETE
          CLOSE     CCSTMPA1,DELETE
          RETURN
.------------------------------------------------------------
. Print Report Heading
.------------------------------------------------------------
HEAD0000  CALL      HEAD132
          MOVE      "9",CLNO
          IF        CPAGENO=1
            PRINT     "Department From  : ",FROMDEPT," to ",TODEPT
            ADD       "1",CLNO
            PRINT     "Pat. Serv. From  : ",FROMPROC," to ",TOPROC
            ADD       "1",CLNO
          ENDIF
          PRINT     "Costing Model    : ",CCMACOM,SP1,CCMADES
          IF        INCIND=0
            PRINT     *50,"***** NB :  Indirect Costs Excluded   *****"
            ADD       "1",CLNO
          ENDIF
          PRINT     "Hospital         : ",CCHCHCD,SP3,CCHCDES:
                    " Year   : ",CCCAYEAR
          PACK      KEY5,FOUR,CCEXSER
          CALL      RDCCPU1
          IF        OVRCD=1
            MOVE      SP70,CCPUDES
          ENDIF
          MATCH     SP70,CCEXSER
          IF        @EQUAL
            MOVE      SP70,CCPUDES
          ENDIF
          PRINT     "Location         : ",CCEXSER,SP1,CCPUDES:
                    " Period : ",FROMPNAM," to ",TOPNAM
          CALL      UND132
          PRINT     "| Clinical Department                      Pat.  |":
                    " Pat. Serv. |   Episodes |        Flex Budget     ":
                    "   |           Actual          |"
          PRINT     "|                                          Cat.  |":
                    "            |            |        Total  Per Episo":
                    "de |        Total  Per Episode |"
          CALL      UND132
HEAD9999  RETURN
.
DPTOT000  MOVE      "99,999,999",F10TOT1
          MOVE      "99,999,999",F10TOT2
          MOVE      CCFED1,FTOTAL1
          MOVE      CCFED1,FTOTAL2
          MOVE      CCFED1,FTOTAL3
          MOVE      CCFED1,FTOTAL4
.
          MOVE      TOTALQTY,F12
          FEDIT     F12,F10TOT1
          MOVE      TOTALNPT,F12
          FEDIT     F12,F10TOT2
.
          MOVE      TOTALBUD,F12P2
          FEDIT     F12P2,FTOTAL1
          ASSIGN    TOTALBUD/TOTALNPT,F12P2
          FEDIT     F12P2,FTOTAL2
          IF        TOTALNPT=0
            MOVE      SP70,FTOTAL2
          ENDIF
.
          MOVE      TOTALCST,F12P2
          FEDIT     F12P2,FTOTAL3
          ASSIGN    TOTALCST/TOTALNPT,F12P2
          FEDIT     F12P2,FTOTAL4
          IF        TOTALNPT=0
            MOVE      SP70,FTOTAL4
          ENDIF
.
          PRINT     "|",*44,"Total | ",F10TOT1," | ",F10TOT2," |":
                    FTOTAL1,FTOTAL2," |",FTOTAL3,FTOTAL4," |"
          CALL      UND132
          MOVE      ZERO,TOTALCST
          MOVE      ZERO,TOTALBUD
          MOVE      ZERO,TOTALQTY
          MOVE      ZERO,TOTALNPT
          RETURN
.
SETOT000  CALL      DPTOT000
          MOVE      CCFED1,FTOTAL1
          MOVE      CCFED1,FTOTAL2
          MOVE      CCFED1,FTOTAL3
          MOVE      CCFED1,FTOTAL4
          MOVE      "99,999,999",F10TOT1
          MOVE      "99,999,999",F10TOT2
.
          MOVE      GRANDQTY,F12
          FEDIT     F12,F10TOT1
          MOVE      GRANDNPT,F12
          FEDIT     F12,F10TOT2
.
          MOVE      GRANDBUD,F12P2
          FEDIT     F12P2,FTOTAL1
          ASSIGN    GRANDBUD/GRANDNPT,F12P2
          FEDIT     F12P2,FTOTAL2
          IF        GRANDNPT=0
            MOVE      SP70,FTOTAL2
          ENDIF
.
          MOVE      GRANDCST,F12P2
          FEDIT     F12P2,FTOTAL3
          ASSIGN    GRANDCST/GRANDNPT,F12P2
          FEDIT     F12P2,FTOTAL4
          IF        GRANDNPT=0
            MOVE      SP70,FTOTAL4
          ENDIF
.
          PRINT     "|",*35,"Location Total | ",F10TOT1," | ",F10TOT2," |":
                    FTOTAL1,FTOTAL2," |",FTOTAL3,FTOTAL4," |"
          MOVE      ZERO,GRANDCST
          MOVE      ZERO,GRANDBUD
          MOVE      ZERO,GRANDQTY
          MOVE      ZERO,GRANDNPT
          RETURN
.
RPTOT000  MOVE      CCFED1,FTOTAL1
          MOVE      CCFED1,FTOTAL2
          MOVE      CCFED1,FTOTAL3
          MOVE      CCFED1,FTOTAL4
          MOVE      "99,999,999",F10TOT1
          MOVE      "99,999,999",F10TOT2
.
          MOVE      REPTQTY,F12
          FEDIT     F12,F10TOT1
          MOVE      REPTNPT,F12
          FEDIT     F12,F10TOT2
.
          MOVE      REPTBUD,F12P2
          FEDIT     F12P2,FTOTAL1
          ASSIGN    REPTBUD/REPTNPT,F12P2
          FEDIT     F12P2,FTOTAL2
          IF        REPTNPT=0
            MOVE      SP70,FTOTAL2
          ENDIF
.
          MOVE      REPTCST,F12P2
          FEDIT     F12P2,FTOTAL3
          ASSIGN    REPTCST/REPTNPT,F12P2
          FEDIT     F12P2,FTOTAL4
          IF        REPTNPT=0
            MOVE      SP70,FTOTAL4
          ENDIF
.
          PRINT     "|",*35,"Report Total   | ",F10TOT1," | ",F10TOT2," |":
                    FTOTAL1,FTOTAL2," |",FTOTAL3,FTOTAL4," |"
          RETURN
.------------------------------------------------------------
. Extract Details from the Patient Service Details
.------------------------------------------------------------
EXTR0000  CALL      CLEAR000
.
          REP       " 0",ENDDATE
          REP       " 0",FROMDATE
          DISPLAY   *P1:24,"Extracting Department : "
          PACK      KEY10,CCHCHCD,FDEPART
          CALL      RDCCDP1
          IF        OVRCD=0
            CALL      RPCCDP1
          ENDIF
.
EXTR1000  CALL      RKCCDP1
          BRANCH    OVRCD,EXTR9000
          MATCH     CCHCHCD,CCDPHCD
          GOTO      EXTR9000 IF NOT EQUAL
          MATCH     CCDPDPT,LDEPART
          GOTO      EXTR9000 IF LESS
.
          MATCH     "1",CCDPDTY
          GOTO      EXTR1000 IF EQUAL
.
          DISPLAY   *P25:24,CCDPDPT
.
          CALL      EDPT0000        * Extract Department Details
.
          GOTO      EXTR1000
.
EXTR9000
EXTR9999  RETURN
.------------------------------------------------------------
. Extract Department Details
.------------------------------------------------------------
EDPT0000  PACK     KEY18,CCDPHCD,CCDPDPT,SP70
          CALL     RSCCPD1
EDPT1000  CALL     RKCCPD1
          BRANCH   OVRCD,EDPT9999
          PACK     KEY10,CCDPHCD,CCDPDPT,SP70
          PACK     KEY18,CCPDHCD,CCPDDPT,SP70
          MATCH    KEY10,KEY18
          GOTO     EDPT9999 IF NOT EQUAL
          MATCH    STRPCD,CCPDPCD
          GOTO     EDPT1000 IF LESS
          MATCH    CCPDPCD,ENDPCD
          GOTO     EDPT1000 IF LESS
.
          CALL     CALC0000    * Calculate Average Cost Over the Period Range
.
          MOVE     ZERO,SECTOR
          PACK     KEY31,CCPDHCD,CCPDDPT,CCPDPCD,FROMDATE,SP70
          CALL     RSCCUP3
.
EDPT2000  CALL     RKCCUP3
          BRANCH   OVRCD,EDPT1000
          PACK     KEY18,CCPDHCD,CCPDDPT,CCPDPCD
          PACK     KEY31,CCUPHCD,CCUPDPT,CCUPPCD,SP70
          MATCH    KEY18,KEY31
          GOTO     EDPT1000 IF NOT EQUAL
          REP      " 0",CCUPDAT
          MATCH    CCUPDAT,ENDDATE
          GOTO     EDPT1000 IF LESS
          ADD      ONE,SECTOR
          IF       (SECTOR%100)=0
            DISPLAY  *P74:24,SECTOR
          ENDIF
.
          CALL     UPEXT000          * Update Extract File
.
          GOTO     EDPT2000
.
EDPT9999  RETURN
.------------------------------------------------------------
. Update Extract File
.------------------------------------------------------------
. CCSEXTA1  IFILE    SQL, FIXED=47, KEY="U1-8,9-12,13-16"
. CCSEXTA2  IFILE    SQL, FIXED=47, KEY="U9-13,1-8,13-16"
.
. CCEXDPT   DIM      8     8     1    Department
. CCEXSER   DIM      4     4     9    Location Code
. CCEXPCT   DIM      4     4     13   Patient Category
. CCEXQTY   FORM     12.2  8     17   Number of Patient Service Performed
. CCEXCST   FORM     12.2  8     25   Total Actual
. CCEXBUD   FORM     12.2  8     33   Total Budget
. CCEXNPT   FORM     10    6     41   Number of Patients
.
. End of Record                  47
.------------------------------------------------------------
UPEXT000  MOVE     ZERO,F1
          MATCH    SP70,CCUPURN
          GOTO     UPEXT100 IF EQUAL
          MATCH    SP70,CCUPENC
          GOTO     UPEXT100 IF EQUAL
          PACK     KEY32,CCUPCL2,SP4,CCUPCL4,CCUPCL1,CCUPENC
          CALL     RDCCTM1
          IF       OVRCD=1
            UNPACK   KEY32,CCTMDPT,CCTMSER,CCTMPCT,CCTMEPS
            CALL     WRCCTM1
            MOVE     ONE,F1
          ENDIF
.
UPEXT100  PACK     KEY16,CCUPCL2,SP4,CCUPCL4,CCUPCL1
          CALL     RDCCEX1
          IF       OVRCD=0
            ADD      CCUPQTY,CCEXQTY
            ASSIGN   CCEXCST+(CCUPQTY*ACTCOST),CCEXCST
            ASSIGN   CCEXBUD+(CCUPQTY*BUDCOST),CCEXBUD
            ADD      F1,CCEXNPT
            CALL     UPCCEX1
          ELSE
            UNPACK   KEY16,CCEXDPT,CCEXSER,CCEXPCT
            MOVE     CCUPQTY,CCEXQTY
            ASSIGN   (CCUPQTY*ACTCOST),CCEXCST
            ASSIGN   (CCUPQTY*BUDCOST),CCEXBUD
            MOVE     F1,CCEXNPT
            CALL     WRCCEX1
          ENDIF
          RETURN
