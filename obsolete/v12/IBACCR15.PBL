. Enter Procedure Range
.------------------------------------------------------------
EPROC000   MATCH    FDEPART,LDEPART
           GOTO     EPROC005 IF EQUAL
           IF       ACCEPT=0
             MOVE     "Invalid Department Range - ",DISPMSG
             CALL     MESSAGE1
           ENDIF
           MOVE     "Start             ",FROMPROC
           MOVE     SP70,STRPCD
           MOVE     "Finish             ",TOPROC
           MOVE     "ZZZZZZZZ",ENDPCD
           GOTO     EPROC999
.
EPROC005   MOVE     "   15",FLDID
           CALL     GETPOSIT
           MOVE     CCHCHCD,CCPDHCD
           MOVE     LDEPART,CCPDDPT
           CALL     KCCPDA00
           BRANCH   EXIT,EPROC100,EPROC999
           MOVE     CCPDPCD,STRPCD
           PACK     FROMPROC,CCPDPCD,SP1,CCPDDES
           GOTO     EPROC150
EPROC100   MOVE     "Start             ",FROMPROC
           MOVE     SP70,STRPCD
.
EPROC150   MOVE     "   16",FLDID
           CALL     GETPOSIT
           MOVE     CCHCHCD,CCPDHCD
           MOVE     LDEPART,CCPDDPT
           CALL     KCCPDA00
           BRANCH   EXIT,EPROC200,EPROC999
           PACK     TOPROC,CCPDPCD,SP1,CCPDDES
           MOVE     CCPDPCD,ENDPCD
           MATCH    STRPCD,ENDPCD
           GOTO     EPROC999 IF EQUAL
           GOTO     EPROC999 IF NOT LESS
           BEEP
           MOVE     "To Pat. Serv. Must Be > From Pat. Serv. - ",DISPMSG
           CALL     MESSAGE1
           GOTO     EPROC000
EPROC200   MOVE     "Finish             ",TOPROC
           MOVE     "ZZZZZZZZ",ENDPCD
.
EPROC999   MOVE     "   15",FLDID
           CALL     GETPOSIT
           RETURN
