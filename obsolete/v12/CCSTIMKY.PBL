.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSTIMKY
.   Routine   :   HCCTMA00 Time Period Ending  
.   Return    :           
.-------------------------------------------------------------------------------
HCCTMA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "01",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCTMA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCTMA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Period Ending"      
.
          MOVE      STDCOL01,STDCOL02
          ADD       "14",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Level 1"            
.
          MOVE      STDCOL02,STDCOL03
          ADD       "11",STDCOL03
          DISPLAY   *PSTDCOL03:STDROW,*V2LON,*ULON,"Level 2"            
.
          MOVE      STDCOL03,STDCOL04
          ADD       "11",STDCOL04
          DISPLAY   *PSTDCOL04:STDROW,*V2LON,*ULON,"Level 3"            
.
          MOVE      STDCOL04,STDCOL05
          ADD       "11",STDCOL05
          DISPLAY   *PSTDCOL05:STDROW,*V2LON,*ULON,"Level 4"            
.
          MOVE      STDCOL05,STDCOL06
          ADD       "25",STDCOL06
HCCTMA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCTMDAT 
          PACK      KEY9,CCTMDEF,CCTMDAT,SP70                                   
          PACK      SAVKEY80,CCTMDEF,CCTMDAT,SP70                               
          SETLPTR   SAVKEY80,1                                                  
          CALL      RSCCTM1 
HCCTMA10
          CALL      RKCCTM1 
          BRANCH    OVRCD,HCCTMA15
          PACK      KEY9,CCTMDEF,CCTMDAT,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HCCTMA15 IF NOT EQUAL                                       
          BRANCH    CSEARCH,HCCTMA12
          CALL      HCCTMA60
          BRANCH    OVRCD,HCCTMA10
HCCTMA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY9,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          CALL      ECCTMA00
          DISPLAY   *PSTDCOL01:STDROW,CPCDATE 
          DISPLAY   *PSTDCOL02:STDROW,CCTMLV2 
          DISPLAY   *PSTDCOL03:STDROW,CCTMLV3 
          DISPLAY   *PSTDCOL04:STDROW,CCTMLV4 
          DISPLAY   *PSTDCOL05:STDROW,CCTMDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCTMA10 IF NOT EQUAL
HCCTMA15
          UNPACK    SAVKEY80,CCTMDEF,CCTMDAT                                    
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCTMA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCTMA30,HCCTMA35,HCCTMA40,HCCTMA45,HCCTMA04:
                              HCCTMA91,HCCTMA50
          GOTO      HCCTMA92
HCCTMA30
          CALL      RKCCTM1 
          BRANCH    OVRCD,HCCTMA85
          PACK      KEY9,CCTMDEF,CCTMDAT,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HCCTMA85 IF NOT EQUAL                                       
          CALL      RPCCTM1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCTMA10
HCCTMA35
          MOVE      ST1,KEY9 
          CALL      RDCCTM1 
          CALL      RPCCTM1 
          BRANCH    OVRCD,HCCTMA85
          PACK      KEY9,CCTMDEF,CCTMDAT,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HCCTMA85 IF NOT EQUAL                                       
          MATCH     SP70,CCTMDAT                                                
          GOTO      HCCTMA85 IF EQUAL                                           
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCTMA36
          CALL      RPCCTM1 
          BRANCH    OVRCD,HCCTMA37
          PACK      KEY9,CCTMDEF,CCTMDAT,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HCCTMA37 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCTMA36 IF NOT EQUAL
HCCTMA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCTMA10
HCCTMA40
          MOVE      ST1,KEY9 
          CALL      RDCCTM1 
          CALL      RPCCTM1 
          BRANCH    OVRCD,HCCTMA85
          PACK      KEY9,CCTMDEF,CCTMDAT,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HCCTMA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCTMA08
HCCTMA45
          CALL      RKCCTM1 
          BRANCH    OVRCD,HCCTMA85
          PACK      KEY9,CCTMDEF,CCTMDAT,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HCCTMA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY9,SAVKEY80,Z70                                           
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCTM1 
HCCTMA46
          CALL      RPCCTM1 
          BRANCH    OVRCD,HCCTMA47
          PACK      KEY9,CCTMDEF,CCTMDAT,SP70                                   
          MATCH     SAVKEY80,KEY9                                               
          GOTO      HCCTMA47 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCTMA46 IF NOT EQUAL
HCCTMA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCTMA10
HCCTMA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCTMA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          KEYIN     *PSTDCOL03:STDROW,*V2LON,SCHFLD03
          RESET     SCHFLD03
          KEYIN     *PSTDCOL04:STDROW,*V2LON,SCHFLD04
          RESET     SCHFLD04
          KEYIN     *PSTDCOL05:STDROW,*V2LON,SCHFLD05
          RESET     SCHFLD05
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCTMA08
HCCTMA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCTMA08
HCCTMA60  MOVE      ZERO,OVRCD
          CALL      ECCTMA00
          MATCH     SP70,SCHFLD01
          GOTO      HCCTMA61 IF EQUAL
          MATCH     SCHFLD01,CPCDATE 
          GOTO      HCCTMA72 IF NOT EQUAL
HCCTMA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCTMA62 IF EQUAL
          MATCH     SCHFLD02,CCTMLV2 
          GOTO      HCCTMA72 IF NOT EQUAL
HCCTMA62
          MATCH     SP70,SCHFLD03
          GOTO      HCCTMA63 IF EQUAL
          MATCH     SCHFLD03,CCTMLV3 
          GOTO      HCCTMA72 IF NOT EQUAL
HCCTMA63
          MATCH     SP70,SCHFLD04
          GOTO      HCCTMA64 IF EQUAL
          MATCH     SCHFLD04,CCTMLV4 
          GOTO      HCCTMA72 IF NOT EQUAL
HCCTMA64
          MATCH     SP70,SCHFLD05
          GOTO      HCCTMA65 IF EQUAL
          MATCH     SCHFLD05,CCTMDES 
          GOTO      HCCTMA72 IF NOT EQUAL
HCCTMA65
HCCTMA66
HCCTMA67
HCCTMA68
HCCTMA69
HCCTMA70
          GOTO      HCCTMA75
HCCTMA72  MOVE      ONE,OVRCD
HCCTMA75  RETURN
HCCTMA85
          LOAD      KEY9,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY9,CCTMDEF,CCTMDAT                                        
          CALL      RDCCTM1 
HCCTMA90
          UNPACK    SAVKEY80,CCTMDEF,CCTMDAT                                    
          BEEP
          GOTO      HCCTMA20
HCCTMA91
          MOVE      SP70,CCTMDES 
          MOVE      SP70,CCTMLV2 
          MOVE      SP70,CCTMLV3 
          MOVE      SP70,CCTMLV4 
          MOVE      SP70,CCTMSPA 
          MOVE      SP70,KEY9 
          MOVE      SP70,CCTMDAT 
          GOTO      HCCTMA96
HCCTMA92
          MOVE      SP70,KEY9 
          MOVE      SP70,CCTMDAT 
          LOAD      KEY9,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY9,CCTMDEF,CCTMDAT                                        
HCCTMA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCTMA99
          RETURN
.
ECCTMA00  UNPACK    CCTMDAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSTIMKY
.   Routine   :   KCCTMA00 Keyin Time Period Ending  
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCTMA00 TOPIC     "CCTMDAT "
          MATCH      SP70,CCTMDAT
          IF         @EQUAL
            PACK       KEY8,CCC,CYY,CMM,CDD
            REP        " 0",KEY8
            UNPACK     KEY8,CCENT,CYEAR,CMON,CDAY
          ELSE
            UNPACK     CCTMDAT,CCENT,CYEAR,CMON,CDAY
          ENDIF
.     
          CALL      KEYDATE
          BRANCH    CQUEST,KCCTMA05
          BRANCH    OVRCD,KCCTMA90
          PACK      CCTMDAT,CCENT,CYEAR,CMON,CDAY
          GOTO      KCCTMA10
.
KCCTMA05  MOVE      CCITEM,SCCITEM
          CALL      HCCTMA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCTMDAT 
          APPEND    SP70,CCTMDAT 
          RESET     CCTMDAT 
.
          MATCH     SP70,CCTMDAT 
          GOTO      KCCTMA00 IF EQUAL
.
          UNPACK    CCTMDAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          DISPLAY   *PCCOL:CVERT,*V2LON,CPCDATE                                 
KCCTMA10
          PACK      KEY9,CCTMDEF,CCTMDAT                                        
          CALL      RACCTM1 
          BRANCH    OVRCD,KCCTMA80
          BRANCH    CKEYTYP,KCCTMA75
          CALL      RDCCTM1 
KCCTMA70
          MOVE      ZERO,EXIT
          GOTO      KCCTMA99
KCCTMA75
          MOVE      "Time Period Ending Already Exist - ",DISPMSG                                    
          CALL      MESSAGE1
          GOTO      KCCTMA00
KCCTMA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCTMA99,KCCTMA99
          MOVE      "Time Period Ending Does Not Exist - ",DISPMSG                                   
          CALL      MESSAGE1
          GOTO      KCCTMA00
KCCTMA90
          MOVE      ONE,EXIT
          GOTO      KCCTMA99
KCCTMA95
          MOVE      TWO,EXIT
KCCTMA99 TOPIC    PRGID
          RETURN
