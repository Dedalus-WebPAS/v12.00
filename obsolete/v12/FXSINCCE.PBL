.*****************************************************************************
.*    Program      : FXSINCCE                                                *
.*    Modification :                                                         *
.*    ---------------------------------------------------------------------  *
.*         V9.04.00 29/03/2005 Peter Vela    CAR 59696                       *
.*                  Created fix program                                      *
.*****************************************************************************
.
          INC       STD001FD
          INC       SINCCEFD/INC
.
FINDFILE  FILE      ASCII, FIXED=51
.
OLDFILE   IFILE     SQL, FIXED=51, KEY="U1-5,11-16,6-10"                   
.OLDFILE1  IFILE     SQL, FIXED=142, KEY="D1-8,9-16,17-18,19-19"
.
.Old File Layout (Commodity Group Code is a DIM 5)
.Name     Type      Length Physical Start Description
.----     ----      ------ -------- ----- -----------
OLDSCST   DIM       5      5        1     Cost Centre
OLDSCOM   DIM       5      5        6     Commodity Group
OLDSDAT   DIM       6      6        11    Date CCYYMM
OLDSAMT   FORM      12.2   8        17    Amount (Requisitions + Imprest)
OLDSBUD   FORM      12.2   8        25    Budget
OLDSSPAR  DIM       18     18       33    Spare
.
.End of Record                      51
.
CMDLINE   DIM       100
CMDLINE1  DIM       100
DIM4      DIM       4
DIM4A     DIM       4
DIM6      DIM       6
DIM6A     DIM       6
DIM60     DIM       60
DIM60A    DIM       60
DDCENT    DIM       2
.
RECNUM    FORM      8
NEWFILE   DIM       60
NEWFILE1  DIM       60
NEWPATH   DIM       60
OLDPATH   DIM       60
DEFPATH   DIM       60
SAVEFLG   FORM      1
SP50      INIT    "                                                  "
SP60      INIT    "                                                            "
.
PRGNAM    INIT      "Conversion SINCCEFD"
PRGID     INIT      "FXSINCCE"
VERSION   INIT      "V12.00.00"
.
. Start of Program
.
MAIN0000  CALL      INIT0000                     * init and open files
          BRANCH    EXIT,MAIN9999                * init failure
.
          CALL      KDIR0000                     * Keyin directory
          BRANCH    EXIT,MAIN9999
.
          CALL      CONTQST                      * Ok to continue ?
          BRANCH    CEXIT,MAIN1000:              * Yes
                          MAIN0000:              * No
                          MAIN9999               * Cancel
.
MAIN1000  CALL      PREP0000                     * preparing files
          BRANCH    EXIT,MAIN9999
.
. Loop thru old file and WRSICE1 records to new file
.
          CALL      READ0000                     * Read old recs and WRSIGA1
          CALL      ENDP0000                     * Save/compress saved file
.
MAIN9999  CHAIN     PGM
+
.********************************************************************
.*                          INIT0000                                *
.*  Display heading and check that the files needed exist&open them *
.********************************************************************
.
INIT0000  CALL      DISPHEAD
          MOVE      ZERO,RECNUM
.
INIT9999  RETURN
+
.********************************************************************
.*                          KDIR0000                                *
.*  Keyin directories                                               *
.********************************************************************
.
KDIR0000  MOVE      ONE,SAVEFLG                  * Default to keep file
          KEYIN     *P1:10,*EL,"Do you want to save the original file (Y/N) ? ":
                    ANS;
          REP       "yYnN",ANS
          CMATCH    "Y",ANS
          GOTO      KDIR1000 IF EQUAL
          MOVE      ZERO,SAVEFLG                 * Not to keep file
          CMATCH    "N",ANS
          GOTO      KDIR1000 IF EQUAL
          BEEP
          GOTO      KDIR0000
.
KDIR1000  CALL      DEFT0000                     * Get the default path
          BRANCH    EXIT,KDIR9999
          MOVE      SP60,OLDPATH
.
          DISPLAY   *P1:24,*EL,"The directory path must end with a slash (/) ";
.
          MOVE      DEFPATH,OLDPATH
          BRANCH    SAVEFLG,KDIR2000
.
.         Keyin the path for the original file
.
          KEYIN     *P1:12,*EL,"Keyin path for temporarily saving original ":
                    "file: ":
                    *P10:13,*EL,*DV,*RV,OLDPATH:
                    *P10:13,OLDPATH;
          GOTO      KDIR3000
.
KDIR2000  KEYIN     *P1:12,*EL,"Keyin path for saving original file: ":
                    *P10:13,*EL,*DV,*RV,OLDPATH:
                    *P10:13,OLDPATH;
.
KDIR3000  ENDSET    OLDPATH
          APPEND    SP60,OLDPATH
          RESET     OLDPATH
.
          MATCH     SP60,OLDPATH
          IF        @EQUAL
            PACK      OLDPATH,DEFPATH            * Use the default
            DISPLAY   *P10:13,*EL,*V2LON,OLDPATH;
          ENDIF
.
          SQUEEZE   OLDPATH
          CMATCH    EXITCHAR,OLDPATH
          GOTO      KDIR9000 IF EQUAL
.
          PACK      DIM60,OLDPATH,SP60
          CALL      VALD0000                     * Check valid directory
          BRANCH    EXIT,KDIR1000
.
. Keyin the path for the new file
.
KDIR5000  MOVE      SP60,NEWPATH
          MOVE      DEFPATH,NEWPATH
          KEYIN     *P1:15,*EL,"Keyin path for the new file: ":
                    *P10:16,*EL,*DV,*RV,NEWPATH:
                    *P10:16,NEWPATH;
          ENDSET    NEWPATH
          APPEND    SP60,NEWPATH
          RESET     NEWPATH
.
          MATCH     SP60,NEWPATH
          IF        @EQUAL
            PACK      NEWPATH,DEFPATH            * Use the default
            DISPLAY   *P10:16,*V2LON,NEWPATH;
          ENDIF
.
          SQUEEZE   NEWPATH
          CMATCH    EXITCHAR,NEWPATH
          GOTO      KDIR9000 IF EQUAL
.
          PACK      DIM60,NEWPATH,SP60
          CALL      VALD0000                     * Check valid directory
          BRANCH    EXIT,KDIR5000
          GOTO      KDIR9999
.
KDIR9000  MOVE      ONE,EXIT
.
KDIR9999  DISPLAY   *P1:24,*EL;
          RETURN
+
.********************************************************************
.*                          PREP0000                                *
.*  Preparing files                                                 *
.********************************************************************
PREP0000  MOVE      ZERO,OVRCD
.
. Open old sincceaf file
.
          TRAP      OVERCOND IF IO
          OPEN      OLDFILE,"sincceaf"          
          TRAPCLR   IO
          BRANCH    OVRCD,PREP5000               * sincgaaf file not found 
          CLOSE     OLDFILE
.
. Open old sinaudga audit file
.
.         MOVE      ZERO,OVRCD
.         TRAP      OVERCOND IF IO
.         OPEN      OLDFILE1,"sinaudga"          
.         TRAPCLR   IO
.         BRANCH    OVRCD,PREP5100               * sinaudga file not found 
.         CLOSE     OLDFILE1
.
          DISPLAY   *P1:24,*EL,"Copying files ... Please wait !! ";
.
. Copy old sincceaf.dat file to the keyin directory
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "cp `ldat sincceaf.dat` ",CMDLINE
          APPEND    OLDPATH,CMDLINE
          APPEND    "oldcceaf.dat",CMDLINE
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
          CMATCH    "0",TASKID
          GOTO      PREP8000 IF NOT EQUAL
.
. Copy old sincceaf.idx file to the keyin directory
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "cp `ldat sincceaf.idx` ",CMDLINE
          APPEND    OLDPATH,CMDLINE
          APPEND    "oldcceaf.idx",CMDLINE
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
          CMATCH    "0",TASKID
          GOTO      PREP8000 IF NOT EQUAL
.
. Copy old sinaudga.dat file to the keyin directory
.
.         PACK      CMDLINE,SP60,SP60
.         CLEAR     CMDLINE
.         APPEND    "cp `ldat sinaudga.dat` ",CMDLINE
.         APPEND    OLDPATH,CMDLINE
.         APPEND    "oldaudga.dat",CMDLINE
.         APPEND    SP60,CMDLINE
.         RESET     CMDLINE
.         EXECUTE   CMDLINE,TASKID
.
.         CMATCH    "0",TASKID
.         GOTO      PREP8100 IF NOT EQUAL
.
. Copy old sinaudga.idx file to the keyin directory
.
.         PACK      CMDLINE,SP60,SP60
.         CLEAR     CMDLINE
.         APPEND    "cp `ldat sinaudga.idx` ",CMDLINE
.         APPEND    OLDPATH,CMDLINE
.         APPEND    "oldaudga.idx",CMDLINE
.         APPEND    SP60,CMDLINE
.         RESET     CMDLINE
.         EXECUTE   CMDLINE,TASKID
.
.         CMATCH    "0",TASKID
.         GOTO      PREP8100 IF NOT EQUAL
.
. Remove the original sincceaf files
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "iserase ",CMDLINE
          APPEND    DEFPATH,CMDLINE
          APPEND    "sincceaf",CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
          CMATCH    "0",TASKID
          IF        !@EQUAL
            DISPLAY   *P1:24,*EL,*B,"Unable to remove the original file.  ";
            CALL      HITENTER
            GOTO      PREP9000
          ENDIF
.
. Remove the original sinaudga files
.
.         PACK      CMDLINE,SP60,SP60
.         CLEAR     CMDLINE
.         APPEND    "iserase ",CMDLINE
.         APPEND    DEFPATH,CMDLINE
.         APPEND    "sinaudga",CMDLINE
.         RESET     CMDLINE
.         EXECUTE   CMDLINE,TASKID
.
.         CMATCH    "0",TASKID
.         IF        !@EQUAL
.           DISPLAY   *P1:24,*EL,*B,"Unable to remove the original ":
.                     "audit file.  ";
.           CALL      HITENTER
.           GOTO      PREP9000
.         ENDIF
.
. Opening old sincceaf file after being copied
.
          MOVE      SP60,DIM60
          CLEAR     DIM60
          APPEND    OLDPATH,DIM60
          APPEND    "oldcceaf",DIM60
          APPEND    SP60,DIM60
          RESET     DIM60
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      OLDFILE,DIM60
          TRAPCLR   IO
          IF        OVRCD = 1
            DISPLAY   *P1:24,*EL,*B,"Unable to open saved original file.  ";
            CALL      HITENTER
            GOTO      PREP9000
          ENDIF
.
. Opening old sinaudga file after being copied
.
.         MOVE      SP60,DIM60
.         CLEAR     DIM60
.         APPEND    OLDPATH,DIM60
.         APPEND    "oldaudga",DIM60A
.         APPEND    SP60,DIM60A
.         RESET     DIM60A
.
.         MOVE      ZERO,OVRCD
.         TRAP      OVERCOND IF IO
.         OPEN      OLDFILE1,DIM60A
.         TRAPCLR   IO
.         IF        OVRCD = 1
.           DISPLAY   *P1:24,*EL,*B,"Unable to open saved original ":
.                     "audit file.  ";
.           CALL      HITENTER
.           CALL      PREP9000
.         ENDIF
.
. Create the new sincceaf file
.
          DISPLAY   *P1:24,*EL,"Creating new files ... Please wait !! ";
          PACK      NEWFILE,SP60
          CLEAR     NEWFILE
          APPEND    NEWPATH,NEWFILE
          APPEND    "sincceaf",NEWFILE
          APPEND    SP60,NEWFILE
          RESET     NEWFILE
          SQUEEZE   NEWFILE
.
. Create the new sinaudga file
.
.         DISPLAY   *P1:24,*EL,"Creating new audit files ... Please wait !! ";
.         PACK      NEWFILE1,SP60
.         CLEAR     NEWFILE1
.         APPEND    NEWPATH,NEWFILE1
.         APPEND    "sinaudga",NEWFILE1
.         APPEND    SP60,NEWFILE1
.         RESET     NEWFILE1
.         SQUEEZE   NEWFILE1
.
. Build files and indexes
.
PREP1000  PACK      CMDLINE,SP60,SP60
.
. Build primary index for sincceaf - isbuild sincceaf 51 UC1-5,12-17,6-11 
. 
          CLEAR     CMDLINE
          APPEND    "isbuild ",CMDLINE
          APPEND    NEWFILE,CMDLINE
.
          APPEND    " 51 UC1-5,12-17,6-11",CMDLINE
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
. Add 2nd index for sincceaf - isadd sincceaf UC1-5,6-11,12-17
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "isadd ",CMDLINE
          APPEND    NEWFILE,CMDLINE
          APPEND    " UC1-5,6-11,12-17",CMDLINE
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
. Build primary index for sinaudga - isbuild sinaudga 142 DC1-8,9-16,17-18,19-19
. 
.         PACK      CMDLINE1,SP60,SP60
.         CLEAR     CMDLINE1
.         APPEND    "isbuild ",CMDLINE1
.         APPEND    NEWFILE1,CMDLINE1
.
.         APPEND    " 142 DC1-8,9-16,17-18,19-19",CMDLINE1
.         APPEND    SP60,CMDLINE1
.         RESET     CMDLINE1
.         EXECUTE   CMDLINE1,TASKID
.
. Open new files
.
          OPEN      SINCCEA1,NEWFILE
.         OPEN      SINAUDGA,NEWFILE1
.
. Set the flag to say we want to continue
.
          DISPLAY   *P1:24,*EL;
          MOVE      ZERO,EXIT
          CALL      IBACLOCK
          REP       " 0",CDATE
          DISPLAY   *P1:18,"Started : ",CDATE,SP1,CTIMEIS:
                    *P1:20,"Record : "
          GOTO      PREP9999
.
PREP5000  DISPLAY   *P1:24,*EL,*B,"Old file not found.  ";
          CALL      HITENTER
          GOTO      PREP9000
.
.PREP5100  DISPLAY   *P1:24,*EL,*B,"Old audit file not found.  ";
.          CALL      HITENTER
.          GOTO      PREP9000
.
PREP8000  DISPLAY   *P1:24,*EL,*B,"Unable to copy original file.  ":
                    "(Error: ",TASKID,")  ";
          CALL      HITENTER
          GOTO      PREP9000
.
.PREP8100  DISPLAY   *P1:24,*EL,*B,"Unable to copy original audit file.  ":
.                    "(Error: ",TASKID,")  ";
.          CALL      HITENTER
.
PREP9000  MOVE      ONE,EXIT
.
PREP9999  RETURN
+
.**********************************************************************
.*                               READ0000                             *
.*             loop thru old file and WRSICE1 each record             *
.**********************************************************************
.
READ0000  DISPLAY   *P1:20,*EL,"Record No. : ";
          PACK      KEY16,SP60
          CALL      READSOLD                      * position at start
READ1000  CALL      READKOLD                      * read next record
          BRANCH    OVRCD,READ6000                * no more records
          ADD       ONE,RECNUM                    * update record counter
.
          MOVE      OLDSCST,SICECST               * Moving from a DIM5 to DIM6
          PACK      SICECOM,OLDSCOM,SP70          * All the rest of the
          MOVE      OLDSDAT,SICEDAT               * variables are the same
          MOVE      OLDSAMT,SICEAMT
          MOVE      OLDSBUD,SICEBUD
          MOVE      OLDSSPAR,SICESPAR
.   
          PACK      KEY17,SICECST,SICEDAT,SICECOM,SP70
          CALL      WRSICE1                       * WRSICE1 to new file
.
          IF        (RECNUM%1000) = 0
            DISPLAY   *P15:20,*V2LON,RECNUM
          ENDIF
          GOTO      READ1000                      * get next record
.
READ6000  CLOSE     SINCCEA1                      * close new file
          CLOSE     OLDFILE                       * close old file
.
          CALL      IBACLOCK
          REP       " 0",CDATE
          DISPLAY   *P1:22,"Ended  : ",CDATE,SP1,CTIMEIS
.
READ9999  RETURN
+
.**********************************************************************
.*                               RAUD0000                             *
.*  Loop through old audit file and write record to new audit file    *
.**********************************************************************
.
.RAUD0000  DISPLAY   *P1:20,*EL,"Record No. : ";
.          MOVE      ZERO,RECNUM
.          PACK      KEY19,SP20
.          CALL      RDSOLD1                      * Position at start
.RAUD1000  CALL      RDKOLD1                      * Read next record
.          BRANCH    OVRCD,RAUD6000               * No more records
.          ADD       ONE,RECNUM                   * Update record counter
.
.          PACK      KEY19,CTCOAUDD,CTCOAUDT,CTCOAUDP,CTCOAUDR
.          CALL      AWCTCOM                      * Write to new audit fiel 
..
.          IF        (RECNUM%1000) = 0
.            DISPLAY   *P15:20,*V2LON,RECNUM
.          ENDIF
.          GOTO      RAUD1000                      * Get next record
..
.RAUD6000  CLOSE     CATAUDCO                      * Close new audit file
.          CLOSE     OLDFILE1                      * Close old audit file
..
.          CALL      IBACLOCK
.          REP       " 0",CDATE
.          DISPLAY   *P1:22,"Ended  : ",CDATE,SP1,CTIMEIS
..
.RAUD9999  RETURN
.+
.**********************************************************************
.*                               VALD0000                             *
.*        Check if it a valid directory                               *
.**********************************************************************
.
VALD0000  MOVE      ZERO,EXIT
          SQUEEZE   DIM60
          ENDSET    DIM60
          CMATCH    SLASH,DIM60
          IF        !@EQUAL
            DISPLAY   *P1:24,*EL,*B,"Directory path must end with a slash (/) ";
            CALL      HITENTER
            GOTO      VALD9000
          ENDIF
          RESET     DIM60
.
          PACK      CMDLINE,SP60,SP60
          CLEAR     CMDLINE
          APPEND    "cd ",CMDLINE
          APPEND    DIM60,CMDLINE
          APPEND    SP60,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID      * check if exists
.
          CMATCH    "0",TASKID
          IF        !@EQUAL
            DISPLAY   *P1:24,*EL,*B,"Directory does not exist ";
            CALL      HITENTER
            GOTO      VALD9000
          ENDIF
          GOTO      VALD9999
.
VALD9000  MOVE      ONE,EXIT            * Not valid
.
VALD9999  RETURN
+
.**********************************************************************
.*                               ENDP0000                             *
.*        Remove or compress save file                                *
.**********************************************************************
.
ENDP0000  MOVE      SP60,DIM60
          MOVE      SP60,DIM60A
.
. Set up the saved oldcceaf file with path
.
          CLEAR     DIM60
          APPEND    OLDPATH,DIM60        
          APPEND    "oldcceaf*",DIM60
          APPEND    SP60,DIM60
          RESET     DIM60
          SQUEEZE   DIM60
.
. Set up the saved oldaudga file with path
.
.         CLEAR     DIM60A
.         APPEND    OLDPATH,DIM60A      
.         APPEND    "oldaudga*",DIM60A
.         APPEND    SP60,DIM60A
.         RESET     DIM60A
.         SQUEEZE   DIM60A
.
          PACK      CMDLINE,SP60,SP60
.         PACK      CMDLINE1,SP60,SP60
          CLEAR     CMDLINE
.         CLEAR     CMDLINE1
.
. The saved files are compressed to keep or they are removed
. 
          IF        SAVEFLG = 1
            APPEND    "compress -f ",CMDLINE
            APPEND    DIM60,CMDLINE
            APPEND    SP60,CMDLINE
            RESET     CMDLINE
            DISPLAY   *P1:24,*EL,"Compressing original file.. ":
                      "Please wait !!  ";
.
.           APPEND    "compress -f ",CMDLINE1
.           APPEND    DIM60A,CMDLINE1
.           APPEND    SP60,CMDLINE1
.           RESET     CMDLINE1
.           DISPLAY   *P1:24,*EL,"Compressing original audit file.. ":
.                     "Please wait !!  ";
          ELSE
            APPEND    "rm ",CMDLINE
            APPEND    DIM60,CMDLINE
            APPEND    SP60,CMDLINE
            RESET     CMDLINE
            DISPLAY   *P1:24,*EL,"Removing original file.. Please wait !!  ";
.
.           APPEND    "rm ",CMDLINE1
.           APPEND    DIM60A,CMDLINE1
.           APPEND    SP60,CMDLINE1
.           RESET     CMDLINE1
.           DISPLAY   *P1:24,*EL,"Removing original audit file.. ":
.                     "Please wait !!  ";
          ENDIF
.
          EXECUTE   CMDLINE,TASKID               * Check if exists
.
          CMATCH    "0",TASKID
          IF        !@EQUAL
            DISPLAY   *P1:24,*EL,*B,"Saved file not compressed or removed.  ";
            CALL      HITENTER
            GOTO      ENDP9999
          ENDIF
.
.         EXECUTE   CMDLINE1,TASKID              * Check if exists
.
.         CMATCH    "0",TASKID
.         IF        !@EQUAL
.           DISPLAY   *P1:24,*EL,*B,"Saved audit file not compressed ":
.                     "or removed.  ";
.           CALL      HITENTER
.           GOTO      ENDP9999
.         ENDIF
.
          CALL      IBACLOCK
          DISPLAY   *P1:24,*EL,*B,"Finish processing.  ";
          CALL      HITENTER
.
ENDP9999  RETURN
+
.**********************************************************************
.         Get the default directory
.**********************************************************************
.
DEFT0000  EXECUTE   "ldat sincceaf.dat > fxsincce.txt",TASKID
.
          PACK      DEFPATH,SP30,SP30
          CLOSE     FINDFILE
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      FINDFILE,"fxsincce"
          TRAPCLR   IO
          BRANCH    OVRCD,DEFT3000
.
          READ      FINDFILE,SEQ;DEFPATH
          GOTO      DEFT3000 IF OVER
.
          ENDSET    DEFPATH
DEFT1000  CMATCH    "/",DEFPATH
          GOTO      DEFT2000 IF EQUAL
.
          BUMP      DEFPATH,-1
          GOTO      DEFT1000 IF NOT EOS
          GOTO      DEFT3000
.
DEFT2000  LENSET    DEFPATH
          RESET     DEFPATH
.
          PACK      DEFPATH,DEFPATH,SP30,SP30,SP30,SP30
          STRIP     DEFPATH
          ENDSET    DEFPATH
          RESET     DEFPATH
          CLOSE     FINDFILE
          MOVE      ZERO,EXIT
          GOTO      DEFT9999
.
DEFT3000  DISPLAY   *P1:24,*EL,*B,"Error finding sincceaf.  ";
          CALL      HITENTER
          CLOSE     FINDFILE
          MOVE      ONE,EXIT
DEFT9999  RETURN
+
.**********************************************************************
. ---- OLD IO ROUTINES ----
.**********************************************************************
.  Copy and paste the Positional Read routine from the original
.  IO file, rename the routine READSOLD and change the index name
.  to OLDFILE
.
READSOLD  MOVE      ZERO,OVRCD
          RESET     KEY16
          READ      OLDFILE,KEY16;;
          RETURN
.
.  Copy and paste the Read Next routine from the original
.  IO file, rename the routine READKOLD and change the index name 
.  to OLDFILE. Careful of variables that have changed length
.
READKOLD  MOVE      ZERO,OVRCD
          READKS    OLDFILE;OLDSCST,OLDSCOM,OLDSDAT,OLDSAMT,OLDSBUD:
                        OLDSSPAR
          GOTO      OVERCOND IF OVER
          RETURN
.
.
. Audits
.
.
.RDSOLD1   RESET     KEY19
.          READ      OLDFILE1,KEY19;;
.          RETURN
.
.RDKOLD1   MOVE      ZERO,OVRCD
.          READKS    OLDFILE1;CTCOAUDD,CTCOAUDT,CTCOAUDP,CTCOAUDR:
.                             CTCOAUDS,CTCOAUDO:
.                             DCTCOADM,CTCODATE,DCTCOMEA,CTCOMENU:
.                             CTCODIET,CTCOWARD,CTCOSIZE,CTCOSTAT:
.                             CTCOBELT,CTCOMTYP,CTCOPRNT,CTCOIT01:
.                             CTCOIT02:
.                             CTCOIT03,CTCOIT04,CTCOIT05,CTCOIT06:
.                             CTCOIT07,CTCOIT08,CTCOIT09,CTCOIT10:
.                             CTCOIT11,CTCOIT12,CTCOIT13,CTCOIT14:
.                             CTCOIT15,CTCOIT16,CTCOIT17,CTCOIT18:
.                             CTCOIT19,CTCOIT20,CTCODNAM,CTCOPAGN:
.                             CTCOOTHR,CTCODTYP,CTCOALVL,CTCOSPAR
.          GOTO      OVERCOND IF OVER
.          MOVE      DCTCOADM,CTCOADMN
.          MOVE      DCTCOMEA,CTCOMEAL
.          RETURN
.
. I/O Includes
.
          INC       SINCCEIO/INC
          INC       STD001IO
+
