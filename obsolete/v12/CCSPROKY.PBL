.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSPROKY
.   Routine   :   HCCPRA00 Profile Code        
.   Return    :           
.-------------------------------------------------------------------------------
HCCPRA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "30",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCPRA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCPRA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Profile"            
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 8",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HCCPRA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCPRPRO 
          PACK      KEY4,CCPRPRO,SP70                                           
.                                                                               
.                                                                               
          CALL      RSCCPR1 
HCCPRA10
          CALL      RKCCPR1 
          BRANCH    OVRCD,HCCPRA15
          PACK      KEY4,CCPRPRO,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HCCPRA12
          CALL      HCCPRA60
          BRANCH    OVRCD,HCCPRA10
HCCPRA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY4,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCPRPRO 
          DISPLAY   *PSTDCOL02:STDROW,CCPRDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCPRA10 IF NOT EQUAL
HCCPRA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCPRA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCPRA30,HCCPRA35,HCCPRA40,HCCPRA45,HCCPRA04:
                              HCCPRA91,HCCPRA50
          GOTO      HCCPRA92
HCCPRA30
          CALL      RKCCPR1 
          BRANCH    OVRCD,HCCPRA85
          PACK      KEY4,CCPRPRO,SP70                                           
.                                                                               
.                                                                               
          CALL      RPCCPR1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPRA10
HCCPRA35
          MOVE      ST1,KEY4 
          CALL      RDCCPR1 
          CALL      RPCCPR1 
          BRANCH    OVRCD,HCCPRA85
          PACK      KEY4,CCPRPRO,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCPRA36
          CALL      RPCCPR1 
          BRANCH    OVRCD,HCCPRA37
          PACK      KEY4,CCPRPRO,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCPRA36 IF NOT EQUAL
HCCPRA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCPRA10
HCCPRA40
          MOVE      ST1,KEY4 
          CALL      RDCCPR1 
          CALL      RPCCPR1 
          BRANCH    OVRCD,HCCPRA85
          PACK      KEY4,CCPRPRO,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPRA08
HCCPRA45
          CALL      RKCCPR1 
          BRANCH    OVRCD,HCCPRA85
          PACK      KEY4,CCPRPRO,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY4,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCPR1 
HCCPRA46
          CALL      RPCCPR1 
          BRANCH    OVRCD,HCCPRA47
          PACK      KEY4,CCPRPRO,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCPRA46 IF NOT EQUAL
HCCPRA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCPRA10
HCCPRA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCPRA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPRA08
HCCPRA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPRA08
HCCPRA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCPRA61 IF EQUAL
          MATCH     SCHFLD01,CCPRPRO 
          GOTO      HCCPRA72 IF NOT EQUAL
HCCPRA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCPRA62 IF EQUAL
          MATCH     SCHFLD02,CCPRDES 
          GOTO      HCCPRA72 IF NOT EQUAL
HCCPRA62
HCCPRA63
HCCPRA64
HCCPRA65
HCCPRA66
HCCPRA67
HCCPRA68
HCCPRA69
HCCPRA70
          GOTO      HCCPRA75
HCCPRA72  MOVE      ONE,OVRCD
HCCPRA75  RETURN
HCCPRA85
          LOAD      KEY4,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY4,CCPRPRO                                                
          CALL      RDCCPR1 
HCCPRA90
.                                                                               
          BEEP
          GOTO      HCCPRA20
HCCPRA91
          MOVE      SP70,CCPRDES 
          MOVE      ZERO,CCPRP01 
          MOVE      ZERO,CCPRP02 
          MOVE      ZERO,CCPRP03 
          MOVE      ZERO,CCPRP04 
          MOVE      ZERO,CCPRP05 
          MOVE      ZERO,CCPRP06 
          MOVE      ZERO,CCPRP07 
          MOVE      ZERO,CCPRP08 
          MOVE      ZERO,CCPRP09 
          MOVE      ZERO,CCPRP10 
          MOVE      ZERO,CCPRP11 
          MOVE      ZERO,CCPRP12 
          MOVE      ZERO,CCPRP13 
          MOVE      SP70,CCPRSPA 
          MOVE      SP70,KEY4 
          MOVE      SP70,CCPRPRO 
          GOTO      HCCPRA96
HCCPRA92
          MOVE      SP70,KEY4 
          MOVE      SP70,CCPRPRO 
          LOAD      KEY4,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY4,CCPRPRO                                                
HCCPRA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCPRA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSPROKY
.   Routine   :   KCCPRA00 Keyin Profile Code        
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCPRA00 TOPIC     "CCPRPRO "
          MOVE      UNDLN70,CCPRPRO 
          KEYIN     *PCCOL:CVERT,*DV,CCPRPRO:                                   
                    *PCCOL:CVERT,*V2LON,CCPRPRO:                                
                    *PCCOL:CVERT,*DV,CCPRPRO 
.
          ENDSET    CCPRPRO 
          APPEND    SP70,CCPRPRO 
          RESET     CCPRPRO 
.
          MATCH     SP70,CCPRPRO 
          GOTO      KCCPRA90 IF EQUAL
.
          CMATCH    EXITCHAR,CCPRPRO 
          GOTO      KCCPRA95 IF EQUAL
.
          CMATCH    QUEST,CCPRPRO 
          GOTO      KCCPRA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCCPRA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCPRPRO 
          APPEND    SP70,CCPRPRO 
          RESET     CCPRPRO 
.
          MATCH     SP70,CCPRPRO 
          GOTO      KCCPRA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CCPRPRO                                 
KCCPRA10
          PACK      KEY4,CCPRPRO                                                
          CALL      RACCPR1 
          BRANCH    OVRCD,KCCPRA80
          BRANCH    CKEYTYP,KCCPRA75
          CALL      RDCCPR1 
KCCPRA70
          MOVE      ZERO,EXIT
          GOTO      KCCPRA99
KCCPRA75
          MOVE      "Profile Code Already Exist - ",DISPMSG                                          
          CALL      MESSAGE1
          GOTO      KCCPRA00
KCCPRA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCPRA99,KCCPRA99
          MOVE      "Profile Code Does Not Exist - ",DISPMSG                                         
          CALL      MESSAGE1
          GOTO      KCCPRA00
KCCPRA90
          MOVE      ONE,EXIT
          GOTO      KCCPRA99
KCCPRA95
          MOVE      TWO,EXIT
KCCPRA99 TOPIC    PRGID
          RETURN
