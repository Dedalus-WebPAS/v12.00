          MOVE      "Start    ",FROMDAT
          MOVE      "Finish   ",TODEB
          MOVE      "Finish   ",TODEP
          MOVE      "Finish   ",TOPTY
          MOVE      "Finish   ",TODAT
          MOVE      SP70,STRDEB
          MOVE      SP70,STRDEP
          MOVE      SP70,STRPTY
          MOVE      SP70,STRDAT
          MOVE      Z70,ENDDEB
          MOVE      Z70,ENDDEP
          MOVE      Z70,ENDPTY
          MOVE      Z70,ENDDAT
          MOVE      "debtexpt",EXPORTFN
          MOVE      ONE,INCINV
          MOVE      ONE,INCCRD
          MOVE      ONE,INCPAY
          MOVE      ONE,INCDRA
          MOVE      ONE,INCCRA
          MOVE      ONE,INCDRA
          MOVE      ONE,INCCRA
          MOVE      ZERO,PRTREP
          RETURN
.
DEFDATE1  MOVE     CCC,CCENT
          MOVE     CYY,CYEAR
          MOVE     CMM,CMON
          MOVE     CDD,CDAY
          REP      " 0",CCENT
          REP      " 0",CYEAR
          REP      " 0",CMON
          REP      " 0",CDAY
          RETURN
.-----------------------------
. Print Report
.-----------------------------
REPT0000  DISPLAY   *P1:24,*EL,"Extracting Transactions ..."
          MOVE      ZERO,TOTEXT
          MOVE      ZERO,TOTTAX
          MOVE      ZERO,TOTTOT
          CALL      MAKEXP00
          MOVE      ZERO,SECTOR
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
          MOVE      SP8,SAVEDEBT
          MOVE      ZERO,CPAGENO
          PACK      KEY8,STRDEB,SP70
          CALL      RDDBDB1
          IF        OVRCD=0
            CALL      RPDBDB1
          ENDIF
REPT1100  CALL      RKDBDB1
          BRANCH    OVRCD,REPT1900
          MATCH     DBDBDEB,ENDDEB
          GOTO      REPT1900 IF LESS
          ADD       ONE,SECTOR
          IF        SECTOR%100=0
            DISPLAY   *P40:24,SECTOR
          ENDIF
          MOVE      DBDBDEB,DIM8
          CALL      GETCDS00    * Get Debtor User Def Codes
          CALL      EXTR0000    * Extract to Temp File for Sorting
          GOTO      REPT1100
.
REPT1900  CALL      TRAN0000    * Enter Transfer Script
          IF        PRTREP=1
            CALL      UND132
            PRINT     *1,"Report Total",*86,TOTEXT,*102,TOTTAX,*118,TOTTOT
            CALL      UND132
          ENDIF
.
          MOVE      ZERO,EXIT
          GOTO      REPT9999
.
REPT9100  MOVE      FOUR,EXIT
          GOTO      REPT9999
.
REPT9999  RETURN
.----------------------------------------------------------------------
. EXTR0000 - Extract Outstanding Transaction into Temp File
.----------------------------------------------------------------------
EXTR0000  PACK      KEY29,DBDBDEB,STRDAT,SP70
          CALL      RSDBFH2            * Process all transactions paid after
EXTR1000  CALL      RKDBFH2            * the as at date.
          BRANCH    OVRCD,EXTR9999
          MATCH     DBFHDEB,DBDBDEB    * End of Debtor
          GOTO      EXTR9999 IF NOT EQUAL
          MATCH     DBFHDDAT,ENDDAT    * End of Date Range
          GOTO      EXTR9999 IF LESS
.
          PACK      KEY24,DBFHDEB,DBFHDTY,DBFHDOC,SP70
          CALL      RSDBFD1            * Process all transactions paid after
EXTR1100  CALL      RKDBFD1            * the as at date.
          BRANCH    OVRCD,EXTR1000
          PACK      KEY21,DBFHDEB,DBFHDTY,DBFHDOC,SP70
          PACK      KEY24,DBFDDEB,DBFDDTY,DBFDDOC,DBFDDLN,SP70
          MATCH     KEY21,KEY24
          GOTO      EXTR1000 IF NOT EQUAL
          CALL      RANGE000           * Check Department/Product Type Range
          BRANCH    EXIT,EXTR1100
          MOVE      DBFDDTY,F1
          LOAD      TRANTYPE,F1,TRANINV,TRANCRD,TRANPAY,TRANDRA,TRANCRA
          LOAD      TRNTYP,F1,TRNINV,TRNCRD,TRNPAY,TRNDRA,TRNCRA
          IF        F1=2 | F1=3 | F1=5
            MULT      SEQ,DBFDQTY
            MULT      SEQ,DBFDTAX
            MULT      SEQ,DBFDTOT
          ENDIF
.
          ASSIGN    DBFDTOT+DBFDTAX,F12P2
          UNPACK    DBFDIDAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          MOVE      CPCDATE,IDATE
          UNPACK    DBFHDDAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          MOVE      CPCDATE,DDATE
          MOVE      SP70,PDATE
          MATCH     "99999999",DBFDPDAT
          IF        !@EQUAL
            UNPACK    DBFDPDAT,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
            MOVE      CPCDATE,PDATE
          ENDIF
.
          CALL      WRTEXP00
.
          COMPARE   ONE,PRTREP
          GOTO      EXTR1100 IF NOT EQUAL
.
          IF        CLNO>56
            CALL      HEAD132
            CALL      UND132
            PRINT     *1,"Debtor",*14,"Debtor Name":
                      *N,*1,"Reference",*17,"Tran Date",*28,"Item":
                      *37,"Item Description",*59,"Quantity",*80,"Price":
                      *87,"Extended Total",*114,"Tax",*128,"Total"
            ADD       TWO,CLNO
            CALL      UND132
          ENDIF
.
          MATCH     SAVEDEBT,DBDBDEB
          IF        !@EQUAL
            MATCH     SP8,SAVEDEBT
            IF        !@EQUAL
              PRINT     " "
              ADD       ONE,CLNO
            ENDIF
            PRINT     *1,DBDBDEB,*14,DBDBNA1
            ADD       ONE,CLNO
            MOVE      DBDBDEB,SAVEDEBT
          ENDIF
.
          MOVE      DBITDES,DIM18
          MATCH     SAVEDOC,DBFDDOC
          IF        @EQUAL
            PRINT     *28,DBFDITM,*37,DIM18:
                      *56,DBFDQTY,*68,DBFDPRI,*86,DBFDTOT,*102,DBFDTAX:
                      *118,F12P2
          ELSE
            PRINT     *1,DBFDDOC,*14,TRNTYP,*17,DDATE,*28,DBFDITM,*37,DIM18:
                      *56,DBFDQTY,*68,DBFDPRI,*86,DBFDTOT,*102,DBFDTAX:
                      *118,F12P2
            MOVE      DBFDDOC,SAVEDOC
          ENDIF
          ADD       ONE,CLNO
          ADD       DBFDTOT,TOTEXT
          ADD       DBFDTAX,TOTTAX
          ADD       F12P2,TOTTOT
.
          GOTO      EXTR1100
.
EXTR9999  RETURN
.--------------------------------------------------------
. Check Ranges
.--------------------------------------------------------
RANGE000  PACK      KEY8,DBFDITM
          CALL      RDDBIT1
          BRANCH    OVRCD,RANGE810
          PACK      KEY8,DBITPTY
          CALL      RDDBPT1
          BRANCH    OVRCD,RANGE820
          PACK      KEY8,DBPTDEP
          CALL      RDDBDE1
          BRANCH    OVRCD,RANGE830
.
          MATCH     STRPTY,DBITPTY    * Product Type Range
          GOTO      RANGE900 IF LESS
          MATCH     DBITPTY,ENDPTY
          GOTO      RANGE900 IF LESS
.
RANGE100  MATCH     STRDEP,DBPTDEP    * Department Range
          GOTO      RANGE900 IF LESS
          MATCH     DBPTDEP,ENDDEP
          GOTO      RANGE900 IF LESS
.
          MOVE      DBFDDTY,F1
          BRANCH    INCINV,RANGE210        * Include Invoices 1=Yes
          BRANCH    F1,RANGE900
.
RANGE210  BRANCH    INCCRD,RANGE220        * Include Credits  1=Yes
          BRANCH    F1,RANGE220,RANGE900
.
RANGE220  BRANCH    INCPAY,RANGE230        * Include Payments 1=Yes
          BRANCH    F1,RANGE230,RANGE230,RANGE900
.
RANGE230  BRANCH    INCDRA,RANGE240        * Include DR Adj 1=Yes
          BRANCH    F1,RANGE240,RANGE240,RANGE240,RANGE900
.
RANGE240  BRANCH    INCCRA,RANGE250        * Include CR Adj 1=Yes
          BRANCH    F1,RANGE250,RANGE250,RANGE250,RANGE250,RANGE900
.
RANGE250  MOVE      ZERO,EXIT
          GOTO      RANGE999
.
RANGE810  CLEAR     DISPMSG
          APPEND    "ERROR : Sales Item Not On File - ",DISPMSG
          APPEND    KEY8,DISPMSG
          APPEND    " - ",DISPMSG
          RESET     DISPMSG
          BEEP
          CALL      MESSAGE1
          GOTO      RANGE900
.
RANGE820  CLEAR     DISPMSG
          APPEND    "ERROR : Product Type Not On File - ",DISPMSG
          APPEND    KEY8,DISPMSG
          APPEND    " - ",DISPMSG
          RESET     DISPMSG
          BEEP
          CALL      MESSAGE1
          GOTO      RANGE900
.
RANGE830  CLEAR     DISPMSG
          APPEND    "ERROR : Department Not On File - ",DISPMSG
          APPEND    KEY8,DISPMSG
          APPEND    " - ",DISPMSG
          RESET     DISPMSG
          BEEP
          CALL      MESSAGE1
          GOTO      RANGE900
.
RANGE900  MOVE      ONE,EXIT
.
RANGE999  RETURN
.---------------------------------------------------
. Get User Def Code Descriptions
.---------------------------------------------------
GETCDS00   MOVE     SP70,DBDBUC1D
           MOVE     SP70,DBDBUC2D
           MOVE     SP70,DBDBUC3D
           MOVE     SP70,DBDBUC4D
           MATCH    SP70,DBDBUC1
           IF       !@EQUAL
             MOVE     "DB01",DBCDCAT
             PACK     KEY7,DBCDCAT,DBDBUC1
             CALL     RDDBCD1
             IF       OVRCD=0
               MOVE     DBCDDES,DBDBUC1D
             ENDIF
           ENDIF
           MATCH    SP70,DBDBUC2
           IF       !@EQUAL
             MOVE     "DB02",DBCDCAT
             PACK     KEY7,DBCDCAT,DBDBUC2
             CALL     RDDBCD1
             IF       OVRCD=0
               MOVE     DBCDDES,DBDBUC2D
             ENDIF
           ENDIF
           MATCH    SP70,DBDBUC3
           IF       !@EQUAL
             MOVE     "DB03",DBCDCAT
             PACK     KEY7,DBCDCAT,DBDBUC3
             CALL     RDDBCD1
             IF       OVRCD=0
               MOVE     DBCDDES,DBDBUC3D
             ENDIF
           ENDIF
           MATCH    SP70,DBDBUC4
           IF       !@EQUAL
             MOVE     "DB04",DBCDCAT
             PACK     KEY7,DBCDCAT,DBDBUC4
             CALL     RDDBCD1
             IF       OVRCD=0
               MOVE     DBCDDES,DBDBUC4D
             ENDIF
           ENDIF
.
GETCDS99   RETURN
.------------------------------------------------------------
. File       : casetran
.
. Functions  : This file contains standard routines for
.              creating and transfering TAB delimited
.              export files to a PC using the unix script
.              keatran.
.
. Routines   : MAKEXP00  - Create the Export File and Headings
.              WRTEXP00  - Write Export details Record to File
.              TRAN0000  - Transfer file to PC using script keatran
.
. Note       : The Variable EXPORTYN is used to determine if a
.              export file is to be created. 0=No, 1=Yes.
.
. Variables to be Declared
. ------------------------
. EXPORTFL  FILE      ASCII, FIXED=400     * Export File (Adjust Length to Suit)
. EXPORTFN  DIM       8                    * Export File Name
. TABDELM   EQU       0x09                 * Tab Delimiter
.------------------------------------------------------------
. Create Export File
.------------------------------------------------------------
MAKEXP00  COMPARE   ONE,EXPORTYN
          GOTO      MAKEXP99 IF NOT EQUAL
          MOVE      "EXPORT",KEY6
          PACK      EXPORTFN,KEY6,PORT
          REP       " 0",EXPORTFN
          PREP      EXPORTFL,EXPORTFN
.
          WRITE     EXPORTFL,SEQ;"Document Reference",TABDELM:
                                 "Document Type",TABDELM:
                                 "Document Date",TABDELM:
                                 "Invoice Reference",TABDELM:
                                 "Invoice Date",TABDELM:
                                 "Date Paid",TABDELM:
                                 "Debtor Code",TABDELM:
                                 "Debtor Name",TABDELM:
                                 "Post Code",TABDELM:
                                 "User Defined Code 1",TABDELM:
                                 "User Defined Name 1",TABDELM:
                                 "User Defined Code 2",TABDELM:
                                 "User Defined Name 2",TABDELM:
                                 "User Defined Code 3",TABDELM:
                                 "User Defined Name 3",TABDELM:
                                 "User Defined Code 4",TABDELM:
                                 "User Defined Name 4",TABDELM:
                                 "Department Code",TABDELM:
                                 "Department Description",TABDELM:
                                 "Product Type",TABDELM:
                                 "Product Type Description",TABDELM:
                                 "Sales Item",TABDELM:
                                 "Sales Item Description",TABDELM:
                                 "Quantity",TABDELM:
                                 "Price",TABDELM:
                                 "Total Excluded Tax",TABDELM:
                                 "Tax",TABDELM:
                                 "Total Included Tax",TABDELM:
                                 "Total Paid"
.
MAKEXP99  RETURN
.------------------------------------------------------------
. Write to Export File
.------------------------------------------------------------
WRTEXP00  COMPARE   ONE,EXPORTYN
          GOTO      WRTEXP99 IF NOT EQUAL
.
          WRITE     EXPORTFL,SEQ;DBFHDOC,TABDELM:
                                 TRANTYPE,TABDELM:
                                 DDATE,TABDELM:
                                 DBFDINV,TABDELM:
                                 IDATE,TABDELM:
                                 PDATE,TABDELM:
                                 DIM8,TABDELM:
                                 DBDBNA1,TABDELM:
                                 DBDBPC,TABDELM:
                                 DBDBUC1,TABDELM:
                                 DBDBUC1D,TABDELM:
                                 DBDBUC2,TABDELM:
                                 DBDBUC2D,TABDELM:
                                 DBDBUC3,TABDELM:
                                 DBDBUC3D,TABDELM:
                                 DBDBUC4,TABDELM:
                                 DBDBUC4D,TABDELM:
                                 DBDEDEP,TABDELM:
                                 DBDEDES,TABDELM:
                                 DBPTPTY,TABDELM:
                                 DBPTDES,TABDELM:
                                 DBITITM,TABDELM:
                                 DBITDES,TABDELM:
                                 DBFDQTY,TABDELM:
                                 DBFDPRI,TABDELM:
                                 DBFDTOT,TABDELM:
                                 DBFDTAX,TABDELM:
                                 F12P2,TABDELM:
                                 DBFDPAID
.
WRTEXP99  RETURN
.------------------------------------------------------------
. Execute Transfer Script eg softerm
.------------------------------------------------------------
TRAN0000  COMPARE   ONE,EXPORTYN
          GOTO      TRAN9999 IF NOT EQUAL
          MOVE      YES,DEFYESNO
          CLOSE     EXPORTFL
          DISPLAY   *P1:24,*EL,"Execute Transfer Now (":
                               *V2LON,"Y",*HOFF,"/":
                               *V2LON,"N",*HOFF,") ? "
          MOVE      "24",CVERT
          MOVE      "30",CCOL
          CALL      YORN0000
          COMPARE   ZERO,FYESNO
          GOTO      TRAN9999 IF EQUAL   *  No Transfer
.
          MOVE      ZERO,HLEF
          CALL      GETSCR00
.
          CLEAR     KEY80
          APPEND    "keatran ",KEY80
          APPEND    EXPORTFN,KEY80
          APPEND    ".txt",KEY80
          RESET     KEY80
          EXECUTE   KEY80,TASKID
.
          CALL      PUTSCR00
.
          DISPLAY   *P1:24,*EL,"Transfer Completed Ok (":
                               *V2LON,"Y",*HOFF,"/":
                               *V2LON,"N",*HOFF,") ? "
          MOVE      "24",CVERT
          MOVE      "31",CCOL
          CALL      YORN0000
          COMPARE   ZERO,FYESNO
          GOTO      TRAN0000 IF EQUAL   *  Not ok
.
          PREP      EXPORTFL,EXPORTFN
          CLOSE     EXPORTFL
.
TRAN9999  RETURN
.
.--------------------------------------------------
. Enter Debtors Range
.--------------------------------------------------
RDEB0000   MOVE     "    4",FLDID
           CALL     GETPOSIT
           CALL     KDBDBA00
           BRANCH   EXIT,RDEB0100,RDEB0999
           MOVE     DBDBDEB,STRDEB
           PACK     FROMDEB,DBDBDEB,SP1,DBDBNA1
           GOTO     RDEB0140
RDEB0100   MOVE     "Start             ",FROMDEB
           MOVE     SP70,STRDEB
.
RDEB0140   MOVE     FROMDEB,VAR
           CALL     DISPITEM
.
RDEB0150   MOVE     "    5",FLDID
           CALL     GETPOSIT
           CALL     KDBDBA00
           BRANCH   EXIT,RDEB0200,RDEB0999
           MOVE     DBDBDEB,ENDDEB
           PACK     TODEB,DBDBDEB,SP1,DBDBNA1
           MATCH    STRDEB,ENDDEB
           GOTO     RDEB0999 IF EQUAL
           GOTO     RDEB0999 IF NOT LESS
           BEEP
           MOVE     "To Debtor Must Be > From Debtor - ",DISPMSG
           CALL     MESSAGE1
           GOTO     RDEB0000
.
RDEB0200   MOVE     "Finish             ",TODEB
           MOVE     Z70,ENDDEB
.
RDEB0999   MOVE     "    4",FLDID
           CALL     GETPOSIT
           RETURN
.--------------------------------------------------
. Enter Department Range
.--------------------------------------------------
RDEP0000   MOVE     "    6",FLDID
           CALL     GETPOSIT
           CALL     KDBDEA00
           BRANCH   EXIT,RDEP0100,RDEP0999
           MOVE     DBDEDEP,STRDEP
           PACK     FROMDEP,DBDEDEP,SP1,DBDEDES
           GOTO     RDEP0140
RDEP0100   MOVE     "Start             ",FROMDEP
           MOVE     SP70,STRDEP
.
RDEP0140   MOVE     FROMDEP,VAR
           CALL     DISPITEM
.
RDEP0150   MOVE     "    7",FLDID
           CALL     GETPOSIT
           CALL     KDBDEA00
           BRANCH   EXIT,RDEP0200,RDEP0999
           MOVE     DBDEDEP,ENDDEP
           PACK     TODEP,DBDEDEP,SP1,DBDEDES
           MATCH    STRDEP,ENDDEP
           GOTO     RDEP0999 IF EQUAL
           GOTO     RDEP0999 IF NOT LESS
           BEEP
           MOVE     "To Department Must Be > From Department - ",DISPMSG
           CALL     MESSAGE1
           GOTO     RDEP0000
.
RDEP0200   MOVE     "Finish             ",TODEP
           MOVE     Z70,ENDDEP
.
RDEP0999   MOVE     "    6",FLDID
           CALL     GETPOSIT
           RETURN
