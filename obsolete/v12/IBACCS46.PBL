.----------------------------------------
. List General Ledger Keys
.----------------------------------------
LCCDS000
          MOVE      "Printing : ",DISPMSG
          CALL      MESSAGE2
          MOVE      SCSLITM,FIELDNO
          SUB       TWO,FIELDNO
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
          PACK      LASTPRT,SP70,SP70
          BRANCH    FIELDNO,LCCDS010,LCCDS020
.
LCCDS010  PACK      KEY14,SP70
          CALL      RSCCDS1
          GOTO      LCCDS100
LCCDS020  PACK      KEY14,SP70
          CALL      RSCCDS2
.
LCCDS100  BRANCH    FIELDNO,LCCDS110,LCCDS120
.
LCCDS110  CALL      RKCCDS1
          GOTO      LCCDS150
.
LCCDS120  CALL      RKCCDS2
.
LCCDS150
          BRANCH    OVRCD,LCCDS999
          COMPARE   "60",CLNO
          CALL      LCCDSHED IF NOT LESS
.
          MOVE      CCDSSEC,KEY4
          CALL      RDPASS1
          IF        OVRCD=1
            MOVE      "Not on File",SECUSER
          ENDIF
.
          PACK      KEY10,CCDSHCD,CCDSDPT
          CALL      RDCCDP1
          IF        OVRCD=1
            MOVE      "Not on File",CCDPDES
          ENDIF
.
          IF        FIELDNO=1
            PACK      NEXTPRT,CCDSSEC
            DISPLAY   *P12:24,NEXTPRT
            MATCH     LASTPRT,NEXTPRT
            IF        !@EQUAL
              PACK      LASTPRT,CCDSSEC
              PRINT     *4,CCDSSEC,SP1,SECUSER
              ADD       ONE,CLNO
            ENDIF
            PRINT     *31,CCDSHCD,"/",CCDSDPT,SP1,CCDPDES
          ELSE
            PACK      NEXTPRT,CCDSHCD,CCDSDPT
            DISPLAY   *P12:24,NEXTPRT
            MATCH     LASTPRT,NEXTPRT
            IF        !@EQUAL
              PACK      LASTPRT,CCDSHCD,CCDSDPT
              PRINT     *4,CCDSHCD,"/",CCDSDPT,SP1,CCDPDES
              ADD       ONE,CLNO
            ENDIF
            PRINT     *31,CCDSSEC,SP1,SECUSER
          ENDIF
          ADD       ONE,CLNO
          GOTO      LCCDS100
.
LCCDS999  CALL      UND80
          PRINT     *40,"*** End of Report ***"
          CALL      MESSAGE3
          MOVE      "Report Completed - ",DISPMSG
          CALL      MESSAGE1
          RETURN
.
LCCDSHED  CALL      HEAD80
          IF        FIELDNO=1
            PRINT     "Clinical Costing Security Listing by User Code "
            CALL      UND80
