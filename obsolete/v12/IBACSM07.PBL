          GOTO     LP01L105 IF NOT EQUAL
          PACK     KEY12,SIIM2SKY,Z70
          CALL     RSSIIM2  * End
          IF       OVRCD=1
            CALL     RPSIIM2 
          ENDIF
          MOVE     ZERO,SECTOR
          WHILE    SECTOR<SIIM2SNO
            CALL     RPSIIM2 
            IF       OVRCD=1
              MOVE     SIIM2SNO,SECTOR
            ENDIF
            PACK     SIIM2MKY,SIIMCAT,SIIMCST                                                                
            MATCH    SIIM2EKY,SIIM2MKY
            IF       !@EQUAL
              MOVE     SIIM2SNO,SECTOR
