.-------------------------------------------------
. External Routine for Stocktake Number Help
. Requires : IBAPASFD & IO
.-------------------------------------------------
EAMSMA00  PACK     KEY4,AMSMOPR
          CALL     RDPASS1
          UNPACK   AMSMDAT,CCENT,CYEAR,CMON,CDAY
          CALL     PACDATE
          RETURN
.
.
.---------------------------------------------------------------------------
.   System    :   Asset Management System            
.   Include   :   AMSSTMKY
.   Routine   :   HAMSMA00 Stocktake           
.   Return    :           
.-------------------------------------------------------------------------------
HAMSMA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "30",HLEF
            MOVE      "80",HRIG
          ENDIF
HAMSMA04
          CALL      GSCRN000
          BRANCH    EXIT,HAMSMA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Stocktake"          
.
          MOVE      STDCOL01,STDCOL02
          ADD       "10",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Created"            
.
          MOVE      STDCOL02,STDCOL03
          ADD       "11",STDCOL03
          DISPLAY   *PSTDCOL03:STDROW,*V2LON,*ULON,"Time"               
.
          MOVE      STDCOL03,STDCOL04
          ADD       " 6",STDCOL04
          DISPLAY   *PSTDCOL04:STDROW,*V2LON,*ULON,"Operator"           
.
          MOVE      STDCOL04,STDCOL05
          ADD       "20",STDCOL05
HAMSMA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,AMSMSTK 
          PACK      KEY5,AMSMSTK,SP70                                           
.                                                                               
.                                                                               
          CALL      RSAMSM1 
HAMSMA10
          CALL      RKAMSM1 
          BRANCH    OVRCD,HAMSMA15
          PACK      KEY5,AMSMSTK,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HAMSMA12
          CALL      HAMSMA60
          BRANCH    OVRCD,HAMSMA10
HAMSMA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY5,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,AMSMSTK 
          CALL      EAMSMA00
          DISPLAY   *PSTDCOL02:STDROW,CPCDATE 
          DISPLAY   *PSTDCOL03:STDROW,AMSMTIM 
          DISPLAY   *PSTDCOL04:STDROW,SECUSER 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HAMSMA10 IF NOT EQUAL
HAMSMA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HAMSMA20
          CALL      HSCRI000
          BRANCH    EXIT,HAMSMA30,HAMSMA35,HAMSMA40,HAMSMA45,HAMSMA04:
                              HAMSMA91,HAMSMA50
          GOTO      HAMSMA92
HAMSMA30
          CALL      RKAMSM1 
          BRANCH    OVRCD,HAMSMA85
          PACK      KEY5,AMSMSTK,SP70                                           
.                                                                               
.                                                                               
          CALL      RPAMSM1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HAMSMA10
HAMSMA35
          MOVE      ST1,KEY5 
          CALL      RDAMSM1 
          CALL      RPAMSM1 
          BRANCH    OVRCD,HAMSMA85
          PACK      KEY5,AMSMSTK,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HAMSMA36
          CALL      RPAMSM1 
          BRANCH    OVRCD,HAMSMA37
          PACK      KEY5,AMSMSTK,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HAMSMA36 IF NOT EQUAL
HAMSMA37
          MOVE      ZERO,XITEMNO
          GOTO      HAMSMA10
HAMSMA40
          MOVE      ST1,KEY5 
          CALL      RDAMSM1 
          CALL      RPAMSM1 
          BRANCH    OVRCD,HAMSMA85
          PACK      KEY5,AMSMSTK,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HAMSMA08
HAMSMA45
          CALL      RKAMSM1 
          BRANCH    OVRCD,HAMSMA85
          PACK      KEY5,AMSMSTK,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY5,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSAMSM1 
HAMSMA46
          CALL      RPAMSM1 
          BRANCH    OVRCD,HAMSMA47
          PACK      KEY5,AMSMSTK,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HAMSMA46 IF NOT EQUAL
HAMSMA47
          MOVE      ZERO,XITEMNO
          GOTO      HAMSMA10
HAMSMA50  COMPARE   ZERO,CSEARCH
          GOTO      HAMSMA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          KEYIN     *PSTDCOL03:STDROW,*V2LON,SCHFLD03
          RESET     SCHFLD03
          KEYIN     *PSTDCOL04:STDROW,*V2LON,SCHFLD04
          RESET     SCHFLD04
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HAMSMA08
HAMSMA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HAMSMA08
HAMSMA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HAMSMA61 IF EQUAL
          MATCH     SCHFLD01,AMSMSTK 
          GOTO      HAMSMA72 IF NOT EQUAL
HAMSMA61
          CALL      EAMSMA00
          MATCH     SP70,SCHFLD02
          GOTO      HAMSMA62 IF EQUAL
          MATCH     SCHFLD02,CPCDATE 
          GOTO      HAMSMA72 IF NOT EQUAL
HAMSMA62
          MATCH     SP70,SCHFLD03
          GOTO      HAMSMA63 IF EQUAL
          MATCH     SCHFLD03,AMSMTIM 
          GOTO      HAMSMA72 IF NOT EQUAL
HAMSMA63
          MATCH     SP70,SCHFLD04
          GOTO      HAMSMA64 IF EQUAL
          MATCH     SCHFLD04,SECUSER 
          GOTO      HAMSMA72 IF NOT EQUAL
HAMSMA64
HAMSMA65
HAMSMA66
HAMSMA67
HAMSMA68
HAMSMA69
HAMSMA70
          GOTO      HAMSMA75
HAMSMA72  MOVE      ONE,OVRCD
HAMSMA75  RETURN
HAMSMA85
          LOAD      KEY5,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY5,AMSMSTK                                                
          CALL      RDAMSM1 
HAMSMA90
.                                                                               
          BEEP
          GOTO      HAMSMA20
HAMSMA91
          MOVE      SP70,AMSMDAT 
          MOVE      SP70,AMSMTIM 
          MOVE      SP70,AMSMOPR 
          MOVE      SP70,AMSMFBLD
          MOVE      SP70,AMSMTBLD
          MOVE      SP70,AMSMFLOC
          MOVE      SP70,AMSMTLOC
          MOVE      SP70,AMSMFTYP
          MOVE      SP70,AMSMTTYP
          MOVE      SP70,AMSMFDPT
          MOVE      SP70,AMSMTDPT
          MOVE      SP70,AMSMFSDT
          MOVE      SP70,AMSMTSDT
          MOVE      SP70,AMSMCOM 
          MOVE      SP70,AMSMUR1 
          MOVE      SP70,AMSMUR2 
          MOVE      SP70,AMSMUD1 
          MOVE      SP70,AMSMUD2 
          MOVE      SP70,AMSMUY1 
          MOVE      SP70,AMSMUY2 
          MOVE      SP70,AMSMSPA 
          MOVE      SP70,KEY5 
          MOVE      SP70,AMSMSTK 
          GOTO      HAMSMA96
HAMSMA92
          MOVE      SP70,KEY5 
          MOVE      SP70,AMSMSTK 
          LOAD      KEY5,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY5,AMSMSTK                                                
HAMSMA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HAMSMA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Asset Management System            
.   Include   :   AMSSTMKY
.   Routine   :   KAMSMA00 Keyin Stocktake           
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KAMSMA00 TOPIC     "AMSMSTK "
          MOVE      UNDLN70,AMSMSTK 
          KEYIN     *PCCOL:CVERT,*DV,AMSMSTK:                                   
                    *PCCOL:CVERT,*V2LON,*JR,AMSMSTK:                            
                    *PCCOL:CVERT,*DV,AMSMSTK 
.
          ENDSET    AMSMSTK 
          APPEND    SP70,AMSMSTK 
          RESET     AMSMSTK 
.
          MATCH     SP70,AMSMSTK 
          GOTO      KAMSMA90 IF EQUAL
.
          ENDSET    AMSMSTK 
          CMATCH    EXITCHAR,AMSMSTK 
          GOTO      KAMSMA95 IF EQUAL
.
          CMATCH    QUEST,AMSMSTK 
          GOTO      KAMSMA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HAMSMA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    AMSMSTK 
          APPEND    SP70,AMSMSTK 
          RESET     AMSMSTK 
.
          MATCH     SP70,AMSMSTK 
          GOTO      KAMSMA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,AMSMSTK                                 
KAMSMA10
          RESET     AMSMSTK 
          PACK      KEY5,AMSMSTK                                                
          CALL      RAAMSM1 
          BRANCH    OVRCD,KAMSMA80
          BRANCH    CKEYTYP,KAMSMA75
          CALL      RDAMSM1 
KAMSMA70
          MOVE      ZERO,EXIT
          GOTO      KAMSMA99
KAMSMA75
          MOVE      "Stocktake Already Exist - ",DISPMSG                                             
          CALL      MESSAGE1
          GOTO      KAMSMA00
KAMSMA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KAMSMA99,KAMSMA99
          MOVE      "Stocktake Does Not Exist - ",DISPMSG                                            
          CALL      MESSAGE1
          GOTO      KAMSMA00
KAMSMA90
          MOVE      ONE,EXIT
          GOTO      KAMSMA99
KAMSMA95
          MOVE      TWO,EXIT
KAMSMA99 TOPIC    PRGID
          RETURN
