.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSCTYKY
.   Routine   :   HCCCTA00 Cost Type           
.   Return    :           
.-------------------------------------------------------------------------------
HCCCTA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "05",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCCTA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCCTA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Code"               
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 5",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Cost Type"          
.
          MOVE      STDCOL02,STDCOL03
          ADD       "35",STDCOL03
HCCCTA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCCTCTY 
          PACK      KEY4,CCCTCTY,SP70                                           
.                                                                               
.                                                                               
          CALL      RSCCCT1 
HCCCTA10
          CALL      RKCCCT1 
          BRANCH    OVRCD,HCCCTA15
          PACK      KEY4,CCCTCTY,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HCCCTA12
          CALL      HCCCTA60
          BRANCH    OVRCD,HCCCTA10
HCCCTA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY4,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCCTCTY 
          DISPLAY   *PSTDCOL02:STDROW,CCCTDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCCTA10 IF NOT EQUAL
HCCCTA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCCTA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCCTA30,HCCCTA35,HCCCTA40,HCCCTA45,HCCCTA04:
                              HCCCTA91,HCCCTA50
          GOTO      HCCCTA92
HCCCTA30
          CALL      RKCCCT1 
          BRANCH    OVRCD,HCCCTA85
          PACK      KEY4,CCCTCTY,SP70                                           
.                                                                               
.                                                                               
          CALL      RPCCCT1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCCTA10
HCCCTA35
          MOVE      ST1,KEY4 
          CALL      RDCCCT1 
          CALL      RPCCCT1 
          BRANCH    OVRCD,HCCCTA85
          PACK      KEY4,CCCTCTY,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCCTA36
          CALL      RPCCCT1 
          BRANCH    OVRCD,HCCCTA37
          PACK      KEY4,CCCTCTY,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCCTA36 IF NOT EQUAL
HCCCTA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCCTA10
HCCCTA40
          MOVE      ST1,KEY4 
          CALL      RDCCCT1 
          CALL      RPCCCT1 
          BRANCH    OVRCD,HCCCTA85
          PACK      KEY4,CCCTCTY,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCCTA08
HCCCTA45
          CALL      RKCCCT1 
          BRANCH    OVRCD,HCCCTA85
          PACK      KEY4,CCCTCTY,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY4,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCCT1 
HCCCTA46
          CALL      RPCCCT1 
          BRANCH    OVRCD,HCCCTA47
          PACK      KEY4,CCCTCTY,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCCTA46 IF NOT EQUAL
HCCCTA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCCTA10
HCCCTA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCCTA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCCTA08
HCCCTA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCCTA08
HCCCTA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCCTA61 IF EQUAL
          MATCH     SCHFLD01,CCCTCTY 
          GOTO      HCCCTA72 IF NOT EQUAL
HCCCTA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCCTA62 IF EQUAL
          MATCH     SCHFLD02,CCCTDES 
          GOTO      HCCCTA72 IF NOT EQUAL
HCCCTA62
HCCCTA63
HCCCTA64
HCCCTA65
HCCCTA66
HCCCTA67
HCCCTA68
HCCCTA69
HCCCTA70
          GOTO      HCCCTA75
HCCCTA72  MOVE      ONE,OVRCD
HCCCTA75  RETURN
HCCCTA85
          LOAD      KEY4,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY4,CCCTCTY                                                
          CALL      RDCCCT1 
HCCCTA90
.                                                                               
          BEEP
          GOTO      HCCCTA20
HCCCTA91
          MOVE      SP70,CCCTDES 
          MOVE      SP70,CCCTFIX 
          MOVE      SP70,CCCTLAB 
          MOVE      SP70,CCCTIND 
          MOVE      SP70,CCCTSPA 
          MOVE      SP70,KEY4 
          MOVE      SP70,CCCTCTY 
          GOTO      HCCCTA96
HCCCTA92
          MOVE      SP70,KEY4 
          MOVE      SP70,CCCTCTY 
          LOAD      KEY4,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY4,CCCTCTY                                                
HCCCTA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCCTA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSCTYKY
.   Routine   :   KCCCTA00 Keyin Cost Type           
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCCTA00 TOPIC     "CCCTCTY "
          MOVE      UNDLN70,CCCTCTY 
          KEYIN     *PCCOL:CVERT,*DV,CCCTCTY:                                   
                    *PCCOL:CVERT,*V2LON,CCCTCTY:                                
                    *PCCOL:CVERT,*DV,CCCTCTY 
.
          ENDSET    CCCTCTY 
          APPEND    SP70,CCCTCTY 
          RESET     CCCTCTY 
.
          MATCH     SP70,CCCTCTY 
          GOTO      KCCCTA90 IF EQUAL
.
          CMATCH    EXITCHAR,CCCTCTY 
          GOTO      KCCCTA95 IF EQUAL
.
          CMATCH    QUEST,CCCTCTY 
          GOTO      KCCCTA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCCCTA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCCTCTY 
          APPEND    SP70,CCCTCTY 
          RESET     CCCTCTY 
.
          MATCH     SP70,CCCTCTY 
          GOTO      KCCCTA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CCCTCTY                                 
KCCCTA10
          PACK      KEY4,CCCTCTY                                                
          CALL      RACCCT1 
          BRANCH    OVRCD,KCCCTA80
          BRANCH    CKEYTYP,KCCCTA75
          CALL      RDCCCT1 
KCCCTA70
          MOVE      ZERO,EXIT
          GOTO      KCCCTA99
KCCCTA75
          MOVE      "Cost Type Already Exist - ",DISPMSG                                             
          CALL      MESSAGE1
          GOTO      KCCCTA00
KCCCTA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCCTA99,KCCCTA99
          MOVE      "Cost Type Does Not Exist - ",DISPMSG                                            
          CALL      MESSAGE1
          GOTO      KCCCTA00
KCCCTA90
          MOVE      ONE,EXIT
          GOTO      KCCCTA99
KCCCTA95
          MOVE      TWO,EXIT
KCCCTA99 TOPIC    PRGID
          RETURN
