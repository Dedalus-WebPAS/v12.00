. Print Patient Service Cost Report
.------------------------------------------------------------
REPT0000  MOVE      ZERO,CPAGENO          * Reset Page No.
          CLOCK     TIME,CTIMEIS          * Get Current Time
          MOVE      "99",CLNO             * Initialize Last Line Number
.
          MOVE      ZERO,EXIT
          COMPARE   ONE,DOWNLOAD          * Create Export File
          CALL      OPEN0000 IF EQUAL
          BRANCH    EXIT,REPT9100
.
          BRANCH    GENERIC,REPT5000
.
. Department/Patient Service Option
.-----------------------------
          MOVE      SP70,SAVEDPT
          PACK      KEY26,CCCAYEAR,CCMACOM,CCHCHCD,SP70
          CALL      RSCCMT1
REPT1000  CALL      RKCCMT1
          BRANCH    OVRCD,REPT9000
          PACK      KEY10,CCCAYEAR,CCMACOM,CCHCHCD,SP70
          PACK      KEY18,CCMTYEAR,CCMTCOM,CCMTHCD,SP70
          MATCH     KEY10,KEY18
          GOTO      REPT9000 IF NOT EQUAL
.
          MATCH     SAVEDPT,CCMTDPT
          CALL      HEAD0000 IF NOT EQUAL
.
          CALL      PRLMT000  * Print Line
          GOTO      REPT1000
.
. Generic Patient Service Option
.--------------------------
REPT5000  PACK      KEY18,CCCAYEAR,CCMACOM,CCHCHCD,SP70
          CALL      RSCCMS1
REPT6000  CALL      RKCCMS1
          BRANCH    OVRCD,REPT9000
          PACK      KEY10,CCCAYEAR,CCMACOM,CCHCHCD,SP70
          PACK      KEY18,CCMSYEAR,CCMSCOM,CCMSHCD,SP70
          MATCH     KEY10,KEY18
          GOTO      REPT9000 IF NOT EQUAL
.
          CALL      PRLMS000  * Print Line
          GOTO      REPT6000
.
REPT9000  MOVE      ZERO,EXIT
          COMPARE   ZERO,CPAGENO
          CALL      HEAD0000 IF EQUAL
.
          CALL      UND132
          PRINT     *40,"****** End of Report ******"
          GOTO      REPT9999
.
REPT9100  MOVE      THREE,EXIT
.
REPT9999  RETURN
