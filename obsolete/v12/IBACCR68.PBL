. Print Report Heading
.------------------------------------------------------------
HEAD0000  CALL     HEAD132
          ADD      "4",CLNO
          IF       DOWNLOAD=1
            ADD      ONE,CLNO
            PRINT    "Export File    : ",DOWNNAME
          ENDIF
          PRINT    "Hospital       : ",CCHCHCD,SP1,CCHCDES
          PRINT    "Cost Model     : ",CCMACOM,SP1,CCMADES
          PRINT    "Financial Year : ",CCCAYEAR
          BRANCH   GENERIC,HEAD1000
.
          MOVE     CCMTDPT,SAVEDPT
          PACK     KEY10,CCMTHCD,CCMTDPT
          CALL     RDCCDP1
          PRINT    "Department     : ",CCDPDPT,SP1,CCDPDES
          CALL     UND132
          PRINT    "| Patient Service                              |":
                   "         Quantity               Cost       Avera":
                   "ge Cost |   Fix        Fixed Price |"
          CALL     UND132
          GOTO     HEAD9999
.
