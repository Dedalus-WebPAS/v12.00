. Print Generic Patient Service Line
.------------------------------------------------------------
PRLMS000  COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          PACK      KEY18,CCMSPCD,SP70
          CALL      RSCCPD3
          CALL      RKCCPD3
          MATCH     CCPDPCD,CCMSPCD
          IF        ((!@EQUAL)|(OVRCD=1))
            MOVE      "Unknown",CCPDDES
          ENDIF
.
          MOVE      "999,999,999.99",F14TOT1
          MOVE      "999,999,999.99",F14TOT2
          MOVE      "999,999,999.99",F14TOT3
          MOVE      SP70,F14TOT4
          MOVE      "No ",KEY3
          IF        CCMSFIX=1
            MOVE      "Yes",KEY3
            MOVE      "999,999,999.99",F14TOT4
            MOVE      CCMSFUC,F12P2
            FEDIT     F12P2,F14TOT4
          ENDIF
.
          MOVE      CCMSQTY,F12P2
          FEDIT     F12P2,F14TOT1
          MOVE      CCMSCST,F12P2
          FEDIT     F12P2,F14TOT2
          MOVE      CCMSUCT,F12P2
          FEDIT     F12P2,F14TOT3
.
          PRINT     "| ",CCMSPCD,SP1,CCPDDES," |   ",F14TOT1:
                    SP5,F14TOT2,SP5,F14TOT3," |   ":
                    KEY3,SP5,F14TOT4," |"
          ADD       ONE,CLNO
          IF        DOWNLOAD=1
            WRITE     DOWNFILE,SEQ;QUOTE,CCMTDPT,QUOTE,COMMA:
                                   QUOTE,CCPDPCD,QUOTE,COMMA:
                                   QUOTE,CCPDDES,QUOTE,COMMA:
                                   CCMTQTY,COMMA,CCMTCST,COMMA:
                                   CCMTUCT,COMMA,CCMTFIX,COMMA:
                                   CCMTFUC
          ENDIF
          RETURN
