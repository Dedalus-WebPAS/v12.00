. Process General Ledger UN-Transfer
.------------------------------------------------------------
PROC0000  DISPLAY   *P1:24,*EL,"Extracting Details ...."
.
          CALL      EXTR0000           * Extract Details & Print Error Report
.
PROC9000  MOVE      ZERO,EXIT          * Exit Program
.
PROC9999  RETURN
.------------------------------------------------------------
. Extract Details to Export File & Print Any Errors
.------------------------------------------------------------
EXTR0000  MOVE      ZERO,CPAGENO       * Setup Report Details
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
.
          MOVE      ZERO,SECTOR
          MOVE      SP70,LASTDEB
          MOVE      ZERO,FLAGDEB
          PACK      KEY25,ONE,SP70
          CALL      RSDBFD5
EXTR1000  CALL      RKDBFD5
          BRANCH    OVRCD,EXTR9000
          MATCH     "1",DBFDGLS
          GOTO      EXTR9000 IF NOT EQUAL
.
          MATCH     "3",DBFDDTY        * NO Cash Transfer.. Done by Rec System
          IF        @EQUAL
            GOTO      EXTR1000
          ENDIF
.
          MATCH     SP70,SELLED
          IF        !@EQUAL
            MATCH     DBFDLED,SELLED
            GOTO      EXTR1000 IF NOT EQUAL
          ENDIF
.
EXTR1100  ADD       ONE,SECTOR
          IF        SECTOR%100=0
            DISPLAY   *P40:24,*V2LON,SECTOR
          ENDIF
          PACK      KEY21,DBFDDEB,DBFDDTY,DBFDDOC,SP70
          CALL      RDDBFH1
          BRANCH    OVRCD,EXTR1000
          MATCH     STRDAT,DBFHDDAT    * Doc. Date Range
          GOTO      EXTR1000 IF LESS
          MATCH     DBFHDDAT,ENDDAT
          GOTO      EXTR1000 IF LESS
.
EXTR1150  PACK      KEY24,DBFDDEB,DBFDDTY,DBFDDOC,DBFDDLN,SP70
          CALL      RDDBFD1
          IF        OVRCD=0
            MOVE      ZERO,DBFDGLS
            CALL      UPDBFD1
          ENDIF
.
          MATCH     DBFDDEB,LASTDEB
          GOTO      EXTR1200 IF NOT EQUAL
          CALL      PRLINE00           * Print Report Line
          GOTO      EXTR1000
.
. New Debtor
.
EXTR1200  COMPARE   ZERO,CPAGENO       * First Page So No Totals to Print
          GOTO      EXTR1300 IF EQUAL
          CALL      DEBTOT00
.
EXTR1300  COMPARE   "55",CLNO
          CALL      PRHEAD00 IF NOT LESS
.
          CALL      PRDEBT00
          CALL      PRLINE00
          GOTO      EXTR1000
.
EXTR9000  IF        CPAGENO<>0
            CALL      DEBTOT00
          ENDIF
          CALL      PEND0000
          MOVE      "Process Complete - ",DISPMSG
          CALL      MESSAGE1
.
EXTR9999
          RETURN
.------------------------------------------------------------
. Print Debtor Name & Code Heading & Clear Totals
.------------------------------------------------------------
PRDEBT00  MOVE      DBFDDEB,KEY8
          CALL      RDDBDB1
          PRINT     DBDBNA1,"(",DBFDDEB,")"
          ADD       ONE,CLNO
          MOVE      DBFDDEB,LASTDEB
          MOVE      ZERO,DTOTAMT
          MOVE      ZERO,DTOTTAX
          RETURN
.------------------------------------------------------------
. Print Report Detail Line
.------------------------------------------------------------
PRLINE00  COMPARE    "55",CLNO
          CALL       PRHEAD00 IF NOT LESS
          UNPACK      DBFHDDAT,CCENT,CYEAR,CMON,CDAY
          CALL        PACDATE
          MOVE        DBFDTOT,TRANSVAL
          ADD         DBFDTAX,TRANSVAL
.
          MOVE        "Invoice   ",DIM10
          MATCH       "1",DBFDDTY
          GOTO        PRLINE20 IF EQUAL
.
          MOVE        "Cr. Note  ",DIM10
          MATCH       "2",DBFDDTY
          GOTO        PRLINE10 IF EQUAL
.
          MOVE        "Dr.Adjust.",DIM10
          MATCH       "4",DBFDDTY
          GOTO        PRLINE20 IF EQUAL
.
          MOVE        "Cr.Adjust.",DIM10
.
PRLINE10  MULT        SEQ,TRANSVAL
.
PRLINE20  MOVE        "999,999,999.99",FTOTAL
          FEDIT       TRANSVAL,FTOTAL
          MOVE        DBFDITM,KEY8
          CALL        RDDBIT1
          PRINT       *7,DIM10,SP1,DBFDDOC,"/",DBFDDLN,SP1,CPCDATE,SP1:
                      DBITDES,"(",DBFDITM,")":
                      *91,DBFDLED,"/",DBFDCAC,SP1,DBFDIAC,FTOTAL
          ADD         ONE,CLNO
.
          ADD         TRANSVAL,DTOTAMT
          ADD         TRANSVAL,RTOTAMT
PRLINE99  RETURN
.------------------------------------------------------------
. Print Debtors Total
.------------------------------------------------------------
DEBTOT00  MOVE        "999,999,999.99",FTOTAL
          FEDIT       DTOTAMT,FTOTAL
          CALL        UND132
          PRINT       "Total for ",DBDBNA1,"(",LASTDEB,")",*119,FTOTAL
          ADD         ONE,CLNO
          MOVE        ZERO,DTOTAMT
          CALL        UND132
          RETURN
.------------------------------------------------------------
. Print Report Total
.------------------------------------------------------------
PEND0000  COMPARE     ZERO,CPAGENO
          CALL        PRHEAD00 IF EQUAL
.
          MOVE        "999,999,999.99",FTOTAL
          FEDIT       RTOTAMT,FTOTAL
          CALL        UND132
          PRINT       "Report Total  ",*119,FTOTAL
          CALL        UND132
          RETURN
