.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSSPCKY
.   Routine   :   KCCSPA00 Keyin Split Cost Model    
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCSPA00 TOPIC     "CCSPMID "
          MOVE      UNDLN70,CCSPMID 
          KEYIN     *PCCOL:CVERT,*DV,CCSPMID:                                   
                    *PCCOL:CVERT,*V2LON,CCSPMID:                                
                    *PCCOL:CVERT,*DV,CCSPMID 
.
          ENDSET    CCSPMID 
          APPEND    SP70,CCSPMID 
          RESET     CCSPMID 
.
          MATCH     SP70,CCSPMID 
          GOTO      KCCSPA90 IF EQUAL
.
          CMATCH    EXITCHAR,CCSPMID 
          GOTO      KCCSPA95 IF EQUAL
.
          CMATCH    QUEST,CCSPMID 
          GOTO      KCCSPA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCCSPA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCSPMID 
          APPEND    SP70,CCSPMID 
          RESET     CCSPMID 
.
          MATCH     SP70,CCSPMID 
          GOTO      KCCSPA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CCSPMID                                 
KCCSPA10
          PACK      KEY3,CCSPMID                                                
          CALL      RACCSP1 
          BRANCH    OVRCD,KCCSPA80
          BRANCH    CKEYTYP,KCCSPA75
          CALL      RDCCSP1 
KCCSPA70
          MOVE      ZERO,EXIT
          GOTO      KCCSPA99
KCCSPA75
          MOVE      "Split Cost Model Already Exist - ",DISPMSG                                      
          CALL      MESSAGE1
          GOTO      KCCSPA00
KCCSPA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCSPA99,KCCSPA99
          MOVE      "Split Cost Model Does Not Exist - ",DISPMSG                                     
          CALL      MESSAGE1
          GOTO      KCCSPA00
KCCSPA90
          MOVE      ONE,EXIT
          GOTO      KCCSPA99
KCCSPA95
          MOVE      TWO,EXIT
KCCSPA99 TOPIC    PRGID
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSSPCKY
.   Routine   :   HCCSPA00 Split Cost Model    
.   Return    :           
.-------------------------------------------------------------------------------
HCCSPA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCSPA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCSPA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Model"              
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 6",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HCCSPA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCSPMID 
          PACK      KEY3,CCSPMID,SP70                                           
.                                                                               
.                                                                               
          CALL      RSCCSP1 
HCCSPA10
          CALL      RKCCSP1 
          BRANCH    OVRCD,HCCSPA15
          PACK      KEY3,CCSPMID,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HCCSPA12
          CALL      HCCSPA60
          BRANCH    OVRCD,HCCSPA10
HCCSPA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCSPMID 
          DISPLAY   *PSTDCOL02:STDROW,CCSPDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCSPA10 IF NOT EQUAL
HCCSPA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCSPA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCSPA30,HCCSPA35,HCCSPA40,HCCSPA45,HCCSPA04:
                              HCCSPA91,HCCSPA50
          GOTO      HCCSPA92
HCCSPA30
          CALL      RKCCSP1 
          BRANCH    OVRCD,HCCSPA85
          PACK      KEY3,CCSPMID,SP70                                           
.                                                                               
.                                                                               
          CALL      RPCCSP1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCSPA10
HCCSPA35
          MOVE      ST1,KEY3 
          CALL      RDCCSP1 
          CALL      RPCCSP1 
          BRANCH    OVRCD,HCCSPA85
          PACK      KEY3,CCSPMID,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCSPA36
          CALL      RPCCSP1 
          BRANCH    OVRCD,HCCSPA37
          PACK      KEY3,CCSPMID,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCSPA36 IF NOT EQUAL
HCCSPA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCSPA10
HCCSPA40
          MOVE      ST1,KEY3 
          CALL      RDCCSP1 
          CALL      RPCCSP1 
          BRANCH    OVRCD,HCCSPA85
          PACK      KEY3,CCSPMID,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCSPA08
HCCSPA45
          CALL      RKCCSP1 
          BRANCH    OVRCD,HCCSPA85
          PACK      KEY3,CCSPMID,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY3,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCSP1 
HCCSPA46
          CALL      RPCCSP1 
          BRANCH    OVRCD,HCCSPA47
          PACK      KEY3,CCSPMID,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCSPA46 IF NOT EQUAL
HCCSPA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCSPA10
HCCSPA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCSPA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCSPA08
HCCSPA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCSPA08
HCCSPA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCSPA61 IF EQUAL
          MATCH     SCHFLD01,CCSPMID 
          GOTO      HCCSPA72 IF NOT EQUAL
HCCSPA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCSPA62 IF EQUAL
          MATCH     SCHFLD02,CCSPDES 
          GOTO      HCCSPA72 IF NOT EQUAL
HCCSPA62
HCCSPA63
HCCSPA64
HCCSPA65
HCCSPA66
HCCSPA67
HCCSPA68
HCCSPA69
HCCSPA70
          GOTO      HCCSPA75
HCCSPA72  MOVE      ONE,OVRCD
HCCSPA75  RETURN
HCCSPA85
          LOAD      KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,CCSPMID                                                
          CALL      RDCCSP1 
HCCSPA90
.                                                                               
          BEEP
          GOTO      HCCSPA20
HCCSPA91
          MOVE      SP70,CCSPDES 
          MOVE      SP70,CCSPFYR 
          MOVE      SP70,CCSPFPR 
          MOVE      SP70,CCSPTYR 
          MOVE      SP70,CCSPTPR 
