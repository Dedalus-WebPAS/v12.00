.-----------------------------------------------
. Date Initialization for KEYDATE/NEWDATE
.-----------------------------------------------
INITDATE
          PACK      KEY8,CCC,CYY,CMM,CDD
          REP       " 0",KEY8
          UNPACK    KEY8,CCENT,CYEAR,CMON,CDAY
          RETURN
.
.------------------------------------
. Save Calendar File Changes
.------------------------------------
WCCCA000
          PACK      KEY6,NEWYEAR,SP70
          CALL      RSCCCA1
WCCCA100
          CALL      RKCCCA1
          BRANCH    OVRCD,WCCCA200
          MATCH     CCCAYEAR,NEWYEAR
          GOTO      WCCCA200 IF NOT EQUAL
          PACK      KEY6,CCCAYEAR,CCCAPER
          CALL      DECCCA1
          GOTO      WCCCA100
WCCCA200
          MOVE      SP70,KEY5
          CALL      TRCCCA11
          COMPARE   ZERO,OVRCD
          GOTO      WCCCA310 IF EQUAL
WCCCA300
          CALL      TKCCCA11
          BRANCH    OVRCD,WCCCA400
WCCCA310
          PACK      KEY6,NEWYEAR,CCCAPER
          UNPACK    KEY6,CCCAYEAR,CCCAPER
          CALL      RACCCA1
          IF        OVRCD=0
            CALL      UPCCCA1
          ELSE
            CALL      WRCCCA1
          ENDIF
          GOTO      WCCCA300
WCCCA400
          PACK      KEY6,NEWYEAR,SP70
          CALL      RACCCA1
          IF        OVRCD=1
            UNPACK    KEY6,CCCAYEAR,CCCAPER
            CALL      WRCCCA1
          ENDIF
          RETURN
.
.-----------------------------------------------
. Clinical Costing Copy Year Function
.-----------------------------------------------
CCCY0000  MOVE      "Copy From Year : ",DISPMSG
          CALL      MESSAGE2
          MOVE      "24",CVERT
          MOVE      "18",CCOL
          MOVE      SP70,CCCAPER
          CALL      KCCCAB00
          BRANCH    EXIT,CCCY9000,CCCY9000
          PACK      KEY4,CCCAYEAR
          MOVE      NEWYEAR,CCCYDIFF
          MOVE      CCCAYEAR,CCCYNYY
          SUB       CCCYNYY,CCCYDIFF
.
          PACK      KEY6,CCCAYEAR,SP70
          CALL      RDCCCA1
          COMPARE   ZERO,OVRCD
          GOTO      CCCY1100 IF EQUAL
.
CCCY1000
          PACK      KEY6,KEY4,CCCAPER
          CALL      RDCCCA1
          CALL      RKCCCA1
          BRANCH    OVRCD,CCCY9000
.
CCCY1100  MATCH     KEY4,CCCAYEAR
          GOTO      CCCY9000 IF NOT EQUAL
.
          UNPACK    CCCATDT,CCCYDYY,CMON,CDAY
          MOVE      CCCYDYY,CCCYNYY
          ADD       CCCYDIFF,CCCYNYY
          PACK      CCCATDT,CCCYNYY,CMON,CDAY
          UNPACK    CCCAFDT,CCCYDYY,CMON,CDAY
          MOVE      CCCYDYY,CCCYNYY
          ADD       CCCYDIFF,CCCYNYY
          PACK      CCCAFDT,CCCYNYY,CMON,CDAY
.
          MOVE      NEWYEAR,CCCAYEAR
          PACK      KEY6,CCCAPER,NEWYEAR
          CALL      RACCCA2
          IF        OVRCD=0
            CALL      UPCCCA2
          ELSE
            CALL      WRCCCA2
          ENDIF
          GOTO      CCCY1000
CCCY9000  CALL      MESSAGE3
          RETURN
.
