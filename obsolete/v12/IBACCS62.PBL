.----------------------------------------
. Departmental Cost Type Check
.----------------------------------------
REPT0000  MOVE      "Checking : ",DISPMSG
          MOVE      ZERO,TOTALDIR
          MOVE      ZERO,TOTALIND
          MOVE      ZERO,GRANDDIR
          MOVE      ZERO,GRANDIND
          CALL      MESSAGE2
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
.
          PACK      KEY10,CCHCHCD,FDEPART
          CALL      RDCCDP1
          IF        OVRCD=0
            CALL      RPCCDP1
          ENDIF
.
REPT1000  CALL      RKCCDP1
          BRANCH    OVRCD,REPT9000
          MATCH     CCHCHCD,CCDPHCD
          GOTO      REPT9000 IF NOT EQUAL
          MATCH     CCDPDPT,LDEPART
          GOTO      REPT9000 IF LESS
.
          DISPLAY   *P12:24,CCDPDPT
.
          PACK      KEY10,CCDPHCD,CCDPDPT,SP70
          PACK      KEY20,CCDPHCD,CCDPDPT,SP70
          CALL      RSCCMB2
REPT2200  CALL      RKCCMB2
          BRANCH    OVRCD,REPT1000
          PACK      KEY20,CCMBHCD,CCMBDPT,CCMBCTY,CCMBYEAR,CCMBPER,SP70
          MATCH     KEY10,KEY20
          GOTO      REPT1000 IF NOT EQUAL
          MATCH     CCCAYEAR,CCMBYEAR
          GOTO      REPT2200 IF NOT EQUAL
.
          PACK      KEY4,CCMBCTY
          CALL      RDCCCT1
          BRANCH    OVRCD,REPT2400
.
          COMPARE   ZERO,CCMBDIR
          GOTO      REPT2200 IF EQUAL
          PACK      KEY14,CCMBHCD,CCMBDPT,CCMBCTY
          CALL      RDCCDA1
          COMPARE   ZERO,OVRCD
          GOTO      REPT2200 IF EQUAL
.
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
          ADD       TWO,CLNO
          PRINT     "Cost Type Not on Department"
          PRINT     CCDPDPT,SP1,CCDPDES,SP1:
                    "Cost Type = ",CCMBCTY,"  Direct = ",CCMBDIR
          GOTO      REPT2200
.
REPT2400  COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
          ADD       TWO,CLNO
          PRINT     "Invalid Cost Type "
          PRINT     CCDPDPT,SP1,CCDPDES,SP1:
                    "Cost Type = ",CCMBCTY,"  Direct = ",CCMBDIR
          GOTO      REPT2200
.
REPT9000  CALL      UND132
          PRINT     *40,"*** End of Report ***"
          RETURN
.
HEAD0000  CALL      HEAD132
          PRINT     "Cost Model      : ",CCMACOM,SP5,CCMADES
          PRINT     "Hospital        : ",CCHCHCD,SP7,CCHCDES
          PRINT     "From Department : ",FROMDEPT:
                    "  To   Department : ",TODEPT
          PRINT     "Year            : ",CCCAYEAR
          CALL      UND132
          RETURN
