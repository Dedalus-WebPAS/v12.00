GETUCD00   MOVE     SP70,DBPTUC1D
           MOVE     SP70,DBPTUC2D
           MOVE     SP70,DBDEDES
           PACK     KEY8,DBPTDEP,SP70
           CALL     RDDBDE1
           MATCH    SP70,DBPTUC1
           IF       !@EQUAL
             MOVE     "PT01",DBCDCAT
             PACK     KEY7,DBCDCAT,DBPTUC1
             CALL     RDDBCD1
             IF       OVRCD=0
               MOVE     DBCDDES,DBPTUC1D
             ENDIF
           ENDIF
           MATCH    SP70,DBPTUC2
           IF       !@EQUAL
             MOVE     "PT02",DBCDCAT
             PACK     KEY7,DBCDCAT,DBPTUC2
             CALL     RDDBCD1
             IF       OVRCD=0
               MOVE     DBCDDES,DBPTUC2D
             ENDIF
           ENDIF
GETUCD99   RETURN
EDEPT000  MOVE      ZERO,CKEYTYP
          CALL      KDBDEA00
          BRANCH    EXIT,EDEPT999,EDEPT999
          IF        DBDEACT=0
            MOVE      "Department NOT Active - ",DISPMSG
            CALL      MESSAGE1
            GOTO      EDEPT000
          ENDIF
          MOVE      DBDEDEP,DBPTDEP
EDEPT999  RETURN
