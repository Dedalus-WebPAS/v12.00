.---------------------------------------------------------------------------
.   System    :   Patient System (New Standards)     
.   Include   :   CEACODKY
.   Routine   :   KPCODA00 Keyin Code                
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KPCODA00  TOPIC     "ACODE "
          MOVE      UNDLN70,ACODE 
          KEYIN     *PCCOL:CVERT,*DV,ACODE:                                   
                    *PCCOL:CVERT,*V2LON,ACODE:                                
                    *PCCOL:CVERT,*DV,ACODE 
.
          ENDSET    ACODE 
          APPEND    SP70,ACODE 
          RESET     ACODE 
.
          MATCH     SP70,ACODE 
          GOTO      KPCODA90 IF EQUAL
.
          CMATCH    EXITCHAR,ACODE 
          GOTO      KPCODA95 IF EQUAL
.
          CMATCH    QUEST,ACODE 
          GOTO      KPCODA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HPCODA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    ACODE 
          APPEND    SP70,ACODE 
          RESET     ACODE 
.
          MATCH     SP70,ACODE 
          GOTO      KPCODA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,ACODE                                 
KPCODA10
          PACK      KEY5,TCODE,ACODE                                        
          CALL      RDACODE1 
          BRANCH    OVRCD,KPCODA80
          BRANCH    CKEYTYP,KPCODA75
          CALL      RDCODE1 
KPCODA70
          MOVE      ZERO,EXIT
          GOTO      KPCODA99
KPCODA75
          MOVE      "Code Already Exist - ",DISPMSG                                                  
          CALL      MESSAGE1
          GOTO      KPCODA00
KPCODA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KPCODA99,KPCODA99
          MOVE      "Code Does Not Exist - ",DISPMSG                                                 
          CALL      MESSAGE1
          GOTO      KPCODA00
KPCODA90
          MOVE      ONE,EXIT
          GOTO      KPCODA99
KPCODA95
          MOVE      TWO,EXIT
KPCODA99  TOPIC     PRGID
          RETURN
.
.---------------------------------------------------------------------------
.   System    :   Patient System (New Standards)     
.   Include   :   CEACODKY
.   Routine   :   HPCODA00 Code                
.   Return    :           
.-------------------------------------------------------------------------------
HPCODA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HPCODA04
          CALL      GSCRN000
          BRANCH    EXIT,HPCODA99
.
          CALL      EPCODA00
.
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Code"               
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 5",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "21",STDCOL03
HPCODA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,ACODE 
          PACK      KEY5,TCODE,ACODE,SP70                                   
          PACK      SAVKEY80,TCODE,ACODE,SP70                               
          SETLPTR   SAVKEY80,2                                                  
          CALL      RDSCODE1 
HPCODA10
          CALL      RDKCODE1 
          BRANCH    OVRCD,HPCODA15
          PACK      KEY5,TCODE,ACODE,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HPCODA15 IF NOT EQUAL                                       
          BRANCH    CSEARCH,HPCODA12
          CALL      HPCODA60
          BRANCH    OVRCD,HPCODA10
HPCODA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY5,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,ACODE 
          DISPLAY   *PSTDCOL02:STDROW,TDESC 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HPCODA10 IF NOT EQUAL
HPCODA15
          UNPACK    SAVKEY80,TCODE,ACODE                                    
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HPCODA20
          CALL      HSCRI000
          BRANCH    EXIT,HPCODA30,HPCODA35,HPCODA40,HPCODA45,HPCODA04:
                              HPCODA91,HPCODA50
          GOTO      HPCODA92
HPCODA30
          CALL      RDKCODE1 
          BRANCH    OVRCD,HPCODA85
          PACK      KEY5,TCODE,ACODE,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HPCODA85 IF NOT EQUAL                                       
          CALL      RDPCODE1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HPCODA10
HPCODA35
          MOVE      ST1,KEY5 
          CALL      RDCODE1 
          CALL      RDPCODE1 
          BRANCH    OVRCD,HPCODA85
          PACK      KEY5,TCODE,ACODE,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HPCODA85 IF NOT EQUAL                                       
          MATCH     SP70,ACODE                                                
          GOTO      HPCODA85 IF EQUAL                                           
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HPCODA36
          CALL      RDPCODE1 
          BRANCH    OVRCD,HPCODA37
          PACK      KEY5,TCODE,ACODE,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HPCODA37 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HPCODA36 IF NOT EQUAL
HPCODA37
          MOVE      ZERO,XITEMNO
          GOTO      HPCODA10
HPCODA40
          MOVE      ST1,KEY5 
          CALL      RDCODE1 
          CALL      RDPCODE1 
          BRANCH    OVRCD,HPCODA85
          PACK      KEY5,TCODE,ACODE,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HPCODA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HPCODA08
HPCODA45
          CALL      RDKCODE1 
          BRANCH    OVRCD,HPCODA85
          PACK      KEY5,TCODE,ACODE,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HPCODA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY5,SAVKEY80,Z70                                           
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RDSCODE1 
HPCODA46
          CALL      RDPCODE1 
          BRANCH    OVRCD,HPCODA47
          PACK      KEY5,TCODE,ACODE,SP70                                   
          MATCH     SAVKEY80,KEY5                                               
          GOTO      HPCODA47 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HPCODA46 IF NOT EQUAL
HPCODA47
          MOVE      ZERO,XITEMNO
          GOTO      HPCODA10
HPCODA50  COMPARE   ZERO,CSEARCH
          GOTO      HPCODA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HPCODA08
HPCODA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HPCODA08
HPCODA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HPCODA61 IF EQUAL
          MATCH     SCHFLD01,ACODE 
          GOTO      HPCODA72 IF NOT EQUAL
HPCODA61
          MATCH     SP70,SCHFLD02
          GOTO      HPCODA62 IF EQUAL
          MATCH     SCHFLD02,TDESC 
          GOTO      HPCODA72 IF NOT EQUAL
HPCODA62
HPCODA63
HPCODA64
HPCODA65
HPCODA66
HPCODA67
HPCODA68
HPCODA69
HPCODA70
          GOTO      HPCODA75
HPCODA72  MOVE      ONE,OVRCD
HPCODA75  RETURN
HPCODA85
          LOAD      KEY5,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY5,TCODE,ACODE                                        
          CALL      RDCODE1 
HPCODA90
          UNPACK    SAVKEY80,TCODE,ACODE                                    
          BEEP
          GOTO      HPCODA20
HPCODA91
          MOVE      SP70,KEY5 
          MOVE      SP70,ACODE 
          MOVE      SP70,TDESC 
          MOVE      SP70,THCSCOD
          MOVE      ZERO,TCFORM6 
          MOVE      SP70,TCINDC1 
          MOVE      SP70,TCINDC2 
          MOVE      SP70,TCINDC3 
          MOVE      SP70,TCINDC4 
          MOVE      SP70,TCINDC5 
          MOVE      SP70,PTCOACTV
          MOVE      SP70,TCINDC6 
          MOVE      SP70,TCINDC7 
          MOVE      SP70,TCINDC8 
          MOVE      SP70,TCINDC9 
          MOVE      SP70,TCINDC10
          MOVE      SP70,TCINDC11
          MOVE      SP70,PTCDGRPV
          MOVE      SP70,PTCDDEFT
          MOVE      SP70,TCSPARE
          GOTO      HPCODA96
HPCODA92
          MOVE      SP70,KEY5 
          MOVE      SP70,ACODE 
          LOAD      KEY5,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY5,TCODE,ACODE                                        
HPCODA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HPCODA99
          RETURN
.
.---------------------------------------------------------------------------
.   System    :   Patient System (New Standards)     
.   Include   :   CEACODKY
.   Routine   :   EPCODA00 Code                
.   Return    :           
.-------------------------------------------------------------------------------
EPCODA00  MOVE      STDCOL00,STDCOL01
          PACK      KEY5,TCODE,SP70
          CALL      RDCODE1
          IF        OVRCD=1
            MOVE      "Invalid Category",TDESC
          ENDIF
.
          ADD       "3",STDCOL01
          SUB       "1",STDROW              12345678901
          DISPLAY   *PSTDCOL01:STDROW,*HON,"Category : ",TCODE,SP1,TDESC
          ADD       "1",STDROW  
.
          RETURN
