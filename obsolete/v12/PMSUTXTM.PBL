.-------------------------------------------------------------------------------
. Add Program Maintenance Comments
.-------------------------------------------------------------------------------
PMUX0000  BRANCH    FLDITMNO,PMUX0100,PMUX0200,PMUX0300,PMUX0400,PMUX0500:
                             PMUX0600,PMUX0700,PMUX0800,PMUX0900,PMUX1000
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      PMUX9999
.
PMUX0100  MATCH     SP70,PMUXURNO
          GOTO      PMUX9999 IF EQUAL
          WRITE     HTMLFILE;PMUXURNO;
          GOTO      PMUX9999
.
PMUX0200  MOVE      "A ",CATEGORY
          MOVE      PMUXATYP,CODE
          CALL      CODITM00
          GOTO      PMUX9999
.
PMUX0300  MOVE      "CL",CATEGORY
          MOVE      PMUXATYP,CODE
          CALL      CODITM00
          GOTO      PMUX9999
.
PMUX0400   UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      ZERO,F1
          MOVE      KEY1,F1
          BRANCH    F1,PMUX0410
          WRITE     HTMLFILE;PMUXHFND;
          GOTO      PMUX9999
.
PMUX0410  PACK      KEY14,PMUXHFND,ZERO,ZERO,ZERO,ZERO,SP10
          CALL      RDFUND1
          IF        OVRCD=0
            WRITE     HTMLFILE;HFNAME;
          ELSE
            WRITE     HTMLFILE;PMUXHFND;
          ENDIF
          GOTO      PMUX9999
.
PMUX0500  MOVE      "HT",CATEGORY
          MOVE      PMUXHTAB,CODE
          CALL      CODITM00
          GOTO      PMUX9999
.
PMUX0600  MATCH     PMUXEXPD,SP70
          GOTO      PMUX9999 IF EQUAL
.
          UNPACK    PMUXEXPD,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUX9999
.
PMUX0700  MATCH     SP70,PMUXCOMT
          GOTO      PMUX9999 IF EQUAL
          WRITE     HTMLFILE;PMUXCOMT;
          GOTO      PMUX9999
.
PMUX0800  MATCH     SP70,PMUXNNUM
          GOTO      PMUX9999 IF EQUAL
          WRITE     HTMLFILE;PMUXNNUM;
.
PMUX0900  MATCH     SP70,PMUXLNUM
          GOTO      PMUX9999 IF EQUAL
          WRITE     HTMLFILE;PMUXLNUM;
.
PMUX1000  MATCH     SP70,PMUXCOMM
          GOTO      PMUX9999 IF EQUAL
          WRITE     HTMLFILE;PMUXCOMM;
          GOTO      PMUX9999
.
PMUX9999  RETURN
.
.------------------------------------------------------------
.   Set Parameters for Program Comments Header
.------------------------------------------------------------
SPMUX000  MOVE      ZERO,EXIT
          UNPACK    QRYNAME,KEY5,KEY3
          MOVE      KEY3,F3
          BRANCH    F3,SPMUX010,SPMUX020,SPMUX030,SPMUX040,SPMUX050:
                       SPMUX060,SPMUX070,SPMUX080,SPMUX090,SPMUX100
.                      
          MOVE      ONE,EXIT
          GOTO      SPMUX999
.
SPMUX010  PACK      PMUTX001,QRYDATA,SP70
          GOTO      SPMUX999
.
SPMUX020  PACK      PMUTX002,QRYDATA,SP70
          GOTO      SPMUX999
.
SPMUX030  PACK      PMUTX003,QRYDATA,SP70
          GOTO      SPMUX999
.
SPMUX040  PACK      PMUTX004,QRYDATA,SP70
          GOTO      SPMUX999
.
SPMUX050  MOVE      QRYDATA,PMUTX005
          GOTO      SPMUX999
.
SPMUX060  UNPACK    QRYDATA,CDAY,KEY1,MTHNAM3,KEY1,CCENT,CYEAR
          CALL      SETMTH00                *Set CMON from MTHNAM3
          PACK      PMUTX016,CCENT,CYEAR,CMON,CDAY
          GOTO      SPMUX999
.
SPMUX070  MOVE      QRYDATA,PMUTX007
          GOTO      SPMUX999
.
SPMUX080  MOVE      QRYDATA,PMUTX008
          GOTO      SPMUX999
.
SPMUX090  MOVE      QRYDATA,PMUTX009
          GOTO      SPMUX999
.
SPMUX100  MOVE      QRYDATA,PMUTX010
          GOTO      SPMUX999
.
SPMUX990  RETURN
.
