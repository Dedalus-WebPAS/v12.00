          IF        !@EQUAL
            MOVE       PMUTX001,PMUXURNO
          ENDIF
.
          MATCH     PMUTX002,Z70
          IF        !@EQUAL
            MOVE      PMUTX002,PMUXATYP
          ENDIF
.
          MATCH     PMUTX003,Z70
          IF        !@EQUAL
