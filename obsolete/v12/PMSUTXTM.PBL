          ENDIF
.
          MATCH     PMUTX004,Z70
          IF        !@EQUAL
            MOVE      PMUTX004,PMUXHFND
          ENDIF
.
          MATCH     PMUTX005,Z70
          IF        !@EQUAL
            MOVE      PMUTX005,PMUXHTAB
          ENDIF
.
          MATCH     PMUTX006,Z70   
          IF        !@EQUAL
            MOVE      PMUTX006,PMUXEXPD
          ENDIF
.
          MATCH     PMUTX007,Z70
          IF        !@EQUAL
            MOVE      PMUTX007,PMUXCOMT
          ENDIF
.
          MATCH     PMUTX008,Z70
          IF        !@EQUAL
            MOVE      PMUTX008,PMUXNNUM
          ENDIF
.
          MATCH     PMUTX009,Z70
          IF        !@EQUAL
            MOVE      PMUTX009,PMUXLNUM
          ENDIF
.
          MATCH     PMUTX010,Z70
          IF        !@EQUAL
