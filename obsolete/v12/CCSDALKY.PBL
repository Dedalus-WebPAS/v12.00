ECCDAA00  MOVE      CCDACTY,KEY4
          CALL      RDCCCT1
          IF        OVRCD=1
            MOVE      "Invalid",CCCTDES
          ENDIF
          RETURN
.
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSDALKY
.   Routine   :   HCCDAA00 Department/Cost Type
.   Return    :           
.-------------------------------------------------------------------------------
HCCDAA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCDAA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCDAA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Cost Type"          
.
          MOVE      STDCOL01,STDCOL02
          ADD       "10",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "35",STDCOL03
HCCDAA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCDACTY 
          PACK      KEY14,CCDAHCD,CCDADPT,CCDACTY,SP70                          
          PACK      SAVKEY80,CCDAHCD,CCDADPT,CCDACTY,SP70                       
          SETLPTR   SAVKEY80,10                                                 
          CALL      RSCCDA1 
HCCDAA10
          CALL      RKCCDA1 
          BRANCH    OVRCD,HCCDAA15
          PACK      KEY14,CCDAHCD,CCDADPT,CCDACTY,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCDAA15 IF NOT EQUAL                                       
          CALL      ECCDAA00
          BRANCH    CSEARCH,HCCDAA12
          CALL      HCCDAA60
          BRANCH    OVRCD,HCCDAA10
HCCDAA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY14,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCDACTY 
          DISPLAY   *PSTDCOL02:STDROW,CCCTDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCDAA10 IF NOT EQUAL
HCCDAA15
          UNPACK    SAVKEY80,CCDAHCD,CCDADPT,CCDACTY                            
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCDAA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCDAA30,HCCDAA35,HCCDAA40,HCCDAA45,HCCDAA04:
                              HCCDAA91,HCCDAA50
          GOTO      HCCDAA92
HCCDAA30
          CALL      RKCCDA1 
          BRANCH    OVRCD,HCCDAA85
          PACK      KEY14,CCDAHCD,CCDADPT,CCDACTY,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCDAA85 IF NOT EQUAL                                       
          CALL      RPCCDA1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCDAA10
HCCDAA35
          MOVE      ST1,KEY14
          CALL      RDCCDA1 
          CALL      RPCCDA1 
          BRANCH    OVRCD,HCCDAA85
          PACK      KEY14,CCDAHCD,CCDADPT,CCDACTY,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCDAA85 IF NOT EQUAL                                       
          MATCH     SP70,CCDACTY                                                
          GOTO      HCCDAA85 IF EQUAL                                           
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCDAA36
          CALL      RPCCDA1 
          BRANCH    OVRCD,HCCDAA37
          PACK      KEY14,CCDAHCD,CCDADPT,CCDACTY,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCDAA37 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCDAA36 IF NOT EQUAL
HCCDAA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCDAA10
HCCDAA40
          MOVE      ST1,KEY14
          CALL      RDCCDA1 
          CALL      RPCCDA1 
          BRANCH    OVRCD,HCCDAA85
          PACK      KEY14,CCDAHCD,CCDADPT,CCDACTY,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCDAA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCDAA08
HCCDAA45
          CALL      RKCCDA1 
          BRANCH    OVRCD,HCCDAA85
          PACK      KEY14,CCDAHCD,CCDADPT,CCDACTY,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCDAA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY14,SAVKEY80,Z70                                          
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCDA1 
HCCDAA46
          CALL      RPCCDA1 
          BRANCH    OVRCD,HCCDAA47
          PACK      KEY14,CCDAHCD,CCDADPT,CCDACTY,SP70                          
          MATCH     SAVKEY80,KEY14                                              
          GOTO      HCCDAA47 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCDAA46 IF NOT EQUAL
HCCDAA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCDAA10
HCCDAA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCDAA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCDAA08
HCCDAA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCDAA08
HCCDAA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCDAA61 IF EQUAL
          MATCH     SCHFLD01,CCDACTY 
          GOTO      HCCDAA72 IF NOT EQUAL
HCCDAA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCDAA62 IF EQUAL
          MATCH     SCHFLD02,CCCTDES 
          GOTO      HCCDAA72 IF NOT EQUAL
HCCDAA62
HCCDAA63
HCCDAA64
HCCDAA65
HCCDAA66
HCCDAA67
HCCDAA68
HCCDAA69
HCCDAA70
          GOTO      HCCDAA75
HCCDAA72  MOVE      ONE,OVRCD
HCCDAA75  RETURN
HCCDAA85
          LOAD      KEY14,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY14,CCDAHCD,CCDADPT,CCDACTY                               
          CALL      RDCCDA1 
HCCDAA90
          UNPACK    SAVKEY80,CCDAHCD,CCDADPT,CCDACTY                            
          BEEP
          GOTO      HCCDAA20
HCCDAA91
          MOVE      SP70,CCDAATY 
          MOVE      SP70,CCDASPA 
          MOVE      SP70,KEY14
          MOVE      SP70,CCDACTY 
          GOTO      HCCDAA96
HCCDAA92
          MOVE      SP70,KEY14
          MOVE      SP70,CCDACTY 
          LOAD      KEY14,CCITEM,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY14,CCDAHCD,CCDADPT,CCDACTY                               
HCCDAA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCDAA99
          RETURN
