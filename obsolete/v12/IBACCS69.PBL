. Patient Service Usage Model Analysis
.----------------------------------------
REPT0000  MOVE      "Printing : ",DISPMSG
          CALL      MESSAGE2
          MOVE      ZERO,CPAGENO
          CLOCK     TIME,CTIMEIS
          MOVE      "99",CLNO
.
          MOVE      CCCAYEAR,KEY4
          PACK      KEY6,KEY4,FPERIOD
          CALL      RDCCCA1
          MATCH     "  ",FPERIOD
          IF        @EQUAL
            CALL      RKCCCA1
          ENDIF
          MOVE      CCCADES,FROMPNAM
          MOVE      KEY4,CCCAYEAR
.
          PACK      KEY6,KEY4,LPERIOD
          CALL      RDCCCA1
          MATCH     "ZZ",LPERIOD
          IF        @EQUAL
            CALL      RPCCCA1
          ENDIF
          MOVE      CCCADES,TOPNAM
          MOVE      KEY4,CCCAYEAR
.
          PACK      KEY10,CCHCHCD,FDEPART
          CALL      RDCCDP1
          IF        OVRCD=0
            CALL      RPCCDP1
          ENDIF
.
REPT1000  CALL      RKCCDP1
          BRANCH    OVRCD,REPT9000
          MATCH     CCHCHCD,CCDPHCD
          GOTO      REPT9000 IF NOT EQUAL
          MATCH     CCDPDPT,LDEPART
          GOTO      REPT9000 IF LESS
.
          MATCH     "1",CCDPDTY
          GOTO      REPT1000 IF EQUAL
.
          DISPLAY   *P12:24,CCDPDPT
          CALL      HEAD0000        * Page Break for each department
.
          CALL      PDPT0000        * Print Department Details
.
          GOTO      REPT1000
.
REPT9000  CALL      UND132
          PRINT     *55,"*** End of Report ***"
          CALL      MESSAGE3
          MOVE      "Report Completed - ",DISPMSG
          CALL      MESSAGE1
          RETURN
.------------------------------------------------------------
. Print Report Heading
.------------------------------------------------------------
HEAD0000  CALL      HEAD132
          MOVE      "9",CLNO
          IF        CPAGENO=1
            PRINT     "Department From  : ",FROMDEPT," to ",TODEPT
            ADD       "1",CLNO
          ENDIF
          PRINT     "Costing Model    : ",CCMACOM,SP5,CCMADES
          IF        INCIND=0
            PRINT     *50,"***** NB :  Indirect Costs Excluded   *****"
            ADD       "1",CLNO
          ENDIF
          PRINT     "Hospital         : ",CCHCHCD,SP7,CCHCDES:
                    " Year   : ",CCCAYEAR
          PRINT     "Department       : ",CCDPDPT,SP1,CCDPDES:
                    " Period : ",FROMPNAM," to ",TOPNAM
          CALL      UND132
          PRINT     "| Seq. Patient Service                         |":
                    "            Volume         |         Total C":
                    "ost        |          Unit Cost        |"
          PRINT     "|                                              |":
                    "       Budget       Actual |       Budget   ":
                    "    Actual |       Budget       Actual |"
          CALL      UND132
HEAD9999  RETURN
.------------------------------------------------------------
. Print Department Details
.------------------------------------------------------------
PDPT0000  MOVE      ZERO,ACTTOTAL
          MOVE      ZERO,BUDTOTAL
          MOVE      ZERO,VACTTOT
          MOVE      ZERO,VBUDTOT
          PACK      KEY10,CCDPHCD,CCDPDPT,SP70
          PACK      KEY21,CCDPHCD,CCDPDPT,SP70
          CALL      RSCCPD4
PDPT1000  CALL      RKCCPD4
          BRANCH    OVRCD,PDPT9000
          PACK      KEY21,CCPDHCD,CCPDDPT,CCPDPCD
          MATCH     KEY10,KEY21
          GOTO      PDPT9000 IF NOT EQUAL
.
          CALL      PPCD0000          * Print Patient Service Code Details
.
          GOTO      PDPT1000
.
. Print Department Totals
.
PDPT9000  MOVE      CCFED1,FTOTAL1
          MOVE      CCFED1,FTOTAL2
          MOVE      CCFED1,FTOTAL3
          MOVE      CCFED1,FTOTAL4
          MOVE      CCFED2,FTOTAL5
          MOVE      CCFED2,FTOTAL6
.
          FEDIT     VBUDTOT,FTOTAL1
          FEDIT     VACTTOT,FTOTAL2
          FEDIT     BUDTOTAL,FTOTAL3
          FEDIT     ACTTOTAL,FTOTAL4
          ASSIGN    BUDTOTAL/VBUDTOT,F12P2
          FEDIT     F12P2,FTOTAL5
          IF        VBUDTOT=0|BUDTOTAL=0
            MOVE      SP70,FTOTAL5
          ENDIF
          ASSIGN    ACTTOTAL/VACTTOT,F12P2
          FEDIT     F12P2,FTOTAL6
          IF        VACTTOT=0|ACTTOTAL=0
            MOVE      SP70,FTOTAL6
          ENDIF
.
          ADD       THREE,CLNO
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
          CALL      UND132
.
          MOVE      SP70,FTOTAL5
          MOVE      SP70,FTOTAL6
          PRINT     "|",*42,"Total |",FTOTAL1,FTOTAL2," |",FTOTAL3,FTOTAL4," |":
                         FTOTAL5,FTOTAL6," |"
          RETURN
.------------------------------------------------------------
. Print Patient Service Code Details
.------------------------------------------------------------
PPCD0000  CALL      CALC0000
.
          ADD       ONE,CLNO
          COMPARE   "50",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          MOVE      CCFED1,FTOTAL1
          MOVE      CCFED1,FTOTAL2
          MOVE      CCFED1,FTOTAL3
          MOVE      CCFED1,FTOTAL4
          MOVE      CCFED2,FTOTAL5
          MOVE      CCFED2,FTOTAL6
.
          FEDIT     VBUDGET,FTOTAL1
          FEDIT     VACTUAL,FTOTAL2
          FEDIT     BUDGET,FTOTAL3
          FEDIT     ACTUAL,FTOTAL4
          ASSIGN    BUDGET/VBUDGET,F12P2
          FEDIT     F12P2,FTOTAL5
          IF        VBUDGET=0|BUDGET=0
            MOVE      SP70,FTOTAL5
          ENDIF
          ASSIGN    ACTUAL/VACTUAL,F12P2
          FEDIT     F12P2,FTOTAL6
          IF        VACTUAL=0|ACTUAL=0
            MOVE      SP70,FTOTAL6
          ENDIF
.
          PRINT     "| ",CCPDSEQ,SP2,CCPDDES,"     |",FTOTAL1,FTOTAL2," |":
                                                      FTOTAL3,FTOTAL4," |":
                                                      FTOTAL5,FTOTAL6," |"
.
PPCD9999  RETURN
