          MOVE      "    5",ITMSSFFI
          CALL      FP01L000
          GOTO     SF01L001
.
SF01L101
          CALL      SELECTED
          BRANCH    EXIT,SF01L001
          MOVE      "CCDS2",NAMSSFFI
          MOVE      SCPSITM,ITMSSFFI
          IF        ACCEPT=0
            PACK      KEY5,SCPSROW,SCPSCOL
            CALL      TRCCDS21
            BRANCH    OVRCD,SF01L001
            MATCH     SCTMROW,SCSLROW
            GOTO      SF01L001 IF NOT EQUAL
            MATCH     SCTMCOL,SCSLCOL
            GOTO      SF01L001 IF NOT EQUAL
            CALL      SM01L000 * Change Line
          ELSE
            IF        CCDS2AFL=0
              MOVE      "CCDS2",NAMSSFFI
              MOVE     "    5",ITMSSFFI
              CALL      AD01L000 * Add Line
            ENDIF
          ENDIF
          GOTO     SF01L001
.
SF01L102
          MOVE      ONE,EXIT
          GOTO      SF01L999
.
SF01L103
          CALL      WCCDS000
          MOVE      ONE,EXIT
          GOTO      SF01L999
.
SF01L104
          MOVE      "CCDS2",NAMSSFFI
          MOVE      "    5",ITMSSFFI
          CALL      NP01L000
          GOTO     SF01L001
.
SF01L105
