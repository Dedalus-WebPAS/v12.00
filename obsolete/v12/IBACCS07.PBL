          PACK      KEY4,CCDSSEC
          CALL      RDPASS1
          IF        OVRCD=1
            MOVE      "User Unknown",SECUSER
          ENDIF
          RETURN
.----------------------------------------
. Write Department Security
.----------------------------------------
WCCDS000
          PACK      KEY14,CCHCHCD,CCDPDPT,SP70
          CALL      RSCCDS2
WCCDS100
          CALL      RKCCDS2
          BRANCH    OVRCD,WCCDS200
          MATCH     CCDSHCD,CCHCHCD
          GOTO      WCCDS200 IF NOT EQUAL
          MATCH     CCDSDPT,CCDPDPT
          GOTO      WCCDS200 IF NOT EQUAL
          PACK      KEY14,CCDSHCD,CCDSDPT,CCDSSEC
          CALL      DECCDS2
          GOTO      WCCDS100
WCCDS200
          MOVE      SP70,KEY5
          CALL      TRCCDS21
          COMPARE   ZERO,OVRCD
          GOTO      WCCDS310 IF EQUAL
WCCDS300
          CALL      TKCCDS21
          BRANCH    OVRCD,WCCDS400
WCCDS310
          PACK      KEY14,CCDSHCD,CCDSDPT,CCDSSEC
          CALL      RACCDS2
          IF        OVRCD=0
            CALL      UPCCDS2
          ELSE
            CALL      WRCCDS2
          ENDIF
          GOTO      WCCDS300
WCCDS400
          RETURN
