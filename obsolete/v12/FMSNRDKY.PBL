.---------------------------------------------------------------------------
.   System    :   Financial Management System
.   Include   :   FMSNRDKY
.   Routine   :   HFMNDA00 Subjective Template
.   Return    :   FMNDCODE
.-------------------------------------------------------------------------------
HFMNDA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "30",HLEF
            MOVE      "80",HRIG
          ENDIF
HFMNDA04
          CALL      GSCRN000
          BRANCH    EXIT,HFMNDA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Subjective Template"
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 5",STDCOL02
          MOVE      STDCOL02,STDCOL03
          ADD       "42",STDCOL03
          DISPLAY   *PSTDCOL03:STDROW,*V2LON,*ULON,"Ledger"
.
          MOVE      STDCOL03,STDCOL04
          ADD       " 4",STDCOL04
HFMNDA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,FMNDCODE
          PACK      KEY3,FMNDCODE,SP70
.
.
          CALL      RSFMND1
HFMNDA10
          CALL      RKFMND1
          BRANCH    OVRCD,HFMNDA15
          PACK      KEY3,FMNDCODE,SP70
.
.
          BRANCH    CSEARCH,HFMNDA12
          CALL      HFMNDA60
          BRANCH    OVRCD,HFMNDA10
HFMNDA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,FMNDCODE
          DISPLAY   *PSTDCOL02:STDROW,FMNDDESC
          DISPLAY   *PSTDCOL03:STDROW,FMNDLEDG
          COMPARE   MAXITEM,XITEMNO
          GOTO      HFMNDA10 IF NOT EQUAL
HFMNDA15
.
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HFMNDA20
          CALL      HSCRI000
          BRANCH    EXIT,HFMNDA30,HFMNDA35,HFMNDA40,HFMNDA45,HFMNDA04:
                              HFMNDA91,HFMNDA50
          GOTO      HFMNDA92
HFMNDA30
          CALL      RKFMND1
          BRANCH    OVRCD,HFMNDA85
          PACK      KEY3,FMNDCODE,SP70
.
.
          CALL      RPFMND1
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMNDA10
HFMNDA35
          MOVE      ST1,KEY3
          CALL      RDFMND1
          CALL      RPFMND1
          BRANCH    OVRCD,HFMNDA85
          PACK      KEY3,FMNDCODE,SP70
.
.
.
.
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HFMNDA36
          CALL      RPFMND1
          BRANCH    OVRCD,HFMNDA37
          PACK      KEY3,FMNDCODE,SP70
.
.
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HFMNDA36 IF NOT EQUAL
HFMNDA37
          MOVE      ZERO,XITEMNO
          GOTO      HFMNDA10
HFMNDA40
          MOVE      ST1,KEY3
          CALL      RDFMND1
          CALL      RPFMND1
          BRANCH    OVRCD,HFMNDA85
          PACK      KEY3,FMNDCODE,SP70
.
.
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMNDA08
HFMNDA45
          CALL      RKFMND1
          BRANCH    OVRCD,HFMNDA85
          PACK      KEY3,FMNDCODE,SP70
.
.
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY3,Z70
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSFMND1
HFMNDA46
          CALL      RPFMND1
          BRANCH    OVRCD,HFMNDA47
          PACK      KEY3,FMNDCODE,SP70
.
.
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HFMNDA46 IF NOT EQUAL
HFMNDA47
          MOVE      ZERO,XITEMNO
          GOTO      HFMNDA10
HFMNDA50  COMPARE   ZERO,CSEARCH
          GOTO      HFMNDA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          KEYIN     *PSTDCOL03:STDROW,*V2LON,SCHFLD03
          RESET     SCHFLD03
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMNDA08
HFMNDA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMNDA08
HFMNDA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HFMNDA61 IF EQUAL
          MATCH     SCHFLD01,FMNDCODE
          GOTO      HFMNDA72 IF NOT EQUAL
HFMNDA61
          MATCH     SP70,SCHFLD02
          GOTO      HFMNDA62 IF EQUAL
          MATCH     SCHFLD02,FMNDDESC
          GOTO      HFMNDA72 IF NOT EQUAL
HFMNDA62
          MATCH     SP70,SCHFLD03
          GOTO      HFMNDA63 IF EQUAL
          MATCH     SCHFLD03,FMNDLEDG
          GOTO      HFMNDA72 IF NOT EQUAL
HFMNDA63
HFMNDA64
HFMNDA65
HFMNDA66
HFMNDA67
HFMNDA68
HFMNDA69
HFMNDA70
          GOTO      HFMNDA75
HFMNDA72  MOVE      ONE,OVRCD
HFMNDA75  RETURN
HFMNDA85
          LOAD      KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,FMNDCODE
          CALL      RDFMND1
HFMNDA90
.
          BEEP
          GOTO      HFMNDA20
HFMNDA91
          MOVE      SP70,FMNDLEDG
          MOVE      SP70,FMNDDESC
          MOVE      SP70,FMNDSPAR
          MOVE      SP70,KEY3
          MOVE      SP70,FMNDCODE
          GOTO      HFMNDA96
HFMNDA92
          MOVE      SP70,KEY3
          MOVE      SP70,FMNDCODE
          LOAD      KEY3,CCITEM,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,FMNDCODE
HFMNDA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HFMNDA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Financial Management System
.   Include   :   FMSNRDKY
.   Routine   :   KFMNDA00 Keyin Subjective Template
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KFMNDA00
          MOVE      UNDLN70,FMNDCODE
          KEYIN     *PCCOL:CVERT,*DV,FMNDCODE:
                    *PCCOL:CVERT,*V2LON,FMNDCODE:
                    *PCCOL:CVERT,*DV,FMNDCODE
.
          ENDSET    FMNDCODE
          APPEND    SP70,FMNDCODE
          RESET     FMNDCODE
.
          MATCH     SP70,FMNDCODE
          GOTO      KFMNDA90 IF EQUAL
.
          CMATCH    EXITCHAR,FMNDCODE
          GOTO      KFMNDA95 IF EQUAL
.
          CMATCH    QUEST,FMNDCODE
          GOTO      KFMNDA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HFMNDA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    FMNDCODE
          APPEND    SP70,FMNDCODE
          RESET     FMNDCODE
.
          MATCH     SP70,FMNDCODE
          GOTO      KFMNDA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,FMNDCODE
KFMNDA10
          PACK      KEY3,FMNDCODE
          CALL      RAFMND1
          BRANCH    OVRCD,KFMNDA80
          BRANCH    CKEYTYP,KFMNDA75
          CALL      RDFMND1
KFMNDA70
          MOVE      ZERO,EXIT
          GOTO      KFMNDA99
KFMNDA75
          MOVE      "Subjective Template Already Exists - ",DISPMSG
          CALL      MESSAGE1
          GOTO      KFMNDA00
KFMNDA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KFMNDA99,KFMNDA99
          MOVE      "Subjective Template Does Not Exist - ",DISPMSG
          CALL      MESSAGE1
          GOTO      KFMNDA00
KFMNDA90
          MOVE      ONE,EXIT
          GOTO      KFMNDA99
KFMNDA95
          MOVE      TWO,EXIT
KFMNDA99
          RETURN
