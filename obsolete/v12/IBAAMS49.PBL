          CALL      HEAD0000 IF EQUAL
          CALL      UND132
          MOVE      "999,999,999,999.99",DIM18
          FEDIT     TOTAMTRA,DIM18
          PRINT     *102,"Total Amount ",DIM18,*N:
                    *45,"*** End of Report ***"
