REPT0000  DISPLAY   *P1:24,*EL,"Printing Report : ";
          MOVE      ZERO,CPAGENO          * reset page no.
          CLOCK     TIME,CTIMEIS          * get current time
          MOVE      "99",CLNO             * init last line number
          MOVE      ZERO,TOTAMTRA         * init total amount
.
          PACK      KEY24,RUNNO,SP70
          PACK      KEY5,RUNNO,SP70
          MOVE      SP70,KEY15
          CALL      RSAMTR2
REPT1000  CALL      RKAMTR2
          BRANCH    OVRCD,REPT1900
          PACK      KEY5,RUNNO,SP70
          MATCH     AMTRIRUN,KEY5
          GOTO      REPT1900 IF NOT EQUAL
          PACK      KEY14,AMTRREG,AMTRASS
          MATCH     KEY14,KEY15
          IF        !@EQUAL
            CALL      RDAMMA1
            BRANCH    OVRCD,REPT1000
          ENDIF
.
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          MATCH     KEY14,KEY15
          IF        !@EQUAL
            MOVE      KEY14,KEY15
            ADD       ONE,CLNO
            PRINT     AMTRREG,"/",AMTRASS,SP1,AMMADES
          ENDIF
.
          ADD       ONE,CLNO
          MOVE      "99,999,999.99",KEY13
          FEDIT     AMTRAMT,KEY13
          ADD       AMTRAMT,TOTAMTRA
          PRINT     *17,AMTRPER,"/",AMTRYEAR,SP6,CPCDATE,SP2,AMTRREF,SP1:
                    AMTRCOM,SP1,AMTRDLED,"/",AMTRDACC,SP3,AMTRCACC,SP1,KEY13
          GOTO      REPT1000
.
