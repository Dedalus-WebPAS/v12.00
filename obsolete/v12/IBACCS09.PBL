          GOTO      WCCPF200 IF NOT EQUAL
          CALL      DECCPK2
          GOTO      WCCPF100
WCCPF200
          MOVE      SP70,KEY5
          CALL      TRCCPK21
          COMPARE   ZERO,OVRCD
          GOTO      WCCPF310 IF EQUAL
WCCPF300
          CALL      TKCCPK21
          BRANCH    OVRCD,WCCPF400
WCCPF310
          PACK      KEY38,CCHCHCD,CCDPDPT,CCPDPCD,CCPKFKY
          UNPACK    KEY38,CCPKHCD,CCPKDPT,CCPKPCD,CCPKFKY
          CALL      RACCPK2
          IF        OVRCD=0
            CALL      UPCCPK2
          ELSE
            CALL      WRCCPK2
