KCCRVI99  RETURN
.
.----------------------------------------
. Save Changes
.----------------------------------------
WCCPD000  PACK      KEY18,CCPDHCD,CCPDDPT,CCPDPCD
          CALL      RACCPD1
          IF        OVRCD=0
            CALL      UPCCPD1
          ELSE
            CALL      WRCCPD1
          ENDIF
.
          PACK      KEY22,CCPDHCD,CCPDDPT,CCPDPCD,SP70
          CALL      RSCCPV1
WCCPD100  CALL      RKCCPV1
          BRANCH    OVRCD,WCCPD200
          MATCH     CCPVHCD,CCPDHCD
          GOTO      WCCPD200 IF NOT EQUAL
          MATCH     CCPVDPT,CCPDDPT
          GOTO      WCCPD200 IF NOT EQUAL
          MATCH     CCPVPCD,CCPDPCD
          GOTO      WCCPD200 IF NOT EQUAL
          PACK      KEY22,CCPVHCD,CCPVDPT,CCPVPCD,CCPVCTY
          CALL      DECCPV1
          CALL      RSCCPV1
          GOTO      WCCPD100
.
WCCPD200  MOVE      SP70,KEY5
          CALL      TRCCPV11
          COMPARE   ZERO,OVRCD
          GOTO      WCCPD310 IF EQUAL
.
WCCPD300  CALL      TKCCPV11
          BRANCH    OVRCD,WCCPD400
WCCPD310  COMPARE   ZERO,CCPVRVI
          GOTO      WCCPD300 IF EQUAL
.
          PACK      KEY22,CCPDHCD,CCPDDPT,CCPDPCD,CCPVCTY
          UNPACK    KEY22,CCPVHCD,CCPVDPT,CCPVPCD,CCPVCTY
          CALL      RACCPV1
          IF        OVRCD=0
            CALL      UPCCPV1
          ELSE
            CALL      WRCCPV1
          ENDIF
          GOTO      WCCPD300
WCCPD400
WCCPD999  RETURN
.
.
.----------------------------------------
. Delete Patient Service
.----------------------------------------
DCCPD000  CALL      DELTYORN
          COMPARE   ZERO,FYESNO
          GOTO      DCCPD900 IF EQUAL
          PACK      KEY18,CCPDHCD,CCPDDPT,CCPDPCD
          CALL      RACCPD1
          IF        OVRCD=0
            CALL      DECCPD1
          ENDIF
.
DCCPD100  PACK      KEY22,CCPDHCD,CCPDDPT,CCPDPCD,SP70
          CALL      RSCCPV1
          CALL      RKCCPV1
          BRANCH    OVRCD,DCCPD900
          PACK      KEY22,CCPVHCD,CCPVDPT,CCPVPCD,CCPVCTY
          MATCH     KEY18,KEY24
          GOTO      DCCPD900 IF NOT EQUAL
.
          CALL      DECCPV1
          GOTO      DCCPD100
DCCPD900
          RETURN
.
.----------------------------------------
. Save Notes
.----------------------------------------
WCCPN000  PACK      KEY21,CCHCHCD,CCDPDPT,CCPDPCD,SP70
          CALL      RSCCPN1
WCCPN200  CALL      RKCCPN1
          BRANCH    OVRCD,WCCPN250
          MATCH     CCHCHCD,CCPNHCD
          GOTO      WCCPN250 IF NOT EQUAL
          MATCH     CCDPDPT,CCPNDPT
          GOTO      WCCPN250 IF NOT EQUAL
          MATCH     CCPDPCD,CCPNPCD
          GOTO      WCCPN250 IF NOT EQUAL
          PACK      KEY21,CCPNHCD,CCPNDPT,CCPNPCD,CCPNLIN
          CALL      DECCPN1
          GOTO      WCCPN200
.
WCCPN250  MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      TEMPWORD,FILCCPN1
          TRAPCLR   IO
          BRANCH    OVRCD,WCCPN999
          MOVE      ZERO,F3
WCCPN260  READ      TEMPWORD,SEQ;CCPNDAT
          GOTO      WCCPN999 IF OVER
          MOVE      F3,CCPNLIN
          PACK      KEY21,CCHCHCD,CCDPDPT,CCPDPCD,CCPNLIN
          UNPACK    KEY21,CCPNHCD,CCPNDPT,CCPNPCD,CCPNLIN
          CALL      WRCCPN1
          ADD       ONE,F3
          GOTO      WCCPN260
WCCPN999
          RETURN
.
RKEY0000   MOVE      CCPKFKY,KEY20
           CALL      RDCCPF1
           IF        OVRCD=1
             MOVE      "Feeder Key Not on File - ",CCPFDES
           ENDIF
           RETURN
