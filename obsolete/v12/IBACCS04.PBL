.------------------------------------------------------------
. Read Cost Type Description for Indirect Dept. Allocation
.------------------------------------------------------------
RCCCTA00
          PACK      KEY4,CCDACTY
          CALL      RDCCCT1
          IF        OVRCD=1
            MOVE      "Unknown Cost Type",CCCTDES
          ENDIF
RCCCTA99  RETURN
.
.-------------------------------------------------------------------------------
. Read Allocation Type Description for Indirect Dept. Allocation Default Alloc.
.-------------------------------------------------------------------------------
RCCATA00
          MATCH     "1",CCDPDTY
          IF        @EQUAL
            PACK      KEY4,CCDPATY
            CALL      RDCCAT1
            IF        OVRCD=1
              MOVE      "Unknown Cost Type",CCATDES
            ENDIF
          ELSE
            MOVE      SP70,CCDPATY
            MOVE      "N/A",CCATDES
          ENDIF
RCCATA99  RETURN
.
CLRIND00  MATCH     "0",CCDPDTY
          IF        @EQUAL
            MOVE      SP70,CCDAATY
          ENDIF
          RETURN
.-------------------------------------------------------------------------------
. Read Allocation Type Description for Indivdual Department Allocations.
.-------------------------------------------------------------------------------
RCCAT000
          MOVE      SP70,CCATDES
          MATCH     "1",CCDPDTY
          IF        @EQUAL
            PACK      KEY4,CCDAATY
            CALL      RDCCAT1
            IF        OVRCD=1
              MOVE      "Unknown Cost Type",CCATDES
            ENDIF
          ENDIF
RCCAT999  RETURN
.
.------------------------------------
. Move if Indirect Department
.------------------------------------
MCCHD000  MATCH     "1",CCDPDTY
          IF        @EQUAL
            CLEAR     ALLOCDES
            APPEND    "Allocation Type",ALLOCDES
            APPEND    SP70,ALLOCDES
            RESET     ALLOCDES
          ELSE
            CLEAR     ALLOCDES
          ENDIF
          RETURN

.------------------------------------
. Write/Update Department
.------------------------------------
WCCDP000  PACK      KEY10,CCDPHCD,CCDPDPT
          CALL      RACCDP1
          IF        OVRCD=0
            CALL      UPCCDP1
          ELSE
            CALL      WRCCDP1
          ENDIF
.
          PACK      KEY10,CCDPHCD,CCDPDPT
          PACK      KEY14,CCDPHCD,CCDPDPT
          CALL      RSCCDA1
WCCDP100  CALL      RKCCDA1
          BRANCH    OVRCD,WCCDP200
          PACK      KEY14,CCDAHCD,CCDADPT,CCDACTY
          MATCH     KEY10,KEY14
          GOTO      WCCDP200 IF NOT EQUAL
          CALL      DECCDA1
          GOTO      WCCDP100
WCCDP200
          MOVE      SP70,KEY5
          CALL      TRCCDA11
          COMPARE   ZERO,OVRCD
          GOTO      WCCDP310 IF EQUAL
WCCDP300
          CALL      TKCCDA11
          BRANCH    OVRCD,WCCDP400
WCCDP310
          UNPACK    KEY10,CCDAHCD,CCDADPT
          PACK      KEY14,CCDAHCD,CCDADPT,CCDACTY
          CALL      RACCDA1
          IF        OVRCD=0
            CALL      UPCCDA1
          ELSE
            CALL      WRCCDA1
          ENDIF
          GOTO      WCCDP300
.
. Remove any RVI details for deleted cost types. This may take some time.
.------------------------------------------------------------------------
WCCDP400  PACK      KEY22,CCDPHCD,CCDPDPT,SP70
          PACK      KEY10,CCDPHCD,CCDPDPT,SP70
          CALL      RSCCPV1
.
WCCDP500  CALL      RKCCPV1
          BRANCH    OVRCD,WCCDP600
          PACK      KEY22,CCPVHCD,CCPVDPT,SP70
          MATCH     KEY10,KEY22
          GOTO      WCCDP600 IF NOT EQUAL
.
          PACK      KEY14,CCPVHCD,CCPVDPT,CCPVCTY
          CALL      RDCCDA1
          COMPARE   ZERO,OVRCD
          GOTO      WCCDP500 IF EQUAL
          PACK      KEY22,CCPVHCD,CCPVDPT,CCPVPCD,CCPVCTY,SP70
          CALL      DECCPV1
          CALL      RSCCPV1
          GOTO      WCCDP500
.
. Remove any Feeder Keys
.-------------------------
WCCDP600  PACK      KEY34,CCDPHCD,CCDPDPT,SP70
          CALL      RSCCGI2
WCCDP700  CALL      RKCCGI2
          BRANCH    OVRCD,WCCDP800
          PACK      KEY10,CCGIHCD,CCGIDPT,SP70
          MATCH     KEY10,KEY34
          GOTO      WCCDP800 IF NOT EQUAL
.
          PACK      KEY14,CCGIHCD,CCGIDPT,CCGICTY
          CALL      RDCCDA1
          COMPARE   ZERO,OVRCD
          GOTO      WCCDP700 IF EQUAL
          PACK      KEY34,CCGIKEY,CCGIHCD,CCGIDPT,CCGICTY
          CALL      RACCGI1
          IF        OVRCD=0
            CALL      DECCGI1
          ENDIF
          GOTO      WCCDP700
.
WCCDP800  RETURN
.------------------------------------
. Delete Department
.------------------------------------
DCCDP000
          CALL      DELTYORN
          COMPARE   ZERO,FYESNO
          GOTO      DCCDP999 IF EQUAL
          PACK      KEY10,CCDPHCD,CCDPDPT
          CALL      RACCDP1
          IF        OVRCD=0
            CALL      DECCDP1
          ENDIF
DCCDP100  PACK      KEY14,CCDPHCD,CCDPDPT,SP70
          CALL      RSCCDA1
          CALL      RKCCDA1
          BRANCH    OVRCD,DCCDP999
          MATCH     CCDPHCD,CCDAHCD
          GOTO      DCCDP999 IF NOT EQUAL
          MATCH     CCDPDPT,CCDADPT
          GOTO      DCCDP999 IF NOT EQUAL
          PACK      KEY14,CCDAHCD,CCDADPT,CCDACTY
          CALL      DECCDA1
          GOTO      DCCDP100
DCCDP999
          RETURN
