          MOVE     ZERO,NUMPER
.
          PACK     KEY6,SAVEYEAR,SP70
          CALL     RDCCCA1
EANNQ100  CALL     RKCCCA1
          BRANCH   OVRCD,EANNQ200
          MATCH    SAVEYEAR,CCCAYEAR
          GOTO     EANNQ200 IF NOT EQUAL
          ADD      ONE,NUMPER
          GOTO     EANNQ100
.
EANNQ200  MOVE     "    5",FLDID
          CALL     GETPOSIT
          BRANCH   EXIT,EANNQ999
          MOVE     SP70,VAR
          CALL     DISPITEM
          KEYIN    *PCCOL:CVERT,*NOMASK,*V2LON,*RV,ANNQTY
          MOVE     ANNQTY,VAR
          CALL     DISPITEM
          CALL     DELALL00
.
EANNQ300  MOVE     "   15",FLDID
          CALL     GETPOSIT
          MOVE     SP70,VAR
          CALL     DISPITEM
          MOVE     "   14",FLDID
          CALL     GETPOSIT
          MOVE     SP70,VAR
          CALL     DISPITEM
          MOVE     ZERO,CKEYTYP
          CALL     KCCPRA00
          BRANCH   EXIT,EANNQ350,EANNQ200
          ASSIGN   (ANNQTY*CCPRP01)/100.0,AM01
          ASSIGN   (ANNQTY*CCPRP02)/100.0,AM02
          ASSIGN   (ANNQTY*CCPRP03)/100.0,AM03
          ASSIGN   (ANNQTY*CCPRP04)/100.0,AM04
          ASSIGN   (ANNQTY*CCPRP05)/100.0,AM05
          ASSIGN   (ANNQTY*CCPRP06)/100.0,AM06
          ASSIGN   (ANNQTY*CCPRP07)/100.0,AM07
          ASSIGN   (ANNQTY*CCPRP08)/100.0,AM08
          ASSIGN   (ANNQTY*CCPRP09)/100.0,AM09
          ASSIGN   (ANNQTY*CCPRP10)/100.0,AM10
          ASSIGN   (ANNQTY*CCPRP11)/100.0,AM11
          ASSIGN   (ANNQTY*CCPRP12)/100.0,AM12
          ASSIGN   (ANNQTY*CCPRP13)/100.0,AM13
          ASSIGN   ANNQTY-AM01-AM02-AM03-AM03-AM04-AM05-AM06,F12P2
          ASSIGN   F12P2-AM07-AM08-AM09-AM10-AM11-AM12-AM13,F12P2
          COMPARE  ZERO,F12P2
          GOTO     EANNQ400 IF EQUAL
          MOVE     "14",SECTOR
EANNQ310  SUB      ONE,SECTOR
          LOAD     F12P2A,SECTOR,AM01,AM02,AM03,AM04,AM05,AM06:
                                 AM07,AM08,AM09,AM10,AM11,AM12,AM13
          COMPARE  ONE,SECTOR
          GOTO     EANNQ320 IF EQUAL
          COMPARE  ZERO,F12P2
          GOTO     EANNQ310 IF EQUAL
.
EANNQ320  ADD      F12P2,F12P2A
          STORE    F12P2A,SECTOR,AM01,AM02,AM03,AM04,AM05,AM06:
                                 AM07,AM08,AM09,AM10,AM11,AM12,AM13
          GOTO     EANNQ400
.
. No Profile Divide by Number of Periods
.
EANNQ350  ASSIGN   (ANNQTY/NUMPER),F12P2
          ASSIGN   ANNQTY-(F12P2*NUMPER)+F12P2,FIRSTQTY
          MOVE     SP70,CCPRPRO
.
EANNQ400  MOVE     ONE,KEYFLAG
          MOVE     ZERO,SECTOR
.
          PACK     KEY6,SAVEYEAR,SP70
          CALL     RDCCCA1
EANNQ600  CALL     RKCCCA1
          BRANCH   OVRCD,EANNQ700
          MATCH    SAVEYEAR,CCCAYEAR
          GOTO     EANNQ700 IF NOT EQUAL
          MOVE     CCCAPER,KEY2
          PACK     KEY24,CCHCHCD,CCDPDPT,CCPDPCD,CCCAYEAR,CCCAPER
          UNPACK   KEY24,CCMEHCD,CCMEDPT,CCMEPCD,CCMEYEAR,CCMEPER
          CALL     RDCCME2
          IF       OVRCD=1
            MOVE      ZERO,CCMEQTY
            MOVE      ZERO,CCMECST
            MOVE      ZERO,CCMEFLX
            MOVE      SP70,CCMESPA
          ENDIF
.
          MATCH     SP70,CCPRPRO
          IF        @EQUAL
            ASSIGN    ANNQTY/NUMPER,F12P2
            MOVE      F12P2,CCMEQTY
            IF        KEYFLAG=1
              MOVE      ZERO,KEYFLAG
              MOVE      FIRSTQTY,CCMEQTY
            ENDIF
          ELSE
            ADD       ONE,SECTOR
            LOAD      CCMEQTY,SECTOR,AM01,AM02,AM03,AM04,AM05,AM06:
                                     AM07,AM08,AM09,AM10,AM11,AM12,AM13
          ENDIF
.
          MOVE      SP70,SCTMROW
          MOVE      SP70,SCTMCOL
          CALL      TWCCME22
          GOTO      EANNQ600
.
EANNQ700  MOVE     SAVEYEAR,CCCAYEAR
          MOVE     "    5",FLDID
          CALL     GETPOSIT
EANNQ999  RETURN
.
