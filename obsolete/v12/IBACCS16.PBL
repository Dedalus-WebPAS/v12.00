PROC0000  PACK      KEY24,CCHCHCD,STRDPT,SP70
          DISPLAY   *P1:24,*EL,"Processing : "
          MOVE      ZERO,SECTOR
          CALL      RSCCME1
PROC1000  CALL      RKCCME1
          BRANCH    OVRCD,PROC9000
          MATCH     CCHCHCD,CCMEHCD
          GOTO      PROC9000 IF NOT EQUAL
          MATCH     CCMEDPT,ENDDPT     * Department Range
          GOTO      PROC9000 IF LESS
.
          MATCH     CCMEYEAR,CCCAYEAR
          GOTO      PROC1000 IF NOT EQUAL
.
          MATCH     STRPCD,CCMEPCD     * Patient Service Range
          GOTO      PROC1000 IF LESS
          MATCH     CCMEPCD,ENDPCD
          GOTO      PROC1000 IF LESS
.
          MATCH     STRPER,CCMEPER     * Period Range
          GOTO      PROC1000 IF LESS
          MATCH     CCMEPER,ENDPER
          GOTO      PROC1000 IF LESS
.
          ADD       ONE,SECTOR
          IF        (SECTOR%100=0)
             DISPLAY   *P15:24,*V2LON,*EL,SECTOR
          ENDIF
.
          COMPARE   ZERO,ADJPER
          GOTO      PROC1100 IF EQUAL
.
          ASSIGN    CCMEQTY+((CCMEQTY*ADJPER)/100.0),CCMEQTY
.
PROC1100  ADD       ADJAMT,CCMEQTY
          CALL      UPCCME1
          GOTO      PROC1000
.
