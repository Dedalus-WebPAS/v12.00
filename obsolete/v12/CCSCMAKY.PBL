.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSCMAKY
.   Routine   :   HCCMAA00 Costing Model Code  
.   Return    :           
.-------------------------------------------------------------------------------
HCCMAA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCMAA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCMAA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Model"              
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 6",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HCCMAA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCMACOM 
          PACK      KEY4,CCMACOM,SP70                                           
.                                                                               
.                                                                               
          CALL      RSCCMA1 
HCCMAA10
          CALL      RKCCMA1 
          BRANCH    OVRCD,HCCMAA15
          PACK      KEY4,CCMACOM,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HCCMAA12
          CALL      HCCMAA60
          BRANCH    OVRCD,HCCMAA10
HCCMAA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY4,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCMACOM 
          DISPLAY   *PSTDCOL02:STDROW,CCMADES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCMAA10 IF NOT EQUAL
HCCMAA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCMAA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCMAA30,HCCMAA35,HCCMAA40,HCCMAA45,HCCMAA04:
                              HCCMAA91,HCCMAA50
          GOTO      HCCMAA92
HCCMAA30
          CALL      RKCCMA1 
          BRANCH    OVRCD,HCCMAA85
          PACK      KEY4,CCMACOM,SP70                                           
.                                                                               
.                                                                               
          CALL      RPCCMA1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCMAA10
HCCMAA35
          MOVE      ST1,KEY4 
          CALL      RDCCMA1 
          CALL      RPCCMA1 
          BRANCH    OVRCD,HCCMAA85
          PACK      KEY4,CCMACOM,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCMAA36
          CALL      RPCCMA1 
          BRANCH    OVRCD,HCCMAA37
          PACK      KEY4,CCMACOM,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCMAA36 IF NOT EQUAL
HCCMAA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCMAA10
HCCMAA40
          MOVE      ST1,KEY4 
          CALL      RDCCMA1 
          CALL      RPCCMA1 
          BRANCH    OVRCD,HCCMAA85
          PACK      KEY4,CCMACOM,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCMAA08
HCCMAA45
          CALL      RKCCMA1 
          BRANCH    OVRCD,HCCMAA85
          PACK      KEY4,CCMACOM,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY4,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCMA1 
HCCMAA46
          CALL      RPCCMA1 
          BRANCH    OVRCD,HCCMAA47
          PACK      KEY4,CCMACOM,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCMAA46 IF NOT EQUAL
HCCMAA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCMAA10
HCCMAA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCMAA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCMAA08
HCCMAA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCMAA08
HCCMAA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCMAA61 IF EQUAL
          MATCH     SCHFLD01,CCMACOM 
          GOTO      HCCMAA72 IF NOT EQUAL
HCCMAA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCMAA62 IF EQUAL
          MATCH     SCHFLD02,CCMADES 
          GOTO      HCCMAA72 IF NOT EQUAL
HCCMAA62
HCCMAA63
HCCMAA64
HCCMAA65
HCCMAA66
HCCMAA67
HCCMAA68
HCCMAA69
HCCMAA70
          GOTO      HCCMAA75
HCCMAA72  MOVE      ONE,OVRCD
HCCMAA75  RETURN
HCCMAA85
          LOAD      KEY4,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY4,CCMACOM                                                
          CALL      RDCCMA1 
HCCMAA90
.                                                                               
          BEEP
          GOTO      HCCMAA20
HCCMAA91
          MOVE      SP70,CCMADES 
          MOVE      SP70,CCMAUR1 
          MOVE      SP70,CCMAUR2 
          MOVE      SP70,CCMAUD1 
          MOVE      SP70,CCMAUD2 
          MOVE      SP70,CCMAUC1 
          MOVE      SP70,CCMAUC2 
          MOVE      SP70,CCMAUY1 
          MOVE      SP70,CCMAUY2 
          MOVE      SP70,CCMASPA 
          MOVE      SP70,KEY4 
          MOVE      SP70,CCMACOM 
          GOTO      HCCMAA96
HCCMAA92
          MOVE      SP70,KEY4 
          MOVE      SP70,CCMACOM 
          LOAD      KEY4,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY4,CCMACOM                                                
HCCMAA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCMAA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSCMAKY
.   Routine   :   KCCMAA00 Keyin Costing Model Code  
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCMAA00 TOPIC     "CCMACOM "
          MOVE      UNDLN70,CCMACOM 
          KEYIN     *PCCOL:CVERT,*DV,CCMACOM:                                   
                    *PCCOL:CVERT,*V2LON,*JR,*ZF,CCMACOM:                        
                    *PCCOL:CVERT,*DV,CCMACOM 
.
          ENDSET    CCMACOM 
          APPEND    SP70,CCMACOM 
          RESET     CCMACOM 
.
          MATCH     SP70,CCMACOM 
          GOTO      KCCMAA90 IF EQUAL
.
          ENDSET    CCMACOM 
          CMATCH    EXITCHAR,CCMACOM 
          GOTO      KCCMAA95 IF EQUAL
.
          CMATCH    QUEST,CCMACOM 
          GOTO      KCCMAA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCCMAA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCMACOM 
          APPEND    SP70,CCMACOM 
          RESET     CCMACOM 
.
          MATCH     SP70,CCMACOM 
          GOTO      KCCMAA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CCMACOM                                 
KCCMAA10
          RESET     CCMACOM 
          PACK      KEY4,CCMACOM                                                
          CALL      RACCMA1 
          BRANCH    OVRCD,KCCMAA80
          BRANCH    CKEYTYP,KCCMAA75
          CALL      RDCCMA1 
KCCMAA70
          MOVE      ZERO,EXIT
          GOTO      KCCMAA99
KCCMAA75
          MOVE      "Costing Model Code Already Exist - ",DISPMSG                                    
          CALL      MESSAGE1
          GOTO      KCCMAA00
KCCMAA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCMAA99,KCCMAA99
          MOVE      "Costing Model Code Does Not Exist - ",DISPMSG                                   
          CALL      MESSAGE1
          GOTO      KCCMAA00
KCCMAA90
          MOVE      ONE,EXIT
          GOTO      KCCMAA99
KCCMAA95
          MOVE      TWO,EXIT
KCCMAA99 TOPIC    PRGID
          RETURN
