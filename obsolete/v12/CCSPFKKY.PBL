.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSPFKKY
.   Routine   :   HCCPFA00 Feeder Key          
.   Return    :           
.-------------------------------------------------------------------------------
HCCPFA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "30",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCPFA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCPFA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Feeder Key"         
.
          MOVE      STDCOL01,STDCOL02
          ADD       "21",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HCCPFA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCPFFKY 
          PACK      KEY20,CCPFFKY,SP70                                          
.                                                                               
.                                                                               
          CALL      RSCCPF1 
HCCPFA10
          CALL      RKCCPF1 
          BRANCH    OVRCD,HCCPFA15
          PACK      KEY20,CCPFFKY,SP70                                          
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HCCPFA12
          CALL      HCCPFA60
          BRANCH    OVRCD,HCCPFA10
HCCPFA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY20,XITEMNO,ST1,ST2,ST3,ST4,ST5:                          
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCPFFKY 
          DISPLAY   *PSTDCOL02:STDROW,CCPFDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCPFA10 IF NOT EQUAL
HCCPFA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCPFA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCPFA30,HCCPFA35,HCCPFA40,HCCPFA45,HCCPFA04:
                              HCCPFA91,HCCPFA50
          GOTO      HCCPFA92
HCCPFA30
          CALL      RKCCPF1 
          BRANCH    OVRCD,HCCPFA85
          PACK      KEY20,CCPFFKY,SP70                                          
.                                                                               
.                                                                               
          CALL      RPCCPF1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPFA10
HCCPFA35
          MOVE      ST1,KEY20
          CALL      RDCCPF1 
          CALL      RPCCPF1 
          BRANCH    OVRCD,HCCPFA85
          PACK      KEY20,CCPFFKY,SP70                                          
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCPFA36
          CALL      RPCCPF1 
          BRANCH    OVRCD,HCCPFA37
          PACK      KEY20,CCPFFKY,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCPFA36 IF NOT EQUAL
HCCPFA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCPFA10
HCCPFA40
          MOVE      ST1,KEY20
          CALL      RDCCPF1 
          CALL      RPCCPF1 
          BRANCH    OVRCD,HCCPFA85
          PACK      KEY20,CCPFFKY,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPFA08
HCCPFA45
          CALL      RKCCPF1 
          BRANCH    OVRCD,HCCPFA85
          PACK      KEY20,CCPFFKY,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY20,Z70                                                   
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCPF1 
HCCPFA46
          CALL      RPCCPF1 
          BRANCH    OVRCD,HCCPFA47
          PACK      KEY20,CCPFFKY,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCPFA46 IF NOT EQUAL
HCCPFA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCPFA10
HCCPFA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCPFA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPFA08
HCCPFA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCPFA08
HCCPFA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCPFA61 IF EQUAL
          MATCH     SCHFLD01,CCPFFKY 
          GOTO      HCCPFA72 IF NOT EQUAL
HCCPFA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCPFA62 IF EQUAL
          MATCH     SCHFLD02,CCPFDES 
          GOTO      HCCPFA72 IF NOT EQUAL
HCCPFA62
HCCPFA63
HCCPFA64
HCCPFA65
HCCPFA66
HCCPFA67
HCCPFA68
HCCPFA69
HCCPFA70
          GOTO      HCCPFA75
HCCPFA72  MOVE      ONE,OVRCD
HCCPFA75  RETURN
HCCPFA85
          LOAD      KEY20,XITEMNO,ST1,ST2,ST3,ST4,ST5:                          
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY20,CCPFFKY                                               
          CALL      RDCCPF1 
HCCPFA90
.                                                                               
          BEEP
          GOTO      HCCPFA20
HCCPFA91
          MOVE      SP70,CCPFDES 
          MOVE      SP70,CCPFSUS 
          MOVE      SP70,CCPFSPA 
          MOVE      SP70,KEY20
          MOVE      SP70,CCPFFKY 
          GOTO      HCCPFA96
HCCPFA92
          MOVE      SP70,KEY20
          MOVE      SP70,CCPFFKY 
          LOAD      KEY20,CCITEM,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY20,CCPFFKY                                               
HCCPFA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCPFA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSPFKKY
.   Routine   :   KCCPFA00 Keyin Feeder Key          
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCPFA00 TOPIC     "CCPFFKY "
          MOVE      UNDLN70,CCPFFKY 
          KEYIN     *PCCOL:CVERT,*DV,CCPFFKY:                                   
                    *PCCOL:CVERT,*V2LON,CCPFFKY:                                
                    *PCCOL:CVERT,*DV,CCPFFKY 
.
          ENDSET    CCPFFKY 
          APPEND    SP70,CCPFFKY 
          RESET     CCPFFKY 
.
          MATCH     SP70,CCPFFKY 
          GOTO      KCCPFA90 IF EQUAL
.
          CMATCH    EXITCHAR,CCPFFKY 
          GOTO      KCCPFA95 IF EQUAL
.
          CMATCH    QUEST,CCPFFKY 
          GOTO      KCCPFA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCCPFA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCPFFKY 
          APPEND    SP70,CCPFFKY 
          RESET     CCPFFKY 
.
          MATCH     SP70,CCPFFKY 
          GOTO      KCCPFA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CCPFFKY                                 
KCCPFA10
          PACK      KEY20,CCPFFKY                                               
          CALL      RACCPF1 
          BRANCH    OVRCD,KCCPFA80
          BRANCH    CKEYTYP,KCCPFA75
          CALL      RDCCPF1 
KCCPFA70
          MOVE      ZERO,EXIT
          GOTO      KCCPFA99
KCCPFA75
          MOVE      "Feeder Key Already Exist - ",DISPMSG                                            
          CALL      MESSAGE1
          GOTO      KCCPFA00
KCCPFA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCPFA99,KCCPFA99
          MOVE      "Feeder Key Does Not Exist - ",DISPMSG                                           
          CALL      MESSAGE1
          GOTO      KCCPFA00
KCCPFA90
          MOVE      ONE,EXIT
          GOTO      KCCPFA99
KCCPFA95
          MOVE      TWO,EXIT
KCCPFA99 TOPIC    PRGID
          RETURN
