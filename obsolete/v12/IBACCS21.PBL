.-----------------------------------------
. Save General Ledger Interface Details
.-----------------------------------------
DELT0000  MOVE     SP70,DISPMSG
          CALL     MESSAGE2
          CALL     DELQST
          CALL     MESSAGE3
          BRANCH   CEXIT,DELT1000,DELT9000,DELT9100
.
DELT1000  PACK      KEY20,CCGKKEY,SP70
          CALL      RACCGK1
          IF        OVRCD=0
            CALL      DECCGK1
          ENDIF
.
          PACK      KEY34,CCGKKEY,SP70
          CALL      RSCCGI1
DELT1100  CALL      RKCCGI1
          BRANCH    OVRCD,DELT1200
          MATCH     CCGIKEY,CCGKKEY
          GOTO      DELT1200 IF NOT EQUAL
          PACK      KEY34,CCGIKEY,CCGIHCD,CCGIDPT,CCGICTY
          CALL      DECCGI1
          GOTO      DELT1100
DELT1200  MOVE     TWO,EXIT
          GOTO     DELT9999
.
DELT9000  MOVE     ONE,EXIT
          GOTO     DELT9999
DELT9100  MOVE     TWO,EXIT
          GOTO     DELT9999
DELT9999  RETURN
.
EHCD0000  MOVE     ZERO,CKEYTYP
          CALL     KCCHCA00
          BRANCH   EXIT,EHCD9999,EHCD9999
.
          MOVE     CCHCHCD,CCGIHCD
.
EHCD9999  RETURN
EDPT0000  MOVE     ZERO,CKEYTYP
          MOVE     CCGIHCD,CCDPHCD
          CALL     KCCDPA00
          BRANCH   EXIT,EDPT9999,EDPT9999
.
          MOVE     CCDPDPT,CCGIDPT
.
EDPT9999  RETURN
ECTY0000  MOVE     ZERO,CKEYTYP
          MOVE     CCGIDPT,CCDADPT
          MOVE     CCGIHCD,CCDAHCD
          CALL     KCCDAA00
          BRANCH   EXIT,ECTY9999,ECTY9999
.
          MOVE     CCDACTY,CCGICTY
.
ECTY9999  RETURN
.-----------------------------------------
. Save General Ledger Interface Details
.-----------------------------------------
WCCGI000  MOVE      SP70,KEY5
          MOVE      ZERO,DEFALLOC
          MOVE      ONE,SECTOR
          CALL      TRCCGI11
          COMPARE   ZERO,OVRCD
          GOTO      WCCGI020 IF EQUAL
WCCGI010  CALL      TKCCGI11
          BRANCH    OVRCD,WCCGI030
.
WCCGI020  ADD       CCGIPER,DEFALLOC
          MOVE      ZERO,SECTOR
          GOTO      WCCGI010
.
WCCGI030  BRANCH    SECTOR,WCCGI040    * Allow for No Feeder Keys
          IF        !(DEFALLOC=1)
            MOVE      "Factors Must Total 1  - ",DISPMSG
            CALL      MESSAGE1
            MOVE      ONE,EXIT
            GOTO      WCCGI999
          ENDIF
.
WCCGI040  PACK      KEY20,CCGKKEY,SP70
          CALL      RACCGK1
          IF        OVRCD=0
            CALL      UPCCGK1
          ELSE
            CALL      WRCCGK1
          ENDIF
.
          PACK      KEY34,CCGKKEY,SP70
          CALL      RSCCGI1
WCCGI100
          CALL      RKCCGI1
          BRANCH    OVRCD,WCCGI200
          MATCH     CCGIKEY,CCGKKEY
          GOTO      WCCGI200 IF NOT EQUAL
.
          PACK      KEY34,CCGIKEY,CCGIHCD,CCGIDPT,CCGICTY
          CALL      DECCGI1
          GOTO      WCCGI100
WCCGI200
          MOVE      SP70,KEY5
          CALL      TRCCGI11
          COMPARE   ZERO,OVRCD
          GOTO      WCCGI310 IF EQUAL
WCCGI300
          CALL      TKCCGI11
          BRANCH    OVRCD,WCCGI400
WCCGI310
          MOVE      CCGKKEY,CCGIKEY
          PACK      KEY34,CCGIKEY,CCGIHCD,CCGIDPT,CCGICTY
          CALL      RACCGI1
          IF        OVRCD=0
            CALL      UPCCGI1
          ELSE
            CALL      WRCCGI1
          ENDIF
          GOTO      WCCGI300
WCCGI400  MOVE      TWO,EXIT
WCCGI999  RETURN
