KLEDG000  CALL      KLMAFM00
.
KLEDG999  RETURN
.----------------------------------------------------------------------
. Enter Cost Centre Range
.----------------------------------------------------------------------
KCOST000  MOVE       "   26",FLDID
          CALL       GETPOSIT
          MOVE       SP70,VAR
          CALL       DISPITEM
          CALL       KCCAFM00
          BRANCH     EXIT,KCOST010,KCOST099
          MOVE       FMCCCOCE,FROMCOST
          PACK       COSTFROM,FMCCCOCE,SP1,FMCCDESC
          GOTO       KCOST020
.
KCOST010  MOVE       SP70,FROMCOST
          MOVE       "Start                              ",COSTFROM
.
KCOST020  MOVE       COSTFROM,VAR
          CALL       DISPITEM
.
KCOST030  MOVE       "   27",FLDID
          CALL       GETPOSIT
          MOVE       SP70,VAR
          CALL       DISPITEM
          CALL       KCCAFM00
          BRANCH     EXIT,KCOST040,KCOST099
          MOVE       FMCCCOCE,ENDCOST
          PACK       COSTEND,FMCCCOCE,SP1,FMCCDESC
          MATCH      FROMCOST,ENDCOST
          GOTO       KCOST099 IF NOT LESS
.
          BEEP
          MOVE       "To Cost Centre Must Be > From Cost Centre - ",DISPMSG
          CALL       MESSAGE1
          MOVE       "    4",FLDID
          CALL       GETPOSIT
          MOVE       SP70,VAR
          CALL       DISPITEM
          GOTO       KCOST000
.
KCOST040  MOVE       "~~~~~",ENDCOST
          MOVE       "End                                   ",COSTEND
.
KCOST099  RETURN
.
