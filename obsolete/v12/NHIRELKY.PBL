.---------------------------------------------------------------------------
.   System    :   NZ National Health System          
.   Include   :   NHIRELKY
.   Routine   :   HNHREA00 Relationship Codes  
.   Return    :           
.-------------------------------------------------------------------------------
HNHREA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HNHREA04
          CALL      GSCRN000
          BRANCH    EXIT,HNHREA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Code"               
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 5",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "25",STDCOL03
HNHREA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,NHRERCD 
          PACK      KEY2,NHRERCD,SP70                                           
.                                                                               
.                                                                               
          CALL      RSNHREL1
HNHREA10
          CALL      RKNHREL1
          BRANCH    OVRCD,HNHREA15
          PACK      KEY2,NHRERCD,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HNHREA12
          CALL      HNHREA60
          BRANCH    OVRCD,HNHREA10
HNHREA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY2,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,NHRERCD 
          DISPLAY   *PSTDCOL02:STDROW,NHREDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HNHREA10 IF NOT EQUAL
HNHREA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HNHREA20
          CALL      HSCRI000
          BRANCH    EXIT,HNHREA30,HNHREA35,HNHREA40,HNHREA45,HNHREA04:
                              HNHREA91,HNHREA50
          GOTO      HNHREA92
HNHREA30
          CALL      RKNHREL1
          BRANCH    OVRCD,HNHREA85
          PACK      KEY2,NHRERCD,SP70                                           
.                                                                               
.                                                                               
          CALL      RPNHREL1
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HNHREA10
HNHREA35
          MOVE      ST1,KEY2 
          CALL      RDNHREL1
          CALL      RPNHREL1
          BRANCH    OVRCD,HNHREA85
          PACK      KEY2,NHRERCD,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HNHREA36
          CALL      RPNHREL1
          BRANCH    OVRCD,HNHREA37
          PACK      KEY2,NHRERCD,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HNHREA36 IF NOT EQUAL
HNHREA37
          MOVE      ZERO,XITEMNO
          GOTO      HNHREA10
HNHREA40
          MOVE      ST1,KEY2 
          CALL      RDNHREL1
          CALL      RPNHREL1
          BRANCH    OVRCD,HNHREA85
          PACK      KEY2,NHRERCD,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HNHREA08
HNHREA45
          CALL      RKNHREL1
          BRANCH    OVRCD,HNHREA85
          PACK      KEY2,NHRERCD,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY2,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSNHREL1
HNHREA46
          CALL      RPNHREL1
          BRANCH    OVRCD,HNHREA47
          PACK      KEY2,NHRERCD,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HNHREA46 IF NOT EQUAL
HNHREA47
          MOVE      ZERO,XITEMNO
          GOTO      HNHREA10
HNHREA50  COMPARE   ZERO,CSEARCH
          GOTO      HNHREA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HNHREA08
HNHREA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HNHREA08
HNHREA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HNHREA61 IF EQUAL
          MATCH     SCHFLD01,NHRERCD 
          GOTO      HNHREA72 IF NOT EQUAL
HNHREA61
          MATCH     SP70,SCHFLD02
          GOTO      HNHREA62 IF EQUAL
          MATCH     SCHFLD02,NHREDES 
          GOTO      HNHREA72 IF NOT EQUAL
HNHREA62
HNHREA63
HNHREA64
HNHREA65
HNHREA66
HNHREA67
HNHREA68
HNHREA69
HNHREA70
          GOTO      HNHREA75
HNHREA72  MOVE      ONE,OVRCD
HNHREA75  RETURN
HNHREA85
          LOAD      KEY2,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY2,NHRERCD                                                
          CALL      RDNHREL1
HNHREA90
.                                                                               
          BEEP
          GOTO      HNHREA20
HNHREA91
          MOVE      SP70,NHREDES 
          MOVE      SP70,NHRESPA 
          MOVE      SP70,KEY2 
          MOVE      SP70,NHRERCD 
          GOTO      HNHREA96
HNHREA92
          MOVE      SP70,KEY2 
          MOVE      SP70,NHRERCD 
          LOAD      KEY2,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY2,NHRERCD                                                
HNHREA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HNHREA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   NZ National Health System          
.   Include   :   NHIRELKY
.   Routine   :   KNHREA00 Keyin Relationship Code   
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KNHREA00 TOPIC     "NHRERCD "
          MOVE      UNDLN70,NHRERCD 
          KEYIN     *PCCOL:CVERT,*DV,NHRERCD:                                   
                    *PCCOL:CVERT,*V2LON,NHRERCD:                                
                    *PCCOL:CVERT,*DV,NHRERCD 
.
          ENDSET    NHRERCD 
          APPEND    SP70,NHRERCD 
          RESET     NHRERCD 
.
          MATCH     SP70,NHRERCD 
          GOTO      KNHREA90 IF EQUAL
.
          CMATCH    EXITCHAR,NHRERCD 
          GOTO      KNHREA95 IF EQUAL
.
          CMATCH    QUEST,NHRERCD 
          GOTO      KNHREA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HNHREA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    NHRERCD 
          APPEND    SP70,NHRERCD 
          RESET     NHRERCD 
.
          MATCH     SP70,NHRERCD 
          GOTO      KNHREA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,NHRERCD                                 
KNHREA10
          PACK      KEY2,NHRERCD                                                
          CALL      RANHREL1
          BRANCH    OVRCD,KNHREA80
          BRANCH    CKEYTYP,KNHREA75
          CALL      RDNHREL1
KNHREA70
          MOVE      ZERO,EXIT
          GOTO      KNHREA99
KNHREA75
          MOVE      "Relationship Code Already Exist - ",DISPMSG                                     
          CALL      MESSAGE1
          GOTO      KNHREA00
KNHREA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KNHREA99,KNHREA99
          MOVE      "Relationship Code Does Not Exist - ",DISPMSG                                    
          CALL      MESSAGE1
          GOTO      KNHREA00
KNHREA90
          MOVE      ONE,EXIT
          GOTO      KNHREA99
KNHREA95
          MOVE      TWO,EXIT
KNHREA99 TOPIC    PRGID
          RETURN
