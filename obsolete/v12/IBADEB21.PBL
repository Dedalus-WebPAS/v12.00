.------------------------------------------------------------
. Enter Debtor Code
.------------------------------------------------------------
EDEBT000  CALL      CHKLST00        * Check for Last Entry Abort
          MOVE      TWO,CKEYTYP
.
          MATCH     SP70,NEXTDEB
          GOTO      EDEBT100 IF NOT EQUAL
.
          CALL      KDBDBA00
          BRANCH    EXIT,EDEBT999,EDEBT999
.
          COMPARE   ZERO,OVRCD            * Add Debtor Details
          GOTO      EDEBT050 IF EQUAL
.
          CALL      CHKSEC00              * Check Security Level
          BRANCH    EXIT,EDEBT000
.
          MATCH     "@",DBDBDEB
          GOTO      EDEBT020 IF EQUAL
          MOVE      "Debtor Does Not Exist - ",DISPMSG
          CALL      MESSAGE1
          GOTO      EDEBT000
.
EDEBT020  CALL      ADDEB000              * Add Debtor Screen
          BRANCH    EXIT,EDEBT000
.
EDEBT050  IF        DBDBACT=0
            MOVE      "Debtor Not Active. - ",DISPMSG
            BEEP
            CALL      MESSAGE1
            GOTO      EDEBT000
          ENDIF
.
          IF        DBDBBAD=1
            MOVE      "WARNING : Debtor Flagged as Bad Debt. - ",DISPMSG
            BEEP
            CALL      MESSAGE1
          ENDIF
.
          MATCH     SP70,DBDBWRN
          IF        !@EQUAL
             MOVE     "DBWR",DBCDCAT
             PACK     KEY7,DBCDCAT,DBDBWRN
             CALL     RDDBCD1
             IF       OVRCD=0
               MOVE     "WARNING SET : ",KEY14
               PACK     DISPMSG,KEY14,DBCDDES
               BEEP
               CALL      MESSAGE1
             ENDIF
          ENDIF
.
          MATCH     SP70,DBDBLIN
          IF        !@EQUAL
            MOVE      "Debtor Code Linked. - ",DISPMSG
            BEEP
            CALL      MESSAGE1
            MOVE      DBDBLIN,KEY8
            CALL      RDDBDB1
          ENDIF
          GOTO      EDEBT150
.
EDEBT100  MOVE      NEXTDEB,KEY8
          CALL      RDDBDB1
          COMPARE   ZERO,OVRCD
          GOTO      EDEBT150 IF EQUAL
          MOVE      SP70,NEXTDEB
          GOTO      EDEBT000
.
EDEBT150  MOVE      DBDBDEB,DBBIDEB
          MOVE      DBDBDEB,LASTDEB
          MOVE      DBDBDEB,NEXTDEB
.
EDEBT500  PACK      PREPAT,SP70
          PACK      PPENNO,SP70
          PACK      PMICRO,SP70
          PACK      ADMDATE,SP70
          PACK      DISDATE,SP70
          MOVE      ZERO,PVIBILL
.
          BRANCH    FLGIBAPT,EDEBT999
.
          COMPARE   EIGHT,CFDBILAY
          GOTO      EDEBT999 IF NOT EQUAL
.
          MOVE      ZERO,PTSNAMNT
.
EDEBT700  PACK      KEY24,DBDBPUR,Z40
          CALL      RDSVISA1                      * Use PATVIS read      v90201
EDEBT710  CALL      RDPVISA1                      * Use PATVIS read      v90201
          BRANCH    OVRCD,EDEBT999
.
          MATCH     DBDBPUR,PVIURNO
          GOTO      EDEBT999 IF NOT EQUAL
.
          COMPARE   THREE,PVITYPE
          GOTO      EDEBT710 IF NOT EQUAL
.
          PACK      KEY8,PVIURNO,SP70
          CALL      RDMAST1                       * Use PATMA1 read      v90201
          BRANCH    OVRCD,EDEBT710
.
          PACK      KEY12,PVIURNO,Z40
          CALL      RSPTSNA1                  * Use PAT SNA read         v90201
          CALL      RPPTSNA1                  * Use PAT SNA read         v00201
          BRANCH    OVRCD,EDEBT712
          MATCH     PVIURNO,PTSNURNO
          GOTO      EDEBT715 IF EQUAL
.
EDEBT712  MOVE      ZERO,PTSNAMNT
.
EDEBT715  PACK      KEY8,PVIBILL,SP70        * Change to PATVIS fldname v90201
          CALL      RDMISS1                   * Use PAT MI1 read         v90201
          BRANCH    OVRCD,EDEBT710
.
          UNPACK    ADATE,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          MOVE      CPCDATE,ADMDATE
          REP       " 0",ADMDATE
.
          BRANCH    ASTAT,EDEBT710,EDEBT999,EDEBT720,EDEBT999
          GOTO      EDEBT710
.
EDEBT720  PACK      KEY8,PVIBILL,SP70        * Change to PATVIS fldname v90201
          CALL      RDDSCH1                   * Use PAT DSC read         v90201
          BRANCH    OVRCD,EDEBT710
.
          UNPACK    DDATE,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          MOVE      CPCDATE,DISDATE
          REP       " 0",DISDATE
          GOTO      EDEBT999
.
EDEBT999  RETURN
.
