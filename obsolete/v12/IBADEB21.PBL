.---------------------------------------------------------
. Open Patient System Files
.---------------------------------------------------------
OPPAT000  MOVE      ONE,FLGIBAPT       * Can't Open IBA Patient Files
          MOVE      ZERO,OVRCD
.
          CALL      TFILENAM
          MOVE      TFILNAME,FNAMPOST
.
          READ      CONTROLF,ZERO;*85,IBCNUGST
          READ      CONTROLF,EIGHTY1;*19,CFDBNFMT,*243,CFDBILAY        *C-75900
.
OPPAT100  TRAP      OVERCOND IF IO
          OPEN      PATMA1A1,"patma1af"
          TRAPCLR   IO
          BRANCH    OVRCD,OPPAT999
          MOVE      ZERO,FLGIBAPT
          OPEN      PATMX1A1,"patmx1af"
.
          COMPARE   EIGHT,CFDBILAY
          GOTO      OPPAT999 IF NOT EQUAL
.
          TRAP      OVERCOND IF IO
          OPEN      PATDSCH1,"patdschf"
          TRAPCLR   IO
          BRANCH    OVRCD,OPPAT999
          MOVE      ZERO,FLGIBAPT
.
          TRAP      OVERCOND IF IO
          OPEN      PATMI1A1,"patmi1af"
          TRAPCLR   IO
          BRANCH    OVRCD,OPPAT999
          MOVE      ZERO,FLGIBAPT
.
          TRAP      OVERCOND IF IO
          OPEN      PATVISA1,"patvisaf"
          TRAPCLR   IO
          BRANCH    OVRCD,OPPAT999
          MOVE      ZERO,FLGIBAPT
.
          TRAP      OVERCOND IF IO
          OPEN      PATSNAA1,"patsnaaf"
          TRAPCLR   IO
          BRANCH    OVRCD,OPPAT999
          MOVE      ZERO,FLGIBAPT
          GOTO      OPPAT999
.
OPPAT999  RETURN
