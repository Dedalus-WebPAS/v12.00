. Print Department Details by Patient Service
.---------------------------------------------------------------------------
PPCS0000  PACK      KEY21,CCDPHCD,CCDPDPT,SP70
          CALL      RSCCPD4
PPCS1000  CALL      RKCCPD4
          BRANCH    OVRCD,PPCS9999
          PACK      KEY21,CCPDHCD,CCPDDPT,SP70
          PACK      KEY10,CCDPHCD,CCDPDPT,SP70
          MATCH     KEY10,KEY21
          GOTO      PPCS9999 IF NOT EQUAL
.
          COMPARE   "50",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          CALL      PCDTOT00      * Calculate Total
.
          MOVE      "999,999.99",F10TOT1
          MOVE      "999,999.99",F10TOT2
          MOVE      "999,999.99",F10TOT3
          MOVE      "999,999.99",F10TOT4
          MOVE      "999,999.99",F10TOT5
          MOVE      "999,999.99",F10TOT6
          MOVE      "999,999.99",F10TOT7
.
          ASSIGN    FIXLAB/TOTALQTY,F12P2
          FEDIT     F12P2,F10TOT1
          MATCH     "*",F10TOT1
          IF        @EQUAL
            MOVE      "9999999.99",F10TOT1
            FEDIT     F12P2,F10TOT1
          ENDIF
.
          ASSIGN    FIXOTH/TOTALQTY,F12P2
          FEDIT     F12P2,F10TOT2
          MATCH     "*",F10TOT2
          IF        @EQUAL
            MOVE      "9999999.99",F10TOT2
            FEDIT     F12P2,F10TOT2
          ENDIF
.
          ASSIGN    (FIXLAB+FIXOTH)/TOTALQTY,F12P2
          FEDIT     F12P2,F10TOT3
          MATCH     "*",F10TOT3
          IF        @EQUAL
            MOVE      "9999999.99",F10TOT3
            FEDIT     F12P2,F10TOT3
          ENDIF
.
          ASSIGN    VARLAB/TOTALQTY,F12P2
          FEDIT     F12P2,F10TOT4
          MATCH     "*",F10TOT4
          IF        @EQUAL
            MOVE      "9999999.99",F10TOT4
            FEDIT     F12P2,F10TOT4
          ENDIF
.
          ASSIGN    VAROTH/TOTALQTY,F12P2
          FEDIT     F12P2,F10TOT5
          MATCH     "*",F10TOT5
          IF        @EQUAL
            MOVE      "9999999.99",F10TOT5
            FEDIT     F12P2,F10TOT5
          ENDIF
.
          ASSIGN    (VARLAB+VAROTH)/TOTALQTY,F12P2
          FEDIT     F12P2,F10TOT6
          MATCH     "*",F10TOT6
          IF        @EQUAL
            MOVE      "9999999.99",F10TOT6
            FEDIT     F12P2,F10TOT6
          ENDIF
.
          ASSIGN    (FIXLAB+FIXOTH+VARLAB+VAROTH)/TOTALQTY,F12P2
          FEDIT     F12P2,F10TOT7
          MATCH     "*",F10TOT7
          IF        @EQUAL
            MOVE      "9999999.99",F10TOT7
            FEDIT     F12P2,F10TOT7
          ENDIF
.
          MOVE      "999.9",F5TOT1
          MOVE      "999.9",F5TOT2
          ASSIGN    (((FIXLAB+FIXOTH)/TOTALQTY)/F12P2)*100.0,F10P1
          FEDIT     F10P1,F5TOT1
          IF        F10P1=100
            MOVE      "100.0",F5TOT1
          ENDIF
          ASSIGN    (((VARLAB+VAROTH)/TOTALQTY)/F12P2)*100.0,F10P1
          FEDIT     F10P1,F5TOT2
          IF        F10P1=100
            MOVE      "100.0",F5TOT2
          ENDIF
.
          ADD       ONE,CLNO
          PRINT     "| ",CCPDSEQ,SP1,CCPDDES,"|":
                    F10TOT1,F10TOT2,F10TOT3," |":
                    F10TOT4,F10TOT5,F10TOT6," |",F10TOT7:
                    " |",F5TOT1," |",F5TOT2," |"
          GOTO      PPCS1000
.
