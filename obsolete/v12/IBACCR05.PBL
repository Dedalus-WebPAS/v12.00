          PACK      KEY24,CCDPHCD,CCDPDPT,CCPDPCD,CCCAYEAR,SP70
          PACK      KEY22,CCDPHCD,CCDPDPT,CCPDPCD,CCCAYEAR,SP70
          CALL      RSCCME2
.
PCDTOT10  CALL      RKCCME2          * Costs by Cost Type
          BRANCH    OVRCD,PCDTOT99
          PACK      KEY24,CCMEHCD,CCMEDPT,CCMEPCD,CCMEYEAR,CCMEPER,SP70
          MATCH     KEY22,KEY24
          GOTO      PCDTOT99 IF NOT EQUAL
          MATCH     CCMEPER,LPERIOD
          GOTO      PCDTOT99 IF LESS
          MATCH     FPERIOD,CCMEPER
          GOTO      PCDTOT10 IF LESS
          ADD       CCMEQTY,TOTALQTY
.
          PACK      KEY28,CCMEHCD,CCMEDPT,CCMEYEAR,CCMEPER,CCMEPCD,SP70
          PACK      KEY24,CCMEHCD,CCMEDPT,CCMEYEAR,CCMEPER,CCMEPCD,SP70
          CALL      RSCCMD1
PCDTOT20  CALL      RKCCMD1
          BRANCH    OVRCD,PCDTOT10
          PACK      KEY28,CCMDHCD,CCMDDPT,CCMDYEAR,CCMDPER,CCMDPCD,SP70
          MATCH     KEY24,KEY28
          GOTO      PCDTOT10 IF NOT EQUAL
.
          CALL      TOTAL000
.
          GOTO      PCDTOT20
.
PCDTOT99  RETURN
.------------------------------------------------------------
. Total Cost Type Analysis
.------------------------------------------------------------
TOTAL000  COMPARE   ZERO,CCMDCST
          GOTO      TOTAL999 IF EQUAL
.
          PACK      KEY4,CCMDCTY
          CALL      RDCCCT1
          BRANCH    OVRCD,TOTAL010
.
          MOVE      CCCTFIX,F1
          BRANCH    F1,TOTAL100
.
          MOVE      CCCTLAB,F1
          BRANCH    F1,TOTAL010
          ASSIGN    VAROTH+(CCMDCST*CCMEQTY),VAROTH
          GOTO      TOTAL999
.
TOTAL010  ASSIGN    VARLAB+(CCMDCST*CCMEQTY),VARLAB
          GOTO      TOTAL999
.
. Fixed Cost
.------------
TOTAL100  MOVE      CCCTLAB,F1
          BRANCH    F1,TOTAL110
          ASSIGN    FIXOTH+(CCMDCST*CCMEQTY),FIXOTH
          GOTO      TOTAL999
.
TOTAL110  ASSIGN    FIXLAB+(CCMDCST*CCMEQTY),FIXLAB
.
TOTAL999  RETURN
