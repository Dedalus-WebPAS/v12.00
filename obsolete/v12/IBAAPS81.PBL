.-----------------------------------------------------------------------------
. Print Bank Statement Import Rules
.-----------------------------------------------------------------------------
REPT0000  DISPLAY   *P1:24,*EL,"Printing Page...   ";
          MOVE      ZERO,CPAGENO                 * reset page no.
          CLOCK     TIME,CTIMEIS                 * get current time
          MOVE      "999",CLNO                   * init last line number
          PACK      KEY3,SP70
          CALL      RSFMBR1
.
REPT1000  CALL      RKFMBR1
          BRANCH    OVRCD,REPT9000
          COMPARE   "55",CLNO                    * end of page ?
          CALL      HEAD0000 IF NOT LESS
.
          PRINT     FMBRRID,SP2,FMBRDES,FMBRCHQ,SP1,FMBRBSC,SP3:
                    FMBRNOR,SP1,FMBRLOG:
                    SP2,FMBRLED,"/",FMBRDAC,SP1,FMBRCAC
          ADD       "1",CLNO           * add one to line number
          GOTO      REPT1000                     * get next record
.
REPT9000  COMPARE   ZERO,CPAGENO
          CALL      HEAD0000 IF EQUAL          * print header if nothing printed
          CALL      UND132                       * print line
          PRINT     *40,"****   End of Report   ****"
.
REPT9999  RETURN
.********************************************************************
. Report Heading
.********************************************************************
HEAD0000  CALL      HEAD132                      * print heading
          DISPLAY   *P25:24,CPAGENO
          CALL      UND132                       * print line
          PRINT     "Code Description                   ":
                    "     Cheque Acct     Code  Remitter         Lodgment Ref.":
                    "       Ledger/Dr Acct. Cr Acct. "
          ADD       "1",CLNO
          CALL      UND132                       * print line
.                                                * re-read current record
HEAD9999  RETURN
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Cancel Changes.
.
.-------------------------------------------------------------
CANC0000  PACK     KEY3,FMBRRID,SP70   * Setup Key for file.
          CALL     RAFMBR1             * Check to see if record on file.
          IF       OVRCD=0
            CALL     UUFMBR1           * Unlock.
          ENDIF
          RETURN
.
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Post Changes.
.-------------------------------------------------------------
POST0000  PACK     KEY3,FMBRRID,SP70  * Setup Key
          CALL     RAFMBR1            * Check for record on file
          IF       OVRCD=0
            CALL     UPFMBR1          * Update Changes
            CALL     UUFMBR1          * Unlock
          ELSE
            CALL     WRFMBR1          * Write New Record
          ENDIF
          RETURN                      * Return to program.
.
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Delete Record.
.-------------------------------------------------------------
DELT0000  MOVE     SP70,DISPMSG        * Clear Line 24 Display Message
          CALL     MESSAGE2            * Save and Clear Line 24
          CALL     DELQST              * Delete (Y/N/C) CEXIT 1/2/3
          CALL     MESSAGE3            * Restore Line 24
          BRANCH   CEXIT,DELT1000,DELT9000,DELT9100
.
DELT1000  PACK     KEY3,FMBRRID,SP70   * Setup Key to File
          CALL     RAFMBR1             * Read Record
          IF       OVRCD=0
            CALL     DEFMBR1           * Delete Record
          ENDIF
          MOVE     TWO,EXIT            * Restart Screen
          GOTO     DELT9999
DELT9000  MOVE     ONE,EXIT            * Return to Prompt on "No"
          GOTO     DELT9999
DELT9100  MOVE     TWO,EXIT            * Restart Screen on "Cancel"
          GOTO     DELT9999
DELT9999  RETURN
.
.-------------------------------------------------------------
. Audit & Locking Standard Routine to Enter Key Field
.-------------------------------------------------------------
EKEYF000  MOVE      TWO,CKEYTYP             * Record Does not have to be on file
.                                           * This allows add and change.
          CALL      KFMBRA00                * Call Standard Keyin Routine with ?
          BRANCH    EXIT,EKEYF999,EKEYF999  * Exit 1 Null Entry 2 Exitchar
          COMPARE   ONE,OVRCD               * New Record so no Lock
          GOTO      EKEYF999 IF EQUAL       * Return
.
EKEYF100  MOVE      SP70,FMDSDESC
          MOVE      SP70,FMRSDESC
          MOVE      SP70,FMAMDESC
          MOVE      SP70,DACNAME
          MOVE      SP70,CACNAME
          MOVE      SP70,FMLADESC
          PACK      KEY3,FMBRRID,SP70       * Setup Key to File
          CALL      RLFMBR1                 * Read and Lock Record
          BRANCH    OVRCD,EKEYF999,EKEYF200 * OVRCD=1 Not on File 2 Locked
          CALL      REDCOD00
.
          MOVE      ZERO,EXIT               * Setup Exit Field
          MOVE      ZERO,OVRCD              * Setup Ovrcd Field
          GOTO      EKEYF999
.
EKEYF200  MOVE      SP70,DISPMSG            * Clear Line 24 Message Display
          CALL      MESSAGE2                * Clear Line 24 and Save
          KEYIN     *P1:24,*EL,"Record Locked - (":
                    *V2LON,"R",*HOFF,")etry or e(":
                    *V2LON,"X",*HOFF,")it ? ",ANS
          CALL      MESSAGE3                * Restore Line 24
          REP       UPPLOW,ANS              * Convert to Upper Case
          MATCH     ANSR,ANS                * Re-try
          GOTO      EKEYF100 IF EQUAL       * Return to locking
          MATCH     ANSX,ANS                * Exit
          GOTO      EKEYF200 IF NOT EQUAL   * Must be a valid response
          GOTO      EKEYF000                * Return to Keyin
.
EKEYF999  RETURN                            * End of Routine
.
REDCOD00  MOVE      FMBRDIS,KEY5
          CALL      RDFMDS1
          MOVE      FMBRRES,KEY4
          CALL      RDFMRS1
          MOVE      FMBRLED,KEY2
          CALL      RDFMLA1
          PACK      KEY14,FMBRLED,FMBRDAC
          CALL      RDFMAM1
          IF        OVRCD=0
            MOVE      FMAMDESC,DACNAME
          ENDIF
.
          PACK      KEY14,FMBRLED,FMBRCAC
          CALL      RDFMAM1
          IF        OVRCD=0
            MOVE      FMAMDESC,CACNAME
          ENDIF
          RETURN
