.---------------------------------------------------------
. Add Service Providers
.--------------------------------------------------------
ADSER000  WRITE    PPDEFFIL,SEQ;";Service Providers"
          WRITE    PPDEFFIL,SEQ;"1 SERVICE ",QUOTE,"All Service Providers",QUOTE
          PACK     KEY3,SP70
          CALL     RSCESP1
ADSER100  CALL     RKCESP1
          BRANCH   OVRCD,ADSER900
          WRITE    PPDEFFIL,SEQ;"2 ",CESPSPC,SP1,QUOTE,CESPDES,QUOTE
          GOTO     ADSER100
ADSER900
ADSER999  RETURN
.---------------------------------------------------------
. Add Time Periods
.--------------------------------------------------------
ADTIM000  WRITE    PPDEFFIL,SEQ;";Time Periods"
          WRITE    PPDEFFIL,SEQ;"1 TIMPER ",QUOTE,"All Years",QUOTE
          MOVE     SP70,LASTYEAR
          PACK     KEY6,SP70
          CALL     RSCEPE1
.
ADTIM100  CALL     RKCEPE1
          BRANCH   OVRCD,ADTIM900
          MATCH    LASTYEAR,CEPEYEAR
          IF       !@EQUAL
            MOVE     CEPEYEAR,LASTYEAR
            WRITE    PPDEFFIL,SEQ;"2 ",CEPEYEAR,SP1,QUOTE,CEPEYEAR,QUOTE
          ENDIF
          PACK     KEY6,CEPEYEAR,CEPEPER
          REP      " 0",KEY6
          STRIP    CEPEDES
          WRITE    PPDEFFIL,SEQ;"3 ",KEY6,SP1,QUOTE,CEPEDES,CEPEYEAR,QUOTE
          GOTO     ADTIM100
ADTIM900
ADTIM999  RETURN
.---------------------------------------------------------
. Add Measures
.------------------------------------------------------------
ADMES000  WRITE    PPDEFFIL,SEQ;";Measures "
          WRITE    PPDEFFIL,SEQ;"1 Casemix Analysis Measures"
          WRITE    PPDEFFIL,SEQ;"2 STD1 ",QUOTE,CCCOCD1,QUOTE:
                                  " Storage=INT_32 Scale=0"
          WRITE    PPDEFFIL,SEQ;"2 STD2 ",QUOTE,CCCOCD2,QUOTE:
                                  " Storage=INT_32 Scale=0"
          WRITE    PPDEFFIL,SEQ;"2 STD3 ",QUOTE,CCCOCD3,QUOTE:
                                  " Storage=INT_32 Scale=0"
          WRITE    PPDEFFIL,SEQ;"2 STD4 ",QUOTE,CCCOCD4,QUOTE:
                                  " Storage=INT_32 Scale=0"
          WRITE    PPDEFFIL,SEQ;"2 STD5 ",QUOTE,CCCOCD5,QUOTE:
                                  " Storage=INT_32 Scale=0"
          WRITE    PPDEFFIL,SEQ;"2 GLA ",QUOTE,"GL Actual",QUOTE:
                                  " Storage=INT_32 Scale=0"
          WRITE    PPDEFFIL,SEQ;"2 GLB ",QUOTE,"GL Budget",QUOTE:
                                  " Storage=INT_32 Scale=0"
          WRITE    PPDEFFIL,SEQ;"2 STD ",QUOTE,"Total Std. Cost",QUOTE," & "
          WRITE    PPDEFFIL,SEQ;"          CALC=STD1+STD2+STD3+STD4+STD5"
.
          RETURN
