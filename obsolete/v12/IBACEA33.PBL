.------------------------------------------------------------
. Extract Service Provider PowerPlay Analysis
.------------------------------------------------------------
MAKPP000  MOVE      "   64",AUDTSECT
          READ      CONTROLF,AUDTSECT;*7,CCCODIR,*59,CCCOCD1,CCCOCD2,CCCOCD3:
                                      CCCOCD4,CCCOCD5
.
          CALL     OPEN0000  * Create Files
.
          CALL     CDAT0000  * Create Data File
.
          CALL     CDEF0000  * Create Definitions File
.
          RETURN
.-----------------------------------------------------------
. Output Data File
.-----------------------------------------------------------
CDAT0000
.
          PACK      KEY9,SP70
          CALL      RSCESA1
CDAT1000  CALL      RKCESA1
          BRANCH    OVRCD,CDAT9000
.
          ASSIGN    CESACST1,COST1
          ASSIGN    CESACST2,COST2
          ASSIGN    CESACST3,COST3
          ASSIGN    CESACST4,COST4
          ASSIGN    CESACST5,COST5
          ASSIGN    CESAGLA,ACTUAL
          ASSIGN    CESAGLB,BUDGET
          WRITE     PPDATFIL,SEQ;QUOTE,CESAYEAR,CESAPER,QUOTE,SP1:
                                CESASPC,SP1:
                                COST1,SP1:
                                COST2,SP1:
                                COST3,SP1:
                                COST4,SP1:
                                COST5,SP1:
                                ACTUAL,SP1:
                                BUDGET
          GOTO      CDAT1000
.
CDAT9000  RETURN
.
