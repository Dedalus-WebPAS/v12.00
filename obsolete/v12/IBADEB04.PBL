           MOVE     SP70,DBITUC2D
           MOVE     SP70,DBITUC3D
           MOVE     SP70,DBITUC4D
           MATCH    SP70,DBITUC1
           IF       !@EQUAL
             MOVE     "IT01",DBCDCAT
             PACK     KEY7,DBCDCAT,DBITUC1
             CALL     RDDBCD1
             IF       OVRCD=0
               MOVE     DBCDDES,DBITUC1D
             ENDIF
           ENDIF
           MATCH    SP70,DBITUC2
           IF       !@EQUAL
             MOVE     "IT02",DBCDCAT
             PACK     KEY7,DBCDCAT,DBITUC2
             CALL     RDDBCD1
             IF       OVRCD=0
               MOVE     DBCDDES,DBITUC2D
             ENDIF
           ENDIF
           MATCH    SP70,DBITUC3
           IF       !@EQUAL
             MOVE     "IT03",DBCDCAT
             PACK     KEY7,DBCDCAT,DBITUC3
             CALL     RDDBCD1
             IF       OVRCD=0
               MOVE     DBCDDES,DBITUC3D
             ENDIF
           ENDIF
           MATCH    SP70,DBITUC4
           IF       !@EQUAL
             MOVE     "IT04",DBCDCAT
             PACK     KEY7,DBCDCAT,DBITUC4
             CALL     RDDBCD1
             IF       OVRCD=0
               MOVE     DBCDDES,DBITUC4D
             ENDIF
           ENDIF
.
           MOVE     SP70,DBPTDES
           PACK     KEY8,DBITPTY,SP70
           CALL     RDDBPT1
.
           MOVE     SP70,FMLADESC
           MOVE     SP70,IACNAME
           MOVE     SP70,CACNAME
           MOVE     SP70,TACNAME
           MOVE     SP70,TCANAME
           BRANCH   FLGIBAGL,GETCDS99
           MOVE     DBITLED,KEY2
           CALL     RDFMLA1
.
           PACK     KEY14,DBITLED,DBITIAC
           CALL     RDFMAM1
           IF       OVRCD=0
             MOVE     FMAMDESC,IACNAME
           ENDIF
.
           PACK     KEY14,DBITLED,DBITCAC
           CALL     RDFMAM1
           IF       OVRCD=0
             MOVE     FMAMDESC,CACNAME
           ENDIF
.
           PACK     KEY14,DBITLED,DBITTAC
           CALL     RDFMAM1
           IF       OVRCD=0
             MOVE     FMAMDESC,TACNAME
           ENDIF
.
           PACK     KEY14,DBITLED,DBITTCA
           CALL     RDFMAM1
           IF       OVRCD=0
             MOVE     FMAMDESC,TCANAME
           ENDIF
.
