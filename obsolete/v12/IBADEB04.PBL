            MOVE      FMCSDEB,DBITCAC
            MOVE      FMCSDEB,DBITTCA
          ENDIF
          MATCH     SP70,FMCSAGST
          IF        @EQUAL
            MOVE      FMLAAGST,DBITTAC
          ELSE
            MOVE      FMCSAGST,DBITTAC
          ENDIF
          PACK      KEY14,DBITLED,DBITCAC,SP70
          CALL      RDFMAM1
          BRANCH    OVRCD,GETC9999
.
          MOVE      FMAMDESC,CACNAME
          MOVE      "   14",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      DBITCAC,VAR
            CALL      DISPITEM
          ENDIF
.
          MOVE      "   36",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      CACNAME,VAR
            CALL      DISPITEM
          ENDIF
.
          PACK      KEY14,DBITLED,DBITTAC,SP70
          CALL      RDFMAM1
          BRANCH    OVRCD,GETC9999
.
          MOVE      FMAMDESC,TACNAME
          MOVE      "   16",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      DBITTAC,VAR
            CALL      DISPITEM
          ENDIF
.
          MOVE      "   37",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      TACNAME,VAR
            CALL      DISPITEM
          ENDIF
.
          MOVE      "   13",FLDID
          CALL      GETPOSIT
.
GETC9999  RETURN
.
