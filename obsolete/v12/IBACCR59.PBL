. Print Line
.------------------------------------------------------------
PLIN0000  MOVE      CCEDFSV,CCVARIAN
          CALL      CALSD000
          MOVE      CCSTDDEV,STDDEV01
.
          MOVE      CCEDTSV,CCVARIAN
          CALL      CALSD000
          MOVE      CCSTDDEV,STDDEV02
.
          MOVE      "99999999.99",F11TOT1
          MOVE      "99999999.99",F11TOT2
          MOVE      "99999999.99",F11TOT3
          MOVE      "99999999.99",F11TOT4
          MOVE      "99999999.99",F11TOT5
          MOVE      "99999999.99",F11TOT6
          MOVE      "99999999.99",F11TOT7
          MOVE      "99999999.99",F11TOT8
          BRANCH    REPTFORM,PLIN0100,PLIN0100,PLIN0100,PLIN1400,PLIN0100
.
PLIN0100  MATCH     SP70,CCEASCM1
          IF        !@EQUAL
            MATCH     "1",CCEAUST
            IF        !@EQUAL
              ASSIGN    CCEDFTC+CCEDFMN+CCEDFMX+CCEDFSV,CCEDFTC
              MOVE      ZERO,CCEDFMN
              MOVE      ZERO,CCEDFMX
              MOVE      ZERO,CCEDFSV
            ENDIF
          ENDIF
          BRANCH    REPTFORM,PLIN1100,PLIN1200,PLIN1300,PLIN1400,PLIN1500
.
PLIN1100  ASSIGN    CCEDFTC/CCEDNEP,F12P2
          FEDIT     F12P2,F11TOT1
          MOVE      CCEDFMN,F12P2
          FEDIT     F12P2,F11TOT2
          MOVE      CCEDFMX,F12P2
          FEDIT     F12P2,F11TOT3
          MOVE      STDDEV01,F12P2
          FEDIT     F12P2,F11TOT4
.
          ASSIGN    CCEDTTC/CCEDNEP,F12P2
          FEDIT     F12P2,F11TOT5
          MOVE      CCEDTMN,F12P2
          FEDIT     F12P2,F11TOT6
          MOVE      CCEDTMX,F12P2
          FEDIT     F12P2,F11TOT7
          MOVE      STDDEV02,F12P2
          FEDIT     F12P2,F11TOT8
          GOTO      PLIN8000
.
PLIN1200
          MOVE      "999999.99 %",F11TOT4
          MOVE      "999999.99 %",F11TOT8
          MOVE      CCEDFTC,F12P2
          FEDIT     F12P2,F11TOT1
          ASSIGN    CCEDFTC/CCEDNEP,F12P2
          FEDIT     F12P2,F11TOT2
          ASSIGN    (STDDEV01/F12P2)*100.0,F12P2
          FEDIT     F12P2,F11TOT4
          MOVE      STDDEV01,F12P2
          FEDIT     F12P2,F11TOT3
.
          MOVE      CCEDTTC,F12P2
          FEDIT     F12P2,F11TOT5
          ASSIGN    CCEDTTC/CCEDNEP,F12P2
          FEDIT     F12P2,F11TOT6
          ASSIGN    (STDDEV02/F12P2)*100.0,F12P2
          FEDIT     F12P2,F11TOT8
          MOVE      STDDEV02,F12P2
          FEDIT     F12P2,F11TOT7
          GOTO      PLIN8000
.
PLIN1300  ASSIGN    CCEDFTC/CCEDNEP,F12P2
          FEDIT     F12P2,F11TOT1
          MOVE      CCEDNEP,CCVARIAN
          CALL      CALSD000
          MOVE      CCSTDDEV,F12P4
          IF        ((CCCONINT>0)&(CCEDNEP>1))
            ASSIGN    CCEDNEP-1,CCSAMSIZ   * Sample Size
            CALL      LODSTD00
            ASSIGN    (CCEDFTC/CCEDNEP)-((CCSTDFAC*STDDEV01)/F12P4),CCEDFMN
            IF        CCEDFMN<0
              MOVE      ZERO,CCEDFMN
            ENDIF
            ASSIGN    (CCEDFTC/CCEDNEP)+((CCSTDFAC*STDDEV01)/F12P4),CCEDFMX
          ELSE
            MOVE      ZERO,CCEDFMN
            MOVE      ZERO,CCEDFMX
          ENDIF
          MOVE      CCEDFMN,F12P2
          FEDIT     F12P2,F11TOT2
          MOVE      CCEDFMX,F12P2
          FEDIT     F12P2,F11TOT3
          ASSIGN    CCEDFMX-CCEDFMN,F12P2
          FEDIT     F12P2,F11TOT4
.
          ASSIGN    CCEDTTC/CCEDNEP,F12P2
          FEDIT     F12P2,F11TOT5
.
          IF        ((CCCONINT>0)&(CCEDNEP>1))
            ASSIGN    CCEDNEP-1,CCSAMSIZ   * Sample Size
            CALL      LODSTD00
            ASSIGN    (CCEDTTC/CCEDNEP)-((CCSTDFAC*STDDEV02)/F12P4),CCEDTMN
            IF        CCEDTMN<0
              MOVE      ZERO,CCEDTMN
            ENDIF
            ASSIGN    (CCEDTTC/CCEDNEP)+((CCSTDFAC*STDDEV02)/F12P4),CCEDTMX
          ELSE
            MOVE      ZERO,CCEDTMN
            MOVE      ZERO,CCEDTMX
          ENDIF
.
          MOVE      CCEDTMN,F12P2
          FEDIT     F12P2,F11TOT6
          MOVE      CCEDTMX,F12P2
          FEDIT     F12P2,F11TOT7
          ASSIGN    CCEDTMX-CCEDTMN,F12P2
          FEDIT     F12P2,F11TOT8
          GOTO      PLIN8000
.
PLIN1400  MOVE      CCEDFMX,F12P2
          FEDIT     F12P2,F11TOT1
          MOVE      CCEDFTC,F12P2
          FEDIT     F12P2,F11TOT2
          MOVE      CCEDFSV,F12P2
          FEDIT     F12P2,F11TOT3
          MOVE      CCEDFMN,F12P2
          FEDIT     F12P2,F11TOT4
          ASSIGN    CCEDFTC+CCEDFMN+CCEDFMX+CCEDFSV,F12P2
          FEDIT     F12P2,F11TOT5
.
          ASSIGN    CCEDTTC+CCEDTMN+CCEDTMX+CCEDTSV,CCEDTTC
          MOVE      CCEDTTC,F12P2
          FEDIT     F12P2,F11TOT6
.
          ASSIGN    CCEDTTC-CCEDFTC-CCEDFMN-CCEDFMX-CCEDFSV,F12P2
          FEDIT     F12P2,F11TOT7
.
          ASSIGN    (F12P2/CCEDTTC)*100.0,F12P2
          FEDIT     F12P2,F11TOT8
.
          GOTO      PLIN8000
.
PLIN1500  MOVE      "99999999999",F11TOT1
          ASSIGN    CCEDTTC+CCEDTMN+CCEDTMX+CCEDTSV,CCEDTTC
          MOVE      CCEDLTO,F12
          FEDIT     F12,F11TOT1
          ASSIGN    CCEDLTO/CCEDNEP,F12P2
          FEDIT     F12P2,F11TOT2
          MOVE      CCEDFTC,F12P2
          FEDIT     F12P2,F11TOT3
          MOVE      CCEDTTC,F12P2
          FEDIT     F12P2,F11TOT4
          ASSIGN    CCEDTTC-CCEDFTC,F12P2
          FEDIT     F12P2,F11TOT5
          ASSIGN    (F12P2/CCEDTTC)*100.0,F12P2
          FEDIT     F12P2,F11TOT6
          ASSIGN    CCEDFTC/CCEDLTO,F12P2
          FEDIT     F12P2,F11TOT7
          ASSIGN    CCEDTTC/CCEDLTO,F12P2
          FEDIT     F12P2,F11TOT8
          GOTO      PLIN8000
.
.
PLIN8000  COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
.
          PACK      KEY13,CCEALV2,CCEDLV2
          CALL      RDCCXH1
          IF        OVRCD=1
