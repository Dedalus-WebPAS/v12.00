.---------------------------------------------------------------------------
.   System    :   Casemix Expense Analysis           
.   Include   :   CEAVSCKY
.   Routine   :   KCEVSA00 Keyin Service Category    
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCEVSA00 TOPIC     "CEVSCAT "
          MOVE      UNDLN70,CEVSCAT 
          KEYIN     *PCCOL:CVERT,*DV,CEVSCAT:                                   
                    *PCCOL:CVERT,*V2LON,CEVSCAT:                                
                    *PCCOL:CVERT,*DV,CEVSCAT 
.
          ENDSET    CEVSCAT 
          APPEND    SP70,CEVSCAT 
          RESET     CEVSCAT 
.
          MATCH     SP70,CEVSCAT 
          GOTO      KCEVSA90 IF EQUAL
.
          CMATCH    EXITCHAR,CEVSCAT 
          GOTO      KCEVSA95 IF EQUAL
.
          CMATCH    QUEST,CEVSCAT 
          GOTO      KCEVSA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCEVSA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CEVSCAT 
          APPEND    SP70,CEVSCAT 
          RESET     CEVSCAT 
.
          MATCH     SP70,CEVSCAT 
          GOTO      KCEVSA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CEVSCAT                                 
KCEVSA10
          PACK      KEY2,CEVSCAT                                                
          CALL      RACEVS1 
          BRANCH    OVRCD,KCEVSA80
          BRANCH    CKEYTYP,KCEVSA75
          CALL      RDCEVS1 
KCEVSA70
          MOVE      ZERO,EXIT
          GOTO      KCEVSA99
KCEVSA75
          MOVE      "Service Category Already Exist - ",DISPMSG                                      
          CALL      MESSAGE1
          GOTO      KCEVSA00
KCEVSA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCEVSA99,KCEVSA99
          MOVE      "Service Category Does Not Exist - ",DISPMSG                                     
          CALL      MESSAGE1
          GOTO      KCEVSA00
KCEVSA90
          MOVE      ONE,EXIT
          GOTO      KCEVSA99
KCEVSA95
          MOVE      TWO,EXIT
KCEVSA99 TOPIC    PRGID
          RETURN
.---------------------------------------------------------------------------
.   System    :   Casemix Expense Analysis           
.   Include   :   CEAVSCKY
.   Routine   :   HCEVSA00 Service Category    
.   Return    :           
.-------------------------------------------------------------------------------
HCEVSA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HCEVSA04
          CALL      GSCRN000
          BRANCH    EXIT,HCEVSA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Cat."               
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 5",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HCEVSA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CEVSCAT 
          PACK      KEY2,CEVSCAT,SP70                                           
.                                                                               
.                                                                               
          CALL      RSCEVS1 
HCEVSA10
          CALL      RKCEVS1 
          BRANCH    OVRCD,HCEVSA15
          PACK      KEY2,CEVSCAT,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HCEVSA12
          CALL      HCEVSA60
          BRANCH    OVRCD,HCEVSA10
HCEVSA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY2,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CEVSCAT 
          DISPLAY   *PSTDCOL02:STDROW,CEVSDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCEVSA10 IF NOT EQUAL
HCEVSA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCEVSA20
          CALL      HSCRI000
          BRANCH    EXIT,HCEVSA30,HCEVSA35,HCEVSA40,HCEVSA45,HCEVSA04:
                              HCEVSA91,HCEVSA50
          GOTO      HCEVSA92
HCEVSA30
          CALL      RKCEVS1 
          BRANCH    OVRCD,HCEVSA85
          PACK      KEY2,CEVSCAT,SP70                                           
.                                                                               
.                                                                               
          CALL      RPCEVS1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCEVSA10
HCEVSA35
          MOVE      ST1,KEY2 
          CALL      RDCEVS1 
          CALL      RPCEVS1 
          BRANCH    OVRCD,HCEVSA85
          PACK      KEY2,CEVSCAT,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCEVSA36
          CALL      RPCEVS1 
          BRANCH    OVRCD,HCEVSA37
          PACK      KEY2,CEVSCAT,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCEVSA36 IF NOT EQUAL
HCEVSA37
          MOVE      ZERO,XITEMNO
          GOTO      HCEVSA10
HCEVSA40
          MOVE      ST1,KEY2 
          CALL      RDCEVS1 
          CALL      RPCEVS1 
          BRANCH    OVRCD,HCEVSA85
          PACK      KEY2,CEVSCAT,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCEVSA08
HCEVSA45
          CALL      RKCEVS1 
          BRANCH    OVRCD,HCEVSA85
          PACK      KEY2,CEVSCAT,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY2,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCEVS1 
HCEVSA46
          CALL      RPCEVS1 
          BRANCH    OVRCD,HCEVSA47
          PACK      KEY2,CEVSCAT,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCEVSA46 IF NOT EQUAL
HCEVSA47
          MOVE      ZERO,XITEMNO
          GOTO      HCEVSA10
HCEVSA50  COMPARE   ZERO,CSEARCH
          GOTO      HCEVSA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCEVSA08
HCEVSA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCEVSA08
HCEVSA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCEVSA61 IF EQUAL
          MATCH     SCHFLD01,CEVSCAT 
          GOTO      HCEVSA72 IF NOT EQUAL
HCEVSA61
          MATCH     SP70,SCHFLD02
          GOTO      HCEVSA62 IF EQUAL
          MATCH     SCHFLD02,CEVSDES 
          GOTO      HCEVSA72 IF NOT EQUAL
HCEVSA62
HCEVSA63
HCEVSA64
HCEVSA65
HCEVSA66
HCEVSA67
HCEVSA68
HCEVSA69
HCEVSA70
          GOTO      HCEVSA75
HCEVSA72  MOVE      ONE,OVRCD
HCEVSA75  RETURN
HCEVSA85
          LOAD      KEY2,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY2,CEVSCAT                                                
          CALL      RDCEVS1 
HCEVSA90
.                                                                               
          BEEP
          GOTO      HCEVSA20
HCEVSA91
          MOVE      SP70,CEVSDES 
          MOVE      SP70,CEVSSEQ 
          MOVE      SP70,CEVSUR1 
          MOVE      SP70,CEVSUR2 
          MOVE      SP70,CEVSUD1 
          MOVE      SP70,CEVSUD2 
          MOVE      SP70,CEVSUY1 
          MOVE      SP70,CEVSUY2 
          MOVE      SP70,CEVSSPA 
          MOVE      SP70,KEY2 
          MOVE      SP70,CEVSCAT 
          GOTO      HCEVSA96
HCEVSA92
          MOVE      SP70,KEY2 
          MOVE      SP70,CEVSCAT 
          LOAD      KEY2,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY2,CEVSCAT                                                
HCEVSA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCEVSA99
          RETURN
