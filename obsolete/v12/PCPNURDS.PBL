.******************************************************************************
.*                              PCPNURDS                                      *
.*                Display Options for nurses codes                            *
.*                                                                            *
.* Modifications :                                                            *
.*      V9.02.00   30/12/2002  Tracey Nguyen  srf 23497                       *
.*                 r5.10 Care Plans Port to V9                                *
.******************************************************************************
PCPNURDS  DISPLAY   *P1:3,*EF:
                    *P34:4,*V2LON,"Nurses Codes"
.
PCPNUR01  MOVE      SP6,KEY6
          CALL      RSPCNUR1
          MOVE      SIX,CLNO
.
PCPNUR02  CALL      RKPCNUR1
          BRANCH    OVRCD,PCPNUR03
.
          DISPLAY   *P1:CLNO,PCNUCODE,"     ",PCNUNAME
          ADD       ONE,CLNO
.
          COMPARE   "22",CLNO
          GOTO      PCPNUR03 IF NOT LESS
.
          GOTO      PCPNUR02
.
PCPNUR03  DISPLAY   *P1:24,"( )ontinue, ( )irst page or ( )ext page : _":
                    *P2:24,*V2LON,"C",*P14:24,"F",*P30:24,"N"
.
          KEYIN     *P43:24,*V2LON,ANS
.
          MATCH     ANS,ANSC
          GOTO      PCPNUR99 IF EQUAL
.
          MATCH     ANS,ANSF
          GOTO      PCPNURDS IF EQUAL
.
          MATCH     ANS,ANSN
          GOTO      PCPNUR04 IF EQUAL
.
          GOTO      PCPNUR03
.
PCPNUR04  MOVE      SIX,CLNO
          DISPLAY   *P1:6,*EF;
          GOTO      PCPNUR02
.
PCPNUR99  RETURN 


