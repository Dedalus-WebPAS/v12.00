.------------------------------------------------------------
. Page Heading
.------------------------------------------------------------
HEAD0000  CALL      HEAD132
          PRINT     "Casemix Extract         : ",CCEAEID,SP1,CCEADES:
                    *80,"Trim Limits             : ",TLDESC
          PRINT     "Trim at Extract Level   : ",TRIMLEVL,SP4,CCSTDES:
                    *80,"Trim Based On           : ",TRIMBASE,SP1,TRIMDESC
          MOVE      NO,KEY3
          LOAD      KEY3,CALCPERC,YES
          PRINT     "Percentage to Trim      : ",TRIMPERC:
                    *80,"Use Quartile Calc.      : ",KEY3
          CALL      UND132
          IF        CALCPERC=0
            PRINT     "| Extract Key                   Number to Trim":
                      "                | Episode             L.O.S ":
                      "      Cost (1)       Cost (2) Excluded",*132,"|"
          ELSE
            PRINT     "| Extract Key                 Trim Point Lower":
                      "         Upper  | Episode             L.O.S ":
                      "      Cost (1)       Cost (2) Excluded",*132,"|"
          ENDIF
          CALL      UND132
          RETURN
.------------------------------------------------------------
. Print Excluded Records
.------------------------------------------------------------
PRTLIN00  ADD       ONE,CLNO
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
          MOVE      NO,KEY3
          MATCH     "1",CCECEXC
          IF        @EQUAL
            MOVE      YES,KEY3
          ENDIF
          PRINT     "|",*63,"| ",CCECHCD,"/",CCECEPS,CCECLOS,CCECFTC,CCECTTC:
                    SP3,KEY3,*132,"|"
          RETURN
.------------------------------------------------------------
. Print Header Record for Trimming
.------------------------------------------------------------
PTRIM000  ADD       ONE,CLNO
          COMPARE   "55",CLNO
          CALL      HEAD0000 IF NOT LESS
          IF        CALCPERC=0
            PRINT     "| ",KEY30,SP4,TRIMNUMB,*63,"|",*132,"|"
          ELSE
            PRINT     "| ",KEY30,SP4,TRIMLW,SP4,TRIMUP,*63,"|",*132,"|"
          ENDIF
          RETURN
.----------------------------------------------------------------------
. Enter Casemix Extract ID
.----------------------------------------------------------------------
EEID0000  MOVE      ZERO,CKEYTYP
          CALL      CCSCOPCL         * Close Files
          CALL      KCCEAA00
          BRANCH    EXIT,EEID9999,EEID9999
.
          MATCH     SP70,CCEASCM1
          IF        !@EQUAL
            MATCH     "1",CCEAUST
            IF        !@EQUAL
             MOVE      "Trimming Not Enabled for Split Cost Extracts - ",DISPMSG
             BEEP
             CALL      MESSAGE1
             GOTO      EEID0000
            ENDIF
          ENDIF
.
          CALL      CCSCOPOP         * Open Files
EEID9999  RETURN
ETLIM000  KEYIN    *PCCOL:CVERT,*V2LON,TRIMLIM
          REP      UPPLOW,TRIMLIM
          MOVE     "Trim Upper Limit Only",TLDESC
          MATCH    "U",TRIMLIM
          GOTO     ETLIM999 IF EQUAL
          MOVE     "Trim Lower Limit Only",TLDESC
          MATCH    "L",TRIMLIM
          GOTO     ETLIM999 IF EQUAL
          MOVE     "Trim at Both Limits",TLDESC
          MATCH    "B",TRIMLIM
          GOTO     ETLIM999 IF EQUAL
          BEEP
          MOVE     "Valid Entries = U,L,B  - ",DISPMSG
          CALL     MESSAGE1
          GOTO     ETLIM000
ETLIM999  RETURN
.------------------------------------------------------------
. Enter Level
.------------------------------------------------------------
ELEVL000  KEYIN     *PCCOL:CVERT,*V2LON,TRIMLEVL
          BRANCH    TRIMLEVL,ELEVL110,ELEVL120,ELEVL130
          BEEP
          GOTO      ELEVL000
.
ELEVL110
          GOTO      ELEVL999
.
ELEVL120
          MATCH     SP70,CCEALV2
          IF        @EQUAL
            BEEP
            MOVE      "Extract Not Valid for this level - ",DISPMSG
            CALL      MESSAGE1
            GOTO      ELEVL000
          ENDIF
          GOTO      ELEVL999
.
ELEVL130
          MATCH     SP70,CCEALV3
          IF        @EQUAL
            BEEP
            MOVE      "Extract Not Valid for this level - ",DISPMSG
            CALL      MESSAGE1
            GOTO      ELEVL000
          ENDIF
          GOTO      ELEVL999
.
ELEVL999  RETURN
.
.------------------------------------------------------------
. Enter Percentage to Trim
.------------------------------------------------------------
EPERC000  MOVE      ZERO,TRIMPERC
          COMPARE   ONE,CALCPERC
          GOTO      EPERC999 IF EQUAL
.
          MOVE      SP70,VAR
          CALL      DISPITEM
          KEYIN     *PCCOL:CVERT,*V2LON,*NOMASK,*RV,TRIMPERC
.
EPERC999  RETURN
.------------------------------------------------------------
. Enter Format
.------------------------------------------------------------
EFORM000  MOVE      SP70,DISPMSG
          CALL      MESSAGE2
          DISPLAY   *P1:24,*EL,"Trim Based on : ":
                    *V2LON,"1",*HOFF,". Length of Stay, ":
                    *V2LON,"2",*HOFF,". Cost (1), ":
                    *V2LON,"3",*HOFF,". Cost (2)"
EFORM100  KEYIN     *PCCOL:CVERT,*V2LON,TRIMBASE
          BRANCH    TRIMBASE,EFORM110,EFORM120,EFORM130
          BEEP
          GOTO      EFORM100
.
EFORM110  MOVE      "Length of Stay    ",TRIMDESC
          GOTO      EFORM999
.
EFORM120  MOVE      "Cost (1)    ",TRIMDESC
          GOTO      EFORM999
.
EFORM130  MOVE      "Cost (2)     ",TRIMDESC
          GOTO      EFORM999
.
EFORM999  CALL      MESSAGE3
          RETURN
