          GOTO     PROC1900 IF NOT EQUAL                               *I-54038
.
          PACK     KEY18,AMDSYEAR,AMDSREG,AMDSASS
          CALL     DEAMDS2
          CALL     RSAMDS2
          GOTO     PROC1000
PROC1900
.
. General Ledger Transfer File
.
          COMPARE  ZERO,PURTRN
          GOTO     PROC2900 IF EQUAL
          DISPLAY  *P1:24,*EL,"Purging G/L Interface History : "
          PACK     KEY25,PURYEAR,SP70
          CALL     RSAMTR3
PROC2000  CALL     RKAMTR3
          BRANCH   OVRCD,PROC2900
          MATCH    AMTRYEAR,PURYEAR
          GOTO     PROC2900 IF NOT EQUAL                               *I-54038
.
          PACK     KEY25,AMTRYEAR,AMTRPER,AMTRREG,AMTRASS,AMTRUNI
          CALL     DEAMTR3
          CALL     RSAMTR3
          GOTO     PROC2000
PROC2900
.
. Purge Dept History
.
          COMPARE  ZERO,PURDPT
          GOTO     PROC3900 IF EQUAL
          DISPLAY  *P1:24,*EL,"Purging Department Transfer History : "
          PACK     KEY22,SP70
          CALL     RSAMDH1
PROC3000  CALL     RKAMDH1
          BRANCH   OVRCD,PROC3900
          MATCH    STRDAT,AMDHDAT
          GOTO     PROC3000 IF LESS
          MATCH    AMDHDAT,ENDDAT
          GOTO     PROC3000 IF LESS
.
          PACK     KEY22,AMDHREG,AMDHASS,AMDHDAT
          CALL     DEAMDH1
          CALL     RSAMDH1
          GOTO     PROC3000
PROC3900
.
