.----------------------------------------
. Move one to accept to for insert mode
.----------------------------------------
MVACCEPT  MOVE      ONE,ACCEPT
          RETURN
DEFAULTY    MOVE     ANSY,DEFYESNO
            RETURN
ECMOD000  MOVE      ZERO,CKEYTYP
          CALL      CCSMOPCL    * Close Model
          CALL      KCCMAA00
          BRANCH    EXIT,ECMOD999,ECMOD999
.
          CALL      CCSMOPOP    * Open Model
          IF        EXIT=1
            MOVE      "Unable to Open Cost Model Files. - ",DISPMSG
            BEEP
            CALL      MESSAGE1
            GOTO      ECMOD000
          ENDIF
ECMOD999  RETURN
RPERD000   MOVE     "    9",FLDID
           CALL     GETPOSIT
           MOVE     "64",SECTOR
           READ     CONTROLF,SECTOR;CCCOYEAR,CCCOPER
           MOVE     CCCOYEAR,CCCAYEAR
           MOVE     "01",CCCAPER
           KEYIN    *PCCOL:CVERT,*V2LON,*RV,*JR,*ZF,CCCAPER,"/":
                                 *RV,CCCAYEAR
           PACK     KEY6,CCCAYEAR,CCCAPER
           CALL     RDCCCA1
           IF       OVRCD=1
             BEEP
             MOVE     "Period Not Found - ",DISPMSG
             CALL     MESSAGE1
             GOTO     RPERD000
           ENDIF
           PACK     STRYRPR,CCCAYEAR,CCCAPER
           PACK     FROMPYR,CCCAPER,SLASH,CCCAYEAR
           GOTO     RPERD140
.
RPERD140   MOVE     FROMPYR,VAR
           CALL     DISPITEM
.
RPERD150   MOVE     "   10",FLDID
           CALL     GETPOSIT
           MOVE     CCCOPER,CCCAPER
           KEYIN    *PCCOL:CVERT,*V2LON,*RV,*JR,*ZF,CCCAPER,"/":
                                 *RV,CCCAYEAR
           PACK     KEY6,CCCAYEAR,CCCAPER
           CALL     RDCCCA1
           IF       OVRCD=1
             BEEP
             MOVE     "Period Not Found - ",DISPMSG
             CALL     MESSAGE1
             GOTO     RPERD150
           ENDIF
           PACK     ENDYRPR,CCCAYEAR,CCCAPER
           PACK     TOPYR,CCCAPER,SLASH,CCCAYEAR
           MATCH    STRYRPR,ENDYRPR
           GOTO     RPERD999 IF EQUAL
           GOTO     RPERD999 IF NOT LESS
           BEEP
           MOVE     "To Period/Year Must Be > From Period/Year - ",DISPMSG
           CALL     MESSAGE1
           GOTO     RPERD000
.
RPERD999   MOVE     "    9",FLDID
           CALL     GETPOSIT
           RETURN
.--------------------------------------------------
. Enter Directory and Validate its Existance
.--------------------------------------------------
EDIRT000  KEYIN     *PCCOL:CVERT,*V2LON,*RV,EXDIRECT
          ENDSET    EXDIRECT
          APPEND    SP70,EXDIRECT
          RESET     EXDIRECT
          MATCH     SP70,EXDIRECT
          GOTO      EDIRT999 IF EQUAL
.
          STRIP     EXDIRECT
          ENDSET    EXDIRECT
          MATCH     "/",EXDIRECT
          IF        !(@EQUAL)
            MOVE      "Directory Name Must End in a /. ",DISPMSG
            BEEP
            CALL      MESSAGE1
            GOTO      EDIRT000
          ENDIF
.
          RESET     EXDIRECT
          STRIP     EXDIRECT
          CLEAR     KEY80
          APPEND    "rm -f testfile.txt;if test -d ",KEY80
          APPEND    EXDIRECT,KEY80
          APPEND    ";then touch testfile.txt;fi",KEY80
          RESET     KEY80
          EXECUTE   KEY80,TASKID
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      COSTFILE,"testfile"
          TRAPCLR   IO
          IF        OVRCD=1
            BEEP
            MOVE      "Directory Does Not Exist.",DISPMSG
            CALL      MESSAGE1
            GOTO      EDIRT000
          ENDIF
.
EDIRT999  RETURN
.
.------------------------------------------------------------
. Enter Format
.------------------------------------------------------------
EDFOR000  MOVE      SP70,DISPMSG
          CALL      MESSAGE2
          DISPLAY   *P1:24,*EL,"Date Format : ":
                    *V2LON,"1",*HOFF,". YYYYMMDD, ":
                    *V2LON,"2",*HOFF,". DD/MM/YYYY"
EDFOR100  MOVE      "1",DATEFORM
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,DATEFORM
          BRANCH    DATEFORM,EDFOR110,EDFOR120
          BEEP
          GOTO      EDFOR100
.
EDFOR110  MOVE      "YYYYMMDD",DATEDESC
          GOTO      EDFOR999
.
EDFOR120  MOVE      "DD/MM/YYYY",DATEDESC
          GOTO      EDFOR999
.
EDFOR999  CALL      MESSAGE3
          RETURN
.
.----------------------------------------
. Export Departmental Database
.----------------------------------------
EXDPT000  DISPLAY  *P1:10,*EF
          IF       EXPCOST=1
            CALL     OPNCTY00 * Open Extract Files
            BRANCH   EXIT,EXDPT999
            MOVE     COSTNAME,KEY80
            STRIP    KEY80
            ENDSET   KEY80
            APPEND   ".txt",KEY80
            RESET    KEY80
            DISPLAY  *P1:15,*EL,"Extracting Department Cost Analysis File"
            DISPLAY  *P1:16,*EL,*V2LON,KEY80
            CALL     EXCTY000 * Extract Period Details by Cost Type
          ENDIF
.
          IF       EXPPROC=1
            CALL     OPNPCD00 * Open Extract Files
            BRANCH   EXIT,EXDPT999
            MOVE     PROCNAME,KEY80
            STRIP    KEY80
            ENDSET   KEY80
            APPEND   ".txt",KEY80
            RESET    KEY80
            DISPLAY  *P1:19,*EL,"Extracting Department Pat. Serv. Analysis File"
            DISPLAY  *P1:20,*EL,*V2LON,KEY80
            CALL     EXPCD000 * Extract Period Details by Patient Service
          ENDIF
.
          CALL    TRAN0000  * Transfer Files Now
.
EXDPT999  RETURN
.--------------------------------------------------------------------------
. Execute Transfer Script eg softerm
.--------------------------------------------------------------------------
TRAN0000  DISPLAY  *P1:24,*EL,"Execute Transfer Now (":
                              *V2LON,"Y",*HOFF,"/":
                              *V2LON,"N",*HOFF,") ? "
          MOVE     "24",CVERT
          MOVE     "30",CCOL
          CALL     YORN0000
          COMPARE  ZERO,FYESNO,
          GOTO     TRAN9999 IF EQUAL   *  No Transfer
          KEYIN    *P1:24,*EL,"Transfer to Execute : ",KEY40
          ENDSET   KEY40
          APPEND   SP70,KEY40
          RESET    KEY40
          MATCH    SP70,KEY40
          GOTO     TRAN0000 IF EQUAL
          DISPLAY  *P67:24,*EL,"Ok (",*V2LON,"Y",*HOFF,"/":
                              *V2LON,"N",*HOFF,") ? "
          MOVE     "24",CVERT
          MOVE     "78",CCOL
          CALL     YORN0000
          COMPARE  ZERO,FYESNO,
          GOTO     TRAN0000 IF EQUAL   *  Not ok
.
          MOVE     ZERO,HLEF
          CALL     GETSCR00
          STRIP    KEY40
          EXECUTE  KEY40,TASKID
.
          CALL     PUTSCR00
.
          DISPLAY  *P1:24,*EL,"Transfer Ok (":
                              *V2LON,"Y",*HOFF,"/":
                              *V2LON,"N",*HOFF,") ? "
          MOVE     "24",CVERT
          MOVE     "21",CCOL
          CALL     YORN0000
          COMPARE  ZERO,FYESNO,
          GOTO     TRAN0000 IF EQUAL   *  Not ok
.
          DISPLAY  *P1:24,*EL,"Delete Files (":
                              *V2LON,"Y",*HOFF,"/":
                              *V2LON,"N",*HOFF,") ? "
          MOVE     "24",CVERT
          MOVE     "22",CCOL
          CALL     YORN0000
          COMPARE  ZERO,FYESNO,
          GOTO     TRAN9999 IF EQUAL   *  Not ok
          IF       EXPCOST=1
            PREP     COSTFILE,COSTNAME
            CLOSE    COSTFILE,DELETE
          ENDIF
          IF       EXPPROC=1
            PREP     PROCFILE,PROCNAME
            CLOSE    PROCFILE,DELETE
          ENDIF
.
TRAN9999  RETURN
.------------------------------------------------------------
. Extract Period Details by Cost Type
.------------------------------------------------------------
EXCTY000  PACK     KEY4,SP70
          CALL     RSCCCT1
EXCTY100  CALL     RKCCCT1
          BRANCH   OVRCD,EXCTY999
          PACK     KEY6,STRYRPR
          CALL     RDCCCA1
          IF       OVRCD=0
            CALL     RPCCCA1
          ENDIF
EXCTY200  CALL     RKCCCA1
          BRANCH   OVRCD,EXCTY100
          PACK     KEY6,CCCAYEAR,CCCAPER
          MATCH    KEY6,ENDYRPR
          GOTO     EXCTY100 IF LESS
.
          PACK     KEY20,CCDPHCD,CCDPDPT,CCCAYEAR,CCCAPER,CCCTCTY
          CALL     RDCCDC1
          IF       OVRCD=1
            MOVE     ZERO,CCDCDIR
            MOVE     ZERO,CCDCIND
          ENDIF
.
          PACK     KEY20,CCDPHCD,CCDPDPT,CCCAYEAR,CCCAPER,CCCTCTY
          CALL     RDCCMB1
          IF       OVRCD=1
            MOVE     ZERO,CCMBDIR
            MOVE     ZERO,CCMBIND
          ENDIF
.
          ADD      CCDCIND,CCDCDIR  * Actual
          ADD      CCMBIND,CCMBDIR  * Budget
          IF       !(CCMBDIR+CCDCIND=0)
            UNPACK   CCCATDT,CCENT,CYEAR,CMON,CDAY
            CALL     PACDATE
            IF       DATEFORM=1
              PACK     KEY10,CCCATDT,SP70
              STRIP    KEY10
            ELSE
              MOVE     CPCDATE,KEY10
            ENDIF
            WRITE    COSTFILE,SEQ;QUOTE,CCCTCTY,QUOTE,COMMA:
                                  QUOTE,CCCTDES,QUOTE,COMMA:
                                  QUOTE,CCCTFIX,QUOTE,COMMA:
                                  QUOTE,CCCTLAB,QUOTE,COMMA:
                                  QUOTE,CCCTIND,QUOTE,COMMA:
                                  KEY10,COMMA:
                                  CCDCDIR,COMMA:
                                  CCMBDIR,COMMA
          ENDIF
          GOTO     EXCTY200
EXCTY999  RETURN
.------------------------------------------------------------
. Open Extract Files
.------------------------------------------------------------
OPNCTY00  STRIP    EXDIRECT
          CLEAR    COSTNAME
          APPEND   EXDIRECT,COSTNAME
          APPEND   "c",COSTNAME
          REP      "/0",CCDPDPT
          REP      "\0",CCDPDPT
          REP      "$0",CCDPDPT
          REP      ".0",CCDPDPT
          REP      "*0",CCDPDPT
          REP      "%0",CCDPDPT
          APPEND   CCDPHCD,COSTNAME
          APPEND   CCDPDPT,COSTNAME
          RESET    COSTNAME
          SQUEEZE  COSTNAME
          MOVE     ZERO,OVRCD
          TRAP     OVERCOND IF IO
          PREP     COSTFILE,COSTNAME
          TRAPCLR  IO
          BRANCH   OVRCD,OPNCTY90
          MOVE     ZERO,EXIT
          GOTO     OPNCTY99
.
OPNCTY90  BEEP
          MOVE     "Unable to Open Export Files. - ",DISPMSG
          CALL     MESSAGE1
          MOVE     ONE,EXIT
          GOTO     OPNCTY99
.
OPNCTY99  RETURN
.------------------------------------------------------------
. Open Extract Files
.------------------------------------------------------------
OPNPCD00  STRIP    EXDIRECT
          CLEAR    PROCNAME
          APPEND   EXDIRECT,PROCNAME
          APPEND   "p",PROCNAME
          REP      "/0",CCDPDPT
          REP      "\0",CCDPDPT
          REP      "$0",CCDPDPT
          REP      ".0",CCDPDPT
          REP      "*0",CCDPDPT
          REP      "%0",CCDPDPT
          APPEND   CCDPHCD,PROCNAME
          APPEND   CCDPDPT,PROCNAME
          RESET    PROCNAME
          SQUEEZE  PROCNAME
          MOVE     ZERO,OVRCD
          TRAP     OVERCOND IF IO
          PREP     PROCFILE,PROCNAME
          TRAPCLR  IO
          BRANCH   OVRCD,OPNPCD90
          MOVE     ZERO,EXIT
          GOTO     OPNPCD99
.
OPNPCD90  BEEP
          MOVE     "Unable to Open Export Files. - ",DISPMSG
          CALL     MESSAGE1
          MOVE     ONE,EXIT
          GOTO     OPNPCD99
.
OPNPCD99  RETURN
.------------------------------------------------------------
. Extract Period Details by Patient Service Code
.------------------------------------------------------------
EXPCD000  PACK     KEY18,CCDPHCD,CCDPDPT,SP70
          CALL     RSCCPD1
EXPCD100  CALL     RKCCPD1
          BRANCH   OVRCD,EXPCD400
.
          PACK     KEY6,STRYRPR,SP70
          CALL     RDCCCA1
          IF       OVRCD=0
            CALL     RPCCCA1
          ENDIF
EXPCD200  CALL     RKCCCA1
          BRANCH   OVRCD,EXPCD100
          PACK     KEY6,CCCAYEAR,CCCAPER
          MATCH    KEY6,ENDYRPR
          GOTO     EXPCD100 IF LESS
.
          PACK     KEY24,CCDPHCD,CCDPDPT,CCCAYEAR,CCCAPER,CCPDPCD
          CALL     RDCCPS1
          IF       OVRCD=1
            MOVE     ZERO,CCPSQTY
            MOVE     ZERO,CCPSCST
          ENDIF
.
          PACK     KEY24,CCDPHCD,CCDPDPT,CCCAYEAR,CCCAPER,CCPDPCD
          CALL     RDCCME1
          IF       OVRCD=1
            MOVE     ZERO,CCMEQTY
            MOVE     ZERO,CCMECST
          ENDIF
.
          IF       (CCMEQTY+CCPSQTY=0)
            GOTO     EXPCD200
          ENDIF
.
          CALL     CALFR000     * Determine Generic/Average Cost
          CALL     CALTO000     * Determine Generic/Average Cost
.
          UNPACK   CCCATDT,CCENT,CYEAR,CMON,CDAY
          CALL     PACDATE
          IF       DATEFORM=1
            PACK     KEY10,CCCATDT,SP70
            STRIP    KEY10
          ELSE
            MOVE     CPCDATE,KEY10
          ENDIF
          WRITE    PROCFILE,SEQ;QUOTE,CCPDPCD,QUOTE,COMMA:
                                QUOTE,CCPDDES,QUOTE,COMMA:
                                      KEY10,COMMA:
                                      CCPSQTY,COMMA:
                                      CCMEQTY,COMMA:
                                      CCPSCST,COMMA:
                                      CCMECST,COMMA:
                                      COST1AMT,COMMA:
                                      COST2AMT
          GOTO     EXPCD200
.
EXPCD400
EXPCD999  RETURN
.
.------------------------------------------------------------
. Determine Actual Unit Cost Based on Annual Average
.------------------------------------------------------------
CALFR000  PACK      KEY18,CCCAYEAR,SP4,CCPDHCD,CCPDPCD
          CALL      RDCCMS1
          IF        OVRCD=1
            PACK      KEY26,CCCAYEAR,SP4,CCPDHCD,CCPDDPT,CCPDPCD
            CALL      RDCCMT1
            IF        OVRCD=1
              MOVE      ZERO,CCMSUCT
              MOVE      ZERO,CCMSFIX
              MOVE      ZERO,CCMSFUC
            ELSE
              MOVE      CCMTUCT,CCMSUCT
              MOVE      CCMTFIX,CCMSFIX
              MOVE      CCMTFUC,CCMSFUC
            ENDIF
          ENDIF
          IF        CCMSFIX=1
            ASSIGN    CCMSFUC,COST1AMT
          ELSE
            ASSIGN    CCMSUCT,COST1AMT
          ENDIF
          RETURN
.
.------------------------------------------------------------
. Determine Model Unit Cost Based on Annual Average
.------------------------------------------------------------
CALTO000  PACK      KEY18,CCCAYEAR,CCMACOM,CCPDHCD,CCPDPCD
          CALL      RDCCMS1
          IF        OVRCD=1
            PACK      KEY26,CCCAYEAR,CCMACOM,CCPDHCD,CCPDDPT,CCPDPCD
            CALL      RDCCMT1
            IF        OVRCD=1
              MOVE      ZERO,CCMSUCT
              MOVE      ZERO,CCMSFIX
              MOVE      ZERO,CCMSFUC
            ELSE
              MOVE      CCMTUCT,CCMSUCT
              MOVE      CCMTFIX,CCMSFIX
              MOVE      CCMTFUC,CCMSFUC
            ENDIF
          ENDIF
          IF        CCMSFIX=1
            ASSIGN    CCMSFUC,COST2AMT
          ELSE
            ASSIGN    CCMSUCT,COST2AMT
          ENDIF
          RETURN
