.------------------------------------------------------------
. Add Field
.------------------------------------------------------------
VPRF0000  MOVE      ZERO,F2
          PACK      MTHNAM3,SP70
          BRANCH    FLDITMNO,VPRF0100,VPRF0200
          GOTO      VPRF9999
.
VPRF0100  PACK      KEY9,EMVPCODE
          CALL      RDEMPRO1 
          UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      ZERO,F1
          MOVE      KEY1,F1
          BRANCH    F1,VPRF0110           * 1= code,(2=selection),0=description
.
          WRITE     HTMLFILE;EMPRDESC 
          GOTO      VPRF9999
.
VPRF0110  WRITE     HTMLFILE;EMPRCODE
          GOTO      VPRF9999
.
.VRPF0120  CALL      PROSEL00
.          GOTO      VPRF9999
.
VPRF0200  MOVE      "Yes",EXTRACT
          MATCH     EMVPSEND,DIMN
          IF        @EQUAL
            MOVE      "No",EXTRACT
          ENDIF
.
          WRITE     HTMLFILE;EXTRACT
          GOTO      VDGF9999
.
VPRF9999  RETURN
.------------------------------------------------------------
. Add Selection Options to form for proc
.------------------------------------------------------------
PROSEL00  PACK      KEY9,SP70
          CALL      RSEMPRO1
PROSEL10  CALL      RSEMPRO2
          BRANCH    OVRCD,PROSEL99
          MATCH     SP70,EMPRCODE
          GOTO      PROSEL10 IF EQUAL
          PACK      KEY15,QUOTE,EMPRCODE,QUOTE,SP70
          MATCH     EMVPCODE,EMPRCODE
          IF        @EQUAL
            WRITE     HTMLFILE;"<option value=",KEY15," selected>":
                               TDESC,"</option>"
          ELSE
            WRITE     HTMLFILE;"<option value=",KEY15,">",TDESC,"</option>"
          ENDIF
          GOTO      PROSEL10
PROSEL99  RETURN
.
