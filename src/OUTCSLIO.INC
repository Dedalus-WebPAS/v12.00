.        INCLUDE NAME : OUTCSLIO.INC
.
.        FILE I/O REQUESTS FOR OUTPATIENT CANCELLED SESSION LOG FILE
.
.OVRCD   FORM     1        0 = NOT OVER
.                          1 = OVER
.KEY27   DIM     27        KEY FOR OUTCLSA1, OUTCLSA2
.
.        I/O routines for the 1st index
.
RAOTCSL1 MOVE     ZERO,OVRCD
         RESET    KEY27
         READ     OUTCSLA1,KEY27;ANS
         GOTO     OVERCOND IF OVER
         RETURN
.
RSOTCSL1 RESET    KEY27
         READ     OUTCSLA1,KEY27;;
         RETURN
.
RKOTCSL1 MOVE     ZERO,OVRCD
         READKS   OUTCSLA1;OTCSSITE,OTCSCLIN,OTCSDATE,OTCSSTRT:
                           DOTCSUNI,OTCSCDTE,OTCSCTIM,OTCSCTYP:
                           OTCSREAS,OTCSSPAR
         GOTO     OVERCOND IF OVER
         MOVE     DOTCSUNI,OTCSUNIQ
.
         RETURN
.
RPOTCSL1 MOVE     ZERO,OVRCD
         READKP   OUTCSLA1;OTCSSITE,OTCSCLIN,OTCSDATE,OTCSSTRT:
                           DOTCSUNI,OTCSCDTE,OTCSCTIM,OTCSCTYP:
                           OTCSREAS,OTCSSPAR
         GOTO     OVERCOND IF OVER
         MOVE     DOTCSUNI,OTCSUNIQ
.
         RETURN
.
RDOTCSL1 MOVE     ZERO,OVRCD
         RESET    KEY27
         READ     OUTCSLA1,KEY27;OTCSSITE,OTCSCLIN,OTCSDATE,OTCSSTRT:
                           DOTCSUNI,OTCSCDTE,OTCSCTIM,OTCSCTYP:
                           OTCSREAS,OTCSSPAR
         GOTO     OVERCOND IF OVER
         MOVE     DOTCSUNI,OTCSUNIQ
.
         RETURN
.
UPOTCSL1 MOVE     OTCSUNIQ,DOTCSUNI
         UPDATE   OUTCSLA1;OTCSSITE,OTCSCLIN,OTCSDATE,OTCSSTRT:
                           DOTCSUNI,OTCSCDTE,OTCSCTIM,OTCSCTYP:
                           OTCSREAS,OTCSSPAR
         RETURN
.
DEOTCSL1 RESET    KEY27
         DELETE   OUTCSLA1,KEY27
         RETURN
.
WROTCSL1 MOVE     ZERO,OVRCD
         MOVE     OTCSUNIQ,DOTCSUNI
         RESET    KEY27
         WRITE    OUTCSLA1,KEY27;KEY27:
                           OTCSCDTE,OTCSCTIM,OTCSCTYP:
                           OTCSREAS,OTCSSPAR
         RETURN
.
.        I/O routines for the 2nd index
.
RAOTCSL2 MOVE     ZERO,OVRCD
         RESET    KEY27
         READ     OUTCSLA2,KEY27;ANS
         GOTO     OVERCOND IF OVER
         RETURN
.
RSOTCSL2 RESET    KEY27
         READ     OUTCSLA2,KEY27;;
         RETURN
.
RKOTCSL2 MOVE     ZERO,OVRCD
         READKS   OUTCSLA2;OTCSSITE,OTCSCLIN,OTCSDATE,OTCSSTRT:
                           DOTCSUNI,OTCSCDTE,OTCSCTIM,OTCSCTYP:
                           OTCSREAS,OTCSSPAR
         GOTO     OVERCOND IF OVER
         MOVE     DOTCSUNI,OTCSUNIQ
.
         RETURN
.
RPOTCSL2 MOVE     ZERO,OVRCD
         READKP   OUTCSLA2;OTCSSITE,OTCSCLIN,OTCSDATE,OTCSSTRT:
                           DOTCSUNI,OTCSCDTE,OTCSCTIM,OTCSCTYP:
                           OTCSREAS,OTCSSPAR
         GOTO     OVERCOND IF OVER
         MOVE     DOTCSUNI,OTCSUNIQ
.
         RETURN
.
RDOTCSL2 MOVE     ZERO,OVRCD
         RESET    KEY27
         READ     OUTCSLA2,KEY27;OTCSSITE,OTCSCLIN,OTCSDATE,OTCSSTRT:
                           DOTCSUNI,OTCSCDTE,OTCSCTIM,OTCSCTYP:
                           OTCSREAS,OTCSSPAR
         GOTO     OVERCOND IF OVER
         MOVE     DOTCSUNI,OTCSUNIQ
.
         RETURN
