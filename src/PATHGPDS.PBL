.******************************************************************************
.* System         :  Patient Billing                                          *
.* Program        :  PATDSHGP                                                 *
.* Description    :  Display All Health Fund Group Codes                      *
.******************************************************************************
.* Date           :  26/09/1995                                               *
.* Author         :  Junior                                                   *
.* Modifications  :                                                           *
.*                                                                            *
.******************************************************************************
.
PATDSHGP
PATDSH00  DISPLAY   *P1:3,*EF,*P30:4,*V2LON,*ULON,"Health Fund Groups";
          MOVE      "5",CVERT
          MOVE      "1",CCOL
          PACK      KEY6,SP6
          CALL      RSPAHGP1                * Position on and read the next
PATDSH10  CALL      RKPAHGP1                  HFG records
          BRANCH    OVRCD,PATDSH30
.
          DISPLAY   *PCCOL:CVERT,*V2LON,PTHGCODE,*HOFF,SP1,PTHGDESC;
          ADD       "1",CVERT
          COMPARE   "24",CVERT
          GOTO      PATDSH20 IF NOT LESS
.
          GOTO      PATDSH10
.
PATDSH20  KEYIN     *P1:24,*EL,"(",*V2LON,"E",*HOFF,")xit, (",*V2LON,"N":
                    *HOFF,")ext Screen ? ",*V2LON,ANS;
.
          REP       UPPLOW,ANS
          MATCH     ANSE,ANS
          GOTO      PATDSH30 IF EQUAL       * Exit question option
.
          MATCH     ANSN,ANS
          GOTO      PATDSH40 IF EQUAL       * Next screen
.
          BEEP
          GOTO      PATDSH20
.
. ----- Return To Keying In HFG Code -----
.
PATDSH30  IF        CNEWDS<>1
            DISPLAY   *P1:24,*EL;
            CALL      HITENTER
          ENDIF
          GOTO      PATDSH99
.
. ----- Next Screen -----
.
PATDSH40  DISPLAY   *P1:5,*EF;
          MOVE      "5",CVERT
          MOVE      "1",CCOL
          GOTO      PATDSH10
.
PATDSH99  RETURN
+
.

