          MOVE      Z70,SICIC002
          MOVE      Z70,SICIC003
          MOVE      Z70,SICIC004
          MOVE      Z70,SICIC005
          MOVE      Z70,SICIC006
          MOVE      Z70,SICIC007
          MOVE      Z70,SICIC008
          MOVE      Z70,SICIC009
          MOVE      Z70,SICIC010
          MOVE      Z70,SICIC011
          MOVE      Z70,SICIC012
          MOVE      Z70,SICIC013
          MOVE      Z70,SICIC014
          MOVE      Z70,SICIC015
          MOVE      Z70,SICIC016
          MOVE      Z70,SICIC017
          MOVE      Z70,SICIC018
          MOVE      Z70,SICIC019
          MOVE      Z70,SICIC020
          MOVE      Z70,SICIC021
          MOVE      Z70,SICIC022
