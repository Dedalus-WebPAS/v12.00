.------------------------------------------------------------
. Purchase Order header file standard tags
.------------------------------------------------------------
SIIC0000  BRANCH    FLDITMNO,SIIC0010,SIIC0020,SIIC0030,SIIC0040,SIIC0050:
                             SIIC0060,SIIC0070,SIIC0080,SIIC0090,SIIC0100:
                             SIIC0110,SIIC0120,SIIC0130,SIIC0140,SIIC0150:
                             SIIC0160,SIIC0170,SIIC0180,SIIC0190,SIIC0200:
                             SIIC0210,SIIC0220
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      SIIC9999
.
SIIC0010  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      ZERO,F1
          MOVE      KEY1,F1
.
          BRANCH    F1,SIIC0011
          WRITE     HTMLFILE;SIICCAT;
          GOTO      SIIC9999
.
SIIC0011  PACK      KEY7,SIICCAT,SP10
          CALL      RDSIIA1
          BRANCH    OVRCD,SIIC9999
.
          WRITE     HTMLFILE;SIIADES;
          GOTO      SIIC9999
.
SIIC0020  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      ZERO,F1
          MOVE      KEY1,F1
.
          BRANCH    F1,SIIC0021
          WRITE     HTMLFILE;SIICSUP;
          GOTO      SIIC9999
.
SIIC0021  PACK      KEY5,SIICSUP,SP10
          CALL      RDAPMA1
          BRANCH    OVRCD,SIIC9999
.
          WRITE     HTMLFILE;APMASN1;
          GOTO      SIIC9999
.
SIIC0030  WRITE     HTMLFILE;SIICSUT;
          GOTO      SIIC9999
.
SIIC0040  WRITE     HTMLFILE;SIICCCT;
          GOTO      SIIC9999
.
SIIC0050  WRITE     HTMLFILE;SIICLCT;
          GOTO      SIIC9999
.
SIIC0060  WRITE     HTMLFILE;SIICLTM;
          GOTO      SIIC9999
.
SIIC0070  WRITE     HTMLFILE;SIICPNO;
          GOTO      SIIC9999
.
SIIC0080  WRITE     HTMLFILE;SIICPD1;
          GOTO      SIIC9999
.
SIIC0090  WRITE     HTMLFILE;SIICPD2;
          GOTO      SIIC9999
.
SIIC0100  WRITE     HTMLFILE;SIICMAN;
          GOTO      SIIC9999
.
SIIC0110  WRITE     HTMLFILE;SIICAPO;
          GOTO      SIIC9999
.
SIIC0120  WRITE     HTMLFILE;SIICUR1;
          GOTO      SIIC9999
.
SIIC0130  WRITE     HTMLFILE;SIICUR2;
          GOTO      SIIC9999
.
SIIC0140  MATCH     SIICUD1,SP70
          GOTO      SIIC9999 IF EQUAL
.         
          UNPACK    SIICUD1,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      SIIC9999
.
SIIC0150  MATCH     SIICUD2,SP70
          GOTO      SIIC9999 IF EQUAL
.         
          UNPACK    SIICUD2,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      SIIC9999
.
SIIC0160  WRITE     HTMLFILE;SIICUC1;
          GOTO      SIIC9999
.
SIIC0170  WRITE     HTMLFILE;SIICUC2;
          GOTO      SIIC9999
.
SIIC0180  WRITE     HTMLFILE;SIICUY1;
          GOTO      SIIC9999
.
SIIC0190  WRITE     HTMLFILE;SIICUY2;
          GOTO      SIIC9999
.
SIIC0200  WRITE     HTMLFILE;SIICUOM;
          GOTO      SIIC9999
.
SIIC0210  WRITE     HTMLFILE;SIICCONT;
          GOTO      SIIC9999
.
SIIC0220  MATCH     SIICCTEX,SP70
          GOTO      SIIC9999 IF EQUAL
.
          UNPACK    SIICCTEX,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      SIIC9999
.
SIIC9999  RETURN
.
.------------------------------------------------------------
.   Set Parameters for the Purchase Order header file
.------------------------------------------------------------
SPSIC000  MOVE      ZERO,EXIT
          UNPACK    QRYNAME,KEY5,KEY3
          MOVE      KEY3,F3
          BRANCH    F3,SPSIC010,SPSIC020,SPSIC030,SPSIC040,SPSIC050:
                       SPSIC060,SPSIC070,SPSIC080,SPSIC090,SPSIC100:
                       SPSIC110,SPSIC120,SPSIC130,SPSIC140,SPSIC150:
                       SPSIC160,SPSIC170,SPSIC180,SPSIC190,SPSIC200:
                       SPSIC210,SPSIC220
.
          MOVE      ONE,EXIT
          GOTO      SPSIC999
.
SPSIC010  MOVE      QRYDATA,SICIC001
          GOTO      SPSIC999
.
SPSIC020  MOVE      QRYDATA,SICIC002
          GOTO      SPSIC999
.
SPSIC030  MOVE      QRYDATA,SICIC003
          GOTO      SPSIC999
.
SPSIC040  MOVE      QRYDATA,SICIC004
          GOTO      SPSIC999
.
SPSIC050  MOVE      QRYDATA,SICIC005
          GOTO      SPSIC999
.
SPSIC060  MOVE      QRYDATA,SICIC006
          GOTO      SPSIC999
.
SPSIC070  MOVE      QRYDATA,SICIC007
          GOTO      SPSIC999
.
SPSIC080  MOVE      QRYDATA,SICIC008
          GOTO      SPSIC999
.
SPSIC090  MOVE      QRYDATA,SICIC009
          GOTO      SPSIC999
.
SPSIC100  MOVE      QRYDATA,SICIC010
          GOTO      SPSIC999
.
SPSIC110  MOVE      QRYDATA,SICIC011
          GOTO      SPSIC999
.
SPSIC120  MOVE      QRYDATA,SICIC012
          GOTO      SPSIC999
.
SPSIC130  MOVE      QRYDATA,SICIC013
          GOTO      SPSIC999
.
SPSIC140  UNPACK    QRYDATA,CDAY,KEY1,MTHNAM3,KEY1,CCENT,CYEAR
          CALL      SETMTH00                *Set CMON from MTHNAM3
          PACK      SICIC014,CCENT,CYEAR,CMON,CDAY
          GOTO      SPSIC999
.
SPSIC150  UNPACK    QRYDATA,CDAY,KEY1,MTHNAM3,KEY1,CCENT,CYEAR
          CALL      SETMTH00                *Set CMON from MTHNAM3
          PACK      SICIC015,CCENT,CYEAR,CMON,CDAY
          GOTO      SPSIC999
.
SPSIC160  MOVE      QRYDATA,SICIC016
          GOTO      SPSIC999
.
SPSIC170  MOVE      QRYDATA,SICIC017
          GOTO      SPSIC999
.
SPSIC180  MOVE      QRYDATA,SICIC018
          GOTO      SPSIC999
.
SPSIC190  MOVE      QRYDATA,SICIC019
          GOTO      SPSIC999
.
SPSIC200  MOVE      QRYDATA,SICIC020
          GOTO      SPSIC999
.
SPSIC210  MOVE      QRYDATA,SICIC021
          GOTO      SPSIC999
.
SPSIC220  UNPACK    QRYDATA,CDAY,KEY1,MTHNAM3,KEY1,CCENT,CYEAR
          CALL      SETMTH00                *Set CMON from MTHNAM3
          PACK      SICIC022,CCENT,CYEAR,CMON,CDAY
          GOTO      SPSIC999
.
SPSIC999  RETURN
.
