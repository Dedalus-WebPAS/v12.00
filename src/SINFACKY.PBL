.
          MOVE      CCITEM,SCCITEM
          CALL      HSIFAB00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    SIFASUPP
          APPEND    SP70,SIFASUPP
          RESET     SIFASUPP
.
          MATCH     SP70,SIFASUPP
          GOTO      KSIFAB00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,SIFASUPP                                
KSIFAB10
          PACK      KEY30,SIFAISSU,SIFASUPP                                     
          CALL      RASIFA1 
          BRANCH    OVRCD,KSIFAB80
          BRANCH    CKEYTYP,KSIFAB75
          CALL      RDSIFA1 
KSIFAB70
          MOVE      ZERO,EXIT
          GOTO      KSIFAB99
KSIFAB75
          MOVE      "Supply Unit Already Exist - ",DISPMSG                                           
          CALL      MESSAGE1
