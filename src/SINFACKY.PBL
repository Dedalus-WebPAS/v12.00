KSIFAB00 TOPIC     "SIFASUPP"
          MATCH     SIFASUPP,SP70
          IF        @EQUAL
            MOVE      UNDLN70,SIFASUPP
            KEYIN     *PCCOL:CVERT,*DV,SIFASUPP:
                      *PCCOL:CVERT,*V2LON,SIFASUPP:
                      *PCCOL:CVERT,*DV,SIFASUPP
          ELSE
            KEYIN     *PCCOL:CVERT,*V2LON,*RV,SIFASUPP:
                      *PCCOL:CVERT,*DV,SIFASUPP
          ENDIF
.
          ENDSET    SIFASUPP
          APPEND    SP70,SIFASUPP
          RESET     SIFASUPP
.
          MATCH     SP70,SIFASUPP
          GOTO      KSIFAB90 IF EQUAL
.
          CMATCH    EXITCHAR,SIFASUPP
          GOTO      KSIFAB95 IF EQUAL
.
          CMATCH    QUEST,SIFASUPP
