.----------------------------------------------------------------------
. Check cost centre security
. Returns SICSSEC : 0 = no access to any cost centres
.                   1 = access to one cost centre
.                   2 = access to more than one cost centre
.                   3 = access to all cost centres
.----------------------------------------------------------------------
CSICS000
          MOVE      "3",SICSSEC             * check for all cc's
          PACK      KEY9,PASSCODE,SP70
          CALL      RDSICS1
          IF        OVRCD=1
            MOVE      "0",SICSSEC           * check for no cc's
            CALL      RKSICS1
            BRANCH    OVRCD,CSICS999
            MATCH     SICSUID,PASSCODE
            GOTO      CSICS999 IF NOT EQUAL
.
            PACK      KEY5,SICSCST,SP70     * read data for first cc
            CALL      RDSICA1
            MOVE      "1",SICSSEC
            CALL      RKSICS1               * check for more than one cc
            BRANCH    OVRCD,CSICS999
            MATCH     SICSUID,PASSCODE
            GOTO      CSICS999 IF NOT EQUAL
.
            MOVE      "2",SICSSEC
          ENDIF
.
CSICS999  RETURN
.----------------------------------------------------------------------
. Keyin Req No.
.----------------------------------------------------------------------
KREQN000
          CALL      UUSIQA1
          CALL      KSIQAA00                     * keyin Req No.
          BRANCH    EXIT,KREQN950,KREQN950
.
          PACK      KEY13,SIQACST,SIQARID,SP70   * lock requisition
          REPEAT
            CALL      RLSIQA1
            BRANCH    OVRCD,KREQN000             * should be there !!
            IF        OVRCD=2
              PACK      DISPMSG,SP70,SP70
              CALL      MESSAGE2
              KEYIN     *P1:24,*EL,"Requisition Locked - (":
                        *V2LON,"R",*HOFF,")etry or e(":
                        *V2LON,"X",*HOFF,")it ? ",*V2LON,ANS
              CALL      MESSAGE3
              PACK      ANS,ANS,SP70
              REP       UPPLOW,ANS
              MATCH     "X",ANS
              GOTO      KREQN000 IF EQUAL
              MATCH     "E",ANS
              GOTO      KREQN000 IF EQUAL
            ENDIF
          UNTIL     OVRCD=0
.
.
          MOVE      SP70,SIIADES                 * get catalog desc
          PACK      KEY7,SIQACAT,SP70
          CALL      RDSIIA1
.
          MOVE      SP70,SICDDESC                * get status desc
          MOVE      "PRC",KEY3
          PACK      KEY6,KEY3,SIQASTA,SP70
          MATCH     SP70,SIQASTA
          CALL      RDSICD1 IF NOT EQUAL
.
          MOVE      ZERO,EXIT
          GOTO      KREQN999
.
KREQN950  MOVE      ONE,EXIT
          LOAD      KEEPCC,SICSSEC,TWO           * quit if access to only 1 cc
.
KREQN999  RETURN
