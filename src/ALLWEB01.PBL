          BRANCH    OVRCD,EQTSEL10
.
          MATCH     KEY3,ALEQDEPT
          GOTO      EQTSEL10 IF NOT EQUAL
.
          MATCH     ACODE,ALEQTYPE
          GOTO      EQTSEL20 IF NOT EQUAL
.
          WRITE     HTMLFILE;"<option value=",ACODE;
.
          MATCH     EQTPFILT,ACODE
          IF        @EQUAL
            WRITE     HTMLFILE;" selected ";
