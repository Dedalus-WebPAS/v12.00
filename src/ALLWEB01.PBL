          ENDSET    WEBTITLE
          APPEND    "first!",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ALLDEP99
.
ALLDEP97  MOVE      "Service Records still exist please delete first!",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ALLDEP99
.
ALLDEP98  MOVE      "Template Records still exist please delete first!",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ALLDEP99
.
