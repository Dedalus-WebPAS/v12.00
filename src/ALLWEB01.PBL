          BRANCH    EXIT,VALITM99
.
          MOVE      ZERO,EFDTFLAG
          MATCH     SP70,VALDTYPE
          IF        @EQUAL
            MOVE      " 0",VALDTYPE
            GOTO      VALITM20
          ENDIF
.
          MATCH     "MB",VALDTYPE
          IF        @EQUAL
            MOVE      "M ",VALDTYPE              * allow 'MB' as well as 'M '
          ENDIF
.
          MATCH     "M ",VALDTYPE
          IF        @EQUAL
            MOVE      " 0",VALDTYPE
          ELSE
            MOVE      " 1",VALDTYPE
          ENDIF
.
