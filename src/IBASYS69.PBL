HEAD3000
          PRINT     *20,"Port ",FROMCODE," to ",TOCODE:
                    *N,*20,"Date ",FROMDDTE," to ",TODDTE:
                    *N:
                    *N,*1,ASK,UNDLN77,ASK:
                    *N,*1,PIPE,SP1,"Port":
                      *8,PIPE,SP4,"Date":
