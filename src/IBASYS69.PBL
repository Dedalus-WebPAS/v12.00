          MOVE        TEN5,VERT
.
          DISPLAY    *P1:VERT,*EF,"Report User To   : ",UNDLN4;
.
          MOVE        TWO,RDISOPTN
          MOVE        TWO,FLAGUSER
          CALL        USER0000                   * Input User Code
.
          BRANCH      EXIT,USR0000
.
          MOVE        USER,TOUSER
.
          MATCH       SP4,USER
          GOTO        USR2000 IF EQUAL
