.**********************************************************************
.*                           INIT0000                                 *
.**********************************************************************
INIT0000  CALL      DISPHEAD
.
          DISPLAY   *P64:24,"Opening"
.
          DISPLAY   *P72:24,"ihapassf";
          OPEN      IHAPASS1,"ihapassf"
.
          DISPLAY   *P72:24,"ibaimenf";
          OPEN      IBAIMEN1,"ibaimenf"
          OPEN      IBAIMEN2,"ibaimenf"
