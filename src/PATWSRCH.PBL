          MATCH     SRCHGNAM,SP70
          IF        @EQUAL
            CLEAR     SRCHGNAM
            APPEND    SP70,SRCHGNAM
            RESET     SRCHGNAM
            LENSET    SRCHGNAM,1
          ELSE
            STRIP     SRCHGNAM
          ENDIF
.
          PACK      GSRSNAM,SRCHSNAM,SP70
          PACK      GSRGNAM,SRCHGNAM,SP70
