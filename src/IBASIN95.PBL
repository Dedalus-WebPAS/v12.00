.********************************************************************
. PRNT - print report                               Called by ML
.         Requires : FROMLAB,TOLAB   (print range display labels)
.                    FROMCODE,TOCODE (print range)
.                    OPTIONA         (report type)
. Requires IBACODFD/INC IBAPRNFD/INC
.          IBAIOCOD/INC IBAIOPRN/INC IBAPRINT
.  Note : already includes IBAPASFD & IBAPASIO
.********************************************************************
PRNT0000
          IF        OPTIONA=1
            MATCH     FROMCODE,TOCODE         * check if range ok
            IF        @LESS
              CLEAR     DISPMSG
              APPEND    "Illegal Range Entered - ",DISPMSG
              CALL      MESSAGE1
              GOTO      PRNT9500
            ENDIF
          ELSE
            MATCH     PDESC1,PDESC2           * check if range ok
            IF        @LESS
              CLEAR     DISPMSG
              APPEND    "Illegal Range Entered - ",DISPMSG
              CALL      MESSAGE1
              GOTO      PRNT9500
            ENDIF
          ENDIF
.
          PACK      DISPMSG,SP70,SP70       * save line 24
          CALL      MESSAGE2
          CALL      OPNPRINT
          CALL      RDLAB000                     * read label parameters
          CALL      FILE0000                    * go to correct position in file
.
          DISPLAY   *P1:24,"Printing :   ";
.
PRNT1000  CALL      NREC0000                     * get next record
          BRANCH    OVRCD,PRNT9000               * end of file ?
.
          IF        OPTIONA=1
            DISPLAY   *P12:24,*V2LON,SICACODE,*EL;
          ELSE
            DISPLAY   *P12:24,*V2LON,SICADESC,*EL;
          ENDIF
.
          CALL      WRIT0000                     * print details
          GOTO      PRNT1000                     * get next record
.
PRNT9000
          CALL      PREND000                     * read label parameters
          CALL      CLSPRINT
          CALL      MESSAGE3                * restore line 24
          MOVE      ZERO,EXIT
          GOTO      PRNT9999
.
PRNT9500  MOVE      ONE,EXIT
.
PRNT9999  RETURN
.**********************************************************************
.  FILE - Set file pointer                             Called By PRNT
.**********************************************************************
FILE0000  BRANCH    OPTIONA,FILE0100,FILE0200
.
.---- print by type 1 ----
.
FILE0100  PACK      KEY5,FROMCODE,SP70
          CALL      RSSICA1
          CALL      RPSICA1
          GOTO      FILE9999
.
.---- print by type 2 ----
.
FILE0200  PACK      KEY45,PDESC1,SP70
          CALL      RSSICA2
          CALL      RPSICA2
          GOTO      FILE9999
.
FILE9999  RETURN
.**********************************************************************
.  NREC - Get next record                              Called By PRNT
.**********************************************************************
NREC0000  BRANCH    OPTIONA,NREC0100,NREC0200
.
.---- print by type 1 ----
.
NREC0100  CALL      RKSICA1
          BRANCH    OVRCD,NREC9500
.
          MATCH     SICACODE,TOCODE
          GOTO      NREC9500 IF LESS             * out of range ?
          GOTO      NREC9000
.
.---- print by type 2 ----
.
NREC0200  CALL      RKSICA2
          BRANCH    OVRCD,NREC9500
.
          MATCH     SICADESC,PDESC2
          GOTO      NREC9500 IF LESS             * out of range ?
          GOTO      NREC9000
.
NREC9000  MOVE      ZERO,OVRCD         * continue
          GOTO      NREC9999
.
NREC9500  MOVE      ONE,OVRCD          * quit
.
NREC9999  RETURN
.********************************************************************
. WRIT - print data line                            Called by PRNT
.********************************************************************
WRIT0000
          MOVE      "COST CENTRE : ",KEY14
          PACK      LLINE1,KEY14,SICACODE,SP70
.
          PACK      KEY60,SICADESC,SP70
          CALL      BKLIN000
          PACK      LLINE2,KEY27,SP70
          CALL      BKLIN000
          PACK      LLINE3,KEY27,SP70
          PACK      LLINE4,KEY60,SP70
.
          PACK      LLINE5,SP70
.
          MOVE      "CONTACT : ",KEY14
          PACK      KEY60,KEY14,SICACONT,SP70
          CALL      BKLIN000
          PACK      LLINE6,KEY27,SP70
          CALL      BKLIN000
          PACK      LLINE7,KEY27,SP70
          PACK      LLINE8,KEY60,SP70
.
          CALL      STLAB000           * store and print labels
.
WRIT9999  RETURN
.----------------------------------------------------------------------
. Read Label Parameters
.----------------------------------------------------------------------
RDLAB000
          READ      CONTROLF,TWENTY3;*188,HLABPG,HLABPRT:
                                     *238,HCOSLAB
          IF        2=HCOSLAB
            MOVE      "43",TABPOS2
            MOVE      "85",TABPOS3
          ELSE
            MOVE      "37",TABPOS2
            MOVE      "73",TABPOS3
          ENDIF
          MOVE        ONE,LENIND
          MOVE        ZERO,LABNUM
          UNPACK      SP70,ALINEA,ALINEB
          UNPACK      SP70,ALINE1,ALINE2
          UNPACK      SP70,ALINE3,ALINE4
          UNPACK      SP70,ALINE5,ALINE6
          UNPACK      SP70,ALINE7,ALINE8
          UNPACK      SP70,BLINEA,BLINEB
          UNPACK      SP70,BLINE1,BLINE2
          UNPACK      SP70,BLINE3,BLINE4
          UNPACK      SP70,BLINE5,BLINE6
          UNPACK      SP70,BLINE7,BLINE8
          UNPACK      SP70,CLINEA,CLINEB
          UNPACK      SP70,CLINE1,CLINE2
          UNPACK      SP70,CLINE3,CLINE4
          UNPACK      SP70,CLINE5,CLINE6
          UNPACK      SP70,CLINE7,CLINE8
.
RDLAB999  RETURN
.********************************************************************
. BKLIN - Break up KEY60 into two neat halves (first being 27 char long)
.         Requires - KEY60   (original line)
.         Returns  - KEY27   (line 1)
.                    KEY60   (line 2)
.********************************************************************
BKLIN000  RESET     KEY60,28
          CMATCH    SP70,KEY60
          WHILE     !@EQUAL&!@EOS
            BUMP      KEY60,-1
            IF        !@EOS
              CMATCH    SP70,KEY60
            ENDIF
          DO
.
          IF        @EOS
            SETLPTR   KEY60
            RESET     KEY60
            UNPACK    KEY60,KEY27,KEY60
          ELSE
            LENSET    KEY60
            RESET     KEY60
            MOVE      KEY60,KEY27
            PACK      KEY27,KEY27,SP70
.
            ENDSET    KEY60
            SETLPTR   KEY60
            BUMP      KEY60
            PACK      KEY60,KEY60,SP70
          ENDIF
.
BKLIN999  RETURN
.----------------------------------------------------------------------
. Keyin From Code
.----------------------------------------------------------------------
KCODF000
          IF        OPTIONA=1
            MOVE      SP70,FROMLAB
            DISPLAY   *PCCOL:CVERT,FROMLAB;
.
            CALL      KSICAA00
            PACK      FROMCODE,SICACODE,SP70
.
            PACK      DESC1,SICADESC,SP70
            LOAD      DESC1,EXIT,SP70,SP70
.
            PACK      FROMLAB,SICACODE,SP70
            LOAD      FROMLAB,EXIT,START,SP70
          ELSE
            MOVE      ZERO,EXIT
            MOVE      SP70,DESC1
            KEYIN     *PCCOL:CVERT,*DV,DESC1:
                      *PCCOL:CVERT,*V2LON,DESC1;
            PACK      PDESC1,DESC1,SP70
            PACK      DESC1,DESC1,SP70
.
            MATCH     DESC1,SP70
            IF        @EQUAL
              PACK      DESC1,START,SP70
            ENDIF
          ENDIF
.
KCODF999  RETURN
.----------------------------------------------------------------------
. Keyin To Code
.----------------------------------------------------------------------
KCODT000
          IF        OPTIONA=1
            MOVE      SP70,TOLAB
            DISPLAY   *PCCOL:CVERT,TOLAB;
.
            CALL      KSICAA00
            PACK      TOCODE,SICACODE,SP70
            LOAD      TOCODE,EXIT,Z70,SP70
.
            PACK      DESC2,SICADESC,SP70
            LOAD      DESC2,EXIT,SP70,SP70
.
            PACK      TOLAB,SICACODE,SP70
            LOAD      TOLAB,EXIT,END,SP70
          ELSE
            MOVE      ZERO,EXIT
            MOVE      SP70,DESC2
            KEYIN     *PCCOL:CVERT,*DV,DESC2:
                      *PCCOL:CVERT,*V2LON,DESC2;
            PACK      PDESC2,DESC2,Z70
            PACK      DESC2,DESC2,SP70
.
            MATCH     DESC2,SP70
            IF        @EQUAL
              PACK      DESC2,END,SP70
            ENDIF
          ENDIF
.
KCODT999  RETURN
.**********************************************************************
. ALIGN- print lineup routine
. Requires IBACODFD/INC IBAPRNFD/INC
.          IBAIOCOD/INC IBAIOPRN/INC IBAPRINT
.  Note : already includes IBAPASFD & IBAPASIO
.**********************************************************************
ALIGN000
          PACK      DISPMSG,SP70,SP70       * save line 24
          CALL      MESSAGE2
.
          CALL      OPNPRINT
.
          CALL      RDLAB000                * read parameters
          CALL      LULAB000                * print routine
          CALL      CLSPRINT
.
ALIGN999
          CALL      MESSAGE3                * restore line 24
          RETURN
.----------------------------------------------------------------------
. Keyin COPY
.----------------------------------------------------------------------
KCOPY000
          READ      CONTROLF,TWENTY3;*188,HLABPG,HLABPRT
          MOVE      HLABPRT,NOCOPIES
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,NOCOPIES
.
          IF        NOCOPIES<1
            BEEP
            CLEAR     DISPMSG
            APPEND    "Must Be Greater Than Zero - ",DISPMSG
            CALL      MESSAGE1
            GOTO      KCOPY000
          ENDIF
.
KCOPY999  RETURN
