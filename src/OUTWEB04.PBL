          MOVE      ZERO,F1
          MOVE      KEY1,F1
          BRANCH    F1,OTPH0701,OTPH0702
.
          WRITE     HTMLFILE;OPHHOSP;
          GOTO      OTPH9999
.
OTPH0701  PACK      KEY3,OPHHOSP,SP70
          CALL      RDPTHSP1
          IF        OVRCD=1
            MOVE      OPHHOSP,PTHSNAME
