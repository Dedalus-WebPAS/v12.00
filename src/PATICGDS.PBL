.******************************************************************************
.
.         PATICGDS.PBL
.         ------------
.         This include displays all Disease Code entries
.
.
.------------------------------------------------------------------------------
.         Display Heading, and then loop over entries
.------------------------------------------------------------------------------
.
PATICGDS  DISPLAY   *P1:3,*EF,*P20:4,*V2LON,*ULON,"Group Codes "
          MOVE      FIVE,CVERT
          MOVE      ONE,CCOL
.
          PACK      KEY5,IDCODE,SP9
          CALL      RDSICG1
ICGD1000  CALL      RDKICG1
          BRANCH    OVRCD,ICGD5000
.
          MATCH     GIDCODE,IDCODE
          GOTO      ICGD5000 IF NOT EQUAL
.
          MATCH     SP4,GGPCODE                * Still only letter desc's
          GOTO      ICGD5000 IF EQUAL         * No - exit
.
          MOVE      GDESC,KEY70
          DISPLAY   *PCCOL:CVERT,*V2LON,GGPCODE,SP1,*HOFF,KEY70
.
          ADD       ONE,CVERT
          COMPARE   TWENTY4,CVERT
          GOTO      ICGD1000 IF LESS
.
          MOVE      FIVE,CVERT
.....          ADD       "40",CCOL
.....          COMPARE   "70",CCOL
.....          GOTO      ICGD1000 IF LESS
.....          GOTO      ICGD1000
.
.         Check for new standards question, or old question
.
          BRANCH    CNEWDS,ICGD2000
.
          KEYIN     *P1:24,*EL," (",*V2LON,"E",*HOFF,")xit, (":
                    *V2LON,"F",*HOFF,")irst Screen, (":
                    *V2LON,"N",*HOFF,")ext Screen : ",*V2LON,ANS;
.
          REP       UPPLOW,ANS 
          CMATCH    ANSE,ANS
          GOTO      ICGD6000 IF EQUAL
          CMATCH    ANSF,ANS
          GOTO      PATICGDS IF EQUAL
.
          GOTO      ICGD4000
.
.         Use the new standards question
.
ICGD2000  KEYIN     *P1:24,*EL," (",*V2LON,"C",*HOFF,")ontinue, (":
                    *V2LON,"F",*HOFF,")irst Screen, (":
                    *V2LON,"N",*HOFF,")ext Screen : ",*V2LON,ANS;
.
          REP       UPPLOW,ANS
          CMATCH    ANSC,ANS
          GOTO      ICGD6000 IF EQUAL
          CMATCH    ANSF,ANS
          GOTO      PATICGDS IF EQUAL
.
ICGD4000  MOVE      FIVE,CVERT
          MOVE      ONE,CCOL
          DISPLAY   *P1:5,*EF
          GOTO      ICGD1000
.
.         End of codes
.
ICGD5000  BRANCH    CNEWDS,ICGD6000
          DISPLAY   *P1:24,*EL;
          CALL      HITENTER
.
ICGD6000  MOVE      ZERO,OVRCD
          RETURN
