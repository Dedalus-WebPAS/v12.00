          IF        @EQUAL
            MOVE      CCC,CCENT
            UNPACK    SP6,CYEAR,CMON,CDAY
          ENDIF
          MATCH     "00000000",KEY8
          IF        @EQUAL
            MOVE      CCC,CCENT
            UNPACK    SP6,CYEAR,CMON,CDAY
          ENDIF
.
          CALL      KEYDATE
          BRANCH    CQUEST,GDVX2000
