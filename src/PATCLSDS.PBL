.
.       PATCLSDS : Display clinical speciality codes
.
.       Mods :
.
PATCLSDS
CLSDS000  DISPLAY   *P1:4,*EF,*P20:4,*ULON,*V2LON,"Clinical Speciality Code ":
                    "& Description";
          MOVE      FIVE,CVERT
          PACK      KEY2,SP2
          CALL      RSPTCLS1
CLSDS100  CALL      RKPTCLS1
          BRANCH    OVRCD,CLSDS500
.
          DISPLAY   *P2:CVERT,PTCICODE,*P6:CVERT,PTCIDESC;
          ADD       ONE,CVERT
          COMPARE   TWENTY4,CVERT
          GOTO      CLSDS100 IF LESS
.
          BRANCH    CNEWDS,CLSDS300
.
          KEYIN     *P1:24,*EL," (",*V2LON,"E",*HOFF,")xit, ":
                    "(",*V2LON,"F",*HOFF,")irst Screen, ":
                    "(",*V2LON,"N",*HOFF,")ext Screen : ",*V2LON,ANS;
.
          REP       UPPLOW,ANS
          CMATCH    ANSE,ANS
          GOTO      CLSDS999 IF EQUAL
          CMATCH    ANSF,ANS
          GOTO      CLSDS000 IF EQUAL
          GOTO      CLSDS400
.
CLSDS300  KEYIN     *P1:24,*EL," (",*V2LON,"C",*HOFF,")ontinue, ":
                    "(",*V2LON,"F",*HOFF,")irst Screen, ":
                    "(",*V2LON,"N",*HOFF,")ext Screen : ",*V2LON,ANS;
.
          REP       UPPLOW,ANS
          CMATCH    ANSC,ANS
          GOTO      CLSDS999 IF EQUAL
          CMATCH    ANSF,ANS
          GOTO      CLSDS000 IF EQUAL
.
CLSDS400  MOVE      FIVE,CVERT
          MOVE      ONE,CCOL
          DISPLAY   *P1:5,*EF;
          GOTO      CLSDS100
.
CLSDS500  BRANCH    CNEWDS,CLSDS999
.
          DISPLAY   *P1:24,*EL;
          CALL      HITENTER
          MOVE      ZERO,OVRCD
.
CLSDS999  RETURN
+
.

