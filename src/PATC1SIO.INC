.        INCLUDE NAME : PATC1SIO.INC
.
.        FILE I/O REQUESTS FOR CLINICAL STATISTICS 1 FILE
.
.OVRCD   FORM     1        0 = NOT OVER
.                          1 = OVER
.KEY16   DIM     16        KEY FOR PATC1SA1
.
RAPTC1S1 MOVE     ZERO,OVRCD
         RESET    KEY16
         READ     PATC1SA1,KEY16;ANS
         GOTO     OVERCOND IF OVER
         RETURN
.
RSPTC1S1 RESET    KEY16
         READ     PATC1SA1,KEY16;;
         RETURN
.
RKPTC1S1 MOVE     ZERO,OVRCD
         READKS   PATC1SA1;PTC1DATE,PTC1TYPE,PTC1DOCT,PTC1TYPP,PTC1DSCH:
                           PTC1TBDY,PTC1PBDY,PTC1SPAR
         GOTO     OVERCOND IF OVER
.
         RETURN
.
RPPTC1S1 MOVE     ZERO,OVRCD
         READKP   PATC1SA1;PTC1DATE,PTC1TYPE,PTC1DOCT,PTC1TYPP,PTC1DSCH:
                           PTC1TBDY,PTC1PBDY,PTC1SPAR
         GOTO     OVERCOND IF OVER
.
         RETURN
.
RDPTC1S1 MOVE     ZERO,OVRCD
         RESET    KEY16
         READ     PATC1SA1,KEY16;PTC1DATE,PTC1TYPE,PTC1DOCT,PTC1TYPP,PTC1DSCH:
                           PTC1TBDY,PTC1PBDY,PTC1SPAR
         GOTO     OVERCOND IF OVER
.
         RETURN
.
UPPTC1S1 UPDATE   PATC1SA1;PTC1DATE,PTC1TYPE,PTC1DOCT,PTC1TYPP,PTC1DSCH:
                           PTC1TBDY,PTC1PBDY,PTC1SPAR
         RETURN
.
DEPTC1S1 RESET    KEY16
         DELETE   PATC1SA1,KEY16
         RETURN
.
WRPTC1S1 MOVE     ZERO,OVRCD
         RESET    KEY16
         WRITE    PATC1SA1,KEY16;PTC1DATE,PTC1TYPE,PTC1DOCT,PTC1TYPP,PTC1DSCH:
                           PTC1TBDY,PTC1PBDY,PTC1SPAR
.
         RETURN
.
