.---------------------------------------------------------------------------
.   System    :   Message System                     
.   Include   :   MSGIDSKY
.   Routine   :   HMSIDA00 Mailing Id Help Scrn
.   Return    :   MSGRNAME
.-------------------------------------------------------------------------------
HMSIDA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
          GOTO      HMSIDA04 IF NOT EQUAL
          MOVE      "10",HTOP
          MOVE      "24",HBOT
          MOVE      "40",HLEF
          MOVE      "80",HRIG
          ENDIF
HMSIDA04
          CALL      GSCRN000
          BRANCH    EXIT,HMSIDA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Code"               
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 9",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Surname"            
.
          MOVE      STDCOL02,STDCOL03
          ADD       "15",STDCOL03
          MOVE      STDCOL03,STDCOL04
          ADD       " 1",STDCOL04
          DISPLAY   *PSTDCOL04:STDROW,*V2LON,*ULON,"Given Name"         
.
          MOVE      STDCOL04,STDCOL05
          ADD       "30",STDCOL05
HMSIDA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,MSIDMLID
          PACK      KEY8,MSIDMLID,SP70                                          
.                                                                               
          CALL      RSMSIDS1
HMSIDA10
          CALL      RKMSIDS1
          BRANCH    OVRCD,HMSIDA15
          PACK      KEY8,MSIDMLID,SP70                                          
.                                                                               
          BRANCH    CSEARCH,HMSIDA12
          CALL      HMSIDA60
          BRANCH    OVRCD,HMSIDA10
HMSIDA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY8,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,MSIDMLID
          DISPLAY   *PSTDCOL02:STDROW,MSIDSNAM
          DISPLAY   *PSTDCOL03:STDROW,SP1     
          DISPLAY   *PSTDCOL04:STDROW,MSIDGNAM
          COMPARE   MAXITEM,XITEMNO
          GOTO      HMSIDA10 IF NOT EQUAL
HMSIDA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HMSIDA20
          CALL      HSCRI000
          BRANCH    EXIT,HMSIDA30,HMSIDA35,HMSIDA40,HMSIDA45,HMSIDA04:
                              HMSIDA91,HMSIDA50
          GOTO      HMSIDA92
HMSIDA30
          CALL      RKMSIDS1
          BRANCH    OVRCD,HMSIDA85
          PACK      KEY8,MSIDMLID,SP70                                          
.                                                                               
.                                                                               
          CALL      RPMSIDS1
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HMSIDA10
HMSIDA35
          MOVE      ST1,KEY8 
          CALL      RDMSIDS1
          CALL      RPMSIDS1
          BRANCH    OVRCD,HMSIDA85
          PACK      KEY8,MSIDMLID,SP70                                          
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HMSIDA36
          CALL      RPMSIDS1
          BRANCH    OVRCD,HMSIDA37
          PACK      KEY8,MSIDMLID,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HMSIDA36 IF NOT EQUAL
HMSIDA37
          MOVE      ZERO,XITEMNO
          GOTO      HMSIDA10
HMSIDA40
          MOVE      ST1,KEY8 
          CALL      RDMSIDS1
          CALL      RPMSIDS1
          BRANCH    OVRCD,HMSIDA85
          PACK      KEY8,MSIDMLID,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HMSIDA08
HMSIDA45
          CALL      RKMSIDS1
          BRANCH    OVRCD,HMSIDA85
          PACK      KEY8,MSIDMLID,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY8,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSMSIDS1
HMSIDA46
          CALL      RPMSIDS1
          BRANCH    OVRCD,HMSIDA47
          PACK      KEY8,MSIDMLID,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HMSIDA46 IF NOT EQUAL
HMSIDA47
          MOVE      ZERO,XITEMNO
          GOTO      HMSIDA10
HMSIDA50  COMPARE   ZERO,CSEARCH
          GOTO      HMSIDA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          KEYIN     *PSTDCOL03:STDROW,*V2LON,SCHFLD03
          RESET     SCHFLD03
          KEYIN     *PSTDCOL04:STDROW,*V2LON,SCHFLD04
          RESET     SCHFLD04
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HMSIDA08
HMSIDA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HMSIDA08
HMSIDA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HMSIDA61 IF EQUAL
          MATCH     SCHFLD01,MSIDMLID
          GOTO      HMSIDA72 IF NOT EQUAL
HMSIDA61
          MATCH     SP70,SCHFLD02
          GOTO      HMSIDA62 IF EQUAL
          MATCH     SCHFLD02,MSIDSNAM
          GOTO      HMSIDA72 IF NOT EQUAL
HMSIDA62
          MATCH     SP70,SCHFLD03
          GOTO      HMSIDA63 IF EQUAL
          MATCH     SCHFLD03,SP1     
          GOTO      HMSIDA72 IF NOT EQUAL
HMSIDA63
          MATCH     SP70,SCHFLD04
          GOTO      HMSIDA64 IF EQUAL
          MATCH     SCHFLD04,MSIDGNAM
          GOTO      HMSIDA72 IF NOT EQUAL
HMSIDA64
HMSIDA65
HMSIDA66
HMSIDA67
HMSIDA68
HMSIDA69
HMSIDA70
          GOTO      HMSIDA75
HMSIDA72  MOVE      ONE,OVRCD
HMSIDA75  RETURN
HMSIDA85
          LOAD      KEY8,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY8,MSIDMLID                                               
          CALL      RDMSIDS1
HMSIDA90
.                                                                               
          BEEP
          GOTO      HMSIDA20
HMSIDA91
          MOVE      SP70,MSIDSNAM
          MOVE      SP70,MSIDGNAM
          MOVE      SP70,MSIDSPAR
          MOVE      SP70,KEY8 
          MOVE      SP70,MSIDMLID
          GOTO      HMSIDA96
HMSIDA92
          MOVE      SP70,KEY8 
          MOVE      SP70,MSIDMLID
          LOAD      KEY8,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY8,MSIDMLID                                               
HMSIDA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HMSIDA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Message System                     
.   Include   :   MSGIDSKY
.   Routine   :   KMSIDA00 Keyin Mailing ID          
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KMSIDA00
          MOVE      UNDLN70,MSIDMLID
          KEYIN     *PCCOL:CVERT,*DV,MSIDMLID:                                  
                    *PCCOL:CVERT,*V2LON,MSIDMLID:                               
                    *PCCOL:CVERT,*DV,MSIDMLID
.
          ENDSET    MSIDMLID
          APPEND    SP70,MSIDMLID
          RESET     MSIDMLID
.
          MATCH     SP70,MSIDMLID
          GOTO      KMSIDA90 IF EQUAL
.
          CMATCH    EXITCHAR,MSIDMLID
          GOTO      KMSIDA95 IF EQUAL
.
          CMATCH    QUEST,MSIDMLID
          GOTO      KMSIDA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HMSIDA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    MSIDMLID
          APPEND    SP70,MSIDMLID
          RESET     MSIDMLID
.
          MATCH     SP70,MSIDMLID
          GOTO      KMSIDA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,MSIDMLID                                
KMSIDA10
          PACK      KEY8,MSIDMLID                                               
          CALL      RAMSIDS1
          BRANCH    OVRCD,KMSIDA80
          BRANCH    CKEYTYP,KMSIDA75
          CALL      RDMSIDS1
KMSIDA70
          MOVE      ZERO,EXIT
          GOTO      KMSIDA99
KMSIDA75
          MOVE      "Mailing ID Already Exist - ",DISPMSG                                            
          CALL      MESSAGE1
          GOTO      KMSIDA00
KMSIDA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KMSIDA99,KMSIDA99
          MOVE      "Mailing ID Does Not Exist - ",DISPMSG                                           
          CALL      MESSAGE1
          GOTO      KMSIDA00
KMSIDA90
          MOVE      ONE,EXIT
          GOTO      KMSIDA99
KMSIDA95
          MOVE      TWO,EXIT
KMSIDA99
          RETURN
