          MOVE      "90312-01",COLCODES[4]
          MOVE      "32084-00",COLCODES[5]
          MOVE      "32084-01",COLCODES[6]
          MOVE      "32087-00",COLCODES[7]
          MOVE      "32090-00",COLCODES[8]
          MOVE      "32090-01",COLCODES[9]
          MOVE      "32093-00",COLCODES[10]
          MOVE      "32094-00",COLCODES[11]
LDCB1399  RETURN
+
.******************************************************************************
.*                                  B13SW000              Called by: WIS13000 *
.*                            Calculate Base WIES13                           *
.******************************************************************************
B13SW000  CALL      D13SC000                * length of stay category
          CALL      C13ST000                * calculate inlier status
.
          MATCH     "L",W13NLIER            * low outlier
          CALL      BW13L000 IF EQUAL       * calculate base WIES13
.
          MATCH     "I",W13NLIER            * inlier
          CALL      BW13N000 IF EQUAL       * calculate base WIES13
.
          MATCH     "H",W13NLIER            * high outlier
          CALL      BW13H000 IF EQUAL       * calculate base WIES13
.
B13SW999  RETURN
+
.******************************************************************************
.*                                  D13SC000              Called by: B13SW000 *
.*                      Determine Length Of Stay Category                     *
.******************************************************************************
D13SC000  MOVE      NBRDAYS,WIS13LOS        * length of stay from NHOSPDAY
          MOVE      "M",LOSCAT              * set LOS category to multiday
          MATCH     ADATE,DDATE             * check for sameday
          IF        @EQUAL
            MOVE      ONE,SAMEDAY           * sameday
            MOVE      ONE,WIS13LOS          * LOS = 1
            MOVE      "S",LOSCAT            * set LOS category to sameday
            GOTO      D13SC999
          ELSE
            MOVE      ZERO,SAMEDAY          * not sameday
          ENDIF
.
          IF        SAMEDAY = 0 & WIS13LOS = 1
            MOVE      "O",LOSCAT            * set LOS category to oneday
            GOTO      D13SC999
          ENDIF
.
          IF        WIS13LOS > 1825
            MOVE      "1825",WIS13LOS       * maximum LOS is 5 years
          ENDIF
.
D13SC999  RETURN
+
.******************************************************************************
.*                                  C13ST000              Called by: B13SW000 *
.*                           Calculate Inlier Status                          *
.******************************************************************************
C13ST000  MOVE      "I",W13NLIER            * set status to inlier
          IF        WIS13LOS < PT13LIBP
            MOVE      "L",W13NLIER          * set status to low outlier
            GOTO      C13ST999
          ENDIF
.
          ASSIGN    (PT13HIBP+ADJMVDAY),FORM12P4   * high bound. + mech.v days
          IF        WIS13LOS > FORM12P4
            MOVE      "H",W13NLIER          * set status to high outlier
          ENDIF
C13ST999  RETURN
+
.******************************************************************************
.*                                  BW13L000              Called by: B13SW000 *
.*                     Calculate Base WIES13 for Low Outlier                  *
.******************************************************************************
BW13L000  MATCH     "S",LOSCAT            * sameday
          IF        @EQUAL
            MOVE      PT13TSDW,TOTBWIES             * base WIES13
            ASSIGN    (TOTBWIES/PT13TIMW),TOTIES13  * IES13
.
            MOVE      PT13TSDW,WIS13WGT             * for display
            MOVE      "Same Day Wgt ",WIS13DSC
            GOTO      BW13L999
          ENDIF
.
          MATCH     "O",LOSCAT            * oneday
          IF        @EQUAL
            MOVE      PT13TODW,TOTBWIES             * base WIES13
            ASSIGN    (TOTBWIES/PT13TIMW),TOTIES13  * IES13
.
            MOVE      PT13TODW,WIS13WGT             * for display
            MOVE      "One Day Wgt  ",WIS13DSC
            GOTO      BW13L999
          ENDIF
.
          MATCH     "M",LOSCAT            * multiday
          IF        @EQUAL
            ASSIGN    (PT13TODW+((WIS13LOS-1)*PT13TLOM)),TOTBWIES
.                                                    * base WIES13
            ASSIGN    (TOTBWIES/PT13TIMW),TOTIES13   * IES13
.
            MOVE      PT13TLOM,WIS13WGT             * for display
            MOVE      "Low Outlr Wgt",WIS13DSC
            GOTO      BW13L999
          ENDIF
BW13L999  RETURN
+
.******************************************************************************
.*                                  BW13N000              Called by: B13SW000 *
.*                      Calculate Base WIES13 for Inlier                      *
.******************************************************************************
BW13N000  MOVE      ONE,TOTIES13          * IES13 = 1
          MATCH     "S",LOSCAT            * sameday
          IF        @EQUAL
            MOVE      PT13TSDW,TOTBWIES   * base WIES13
.
            MOVE      PT13TSDW,WIS13WGT             * for display
            MOVE      "Same Day Wgt ",WIS13DSC
            GOTO      BW13N999
          ENDIF
.
          MATCH     "O",LOSCAT            * oneday
          IF        @EQUAL
            MOVE      PT13TODW,TOTBWIES   * base WIES13
.
            MOVE      PT13TODW,WIS13WGT             * for display
            MOVE      "One Day Wgt  ",WIS13DSC
            GOTO      BW13N999
          ENDIF
.
          MATCH     "M",LOSCAT            * multiday
          IF        @EQUAL
            MOVE      PT13TIMW,TOTBWIES   * base WIES13
.
            MOVE      PT13TIMW,WIS13WGT             * for display
            MOVE      "Inlier Wgt   ",WIS13DSC
            GOTO      BW13N999
          ENDIF
BW13N999  RETURN
+
.******************************************************************************
.*                                  BW13H000              Called by: B13SW000 *
.*                    Calculate Base WIES13 for High Outlier                  *
.******************************************************************************
BW13H000  ASSIGN    ((WIS13LOS-PT13HIBP)-ADJMVDAY),HIGHDAYS  * high outlr days
          COMPARE   ZERO,HIGHDAYS
          IF        @LESS
            MOVE      ZERO,HIGHDAYS         * highdays must be >= zero
          ENDIF
.
          COMPARE   HIGHDAYS,HIHLOS
          IF        @LESS
            MOVE      HIHLOS,HITHDAYS       * get lesser of highdays and
          ELSE                              *  HIHLOS
            MOVE      HIGHDAYS,HITHDAYS
          ENDIF
.
          ASSIGN    (HIGHDAYS-HITHDAYS)*PT13THOM,TOTBWIES    *high WIES
          ASSIGN    (HITHDAYS*PT13THIH)+TOTBWIES,TOTBWIES    *HITH WEIS
          ASSIGN    (PT13TIMW+TOTBWIES),TOTBWIES             *base WEIS13
          ASSIGN    (TOTBWIES/PT13TIMW),TOTIES13             * IES13
.
          MOVE      PT13THOM,WIS13WGT                        * for display
          MOVE      "High Outl Wgt",WIS13DSC
BW13H999  RETURN
+
.******************************************************************************
.*                                  C13AB000              Called by: WIS13000 *
.*                          Check for Aboriginality                           *
.******************************************************************************
C13AB000  MOVE      ZERO,ATSIWIES                * set ATSI WIES to zero
          IF        PNSWRACE<1 | PNSWRACE>14
            GOTO      C13AB999              * Invalid parameter
          ENDIF
.
          LOAD      TCODE,PNSWRACE,CATP1,CATP2,CATP3,CATP4,CATP5:
                                   CATP6,CATA1,CATA2,CATA3,CATA4:
                                   CATA5,CATA6,CATA7,CATVA
          LOAD      ACODE,PNSWRACE,PUSR1,PUSR2,PUSR3,PUSR4,PUSR5:
                                   PUSR6,AUSR1,AUSR2,AUSR3,AUSR4:
                                   AUSR5,AUSR6,AUSR7,PMPXABRG
.
          MATCH     ACODE,SP3
          GOTO      C13AB999 IF EQUAL
.
          PACK      KEY5,TCODE,ACODE
          CALL      RDCODE1                 * Read a code file record
          BRANCH    OVRCD,C13AB999
.
          MOVE      THCSCOD,KEY1
          TYPE      KEY1
          GOTO      C13AB999 IF NOT EQUAL   * Non numeric, error
.
          MOVE      KEY1,FORM1
.
. ---- Calculate ATSI Loading if aboriginality = 5, 6, or 7 ----
.
          IF        FORM1=5 | FORM1=6 | FORM1=7
            MOVE      TOTBWIES,FORM12P4       * base WIES
            ADD       MVCOPAY,FORM12P4        * mechv copayment
            ADD       THCOPAY,FORM12P4        * thal  copayment
            ADD       AAACOPAY,FORM12P4       * AAA   copayment
            ADD       ASDCOPAY,FORM12P4       * ASD   copayment
            ADD       COLCOPAY,FORM12P4       * colon copayment
            ASSIGN    (0.3*FORM12P4),ATSIWIES
          ENDIF
.
C13AB999  RETURN
+
