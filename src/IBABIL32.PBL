          READ      MAPCLMA1,KEY3;;
          RETURN
.
RKTMCLA1  MOVE      ZERO,OVRCD
          READKS    MAPCLMA1;TMPOLDCL,TMPNEWCL
          GOTO      OVERCOND IF OVER
          RETURN
.
DETMCLA1  RESET     KEY3
          DELETE    MAPCLMA1,KEY3
          RETURN
.
WRTMCLA1  RESET     KEY3
          WRITE     MAPCLMA1,KEY3;TMPOLDCL,TMPNEWCL
          RETURN
.

