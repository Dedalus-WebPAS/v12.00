.---------------------------------------------------------------------------
.   System    :   Supply and Inventory               
.   Include   :   SINSCMKY
.   Routine   :   HSISCA00 Contract Code Keyin 
.   Return    :           
.-------------------------------------------------------------------------------
HSISCA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HSISCA04
          CALL      GSCRN000
          BRANCH    EXIT,HSISCA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Contract"           
.
          MOVE      STDCOL01,STDCOL02
          ADD       "11",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "41",STDCOL03
HSISCA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,SISCCON 
          PACK      KEY10,SISCCON,SP70                                          
.                                                                               
.                                                                               
          CALL      RSSISC1 
HSISCA10
          CALL      RKSISC1 
          BRANCH    OVRCD,HSISCA15
          PACK      KEY10,SISCCON,SP70                                          
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HSISCA12
          CALL      HSISCA60
          BRANCH    OVRCD,HSISCA10
HSISCA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY10,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,SISCCON 
          DISPLAY   *PSTDCOL02:STDROW,SISCDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HSISCA10 IF NOT EQUAL
HSISCA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HSISCA20
          CALL      HSCRI000
          BRANCH    EXIT,HSISCA30,HSISCA35,HSISCA40,HSISCA45,HSISCA04:
                              HSISCA91,HSISCA50
          GOTO      HSISCA92
HSISCA30
          CALL      RKSISC1 
          BRANCH    OVRCD,HSISCA85
          PACK      KEY10,SISCCON,SP70                                          
.                                                                               
.                                                                               
          CALL      RPSISC1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HSISCA10
HSISCA35
          MOVE      ST1,KEY10
          CALL      RDSISC1 
          CALL      RPSISC1 
          BRANCH    OVRCD,HSISCA85
          PACK      KEY10,SISCCON,SP70                                          
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HSISCA36
          CALL      RPSISC1 
          BRANCH    OVRCD,HSISCA37
          PACK      KEY10,SISCCON,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HSISCA36 IF NOT EQUAL
HSISCA37
          MOVE      ZERO,XITEMNO
          GOTO      HSISCA10
HSISCA40
          MOVE      ST1,KEY10
          CALL      RDSISC1 
          CALL      RPSISC1 
          BRANCH    OVRCD,HSISCA85
          PACK      KEY10,SISCCON,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HSISCA08
HSISCA45
          CALL      RKSISC1 
          BRANCH    OVRCD,HSISCA85
          PACK      KEY10,SISCCON,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY10,Z70                                                   
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSSISC1 
HSISCA46
          CALL      RPSISC1 
          BRANCH    OVRCD,HSISCA47
          PACK      KEY10,SISCCON,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HSISCA46 IF NOT EQUAL
HSISCA47
          MOVE      ZERO,XITEMNO
          GOTO      HSISCA10
HSISCA50  COMPARE   ZERO,CSEARCH
          GOTO      HSISCA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HSISCA08
HSISCA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HSISCA08
HSISCA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HSISCA61 IF EQUAL
          MATCH     SCHFLD01,SISCCON 
          GOTO      HSISCA72 IF NOT EQUAL
HSISCA61
          MATCH     SP70,SCHFLD02
          GOTO      HSISCA62 IF EQUAL
          MATCH     SCHFLD02,SISCDES 
          GOTO      HSISCA72 IF NOT EQUAL
HSISCA62
HSISCA63
HSISCA64
HSISCA65
HSISCA66
HSISCA67
HSISCA68
HSISCA69
HSISCA70
          GOTO      HSISCA75
HSISCA72  MOVE      ONE,OVRCD
HSISCA75  RETURN
HSISCA85
          LOAD      KEY10,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY10,SISCCON                                               
          CALL      RDSISC1 
HSISCA90
.                                                                               
          BEEP
          GOTO      HSISCA20
HSISCA91
          MOVE      SP70,SISCDES 
          MOVE      SP70,SISCSDT 
          MOVE      SP70,SISCFI2 
          MOVE      SP70,SISCEDT 
          MOVE      SP70,SISCUR1 
          MOVE      SP70,SISCUR2 
          MOVE      SP70,SISCUD1 
          MOVE      SP70,SISCUD2 
          MOVE      SP70,SISCUC1 
          MOVE      SP70,SISCUC2 
          MOVE      SP70,SISCUY1 
          MOVE      SP70,SISCUY2 
          MOVE      SP70,SISCSPA 
          MOVE      SP70,KEY10
          MOVE      SP70,SISCCON 
          GOTO      HSISCA96
HSISCA92
          MOVE      SP70,KEY10
          MOVE      SP70,SISCCON 
          LOAD      KEY10,CCITEM,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY10,SISCCON                                               
HSISCA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HSISCA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Supply and Inventory               
.   Include   :   SINSCMKY
.   Routine   :   KSISCA00 Keyin Contact Code        
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KSISCA00 TOPIC     "SISCCON "
          MOVE      UNDLN70,SISCCON 
          KEYIN     *PCCOL:CVERT,*DV,SISCCON:                                   
                    *PCCOL:CVERT,*V2LON,SISCCON:                                
                    *PCCOL:CVERT,*DV,SISCCON 
.
          ENDSET    SISCCON 
          APPEND    SP70,SISCCON 
          RESET     SISCCON 
.
          MATCH     SP70,SISCCON 
          GOTO      KSISCA90 IF EQUAL
.
          CMATCH    EXITCHAR,SISCCON 
          GOTO      KSISCA95 IF EQUAL
.
          CMATCH    QUEST,SISCCON 
          GOTO      KSISCA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HSISCA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    SISCCON 
          APPEND    SP70,SISCCON 
          RESET     SISCCON 
.
          MATCH     SP70,SISCCON 
          GOTO      KSISCA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,SISCCON                                 
KSISCA10
          PACK      KEY10,SISCCON                                               
          CALL      RASISC1 
          BRANCH    OVRCD,KSISCA80
          BRANCH    CKEYTYP,KSISCA75
          CALL      RDSISC1 
KSISCA70
          MOVE      ZERO,EXIT
          GOTO      KSISCA99
KSISCA75
          MOVE      "Contact Code Already Exist - ",DISPMSG                                          
          CALL      MESSAGE1
          GOTO      KSISCA00
KSISCA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KSISCA99,KSISCA99
          MOVE      "Contact Code Does Not Exist - ",DISPMSG                                         
          CALL      MESSAGE1
          GOTO      KSISCA00
KSISCA90
          MOVE      ONE,EXIT
          GOTO      KSISCA99
KSISCA95
          MOVE      TWO,EXIT
KSISCA99 TOPIC    PRGID
          RETURN
