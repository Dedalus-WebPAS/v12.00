.*                             POST0000                                   *
.*  Print report                                                          *
.**************************************************************************
POST0000
          PACK      DISPMSG,SP70,SP70
          CALL      MESSAGE2
.
          MATCH     FROMDATE,TODATE
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Date Range - ";
            CALL      HITENTER
            GOTO      POST9500
          ENDIF
.
          CLOCK     TIME,CTIMEIS             * Set time of report
          MOVE      ONE,CNOUNDLN             * Print just heading on new page
          MOVE      ZERO,CPAGENO             * Page number initialized to zero
          CALL      HEAD0000
.
          DISPLAY   *P1:24,*EL,"Printing purchase order :";
.
          PACK      KEY7,SP7                 * Start with first record
          CALL      RSSITR1
.
POST1000  CALL      RKSITR1
          BRANCH    OVRCD,POST9000           * EOF
.
          MATCH     "2",SITRSTAT
          GOTO      POST1000 IF LESS         * Must be >= 2
.
          MATCH     FROMDATE,SITRTDAT
          GOTO      POST1000 IF LESS         * Must be >= start date
.
          MATCH     SITRTDAT,TODATE
          GOTO      POST1000 IF LESS         * Must be <= finish date
.
          DISPLAY   *P27:24,*EL,*V2LON,SITRPURC; * Show some screen action
.
          PRINT     "| ",SITRPURC," | ";
.
          MATCH     "2",SITRSTAT
          GOTO      POST2000 IF EQUAL
.
          MATCH     "3",SITRSTAT
          GOTO      POST3000 IF EQUAL
.
          MATCH     "9",SITRSTAT
          GOTO      POST4000 IF EQUAL
.
          PRINT     "**  STATUS UNKNOWN **   ";
          GOTO      POST5000
.
POST2000  PRINT     "Acknowledgement received";
          GOTO      POST5000
.
POST3000  PRINT     "Transmission complete   ";
          GOTO      POST5000
.
POST4000  PRINT     "Cancelled               ";
.
POST5000
          UNPACK    SITREDAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          PRINT     " | ",SITRUSER,"    | ",SITRORG,"       | ",CPCDATE," | ";
          UNPACK    SITRTDAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          PRINT     CPCDATE," |    ",SITRTNUM,"    | ",SITRCREA,*132,"|"
.
.         We need to delete this record if delete was chosen
.
          IF        PURGEDAT=1
            PACK      KEY7,SITRPURC,SP10
            CALL      DESITR1
            CALL      RSSITR1               * Reposition file pointer
          ENDIF
.
          ADD       ONE,CLNO
          COMPARE   "55",CLNO
          GOTO      POST1000 IF LESS
.
          CALL      UND132
          CALL      HEAD0000
          GOTO      POST1000
.
POST9000  CALL      UND132
          PRINT     *N,"*** End of report ***"
.
          MOVE      ZERO,EXIT
          GOTO      POST9999
.
POST9500  MOVE      ONE,EXIT
.
POST9999
          CALL      MESSAGE3
          RETURN
.
