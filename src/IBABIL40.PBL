            REP       " 0",DDATE
            UNPACK    DDATE,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE                 * Format the start date
            UNPACK    DTIME,KEY5
            PRINT     *75,CPDATE," ",KEY5;
          ENDIF
