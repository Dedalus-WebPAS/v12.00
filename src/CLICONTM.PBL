.------------------------------------------------------------------------
.   Clear Parameters for CLICONTD
.------------------------------------------------------------------------
CPCL0000  MOVE      Z70,CLICN001
.
CPCL9999  RETURN
.
.------------------------------------------------------------
. Display fields from PATCONFD - Sector109 Tab204
.------------------------------------------------------------
CLCN0000  BRANCH    FLDITMNO,CLCN1010
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      CLCN9999
.
CLCN1010  WRITE     HTMLFILE;PTCNRSYR;
          GOTO      CLCN9999
.
CLCN9999  RETURN
.
.------------------------------------------------------------
.   Set Parameters for CLICONTD
.------------------------------------------------------------
SPCLI000  MOVE      ZERO,EXIT
          UNPACK    QRYNAME,KEY5,KEY3
          MOVE      KEY3,F3
          BRANCH    F3,SPCLI001
.
          MOVE      ONE,EXIT
          GOTO      SPCLI999
.
SPCLI001  MOVE      QRYDATA,CLICN001
          GOTO      SPCLI999
.
SPCLI999  RETURN
.
.------------------------------------------------------------
.  Update PATCONFD file variables Sector109 Tab204
.------------------------------------------------------------
MVCL0000  MATCH     Z70,CLICN001 
          IF        !@EQUAL
            MOVE      CLICN001,PTCNRSYR
          ENDIF
.
MVCL9999  RETURN
.
