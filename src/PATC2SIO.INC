.        INCLUDE NAME : PATC2SIO.INC
.
.        FILE I/O REQUESTS FOR CLINICAL STATISTICS 2 FILE 
.
.OVRCD   FORM     1        0 = NOT OVER
.                          1 = OVER
.KEY15   DIM     15        KEY FOR PATC2SA1
.
RAPTC2S1 MOVE     ZERO,OVRCD
         RESET    KEY15 
         READ     PATC2SA1,KEY15;ANS
         GOTO     OVERCOND IF OVER
         RETURN
.
RSPTC2S1 RESET    KEY15
         READ     PATC2SA1,KEY15;;
         RETURN
.
RKPTC2S1 MOVE     ZERO,OVRCD
         READKS   PATC2SA1;PTC2DATE,PTC2CLSS,PTC2DOCT,PTC2EMRG,PTC2ELCT:
                           PTC2DSCH,PTC2DEAD,PTC2STDP,PTC2PRVP,PTC2STDB:
                           PTC2PRVB,PTC2RNG1,PTC2RNG2,PTC2RNG3,PTC2RNG4:
                           PTC2SPAR
         GOTO     OVERCOND IF OVER
.
         RETURN
.
RPPTC2S1 MOVE     ZERO,OVRCD
         READKP   PATC2SA1;PTC2DATE,PTC2CLSS,PTC2DOCT,PTC2EMRG,PTC2ELCT:
                           PTC2DSCH,PTC2DEAD,PTC2STDP,PTC2PRVP,PTC2STDB:
                           PTC2PRVB,PTC2RNG1,PTC2RNG2,PTC2RNG3,PTC2RNG4:
                           PTC2SPAR
         GOTO     OVERCOND IF OVER
.
         RETURN
.
RDPTC2S1 MOVE     ZERO,OVRCD
         RESET    KEY15
         READ     PATC2SA1,KEY15;PTC2DATE,PTC2CLSS,PTC2DOCT,PTC2EMRG,PTC2ELCT:
                           PTC2DSCH,PTC2DEAD,PTC2STDP,PTC2PRVP,PTC2STDB:
                           PTC2PRVB,PTC2RNG1,PTC2RNG2,PTC2RNG3,PTC2RNG4:
                           PTC2SPAR
         GOTO     OVERCOND IF OVER
.
         RETURN
.
UPPTC2S1 UPDATE   PATC2SA1;PTC2DATE,PTC2CLSS,PTC2DOCT,PTC2EMRG,PTC2ELCT:
                           PTC2DSCH,PTC2DEAD,PTC2STDP,PTC2PRVP,PTC2STDB:
                           PTC2PRVB,PTC2RNG1,PTC2RNG2,PTC2RNG3,PTC2RNG4:
                           PTC2SPAR
         RETURN
.
DEPTC2S1 RESET    KEY15
         DELETE   PATC2SA1,KEY15
         RETURN
.
WRPTC2S1 MOVE     ZERO,OVRCD
         RESET    KEY15
         WRITE    PATC2SA1,KEY15;PTC2DATE,PTC2CLSS,PTC2DOCT,PTC2EMRG,PTC2ELCT:
                           PTC2DSCH,PTC2DEAD,PTC2STDP,PTC2PRVP,PTC2STDB:
                           PTC2PRVB,PTC2RNG1,PTC2RNG2,PTC2RNG3,PTC2RNG4:
                           PTC2SPAR
.
         RETURN
.
