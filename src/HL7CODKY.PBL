.---------------------------------------------------------------------------
.   System    :   Heakth Level 7                     
.   Include   :   HL7CODKY
.   Routine   :   KHLCOA00 Keyin HL7 Code            
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KHLCOA00 TOPIC     "HLCOCOD "
          MOVE      UNDLN70,HLCOCOD 
          KEYIN     *PCCOL:CVERT,*DV,HLCOCOD:                                   
                    *PCCOL:CVERT,*V2LON,HLCOCOD:                                
                    *PCCOL:CVERT,*DV,HLCOCOD 
.
          ENDSET    HLCOCOD 
          APPEND    SP70,HLCOCOD 
          RESET     HLCOCOD 
.
          MATCH     SP70,HLCOCOD 
          GOTO      KHLCOA90 IF EQUAL
.
          CMATCH    EXITCHAR,HLCOCOD 
          GOTO      KHLCOA95 IF EQUAL
.
          CMATCH    QUEST,HLCOCOD 
          GOTO      KHLCOA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HHLCOA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    HLCOCOD 
          APPEND    SP70,HLCOCOD 
          RESET     HLCOCOD 
.
          MATCH     SP70,HLCOCOD 
          GOTO      KHLCOA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,HLCOCOD                                 
KHLCOA10
          PACK      KEY14,HLCOTID,HLCOCOD                                       
          CALL      RAHLCO1 
          BRANCH    OVRCD,KHLCOA80
          BRANCH    CKEYTYP,KHLCOA75
          CALL      RDHLCO1 
KHLCOA70
          MOVE      ZERO,EXIT
          GOTO      KHLCOA99
KHLCOA75
          MOVE      "HL7 Code Already Exist - ",DISPMSG                                              
          CALL      MESSAGE1
          GOTO      KHLCOA00
KHLCOA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KHLCOA99,KHLCOA99
          MOVE      "HL7 Code Does Not Exist - ",DISPMSG                                             
          CALL      MESSAGE1
          GOTO      KHLCOA00
KHLCOA90
          MOVE      ONE,EXIT
          GOTO      KHLCOA99
KHLCOA95
          MOVE      TWO,EXIT
KHLCOA99 TOPIC    PRGID
          RETURN
