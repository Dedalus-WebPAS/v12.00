.----------------------------------------------------------------------
.  data includes
.----------------------------------------------------------------------
.
          INC       STDGENDF
          INC       PATCONFD/INC
          INC       WEBPARFD/INC
          INC       WEBPKIFD/INC
.
.----------------------------------------------------------------------
.  local variables
.----------------------------------------------------------------------
.
.
PRGID     INIT      "GENRWPKI"
PRGNAM    INIT      "Fix webpkiaf"
VERSION   INIT      "V12.00.00"
.
.----------------------------------------------------------------------
.  program code
.----------------------------------------------------------------------
.
.---- initialise screen and confirm desire to execute program ----
.
ML0000    CALL      DISPHEAD
.
          DISPLAY   *P1:10,"Generate WebServices Participant Keyword records."
          CALL      CONTQST
          SUB       ONE,CEXIT
          BRANCH    CEXIT,ML9999,ML9999
.
.---- open files ----
.
          OPEN      WEBPARA1,"webparaf"
          OPEN      WEBPKIA1,"webpkiaf"
          OPEN      WEBPKIA2,"webpkiaf"
          OPEN      CONTROLF,"controlf"
.
.---- loop through webparaf ----
.
          PACK      KEY9,SP70
          MOVE      ZERO,SECTOR
.
ML1000    CALL      RSWBPAR1           * read next record
.
ML1100    CALL      RKWBPAR1
          BRANCH    OVRCD,ML9000
.
          ADD       ONE,SECTOR         * display every 100th record
          IF        SECTOR%1000=1
            DISPLAY   *P1:24,*V2LON,SECTOR," - ",WBPRCODE,*EF;
          ENDIF
.
          PROC      WEBPKIUP
          GOTO      ML1100
.
ML9000    CALL      IBACLOCK
          PACK      PTCNWPKG,CCC,CYY,CMM,CDD
          REP       " 0",PTCNWPKG
.
          WRITAB    CONTROLF,HUND29;*80,PTCNWPKG
.
.---- exit program ----
.
ML9999    CHAIN     PGM
.----------------------------------------------------------------------
.  code includes
.----------------------------------------------------------------------
.
          INC       WEBPARIO/INC
          INC       WEBPKIIO/INC
          INC       WEBPKIPR
          INC       STDGENCD
.
