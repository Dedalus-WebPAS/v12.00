          CALL      RASCFM1
          IF        OVRCD=0
            CALL      DESCFM1
          ENDIF
          PACK      KEY11,SYKVSYS,SYKVFIL,SYKVKFLD,SP70
          PACK      KEY8,SYKVSYS,SYKVFIL,SYKVKFLD,SP70
          CALL      RSIBHLP1
DSYKV100  CALL      RKIBHLP1
          BRANCH    OVRCD,DSYKV150
          MATCH     KEY8,IBHPNAME
          GOTO      DSYKV150 IF NOT EQUAL
          PACK      KEY11,IBHPNAME,IBHPLINE
          CALL      DEIBHLP1
          GOTO      DSYKV100
.
DSYKV150
          RETURN

.------------------------------------------
. Keyin "Keyin Type"
.------------------------------------------
KSYKT000  MOVE     "Type : 0=Normal, 1=*JR, 2=*JR,*ZF, 3=Full Key",DISPMSG
          CALL     MESSAGE2
          KEYIN    *PCCOL:CVERT,*DV,SP20:
                   *PCCOL:CVERT,*V2LON,SYKVKTYP
          IF       SYKVKTYP>3
            BEEP
            GOTO     KSYKT000
          ENDIF
          CALL     MESSAGE3
          RETURN
.------------------------------------------
. Setup KEYTYPE for Display
.------------------------------------------
DSYKT000  BRANCH   SYKVKTYP,DSYKT100,DSYKT200,DSYKT300
          MOVE     "Normal    ",KEYTYPE
          GOTO     DSYKT999
DSYKT100
          MOVE     "*JR       ",KEYTYPE
          GOTO     DSYKT999
DSYKT200
          MOVE     "*JR,*ZF   ",KEYTYPE
          GOTO     DSYKT999
DSYKT300
          MOVE     "Full Key  ",KEYTYPE
DSYKT999  RETURN
.---------------------------------------------------
. Generate Keyin Routine
.---------------------------------------------------
GSYKY000  MOVE      ZERO,HLEF
          CALL      GETSCR00
          CALL      GENC0000
          CALL      PUTSCR00
          RETURN
.----------------------------------------------------------------------
. GENC0000 - Generate Keyin Code
.----------------------------------------------------------------------
GENC0000  PACK      KYNAME,SYSCID3,SYFIID3,KY,SP70
          SPLOPEN   KYNAME
          CALL      GENF0000           * Generate File Heading.
.
          PACK      KLABEL,ANSK,SYKVSYS,SYKVFIL,SYKVRID
          PACK      HLABEL,ANSH,SYKVSYS,SYKVFIL,SYKVHID
          PACK      KEYFLD,SYKVSYS,SYKVFIL,SYKVKFLD
          BRANCH    SYFITIO,GENC0100,GENC0200
.
          PACK      RACALLID,RA,SYSCSYS,SYFIFIL,SYKVIDX
          PACK      RDCALLID,RD,SYSCSYS,SYFIFIL,SYKVIDX
          GOTO      GENC0500
.
GENC0100  PACK      RACALLID,RA,SYSCSYS,SYFIID3,SYKVIDX
          PACK      RDCALLID,RD,SYSCSYS,SYFIID3,SYKVIDX
          GOTO      GENC0500
.
GENC0200  PACK      RACALLID,RDA,SYFIID3,SYFIID4,SYKVIDX
          PACK      RDCALLID,RD,SYFIID3,SYFIID4,SYKVIDX
          PACK      KEY8,SYKVSYS,SYKVFIL,SYKVKFLD
          CALL      RDSYFL2
          PACK      KEYFLD,SYFLV4N
.
GENC0500  PACK      DIM80,SP70,SP70
          CLEAR     DIM80
          MOVE      ZERO,COUNT
          MOVE      ZERO,KEYLEN
GENC1000  ADD       ONE,COUNT
          LOAD      SEGMENT,COUNT,SYIDFL1,SYIDFL2,SYIDFL3,SYIDFL4:
                                  SYIDFL5,SYIDFL6,SYIDFL7,SYIDFL8
          MATCH     SP70,SEGMENT
          GOTO      GENC1100 IF EQUAL
.
          PACK      KEY8,SYIDSYS,SYIDFIL,SEGMENT
          CALL      RDSYFL2
          ADD       SYFLPHY,KEYLEN
          APPEND    COMMA,DIM80
          COMPARE   TWO,SYFITIO
          GOTO      GENC1110 IF NOT EQUAL
          MOVE      SYFLV4N,KEY8
.
GENC1110  APPEND    KEY8,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
.
          COMPARE   EIGHT,COUNT
          GOTO      GENC1000 IF NOT EQUAL
.
GENC1100  MOVE      KEYLEN,DIM4
          SQUEEZE   DIM4
          PACK      PPACKVAL,SP70,SP70
          RESET     DIM80
          PACK      PPACKVAL,PPACKKEY,DIM4,DIM80
          COMPARE   "3",SYKVKTYP
          GOTO      GENC1200 IF NOT EQUAL
          CLEAR     KEYFLD
          APPEND    "KEY",KEYFLD
          APPEND    DIM4,KEYFLD
          RESET     KEYFLD
.
GENC1200  CALL      GENK0000 * Generate Standard Keyin
          SPLCLOSE
          PACK      PRNAME,SYSCID3,SYFIID3,KY,PRT
          PACK      KYNAME,SYSCID3,SYFIID3,KY,PBL
          DISPLAY   *P10:20;
          PACK      FILENAME,TEMPNAME,PORT
          REP       " 0",FILENAME
          PREP      TEMPFILE,FILENAME
          WRITE     TEMPFILE,SEQ;"/^",KLABEL,"00/-7,/^",KLABEL,"99/+1d"
          WRITE     TEMPFILE,SEQ;"w"
          WRITE     TEMPFILE,SEQ;"q"
          CLOSE     TEMPFILE
          PACK      COMMAND,ED,KYNAME,INPUT,FILENAME,TXT
          EXECUTE   COMMAND,TASKID
.
GENC2000  PACK      COMMAND,CAT,TBSPOOL,PRNAME,OUTPUT,KYNAME
          EXECUTE   COMMAND,TASKID
.
          PREP      TEMPFILE,FILENAME
          CLOSE     TEMPFILE
.
          PACK      COMMAND,RM,TBSPOOL,PRNAME
          EXECUTE   COMMAND,TASKID
.
          RETURN
.----------------------------------------------------------------------
. GENK0000 - Output Keyin Routine
.----------------------------------------------------------------------
GENK0000
          PRINT     KLABEL,"00 TOPIC     #"",SYKVSYS,SYKVFIL,SYKVKFLD,"#""
          PRINT     UNDMV,KEYFLD
          BRANCH    SYKVKTYP,GENK0100,GENK0200
          CALL      KNORM000
          GOTO      GENK0300
.
GENK0100  CALL      KEYJR000
          GOTO      GENK0300
.
GENK0200  CALL      KJRZF000
.
GENK0300  PRINT     PBLANK
          PRINT     PENDSET,KEYFLD
          PRINT     PAPPEND,KEYFLD
          PRINT     PRESET,KEYFLD
          PRINT     PBLANK
          PRINT     PMSP70,KEYFLD
          PRINT     PGOTO,KLABEL,"90",PIFEQU
.
          PRINT     PBLANK
          IF        SYKVKTYP=1
            PRINT     PENDSET,KEYFLD
          ENDIF
          IF        SYKVKTYP=2
            PRINT     PENDSET,KEYFLD
          ENDIF
          PRINT     PMEXIT,KEYFLD
          PRINT     PGOTO,KLABEL,"95",PIFEQU
.
          PRINT     PBLANK
          PRINT     PMQUEST,KEYFLD
          PRINT     PGOTO,KLABEL,"10",PIFNOTEQ
.
          PRINT     PBLANK
          PRINT     SAVCCITM
          PRINT     PCALL,HLABEL,"00"
          PRINT     RESCCITM
          PRINT     PBLANK
          PRINT     PENDSET,KEYFLD
          PRINT     PAPPEND,KEYFLD
          PRINT     PRESET,KEYFLD
          PRINT     PBLANK
          PRINT     PMSP70,KEYFLD
          PRINT     PGOTO,KLABEL,"00",PIFEQU
          PRINT     PBLANK
.
          PACK      DIM80,SP70,SP70
          CLEAR     DIM80
          APPEND    DISPLAYH,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    KEYFLD,DIM80
          RESET     DIM80
          PRINT     DIM80
.
          PRINT     KLABEL,"10"
          IF        SYKVKTYP=1
            PRINT     PRESET,KEYFLD
          ENDIF
          IF        SYKVKTYP=2
            PRINT     PRESET,KEYFLD
          ENDIF
          COMPARE   "3",SYKVKTYP
          GOTO      GENK1000 IF EQUAL
          PRINT     PPACKVAL
.
GENK1000  PRINT     PCALL,RACALLID
          PRINT     PBRANOV,KLABEL,"80"
          PRINT     PBRANTYP,KLABEL,"75"
          PRINT     PCALL,RDCALLID
          PRINT     KLABEL,"70"
          PRINT     PM0EXIT
          PRINT     PGOTO,KLABEL,"99"
.
          PRINT     KLABEL,"75"
          PACK      DIM80,SP70,SP70
          CLEAR     DIM80
          APPEND    SYKVKDES,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    PONFILE,DIM80
          RESET     DIM80
          PRINT     PDISER1,DIM80
          PRINT     PCALHIT
          PRINT     PGOTO,KLABEL,"00"
.
          PRINT     KLABEL,"80"
          PRINT     PM0EXIT
          PRINT     PBRANTYP,KLABEL,"99,",KLABEL,"99"
          PACK      DIM80,SP70,SP70
          CLEAR     DIM80
          APPEND    SYKVKDES,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    PDNEX,DIM80
          RESET     DIM80
          PRINT     PDISER1,DIM80
          PRINT     PCALHIT
          PRINT     PGOTO,KLABEL,"00"
.
          PRINT     KLABEL,"90"
          PRINT     PM1EXIT
          PRINT     PGOTO,KLABEL,"99"
.
          PRINT     KLABEL,"95"
          PRINT     PM2EXIT
.
          PRINT     KLABEL,"99 TOPIC    PRGID"
          PRINT     PRETURN
          RETURN
.
KNORM000  PACK      DIM80,SP70,SP70
          CLEAR     DIM80
          APPEND    KEYIN1,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    KEYFLD,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    COLON,DIM80
          RESET     DIM80
          PRINT     DIM80
.
          PACK      DIM80,SP70,SP70
          CLEAR     DIM80
          APPEND    KEYIN2,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    KEYFLD,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    COLON,DIM80
          RESET     DIM80
          PRINT     DIM80
.
          PRINT     KEYIN3,KEYFLD
          RETURN
.
KEYJR000  PACK      DIM80,SP70,SP70
          CLEAR     DIM80
          APPEND    KEYINJR1,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    KEYFLD,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    COLON,DIM80
          RESET     DIM80
          PRINT     DIM80
.
          PACK      DIM80,SP70,SP70
          CLEAR     DIM80
          APPEND    KEYINJR2,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    KEYFLD,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    COLON,DIM80
          RESET     DIM80
          PRINT     DIM80
.
          PRINT     KEYINJR3,KEYFLD
          RETURN
.
KJRZF000  PACK      DIM80,SP70,SP70
          CLEAR     DIM80
          APPEND    KEYINJZ1,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    KEYFLD,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    COLON,DIM80
          RESET     DIM80
          PRINT     DIM80
.
          PACK      DIM80,SP70,SP70
          CLEAR     DIM80
          APPEND    KEYINJZ2,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    KEYFLD,DIM80
          RESET     DIM80
          STRIP     DIM80
          ENDSET    DIM80
          APPEND    COLON,DIM80
          RESET     DIM80
          PRINT     DIM80
.
          PRINT     KEYINJZ3,KEYFLD
          RETURN
.
.----------------------------------------------------------------------
. GENF0000 - Output File Heading Details
.----------------------------------------------------------------------
GENF0000  PRINT     ".-----------------------------------------":
                    "----------------------------------"
          PRINT     ".   System    :   ",SYSCDES
          PRINT     ".   Include   :   ",SYSCID3,SYFIID3,"KY"
          PRINT     ".   Routine   :   K",SYSCSYS,SYFIFIL,SYKVRID:
                    "00 Keyin ",SYKVKDES
          PRINT     ".   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept,":
                    " 2 = Either"
          PRINT     ".   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered"
          PRINT     ".-----------------------------------------":
                    "--------------------------------------"
          RETURN
.

.------------------------------------
. Generate a Range of Keyin Routines
.------------------------------------
GRKV0000  MOVE      ZERO,HLEF
          CALL      GETSCR00
          CALL      RGEN0000
          CALL      PUTSCR00
          RETURN
.
RGEN0000  HLINE     *G37,17,1,80
          DISPLAY   *P1:18,*EF:
                    *P1:19,*ULON,*V2LON,"Generate a Range of Keyin Routines":
                    *P1:21,*HOFF,*EL,"First Sys/Fil/ID : ":
                    *P1:22,*EL,"Last  Sys/Fil/ID : "
          MOVE      "20",CCOL
          MOVE      "21",CVERT
          CALL      KSYKVB00
          BRANCH    EXIT,RGEN9000,RGEN9000
          PACK      FSTRID,KEY5
          DISPLAY   *PCCOL:CVERT,*V2LON,KEY5,SP1,*HOFF,SYKVKDES
.
          MOVE      "20",CCOL
          MOVE      "22",CVERT
          CALL      KSYKVB00
          BRANCH    EXIT,RGEN9000,RGEN9000
          PACK      LSTRID,KEY5
          DISPLAY   *PCCOL:CVERT,*V2LON,KEY5,SP1,*HOFF,SYKVKDES
.
          DISPLAY   *P1:24,*EL,"Process (":
                    *V2LON,"Y",*HOFF,"/":
                    *V2LON,"N",*HOFF,") ? "
          MOVE      "17",CCOL
          MOVE      "24",CVERT
          CALL      YORN0000
          COMPARE   ZERO,FYESNO
          GOTO      RGEN9000 IF EQUAL
.
          MOVE      FSTRID,KEY5
          CALL      RDSYKV1
          COMPARE   ZERO,OVRCD
          GOTO      RGEN1100 IF EQUAL
RGEN1000  CALL      RKSYKV1
          BRANCH    OVRCD,RGEN9000
          PACK      KEY5,SYKVSYS,SYKVFIL,SYKVRID
          MATCH     KEY5,LSTRID
          GOTO      RGEN9000 IF LESS
RGEN1100
          MOVE      SYKVSYS,KEY2
          CALL      RDSYSC1
          PACK      KEY4,SYKVSYS,SYKVFIL
          CALL      RDSYFI1
          PACK      KEY5,SYKVSYS,SYKVFIL,SYKVIDX
          CALL      RDSYID1
          PACK      KEY8,SYKVSYS,SYKVFIL,SYKVKFLD
          CALL      RDSYFL2
          PACK      KEY5,SYKVSYS,SYKVFIL,SYKVHID
          CALL      RDSYHV1
          PACK      KEY5,SYKVSYS,SYKVFIL,SYKVRID
          CALL      RDSYKV1
          DISPLAY   *P1:1,*ES,"Generating : ",*V2LON,KEY5,*HOFF,SP1,SYKVKDES
.
          CALL      GENC0000
.
          GOTO      RGEN1000
.
RGEN9000
RGEN9999  RETURN
