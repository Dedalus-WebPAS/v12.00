VKYPBL00  MOVE      ZERO,HLEF
          CALL      GETSCR00
          CLEAR     COMMAND
          APPEND    "vi ",COMMAND
          APPEND    SYSCID3,COMMAND
          APPEND    SYFIID3,COMMAND
          APPEND    "KY.PBL",COMMAND
          RESET     COMMAND
          EXECUTE   COMMAND,TASKID
          CALL      PUTSCR00
          RETURN

