          IF        @EQUAL
            CALL      RKNZRB11
          ELSE
            CALL      RKNZRBR1
          ENDIF
          BRANCH    OVRCD,INBV7000
.
          MATCH     NZRBHOSP,NZPFHOSP            * same hospital
          GOTO      INBV7000 IF NOT EQUAL
.
          MATCH     NZRBCLMC,NZPFCLMC            * same claim code
          GOTO      INBV7000 IF NOT EQUAL
.
          MATCH     NZRBCNTR,NZPFCNTR            * same health fund
          GOTO      INBV7000 IF NOT EQUAL
