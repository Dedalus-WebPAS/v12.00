          GOTO      SGLD0200 IF EQUAL
.
          ADD       ONE,INDEXCNT
          MOVE      BDFEE002,KEYSTARR[INDEXCNT]   * Single Health Fund
.
          BRANCH    BDFEE012,SGLD0100             * All Health Fund Table
.
          ADD       ONE,INDEXCNT
          MOVE      BDFEE015,KEYSTARR[INDEXCNT]   * Single Health Fund Table
.
