                    *P1:6,*V2LON," 3",*HOFF," = ",DLABLAY3:
                    *P1:7,*V2LON," 4",*HOFF," = ",DLABLAY4:
                    *P1:8,*V2LON," 5",*HOFF," = ",DLABLAY5:
                    *P1:9,*V2LON," 6",*HOFF," = ",DLABLAY6:
                    *P1:10,*V2LON," 7",*HOFF," = ",DLABLAY7:
                    *P1:11,*V2LON," 8",*HOFF," = ",DLABLAY8:
