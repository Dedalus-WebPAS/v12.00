                    *P41:15,*V2LON,"25",*HOFF,". Audit Y (Not used) :":
                    *P41:16,*V2LON,"26",*HOFF,". Audit Z (Not used) :"
.
          MOVE      ZERO,FORM2
          MOVE      THREE,CVERT
          MOVE      "26",CCOL
.
AUDLOOP1  ADD       ONE,FORM2
          ADD       ONE,CVERT
          COMPARE   "27",FORM2
          RETURN    IF EQUAL
.
          COMPARE   "17",CVERT
          GOTO      AUDPRNT1 IF LESS
.
          MOVE      FOUR,CVERT
          MOVE      "66",CCOL
.
