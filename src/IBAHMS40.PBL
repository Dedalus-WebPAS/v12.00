          PACK      FNAMEINP WITH SP70,SP70,SP70
.
          DISPLAY   *P26:11,*EF,"Upload Doctors eAccreditation File";
.
          KEYIN     *P1:13,*EL,"Upload Path & File Name : ",*V2LON,FNAMEINP
