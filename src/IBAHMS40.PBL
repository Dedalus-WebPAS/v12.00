            SQUEEZE   FNAMEINP
            ENDSET    FNAMEINP
            APPEND    ".txt",FNAMEINP              * add ".txt" if necessary
            APPEND    SP70,FNAMEINP
            RESET     FNAMEINP
          ENDIF
          RESET     FNAMEINP
.
          DISPLAY   *P27:13,*V2LON,*EL,FNAMEINP;
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
