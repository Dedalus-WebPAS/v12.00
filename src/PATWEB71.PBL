          IF        OVRCD=1
            MOVE      ZERO,PTELPPRI
            MOVE      ZERO,PTELPPUT
            MOVE      ZERO,PTELPSHA
            MOVE      ZERO,PTELPSUT
            MOVE      ZERO,PTELPSDY
            MOVE      ZERO,PTELPCCU
            MOVE      ZERO,PTELPSDU
            MOVE      ZERO,PTELPHSP
            MOVE      ZERO,PTELPHSU
            MOVE      ZERO,PTELPTHE
            MOVE      ZERO,PTELPTUT
            MOVE      ZERO,PTELXCSS
            MOVE      ZERO,PTELXCSD
