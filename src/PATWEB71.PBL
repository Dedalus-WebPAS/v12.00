          IF        IBCNMHOS=1
            MOVE      SP10,PMVXMHOS
            PACK      KEY8,ADMISSNO,SP10
            CALL      RDPMVX11
            MOVE      SP10,PTHSDCLM
            MOVE      SP10,PTHSTFEE
            PACK      KEY3,PMVXMHOS,SP10
            CALL      RDPTHSP1               * get Hospital Using Theatre Fee
          ENDIF
.
          MOVE      ADMISSNO,KEY8
          CALL      RDMISS1
          BRANCH    OVRCD,SADJ9200
.
