          MATCH     "1",PTCNISBZ         * Misc.item with quantity of zero?
          IF        !@EQUAL
            IF        PMMISERV=0
              MOVE       ONE,PMMISERV
            ENDIF
          ENDIF
.
          MOVE      SAVAMNT,LINETOT         * charge per day
          IF        SAVAMNT > MAXIDAYB
            MOVE      MAXIDAYB,LINETOT      * Max day charge
          ENDIF
          MULT      PMMISERV,LINETOT
          MOVE      LINETOT,FORM12P2
