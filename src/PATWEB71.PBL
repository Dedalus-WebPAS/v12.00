          PACK      KEY5,ANSC,ANSL,ACLAIM
          CALL      RDCODE1
          BRANCH    OVRCD,DELE9000
.
          MATCH     "A",TCINDC2
          GOTO      DELE9000 IF NOT EQUAL    * not ABF Funding
