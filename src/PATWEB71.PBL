          MATCH     ALACDTE,ADATE
          IF        @EQUAL
            MOVE      ATIME,TTIME
            MATCH     ATIME,CHNGTIME
            GOTO      SADJ9530 IF LESS
          ELSE
            CALL      CHKTIM00                 * Check /time 
            BRANCH    EXIT,SADJ9530
          ENDIF
.
SADJ0020  BRANCH    ASTAT,SADJ9310,SADJ0100,SADJ0100,SADJ9320,SADJ9330
          GOTO      SADJ9200
.
