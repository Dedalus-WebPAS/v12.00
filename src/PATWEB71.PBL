          REP       " +",ADMISSNO
          REP       " +",URNUMBER
          REP       " +",FILTHFND
          REP       " +",FILTCLAM
          REP       " +",FILTHOLD
.
          WRITE     HTMLFILE;"patweb71.pbl":
                                 "?reportno=",F1:
                                 "&template=",FLDPARA:
                                 "&startnum=",KEY3:
                                 "&norecord=",KEY2:
                                 "&filtvtyp=",FILTVTYP:
                                 "&filthfnd=",FILTHFND:
                                 "&filtclam=",FILTCLAM:
