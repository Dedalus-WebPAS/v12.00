          IF        IBCNUGST=2 | IBCNUGST=3
            MOVE      PMMIGSTA,PTDTGSTA
            MOVE      PMMIGSTC,PTDTGSTC
            CALL      IGST0000
            MOVE      LINETOT,LINEGST
            MULT      IBGEAMNT,LINEGST
            DIV       "100.00",LINEGST
          ENDIF
