.
          MOVE      SBFEND1,BFEND1
          MOVE      SBFEND2,BFEND2
          MOVE      SBFEND3,BFEND3
          MOVE      SBFEND4,BFEND4
          MOVE      SBFEND5,BFEND5
          MOVE      SBFEND6,BFEND6
.
          IF        CFEETYP=0
            CALL      UPKEY000            * Unpack into an array variable
            MOVE      ZERO,SAVEND
            IF        BFECOLNO<>0
              MOVE      BFENDARY[BFECOLNO],SAVEND
            ENDIF
          ELSE
            LOAD      SAVEND WITH BFECOLNO OF BFEND1,BFEND2,BFEND3,BFEND4:
