.   Clear Other Claimant Parameters
.------------------------------------------------------------------------
CLPMCL00  MOVE      Z70,PMCLT001
          MOVE      Z70,PMCLT002
          MOVE      Z70,PMCLT003
          MOVE      Z70,PMCLT004
          MOVE      Z70,PMCLT005
          MOVE      Z70,PMCLT006
          MOVE      Z70,PMCLT007
          MOVE      Z70,PMCLT008
          MOVE      Z70,PMCLT009
          MOVE      Z70,PMCLT010
          MOVE      Z70,PMCLT011
          MOVE      Z70,PMCLT012
          MOVE      Z70,PMCLT013
          MOVE      Z70,PMCLT014
          MOVE      Z70,PMCLT015
.
CLPMCL99  RETURN
.
.------------------------------------------------------------
. Other Claimant Tags
.------------------------------------------------------------
PMCL0000  BRANCH    FLDITMNO,PMCL0010,PMCL0020,PMCL0030,PMCL0040,PMCL0050:
                             PMCL0060,PMCL0070,PMCL0080,PMCL0090,PMCL0100:
                             PMCL0110,PMCL0120,PMCL0130,PMCL0140,PMCL0150
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      PMCL9999
.
PMCL0010  WRITE     HTMLFILE;PMCLWMOD;           * WebPAS Module
          GOTO      PMCL9999
.
PMCL0020  WRITE     HTMLFILE;PMCLINUM;           * Invoice Number
          GOTO      PMCL9999
.
PMCL0030  WRITE     HTMLFILE;PMCLSNAM;           * Claimant Family Nam
          GOTO      PMCL9999
.
PMCL0040  WRITE     HTMLFILE;PMCLGNAM;           * Claimant First Name
          GOTO      PMCL9999
.
PMCL0050  MATCH     PMCLCDOB,SP70                * Claimant Date of Birth
          GOTO      PMCL9999 IF EQUAL
.
          UNPACK    PMCLCDOB,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMCL9999
.
PMCL0060  WRITE     HTMLFILE;PMCLMCNO;           * Claimant Medicare Number
          GOTO      PMCL9999
.
PMCL0070  WRITE     HTMLFILE;PMCLCMRF;           * Medicare Reference Number
          GOTO      PMCL9999
.
PMCL0080  WRITE     HTMLFILE;PMCLADD1;           * Claimant Address Line 1
          GOTO      PMCL9999
.
PMCL0090  WRITE     HTMLFILE;PMCLADD2;           * Claimant Address Line 2
          GOTO      PMCL9999
.
PMCL0100  WRITE     HTMLFILE;PMCLADD3;           * Claimant Address Line 3
          GOTO      PMCL9999
.
PMCL0110  WRITE     HTMLFILE;PMCLADD4;           * Claimant Address Line 4
          GOTO      PMCL9999
.
PMCL0120  WRITE     HTMLFILE;PMCLPOST;           * Claimant Address Postcode
          GOTO      PMCL9999
.
PMCL0130  WRITE     HTMLFILE;PMCLBNAM;           * Bank Account Name
          GOTO      PMCL9999
.
PMCL0140  WRITE     HTMLFILE;PMCLBACN;           * Bank Account Number
          GOTO      PMCL9999
.
PMCL0150  WRITE     HTMLFILE;PMCLBBSB;           * BSB Code
          GOTO      PMCL9999
.
PMCL9999  RETURN
.
.------------------------------------------------------------
.   Set Claimant Parameters 
.------------------------------------------------------------
SPMCL000  MOVE      ZERO,EXIT
          UNPACK    QRYNAME,KEY5,KEY3
          MOVE      KEY3,F3
          BRANCH    F3,SPMCL001,SPMCL002,SPMCL003,SPMCL004,SPMCL005:
                       SPMCL006,SPMCL007,SPMCL008,SPMCL009,SPMCL010:
                       SPMCL011,SPMCL012,SPMCL013,SPMCL014,SPMCL015
.
          MOVE      ONE,EXIT
          GOTO      SPMCL999
.
SPMCL001  PACK      PMCLT001,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL002  PACK      PMCLT002,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL003  PACK      PMCLT003,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL004  PACK      PMCLT004,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL005  UNPACK    QRYDATA,CDAY,KEY1,MTHNAM3,KEY1,CCENT,CYEAR
          CALL      SETMTH00                *Set CMON from MTHNAM3
          PACK      PMCLT005,CCENT,CYEAR,CMON,CDAY
          GOTO      SPMCL999
.
SPMCL006  PACK      PMCLT006,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL007  PACK      PMCLT007,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL008  PACK      PMCLT008,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL009  PACK      PMCLT009,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL010  PACK      PMCLT010,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL011  PACK      PMCLT011,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL012  PACK      PMCLT012,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL013  PACK      PMCLT013,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL014  PACK      PMCLT014,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL015  PACK      PMCLT015,QRYDATA,SP70
          GOTO      SPMCL999
.
SPMCL999  RETURN
.
