.        INCLUDE NAME : PATDASIO.INC
.
.        FILE I/O REQUESTS FOR DISCHARGE & ADMISSION STATISTICS FILE
.
.OVRCD   FORM     1        0 = NOT OVER
.                          1 = OVER
.KEY11   DIM     11        KEY FOR PATDASA1
.
RAPTDAS1 MOVE     ZERO,OVRCD
         RESET    KEY11
         READ     PATDASA1,KEY11;ANS
         GOTO     OVERCOND IF OVER
         RETURN
.
RSPTDAS1 RESET    KEY11
         READ     PATDASA1,KEY11;;
         RETURN
.
RKPTDAS1 MOVE     ZERO,OVRCD
         READKS   PATDASA1;DPTDAREF,PTDADATE,PTDACODE,PTDAADLT,PTDACHLD:
                           PTDAMALE,PTDAFEML,PTDAUNKN,PTDAINDT,PTDAITSX:
                           PTDASPAR
         MOVE     DPTDAREF,PTDAREF
         GOTO     OVERCOND IF OVER
.
         RETURN
.
RPPTDAS1 MOVE     ZERO,OVRCD
         READKP   PATDASA1;DPTDAREF,PTDADATE,PTDACODE,PTDAADLT,PTDACHLD:
                           PTDAMALE,PTDAFEML,PTDAUNKN,PTDAINDT,PTDAITSX:
                           PTDASPAR
         MOVE     DPTDAREF,PTDAREF
         GOTO     OVERCOND IF OVER
.
         RETURN
.
RDPTDAS1 MOVE     ZERO,OVRCD
         RESET    KEY11
         READ     PATDASA1,KEY11;DPTDAREF,PTDADATE,PTDACODE,PTDAADLT,PTDACHLD:
                           PTDAMALE,PTDAFEML,PTDAUNKN,PTDAINDT,PTDAITSX:
                           PTDASPAR
         MOVE     DPTDAREF,PTDAREF
         GOTO     OVERCOND IF OVER
.
         RETURN
.
UPPTDAS1 MOVE     PTDAREF,DPTDAREF
         UPDATE   PATDASA1;DPTDAREF,PTDADATE,PTDACODE,PTDAADLT,PTDACHLD:
                           PTDAMALE,PTDAFEML,PTDAUNKN,PTDAINDT,PTDAITSX:
                           PTDASPAR
         RETURN
.
DEPTDAS1 RESET    KEY11
         DELETE   PATDASA1,KEY11
         RETURN
.
WRPTDAS1 MOVE     ZERO,OVRCD
         MOVE     PTDAREF,DPTDAREF
         RESET    KEY11
         WRITE    PATDASA1,KEY11;DPTDAREF,PTDADATE,PTDACODE,PTDAADLT,PTDACHLD:
                           PTDAMALE,PTDAFEML,PTDAUNKN,PTDAINDT,PTDAITSX:
                           PTDASPAR
.
         RETURN
.
