          MOVE     SP70,SIIEWAR
          CALL     RSSIIE2
          CALL     RKSIIE2
          MATCH    SIIEWAR,SIWAWAR
          IF       @EQUAL
            BEEP
            CLEAR     DISPMSG
            APPEND    "ERROR - Catalogs Linked to this Warehouse - ",DISPMSG
            CALL      MESSAGE1
            GOTO      DELT9100
          ENDIF
.
          PACK     KEY5,SIWAWAR
          CALL     RASIWA1
          IF       OVRCD=0
            CALL     DASIWA00               * Delete Before Audit
            MOVE     FOUR,AUDTTYPE
            CALL     WASIWA00               * Delete Audit
            CALL     DESIWA1                * delete record
          ENDIF
          GOTO     DELT9100
.
DELT9000  MOVE     ONE,EXIT                 * return to prompt
          GOTO     DELT9999
.
DELT9100  CALL     CANC0000
          MOVE     TWO,EXIT                 * restart
          GOTO     DELT9999
.
DELT9999  RETURN

CANC0000  PACK     KEY5,SIWAWAR
          CALL     RASIWA1
          IF       OVRCD=0
            CALL     DASIWA00                 * Delete Before Audit
            CALL     UUSIWA1                  * Unlovk
          ENDIF
          RETURN
KNPO0000
           KEYIN    *PCCOL:CVERT,*V2LON,NEXTPNO
           MOVE     NEXTPNO,SIWANPO
           RETURN
.
ESTPL000
          IF        HACCNT<>1
            IF        ACCEPT=1
              GOTO      ESTPL999
            ENDIF
            BEEP
            CLEAR     DISPMSG
            APPEND    "GL Interface Not Active - ",DISPMSG
            RESET     DISPMSG
            CALL      MESSAGE1
            GOTO      ESTPL999
          ENDIF
.
          MOVE        ZERO,CKEYTYP
          MOVE        ONE,CACCTIND
          MOVE        SIWALED,FMAMLEDG
          CALL        KFMAMA00
          BRANCH      EXIT,ESTPL900,ESTPL999
          MOVE        FMAMACCT,SIWASTA
          MOVE        FMAMDESC,STADESC
          GOTO        ESTPL999
.
ESTPL900  UNPACK      SP70,SIWASTA,STADESC
.
ESTPL999  RETURN
.
.--------------------------------------------------
. Enter Mark Up Percentage
.--------------------------------------------------
EMUAC000  IF        HACCNT<>1
            IF        ACCEPT=1
              GOTO      EMUAC999
            ENDIF
            BEEP
            CLEAR     DISPMSG
            APPEND    "GL Interface Not Active - ",DISPMSG
            RESET     DISPMSG
            CALL      MESSAGE1
            GOTO      EMUAC999
          ENDIF
          MOVE        ZERO,CKEYTYP
          MOVE        ONE,CACCTIND
          MOVE        SIWALED,FMAMLEDG
          CALL        KFMAMA00
          BRANCH      EXIT,EMUAC900,EMUAC999
          MOVE        FMAMACCT,SIWAMUAC
          MOVE        FMAMDESC,MUACDESC
          GOTO        EMUAC999
.
EMUAC900  UNPACK      SP70,SIWAMUAC,MUACDESC
.
EMUAC999  RETURN
.
