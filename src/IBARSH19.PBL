          MOVE      ZERO,OVRCD          * Assume successful read
          READ      WORK,KEY31;XMUFHOSP,XMUFUOGR,XMUFUSER,XMUFFAVG,XMUFGHED:
                               XMUFCODE,XMUFERRO
          GOTO      OVERCOND IF OVER    * Unsuccessful read
          RETURN
.
