          IF        !@EQUAL
            MATCH     FILTDPST,PMWODPST
            GOTO      EROWD999 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,FILTCLAI           * Claim code filter
          IF        !@EQUAL
            MATCH     FILTCLAI,ACLAIM
            GOTO      EROWD999 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,WARDCODE           * Ward filter
          IF        !@EQUAL
            MATCH     WARDCODE,AWARD
            GOTO      EROWD999 IF NOT EQUAL
          ENDIF
.
          MOVE      "023",ATRTYPE
          CALL      GPTATR00
          IF        NOTFOUND=1
            CALL      CLPATATR
          ENDIF
.
