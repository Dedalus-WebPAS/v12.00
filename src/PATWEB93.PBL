          PACK     KEY6,AWARD,SP70
          MATCH    SP70,AWARD
          IF       @EQUAL
            PACK      KEY14,AADMNO,SP70
            CALL      RDSWARD4
            CALL      RDKWARD4
            BRANCH    OVRCD,STANDP99
            MATCH     WSTBY,AADMNO
            GOTO      STANDP99 IF NOT EQUAL
            MOVE      WWARD,AWARD
            MOVE      WBED,ABED
            CLEAR     KEY10
            APPEND    SP1,KEY10
            APPEND    "(s)",KEY10
            RESET     KEY10
          ENDIF
          GOTO     STANDP99
