          MOVE      SP70,RCDTRELD
          MOVE      SP70,RCDTRELT
          PACK      RCDTCDAT,CCC,CYY,CMM,CDD
          REP       " 0",RCDTCDAT
          MOVE      SP70,RCDTPRAC
          MOVE      CTIMEIS,RCDTCTIM
          MOVE      WBSEUID,RCDTCUID
          MOVE      SP70,RCDTUDAT
          MOVE      SP70,RCDTUTIM
          MOVE      SP70,RCDTUUID
          MOVE      SP70,RCDTDPTY
          MOVE      SP70,RCDTGLEX
          MOVE      PMECBATN,RCDTBATN
          MOVE      SP70,RCDTSPAR
