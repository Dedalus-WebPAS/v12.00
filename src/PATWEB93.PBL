            ENDIF
          ENDIF 
.
          CALL      GETRGM00
.
          IF        OVERRIND=1
            MATCH     "0",PTBDOVER
            GOTO      BDAR1000 IF NOT EQUAL
          ENDIF
.
          IF        OVERRIND=2
