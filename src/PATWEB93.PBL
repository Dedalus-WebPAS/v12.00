UPATRA00  CALL      CLPATATR
          MOVE      AURNO,PTARURNO
          MOVE      DAADMNO,PTARVISN
          MOVE      "023",PTARTYPE
          PACK      PTARDATE,CCC,CYY,CMM,CDD
          REP       " 0",PTARDATE
