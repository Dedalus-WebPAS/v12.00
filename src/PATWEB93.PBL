.
          WRITE     HTMLFILE;"<td>",DOCFNAME,"&nbsp;</td>";
.                             "<td>",PFUNDH,SLASH,PFNDTB,"&nbsp;</td>";
.
          PACK      KEY10,ADMISSNO,SP1,ONE,DIM127
          CALL      RDPTRGM1
          IF        OVRCD<>1
