                         " class=ListIcon onclick=displayRemH('",PMEHDATE,"','":
      PMEHFTID,"','",PMEHRADV,"','",PMEHPNUM,"','",PMEHHOSP,"','002');> ",DIM11," ",PMEHNAME,"#",";
            ENDIF
.
          ELSE
.
        WRITE     HTMLFILE;"t.addRow(#"","<img src='../images/CommentIcon.gif'":
                         " class=ListIcon onclick=displayRemH('",PMEHDATE,"','":
      PMEHFTID,"','",PMEHRADV,"','",PMEHPNUM,"','",PMEHHOSP,"','002');> ",DIM11," ",PMEHNAME,"#",";
          ENDIF
