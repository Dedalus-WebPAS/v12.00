              ELSE
                MATCH     SP70,WEXTN
                IF        !@EQUAL
                  STRIP     WEXTN
                  WRITE     HTMLFILE;" - ",*+,WEXTN,*-;
                ENDIF
              ENDIF
            ELSE
.
.             No linked Nursing Station
.
              MATCH     SP70,WEXTN
              IF        !@EQUAL
                STRIP     WEXTN
                WRITE     HTMLFILE;" - ",*+,WEXTN,*-;
              ENDIF
            ENDIF
          ELSE
.
.           Bed/Ward...
