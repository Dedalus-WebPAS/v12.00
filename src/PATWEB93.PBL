          MATCH     STATUS,OPTIONZ
          GOTO      BTABA100 IF NOT EQUAL  
.
BTABA200  PACK      KEY5,CAT,BKRETYPE
          CALL      RDCODE1
          BRANCH    OVRCD,BTABA300
          MATCH     TCODE,CAT
          GOTO      BTABA300 IF NOT EQUAL
          MATCH     ACODE,BKRETYPE 
          GOTO      BTABA300 IF NOT EQUAL               
          MATCH     "THE ",THCSCOD
          IF        @EQUAL
            PACK      KEY31,BKREBOOK,SP70
            CALL      RSOPDEA2
            CALL      RKOPDEA2 
            BRANCH    OVRCD,BTABA300
            MOVE      BKREBOOK,DIM8
            MATCH     DIM8,OPDAADMN
            GOTO      BTABA300 IF NOT EQUAL
          ENDIF 
          MOVE      ZERO,TFLAG
          GOTO      BTABA400  
.
