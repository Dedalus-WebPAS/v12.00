          MATCH     "1",PMVXVALW
          IF        @EQUAL
            MOVE      ANSV,DIM1
          ENDIF
          APPEND    LBRACKT,VISPHONE
          APPEND    DIM1,VISPHONE
          APPEND    COMMA,VISPHONE
.
          MOVE      ANSN,DIM1
          MATCH     "1",PMVXPALW
          IF        @EQUAL
            MOVE ANSP,DIM1
          ENDIF
          APPEND    DIM1,VISPHONE
          APPEND    RBRACKT,VISPHONE
          RESET     VISPHONE
