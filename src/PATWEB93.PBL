          BRANCH    OVRCD,ECRH9000
.
          PACK      KEY13,USERID,SP70                * All hospital access
          CALL      RDMLSEC1
          IF        OVRCD=1
            PACK      KEY13,USERID,PTHSHOSP,SP70     * This hospital access
            CALL      RDMLSEC1
            BRANCH    OVRCD,ECRH5000
          ENDIF
.
          MOVE      PTHSHOSP,FILTHOSP
          CALL      ECRT0000
.
          PACK      KEY3,FILTHOSP,SP70               * reposition
          CALL      RSPTHSP1
          GOTO      ECRH5000
.
