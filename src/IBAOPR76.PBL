          OPEN      PATHSPA1,"pathspaf"
          OPEN      PATTFEE1,"pattfeef"
          OPEN      PATMCHG1,"patmchgf"
          OPEN      PATWR1A1,"patwr1af"
          OPEN      OPRUPLA1,"opruplaf"
