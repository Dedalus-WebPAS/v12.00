          MATCH     SP70,PMSGC004
          IF        !@EQUAL
            MATCH     PMSGC004,PMSGICDC
            GOTO      SGATB100 IF NOT EQUAL   * Yes, check next record
          ENDIF
.
          ADD       ONE,RECNUMB
          IF        STARTNUM>=RECNUMB
            GOTO     SGATB100
          ENDIF
.
