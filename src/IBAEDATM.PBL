IBEA0000  MOVE      ZERO,EXIT
          OPEN      PATEORA1,"pateoraf"
          OPEN      PATEORA2,"pateoraf"
          OPEN      IBAEDAA1,"ibaedaaf"
.
.                              UR      VISIT    COUNT     Y/N      0->9 
IBEA0001  BRANCH    FLDITMNO,IBEA0010,IBEA0020,IBEA0030,IBEA0040,IBEA0050:
.                             00->99    DIM6     DIM6     DIM8     DIM8     
                             IBEA0060,IBEA0070,IBEA0080,IBEA0090,IBEA0100:
.                              DIM10   DIM50    DIM70    DIM200
                             IBEA0110,IBEA0120,IBEA0130,IBEA0140,IBEA0150:
                             IBEA0160,IBEA0170,IBEA0180,IBEA0190
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      IBEA9999
.         
IBEA0010  WRITE     HTMLFILE;IBEAURNO;
          GOTO      IBEA9999
.
IBEA0020  WRITE     HTMLFILE;IBEAVISN;
          GOTO      IBEA9999
.
IBEA0030  WRITE     HTMLFILE;IBEACNTR;
          GOTO      IBEA9999
.
IBEA0040  UNPACK    DIM127,D1,KEY2,DIM127
          MOVE      ZERO,F1
          MOVE      ZERO,F2
          MOVE      KEY2,F2
          MOVE      "0",EDAFLAG
          LOAD      EDAFLAG,F2,IBEAFL01,IBEAFL02,IBEAFL03,IBEAFL04,IBEAFL05:
                               IBEAFL06,IBEAFL07,IBEAFL08,IBEAFL09,IBEAFL10:
                               IBEAFL11,IBEAFL12,IBEAFL13,IBEAFL14,IBEAFL15:
                               IBEAFL16,IBEAFL17,IBEAFL18,IBEAFL19,IBEAFL20:
                               IBEAFL21,IBEAFL22,IBEAFL23,IBEAFL24,IBEAFL25:
                               IBEAFL26,IBEAFL27,IBEAFL28,IBEAFL29,IBEAFL30:
                               IBEAFL31,IBEAFL32,IBEAFL33,IBEAFL34,IBEAFL35:
                               IBEAFL36,IBEAFL37,IBEAFL38,IBEAFL39,IBEAFL40:
                               IBEAFL41,IBEAFL42,IBEAFL43,IBEAFL44,IBEAFL45 
.
          UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      KEY1,F1                   * output yes/no or a description
          BRANCH    F1,IBEA0042
.
          WRITE     HTMLFILE;EDAFLAG;         * Assessment Flag (0=No,1=Yes)
          GOTO      IBEA9999
.
IBEA0042  MOVE      ZERO,F1
          MOVE      EDAFLAG,F1
          MATCH     "0",EDAFLAG
          IF        @EQUAL
            WRITE     HTMLFILE;NO;            * Assessment Flag (0=No)
          ENDIF
          MATCH     "1",EDAFLAG
          IF        @EQUAL
            WRITE     HTMLFILE;YES;           * Assessment Flag (1=Yes)
          ENDIF
          GOTO      IBEA9999
.
.
IBEA0050  UNPACK    DIM127,D1,KEY2,DIM127
          MOVE      ZERO,F1
          MOVE      ZERO,F2
          MOVE      KEY2,F2
          MOVE      "0",EDAFLAG
          LOAD      EDAFLAG,F2,IBEACD01,IBEACD02,IBEACD03,IBEACD04,IBEACD05 
.
          UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      KEY1,F1                   * output number or a description
          BRANCH    F1,IBEA0052
.
          WRITE     HTMLFILE;EDAFLAG;         * Numeric Code 0 to 9
          GOTO      IBEA9999

IBEA0052  PACK      DIM20A,SP70
          MOVE      EDAFLAG,F1
          BRANCH    F2,IBEA0054,IBEA0055,IBEA0056,IBEA0057,IBEA0058 
.
          MOVE      "Unknown",DIM20A
          GOTO      IBEA0059
.
IBEA0054  LOAD      DIM20A,F1,PLC001,PLC002,PLC003
          GOTO      IBEA0059
.
IBEA0055  LOAD      DIM20A,F1,ENG001,ENG002,ENG003,ENG004,ENG005,ENG006
          GOTO      IBEA0059
.
IBEA0056  LOAD      DIM20A,F1,ENG001,ENG002,ENG003,ENG004,ENG005,ENG006
          GOTO      IBEA0059
.
IBEA0057  LOAD      DIM20A,F1,ATT001,ATT002,ATT003,ATT004
          GOTO      IBEA0059
.
IBEA0058  LOAD      DIM20A,F1,TEST01,TEST02,TEST03
          GOTO      IBEA0059
.
IBEA0059  WRITE     HTMLFILE;DIM20A;
          GOTO      IBEA9999
.
.
IBEA0060  UNPACK    DIM127,D1,KEY2,DIM127
          MOVE      ZERO,F1
          MOVE      ZERO,F2
          MOVE      KEY2,F2
          MOVE      "00",EDAFLG2
          LOAD      EDAFLG2,F2,IBEALC01,IBEALC02,IBEALC03,IBEALC04
          REP       " 0",EDAFLG2
.
          UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      KEY1,F1                   * output code or a description
          BRANCH    F1,IBEA0062
.
          WRITE     HTMLFILE;EDAFLG2;         * User Long Code (00 to 99)    
          GOTO      IBEA9999
.
IBEA0062  PACK      DIM20A,SP70
          BRANCH    F2,IBEA0064,IBEA0065,IBEA0066,IBEA0067
.
          MOVE      "Unknown",DIM20A
          GOTO      IBEA0069
.
IBEA0064  MOVE      EDAFLG2,F2
          LOAD      DIM20A,F2,YEAR01,YEAR02,YEAR03,YEAR04,YEAR05,YEAR06:
                              YEAR07,YEAR08,YEAR09,YEAR10,YEAR11,YEAR12:
                              YEAR13,YEAR14
          GOTO      IBEA0069
.
IBEA0065  MOVE      EDAFLG2,F2
          LOAD      DIM20A,F2,TEST01,TEST02,TEST03,TEST01,TEST02,TEST03:
                              TEST01,TEST02,TEST03
          GOTO      IBEA0069
.
IBEA0066  MOVE      EDAFLG2,F2
          LOAD      DIM20A,F2,TEST01,TEST02,TEST03,TEST01,TEST02,TEST03:
                              TEST01,TEST02,TEST03
          GOTO      IBEA0069
.
IBEA0067  MOVE      EDAFLG2,F2
          LOAD      DIM20A,F2,TEST01,TEST02,TEST03,TEST01,TEST02,TEST03:
                              TEST01,TEST02,TEST03
          GOTO      IBEA0069
.
IBEA0069  WRITE     HTMLFILE;DIM20A;
          GOTO      IBEA9999
.
.                                           * read thru External Org. Table
IBEA0070  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      ZERO,F1
          MOVE      KEY1,F1
          BRANCH    F1,IBEA0072,IBEA0074
.
          WRITE     HTMLFILE;IBEASD01;           * DIM6
          GOTO      IBEA9999
.
IBEA0072  MOVE      "School not found",KEY30     * get School Name
          PACK      KEY7,ANSC,IBEASD01
          CALL      RDPTEOR1
          BRANCH    OVRCD,IBEA0073
          MOVE      PTERNAME,KEY30
IBEA0073  WRITE     HTMLFILE;KEY30
          GOTO      IBEA9999
.
IBEA0074  PACK      KEY7,ANSC,SP10               * create School list
          CALL      RSPTEOR1
IBEA0075  CALL      RKPTEOR1
          BRANCH    OVRCD,IBEA9999
.                                             
          MATCH     ANSC,PTERTYPE
          GOTO      IBEA9999 IF NOT EQUAL
.
          CLEAR     KEY40,
          APPEND    "(",KEY40
          APPEND    PTERCODE,KEY40
          APPEND    ")  ",KEY40
          APPEND    PTERNAME,KEY40
          RESET     KEY40
          MOVE      PTERNAME,KEY30
          PACK      KEY8,QUOTE,PTERCODE,QUOTE
          MATCH     IBEASD01,PTERCODE
          IF        @EQUAL
            WRITE     HTMLFILE;"<option value=",KEY8," selected>":
                               KEY40,"</option>"
          ELSE
            WRITE     HTMLFILE;"<option value=",KEY8,">",KEY40,"</option>"
          ENDIF
          GOTO      IBEA0075
.
.
IBEA0080  WRITE     HTMLFILE;IBEASD02;                * DIM6
          GOTO      IBEA9999
.
IBEA0090  WRITE     HTMLFILE;IBEASD03;                * DIM8
          GOTO      IBEA9999
.
IBEA0100  WRITE     HTMLFILE;IBEASD04;                * DIM8
          GOTO      IBEA9999
.
IBEA0110  WRITE     HTMLFILE;IBEASD05;                * DIM10
          GOTO      IBEA9999
.
.
IBEA0120  UNPACK    DIM127,D1,KEY2,DIM127
          MOVE      SP70,KEY50
          MOVE      KEY2,F2
          LOAD      KEY50,F2,IBEAMD01,IBEAMD02,IBEAMD03,IBEAMD04,IBEAMD05 
.
          WRITE     HTMLFILE;KEY50;         * DIM 50 Free Format Field
          GOTO      IBEA9999
.
IBEA0130  UNPACK    DIM127,D1,KEY2,DIM127
          MOVE      SP70,KEY70
          MOVE      KEY2,F2
          LOAD      KEY70,F2,IBEALD01,IBEALD02,IBEALD03,IBEALD04,IBEALD05:
                             IBEALD06,IBEALD07,IBEALD08,IBEALD09,IBEALD10
.
          WRITE     HTMLFILE;KEY70;         * DIM70 Free Format Field
          GOTO      IBEA9999
.
.
IBEA0140  UNPACK    DIM127,D1,KEY2,DIM127
          MOVE      KEY2,F2
          LOAD      DIM200A,F2,IBEAXL01,IBEAXL02,IBEAXL03,IBEAXL04,IBEAXL05:
                             IBEAXL06,IBEAXL07,IBEAXL08
.
          WRITE     HTMLFILE;DIM200A;       * DIM200 Free Format Field
          GOTO      IBEA9999
.
.
IBEA0150  MATCH     IBEACDAT,SP70           * Assessment date
          GOTO      IBEA9999 IF EQUAL
          UNPACK    IBEACDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR
          GOTO      IBEA9999
.
IBEA0160  WRITE     HTMLFILE;IBEACTIM;
          GOTO      IBEA9999
.
IBEA0170  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      ZERO,F1
          MOVE      KEY1,F1
          BRANCH    F1,IBEA0172
.
          WRITE     HTMLFILE;IBEACUID;        * User ID
          GOTO      IBEA9999
.
IBEA0172  PACK      WBSENAM,IBEACUID,SP30
          PACK      KEY10,IBEACUID,SP10
          CALL      RDWBSE1
          WRITE     HTMLFILE;WBSENAM;         * User Name
          GOTO      IBEA9999
.
IBEA0180  MOVE      "School not found",KEY60     * get & format School Address
          PACK      KEY7,ANSC,IBEASD01
          CALL      RDPTEOR1
          BRANCH    OVRCD,IBEA0189
          STRIP     PTERADD1
          STRIP     PTERADD2
          STRIP     PTERADD3
          STRIP     PTERPOST
          PACK      KEY60,PTERADD1,SP2,PTERADD2,SP2,PTERADD3,SP2,PTERPOST
IBEA0189  WRITE     HTMLFILE;KEY60
          GOTO      IBEA9999
.
IBEA0190  MOVE      SP30,KEY60             * get & format Phone & Region
          PACK      KEY7,ANSC,IBEASD01
          CALL      RDPTEOR1
          BRANCH    OVRCD,IBEA0199
          MOVE      "Phone ",DIM6  
          MOVE      "Reg. - ",DIM10 
          MOVE      SP30,KEY30
          MATCH     SP3,PTERREGN
          IF        !@EQUAL
            UNPACK    SP70,TDESC
            PACK      KEY5,CATEF,PTERREGN,SP5
            CALL      RDCODE1
            PACK      KEY30,TDESC,SP30
          ENDIF
          PACK      KEY60,DIM6,PTERTELP,SP2,DIM10,SP2,KEY30
IBEA0199  WRITE     HTMLFILE;KEY60
          GOTO      IBEA9999
.
.
IBEA9999  RETURN
