.                                           * Medicare Eligibility Status
PROC1013  MOVE      "9",TRANMEST            * Medicare Elig. Status - Not Stated
          PACK      KEY5,ANST,SP1,PTYPE
          CALL      RDCODE1                 * Cat."T "
          IF        OVRCD = 0
            MATCH     "1",TCINDC1
            IF        @EQUAL
              MOVE      "1",TRANMEST        * Medicare Elig. Status - Eligible
            ELSE
              MOVE      "2",TRANMEST        * Medicare Elig. Status - Ineligible
            ENDIF
          ENDIF
