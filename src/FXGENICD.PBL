.****************************************************************************
.*      Regeneration of ICD9 Disease Key Word Index                         *
.****************************************************************************
DNINE000  DISPLAY   *P1:24,*EL,"Recalibrating Key Word Index ..."
          MOVE      ZERO,RECNUM
          PACK      KEY24,SP70
          CALL      RSPTDI1
DNINE100  CALL      RKPTDI1
          BRANCH    OVRCD,DNINE200
          ADD       ONE,RECNUM
          IF        RECNUM%100=0
            DISPLAY       *P40:24,*V2LON,RECNUM
          ENDIF
          PACK      KEY24,PTDIICD,PTDIKWD,SP70
          CALL      DEPTDI1
          GOTO      DNINE100
.
DNINE200  DISPLAY   *P1:24,*EL,"Creating ICD9 Disease Keyword Index..."
          MOVE      ZERO,RECNUM
          PACK      KEY9,SP70
          CALL      RS10T9D1
DNINE300  CALL      RK10T9D1
          BRANCH    OVRCD,DNINE999
          ADD       ONE,RECNUM
          IF        RECNUM%100=0
            DISPLAY       *P40:24,*V2LON,RECNUM
          ENDIF
          PROC      PATDICUP       * Update ICD9 Disease Keyword Index
          GOTO      DNINE300
.
DNINE999  RETURN
