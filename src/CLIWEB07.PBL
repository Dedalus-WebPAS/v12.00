CODRW000  MATCH     SP70,HLCODES 
          IF        @EQUAL
            MOVE      "&nbsp;",TABDES
          ELSE
            MOVE      HLCODES,TABDES
          ENDIF
.
.          MATCH     SP70,HLCOCAT         * IBA Category
.          IF        @EQUAL 
.            MOVE      "&nbsp;",KEY2  
.          ELSE
.            MOVE      HLCOCAT,KEY2
.            MOVE      "-",KEY1
.          ENDIF
.
.          MATCH     SP70,HLCOICD        * IBA Code 
.          IF        @EQUAL 
.            MOVE      "&nbsp;",KEY3  
.          ELSE
.            MOVE      HLCOICD,KEY3
.          ENDIF
.                                             * Packed with Category & Code
.          PACK      KEY8,KEY2,KEY1,KEY3      * format = XX - XXX 
.
          WRITE     HTMLFILE;"<tr><td nowrap><A HREF='javascript:ShowDetail04":
                             "(#"",LINKPRG,"#.pbl":
                             "?reportno=",LINKREP:
                             "&template=",LINKTEM:
                             "&hltab001=",TABLID:
                             "&hlcod001=",HLCOCOD,"#")'>":
                             "<img src=../images/MaintenanceIcon.gif hspace=4":
                             " border=0 align=absmiddle></a>":
                             HLCOCOD,"</td>":
                             "<td>",TABDES,"</td>";
          MATCH     "0074",TABLID
          IF        @EQUAL
            MATCH     SP70,HLCOICD
            IF        !@EQUAL
              PACK      KEY5,CATDG,HLCOICD,SP70
              CALL      RDCODE1
              WRITE     HTMLFILE;"<td>",TDESC,"</td>";
            ELSE
              WRITE     HTMLFILE;"<td>&nbsp;</td>";
            ENDIF
          ENDIF
. * Category/Code column       "<td>",KEY8,"</td>": *****************
          WRITE     HTMLFILE;"</tr>"
.
CODRW999 RETURN
