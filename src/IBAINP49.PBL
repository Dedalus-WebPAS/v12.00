          ADD       ONE,NUMBER
          PRINT     *1,"|",*12,NUMBER,*15,"Sub Total (=15+16)":
                    *44,"| ",ST2P2CA,*53,"| ",ST2P2CB,*62,"| ",ST2P2CC:
                    *71,"| ",ST2P2CD,*80,"|"
