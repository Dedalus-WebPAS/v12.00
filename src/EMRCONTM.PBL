                             EMCN1310,EMCN1320,EMCN1330,EMCN1340,EMCN1350:
                             EMCN1360,EMCN1370,EMCN1380,EMCN1390,EMCN1400:
                             EMCN1410,EMCN1420,EMCN1430,EMCN1440,EMCN1450:
                             EMCN1460,EMRCN047,EMRCN048,EMRCN049,EMRCN050:
                             EMRCN051,EMRCN052
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      EMCN9999
.
EMCN1010  WRITE     HTMLFILE;EMCNWTTM;  * Emergency waiting time parameter
          GOTO      EMCN9999 
.
EMCN1020  WRITE     HTMLFILE;EMCNDOCT;  * Use patdo1af or emrdocaf file    
          GOTO      EMCN9999 
.
EMCN1030  WRITE     HTMLFILE;EMCNBAKH;  * Allow backdating before X hours  
          GOTO      EMCN9999 
.
EMCN1040  WRITE     HTMLFILE;EMCNLOSW;  * Emergency LOS warning X minutes  
          GOTO      EMCN9999
.
EMCN1050  WRITE     HTMLFILE;EMCNA44M;  * PAS-HL7 Send A44 (change U/R)
          GOTO      EMCN9999 
.
EMCN1060  WRITE     HTMLFILE;EMCNA03M;  * PAS-HL7 Send A03 (discharge)
          GOTO      EMCN9999 
.
EMCN1070  WRITE     HTMLFILE;EMCNA04M;  * PAS-HL7 Send A04 (register visit)
          GOTO      EMCN9999 
.
EMCN1080  WRITE     HTMLFILE;EMCNA08M;  * PAS-HL7 Send A08 (update visit)
          GOTO      EMCN9999 
.
EMCN1090  WRITE     HTMLFILE;EMCNA11M;  * PAS-HL7 Send A11 (cancel visit)
          GOTO      EMCN9999 
.
EMCN1100  WRITE     HTMLFILE;EMCNA13M;  * PAS-HL7 Send A13 (cancel discharge)
          GOTO      EMCN9999 
.
EMCN1110  WRITE     HTMLFILE;EMCNAPRO;  * Add Procedure after invoiced
          GOTO      EMCN9999
.
EMCN1120  WRITE     HTMLFILE;EMCNDPOC;  * Display Out of pocket running total
          GOTO      EMCN9999
.
EMCN1130  WRITE     HTMLFILE;EMCNOPOC;  * Out of pocket ceiling amount
          GOTO      EMCN9999
.
EMCN1140  WRITE     HTMLFILE;EMCNGINV;  * Group Invoice by Service Doctor
          GOTO      EMCN9999
.
EMCN1150  WRITE     HTMLFILE;EMCNRETC;  * Using Retain Cubicle             
          GOTO      EMCN9999
.
EMCN1160  WRITE     HTMLFILE;EMCNRINV;  * Redirect to invoice after discharge  
          GOTO      EMCN9999
.
EMCN1170  WRITE     HTMLFILE;EMCNDPRS;  * Display Diagnosis Over Presenting Comp
          GOTO      EMCN9999
.
EMCN1180  WRITE     HTMLFILE;EMCNPCEL;  * Display 3 Line Patient Cell  
          GOTO      EMCN9999
.
EMCN1190  WRITE     HTMLFILE;EMCNEVRM;  * Extended Visit Reason In X Minutes
          GOTO      EMCN9999
.
