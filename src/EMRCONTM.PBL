          GOTO      EMCN9999
.
EMCN1210  WRITE     HTMLFILE;EMCNMSRS;  * Display Same Surnames in RED Emr MS
          GOTO      EMCN9999
.
EMCN1220  WRITE     HTMLFILE;EMCNDEDI;  * Emr Diagnosis / Bed Req  RED Emr MS
          GOTO      EMCN9999
.
EMCN1230  WRITE     HTMLFILE;EMCNTCAT;  * Using Triage Category 9
          GOTO      EMCN9999
.
EMCN1240  WRITE     HTMLFILE;EMCNNWBR;  * No Warning when Making a Bed Request
          GOTO      EMCN9999
.
EMCN1250  WRITE     HTMLFILE;EMCNLDIA;  * Using Last Diagnosis Only
          GOTO      EMCN9999
.
EMCN1260  WRITE     HTMLFILE;EMCNDDSS;  * Display discharged patients in Short 
          GOTO      EMCN9999            * Stay if admission ward is EOU or EMU
.
EMCN1270  WRITE     HTMLFILE;EMCNSFSM;  * Display Red Sad Face after x minutes 
          GOTO      EMCN9999            * for Short Stay Unit
.
EMCN1280  WRITE     HTMLFILE;EMCNEDFD;  * ED Fax Scheduler Delay (mins)
          GOTO      EMCN9999            
.
EMCN1290  WRITE     HTMLFILE;EMCNEDFP;  * ED Fax Printer
          GOTO      EMCN9999            
.
EMCN1300  WRITE     HTMLFILE;EMCNEDST;  * ED Fax Printer
          GOTO      EMCN9999
.
EMCN1310  WRITE     HTMLFILE;EMCNDADR;  * Default Admitting Dr from Ward/Bed Req
          GOTO      EMCN9999
.
EMCN1320  WRITE     HTMLFILE;EMCNSCLK;  * Stop the clock 
          GOTO      EMCN9999
.
EMCN1330  WRITE     HTMLFILE;EMCNLODD;  * Display Additional Information in  
.                                         Emergency Location Dropdown         
          GOTO      EMCN9999
.
EMCN1340  WRITE     HTMLFILE;EMCNDFSD;  * Default first seen date/tim to adm 
          GOTO      EMCN9999
.
EMCN1350  WRITE     HTMLFILE;EMCNUDHF;  * Using the Doctor Handover Form
          GOTO      EMCN9999
.
EMCN1360  WRITE     HTMLFILE;EMCNMAPH;  * Maximum age for Paediatric Hydration
          GOTO      EMCN9999
.
EMCN1370  WRITE     HTMLFILE;EMCNURSE;  * Use oprnuraf/pmshcpaf for ED Nurse
          GOTO      EMCN9999
.
EMCN1380  WRITE     HTMLFILE;EMCNSEIC;  * Send EMR ICD10 Codes in HL7
          GOTO      EMCN9999
.
EMCN1390  WRITE     HTMLFILE;EMCNDDCT;  * Default Discharge Comments with
.                                         Discharge Text
          GOTO      EMCN9999
.
EMCN1400  WRITE     HTMLFILE;EMCDTHRS;  * Disable restrictions for Telehealth
.                                         visits on the ED Map (StV Map Only)
          GOTO      EMCN9999
.
EMCN1410  WRITE     HTMLFILE;EMCNPBSD;  * Patient Banner Seen Date/Time
          GOTO      EMCN9999
.
EMCN1420  WRITE     HTMLFILE;EMCNPTTI;  * Patient Toggle to check if they were
.                                         recently in IP or OP
          GOTO      EMCN9999
.
EMCN1430  WRITE     HTMLFILE;EMCNTINU;  * Time in Hours From Most Recent Visit
          GOTO      EMCN9999
.
EMCN1440  WRITE     HTMLFILE;EMCNALEX;  * Allow a current ED Patient to be a
.                                       * new Expected Pat at another ED Site 
          GOTO      EMCN9999
.
EMCN1450  UNPACK    EMCNSDSA,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
.
          GOTO      EMCN9999
.
EMCN1460  WRITE     HTMLFILE;EMCNSEDR;  * Service Event Delay Reason Cat (EDWRD)
.
          GOTO      EMCN9999
.
EMRCN047 WRITE     HTMLFILE;EMCNHSID;  * Display EDWARD ED Extract
         GOTO      EMCN9999
.
EMRCN048 WRITE     HTMLFILE;EMCNHISA;  * Using emergency history audit
         GOTO      EMCN9999
.
EMRCN049 WRITE     HTMLFILE;EMCNPCTY;  * expected patients contact types
         GOTO      EMCN9999
.
EMRCN050 WRITE     HTMLFILE;EMCNEMST;  * expected patients emergency site
         GOTO      EMCN9999
.
EMRCN051 WRITE     HTMLFILE;EMCPRCMP;  * Additional Presenting ComplaintComment
         GOTO      EMCN9999
.
EMRCN052 WRITE     HTMLFILE;EMCNAZTR;  * Send ZTR with A03, A04, A08
         GOTO      EMCN9999
.
EMCN9999  RETURN
