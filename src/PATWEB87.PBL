          ENDIF
          MOVE      ZERO,RECNUMB
          MOVE      ZERO,DISNUMB
.
          PACK      KEY23,FILTATYP,SP70
          CALL      RSPMHPG1
PRTAB100  CALL      RKPMHPG1
          BRANCH    OVRCD,PRTAB900
.
          MATCH     SP70,FILTATYP
          IF        !@EQUAL
            MATCH     FILTATYP,PMHPATYP
            GOTO      PRTAB999 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,FILTCLAM
          IF        !@EQUAL
            MATCH     FILTCLAM,PMHPCLAM
            GOTO      PRTAB100 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,FILTFUND
          IF        !@EQUAL
            MATCH     FILTFUND,PMHPFUND
            GOTO      PRTAB100 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,FILTTABT
          IF        !@EQUAL
            MATCH     FILTTABT,PMHPTABT
            GOTO      PRTAB100 IF NOT EQUAL
          ENDIF
.
          ADD       ONE,RECNUMB
          IF        STARTNUM>=RECNUMB
            GOTO      PRTAB100
          ENDIF
.
          CALL      PRROWA00                * Program Row
.
          ADD       ONE,DISNUMB
          COMPARE   NORECORD,DISNUMB
          GOTO      PRTAB100 IF NOT EQUAL
          GOTO      PRTAB999
