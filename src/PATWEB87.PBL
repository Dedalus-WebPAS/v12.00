          APPEND    "There is already data for the copy to Year - ",WEBTITLE
          APPEND    TDRGYEAR,WEBTITLE
          RESET     WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      VCOP9999
.
VCOP9999  RETURN
