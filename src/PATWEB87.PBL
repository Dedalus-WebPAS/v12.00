          CALL      RDPMHCP1
          IF        OVRCD=1
            MOVE      PMDLHCPC,DOCFNAME
            GOTO      HCPROW10
          ENDIF
.
          MOVE      PMHCTITL,FMTTITLE
          MOVE      PMHCGNAM,FMTGNAME
          MOVE      PMHCSNAM,FMTSNAME
          CALL      DOCNM000
.
          MATCH     "0",PMHCSTTS 
          IF        @EQUAL
            MOVE      "Active",KEY8
          ELSE
