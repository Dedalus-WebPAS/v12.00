          MOVE      ZERO,EXIT
          MOVE      TMBNTDAT,TMBDTDAT
          MOVE      TMBNRECN,TMBDRECN
          MOVE      "  1",TMBDUNIQ
          MOVE      TMBNMHOS,TMBDMHOS
          MOVE      TMBNMDHS,TMBDMDHS
          MOVE      TMBNTOTP,TMBDPAMT
