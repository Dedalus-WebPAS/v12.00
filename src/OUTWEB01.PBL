            WRITE      HTMLFILE;DAYNAM3," ",CDAY," ",MTHNAM3," ",CCENT,CYEAR:
                               "</td><td align=center>",OBATIME," </td>":
                               "<td>",TDESC,"</td>":
                               "<td>",OCTDESC,"</td>":
                               "<td>&nbsp;",OCADESC,"</td>":
                               "<td>&nbsp;",DIM20,"</td></tr>"
          ELSE
            WRITE     HTMLFILE;"#"visit#":#"",TDESC,"#",":
                               "#"type#":#"",OCTDESC,"#",":
                               "#"clinic#":#"",OCADESC,"#"},";
