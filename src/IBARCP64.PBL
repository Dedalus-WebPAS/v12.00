.
          IF        HSYS3=1
..            DISPLAY   *P64:24,"pridebtf";
..            OPEN      PRIDEBT1,"pridebtf"
            OPEN      PRIINVO1,"priinvof"
            DISPLAY   *P64:24,"priinvof";
          ENDIF
.
.         Open files for new account receivable
.
          IF        HSYS6=1
            DISPLAY   *P64:24,"debdbtaf";
            OPEN      DEBDBTA1,"debdbtaf"
