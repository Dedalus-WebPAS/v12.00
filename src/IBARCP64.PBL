.
            CALL      STMPL000
            IF        TMPLTFLG=1
              MOVE      "Parameter setting Wrong. Contact I.B.A. - ",DISPMSG
              CALL      MESSAGE1
              MOVE      ONE,EXIT
              GOTO      INIT9999
            ENDIF
.
            LOAD      MAXCLNO,HFORMAT,ZERO,ZERO,ZERO,ZERO,ZERO:
                                      ZERO,ZERO,THIRTY3,ZERO
.
            PACK      CRCPDATE,CCC,CYY,CMM,CDD,SP70
          ENDIF
.
          CALL      SELPRINT
INIT9999  RETURN
