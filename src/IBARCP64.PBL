          PACK      DIM12,CODEIR,SP10
          IF HFORMAT=5|HFORMAT=6|HFORMAT=7|HFORMAT=8|HFORMAT=9
            PACK      PRNTNAME,TRNPAYEE,SP30
          ENDIF
          GOTO      PRT4000
