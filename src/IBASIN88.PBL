          IF        TOTONLY=1
            PRINT     *40,"COST CENTRE TOTALS ONLY"
          ELSE
            PRINT     *40,SICADESC,"(",SICACODE,")"
          ENDIF
          IF        REPTTYP=1
            PRINT     *40,"Purchase Order Placed":
