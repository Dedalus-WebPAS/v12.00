. GENH0000 - Output Help Routine
.----------------------------------------------------------------------
GENH0000  
          PRINT     HLABEL,"00"
          MATCH     SP70,SYHVCBF
          GOTO      GENH0010 IF EQUAL
          PRINT     "          CALL      ",SYHVCBF
GENH0010  PRINT     "          MOVE      ZERO,CSCRSIZ"
