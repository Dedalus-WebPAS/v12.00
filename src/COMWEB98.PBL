.------------------------------------------------------------
. Patient Billing Parameter Maintenance
.------------------------------------------------------------
BCON0000  MATCH     SP70,UPDTTYPE
          GOTO      BCON7000 IF EQUAL
.
          MATCH     "U",UPDTTYPE       * Update
          GOTO      BCON1000 IF EQUAL
.
          GOTO      BCON9000
.
BCON1000  CALL      RPAT0000           * Read PATCONFD file
          CALL      SISC0000           * Save EDWARD ISP Audit vars
.
          CALL      MVPT0000           * Move cgi variables to file variables
.
          CALL      UPAT0000           * Update PATCONFD file
.
          MATCH     SP70,PTCNNSSI
          IF        !@EQUAL
            MATCH     "0000000000",PTCNNSSI
            IF        !@EQUAL
              OPEN      PMSIDWA1,"pmsidwaf"      * EDWARD ISP Audit
            ENDIF
          ENDIF
          CALL      WISC0000           * Write EDWARD ISP Audit vars
.
          MOVE      ZERO,EXIT
          GOTO      BCON9999
.
BCON7000  CALL      RPAT0000           * Read PATCONFD file
.
          CLEAR     WEBTITLE
          MOVE      "Billing Parameter Maintenance",WEBTITLE
          RESET     WEBTITLE
.
          CALL      WEBHED00   * Create Output File and Write HTML Page Header
          BRANCH    EXIT,BCON9999
          CALL      WEBBOD00   * Process Web Body
          CALL      WEBEND00   * Process End of HTML File
.
          MOVE      ONE,EXIT
          GOTO      BCON9999
.
BCON9000  MOVE      "Invalid Form Action.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      BCON9999
.
BCON9999  RETURN
.
