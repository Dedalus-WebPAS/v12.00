.------------------------------------------------------------
PRTSEL00  PACK      KEY6,SP70               * Read the printer file
          CALL      RDSPRTA1
PRTSEL10  CALL      RDKPRTA1
          BRANCH    OVRCD,PRTSEL99
.
.         MATCH     WBSEHOSP,IBPTHOSP       * Same hospital?
.         GOTO      PRTSEL10 IF NOT EQUAL
.
          PACK      KEY8,QUOTE,QPRTCODE,QUOTE
          WRITE     HTMLFILE;"<option value=",KEY8,">",PRTDESC,"</option>"
          GOTO      PRTSEL10
.
PRTSEL99  RETURN
+
.------------------------------------------------------------
. Allied Health Parameter Maintenance
.------------------------------------------------------------
ACON0000  MATCH     SP70,UPDTTYPE
          GOTO      ACON7000 IF EQUAL
.
          MATCH     "U",UPDTTYPE       * Update
          GOTO      ACON1000 IF EQUAL
.
          GOTO      ACON9000

.
ACON1000  CALL      RALL0000           * Read ALLCONFD file
.
          CALL      MVAL0000           * Move cgi variables to file variables
.
          CALL      UALL0000           * Update ALLCONFD file
.
          MOVE      ZERO,EXIT
          GOTO      ACON9999
.
ACON7000  CALL      RALL0000           * Read ALLCONFD file
.
          CLEAR     WEBTITLE
          MOVE      "Allied Health Parameter Maintenance",WEBTITLE
          RESET     WEBTITLE
.
          CALL      WEBHED00   * Create Output File and Write HTML Page Header
          BRANCH    EXIT,ACON9999
          CALL      WEBBOD00   * Process Web Body
          CALL      WEBEND00   * Process End of HTML File
.
          MOVE      ONE,EXIT
          GOTO      ACON9999
.
ACON9000  MOVE      "Invalid Form Action.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ACON9999
.
ACON9999  RETURN
.
