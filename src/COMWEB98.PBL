.----------------------------------------------
. Hospital Level Parameters - comparaf
.----------------------------------------------
HPAR0000  BRANCH    NOHOSPAR,HPAR9300            * Trap comparaf open
.
          MATCH     SP70,UPDTTYPE                * Display
          GOTO      HPAR7000 IF EQUAL
.
          MATCH     "A",UPDTTYPE                 * Add
          GOTO      HPAR1000 IF EQUAL
.
          MATCH     "U",UPDTTYPE                 * Update
          GOTO      HPAR2000 IF EQUAL
.
          MATCH     "D",UPDTTYPE                 * Delete
          GOTO      HPAR3000 IF EQUAL
.
          GOTO      HPAR9000
.
HPAR1000  CALL      CLCOMPAR                     * Clear comparaf file vars
.
          PACK      KEY11,CMPAR001,CMPAR002,SP70
          CALL      RACMPAR1                     * Check if record exists
          COMPARE   ZERO,OVRCD
          GOTO      HPAR9100 IF EQUAL
.
          CALL      MVCMPA00                     * Populate file vars
.
          CALL      VHPR0000                     * Validate values
          BRANCH    EXIT,HPAR9999
.
          PACK      KEY8,CCC,CYY,CMM,CDD
          REP       " 0",KEY8
          MOVE      KEY8,CMPACDAT                * System date
          CLOCK     TIME,CMPACTIM                * System time
          MOVE      USERID,CMPACUID              * web user id
.
          PACK      KEY11,CMPAHOSP,CMPAPARM,SP70
          CALL      WRCMPAR1                     * Write record
.
          MOVE      ZERO,EXIT
          GOTO      HPAR9999
.
HPAR2000  PACK      KEY11,CMPAHOSP,CMPAPARM,SP70
          CALL      RDCMPAR1                     * Record exists
          BRANCH    OVRCD,HPAR9200
.
          CALL      MVCMPA00                     * Populate file vars
.
          CALL      VHPR0000                     * Validate values
          BRANCH    EXIT,HPAR9999
.
          PACK      KEY8,CCC,CYY,CMM,CDD
          REP       " 0",KEY8
          MOVE      KEY8,CMPAUDAT                * System date
          CLOCK     TIME,CMPAUTIM                * System time
          MOVE      USERID,CMPAUUID              * web user id
.
          CALL      UPCMPAR1                     * Delete record
.
          MOVE      ZERO,EXIT
          GOTO      HPAR9999
.
HPAR3000  PACK      KEY11,CMPAHOSP,CMPAPARM,SP70
          CALL      RDCMPAR1                     * Record exists
          BRANCH    OVRCD,HPAR9200
          
          CALL      DECMPAR1                     * Delete record
.
          MOVE      ZERO,EXIT
          GOTO      HPAR9999
.
HPAR7000  PACK      KEY11,CMPAR001,CMPAR002,SP70
          CALL      RDCMPAR1
          IF        OVRCD=1
            CALL      CLCOMPAR
          ENDIF
.
          CLEAR     WEBTITLE
          MOVE      "Hospital Level Parameter Maintenance",WEBTITLE
          RESET     WEBTITLE
.
          CALL      WEBHED00
          BRANCH    EXIT,HPAR9999
          CALL      WEBBOD00
          CALL      WEBEND00
.
          MOVE      ONE,EXIT
          GOTO      HPAR9999
.
HPAR9000  MOVE      "Invalid Form Action.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      HPAR9999
.
HPAR9100  MOVE      "Parameter Record Already Exists.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      HPAR9999
.
HPAR9200  MOVE      "Invalid Parameter Record.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      HPAR9999
.
HPAR9300  MOVE      "Hosital Level Parameters Not Available.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      HPAR9999
.
HPAR9999  RETURN
