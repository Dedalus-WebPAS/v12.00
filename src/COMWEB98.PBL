.------------------------------------------------------------
. Hospital Selection - only hospitals that user has access to
.------------------------------------------------------------
HSLU0000  COMPARE   ONE,IBCNMHOS
          GOTO      HSLU9999 IF NOT EQUAL
.
          MOVE      ZERO,F1
          PACK      KEY13,USERID,SP70
          CALL      RDMLSEC1
          IF        OVRCD<>1
            MOVE      ONE,F1               * Access to all hospitals
          ENDIF
.
          PACK      KEY3,SP70
          CALL      RSPTHSP1
HSLU0100  CALL      RKPTHSP1
          BRANCH    OVRCD,HSLU9999
.
          BRANCH    F1,HSLU0200
.
          PACK      KEY13,WBSEUID,PTHSHOSP,SP70
          CALL      RDMLSEC1
          BRANCH    OVRCD,HSLU0100
.
HSLU0200  MOVE      PTHSNAME,KEY50
          PACK      KEY5,QUOTE,PTHSHOSP,QUOTE
          MATCH     DIM3,PTHSHOSP
          IF        @EQUAL
            WRITE     HTMLFILE;"<option value=",KEY5," selected>":
                               KEY50,"</option>"
          ELSE
            WRITE     HTMLFILE;"<option value=",KEY5,">",KEY50,"</option>"
          ENDIF
          GOTO      HSLU0100
.
HSLU9999  RETURN
+
.------------------------------------------------------------
. List of hospital parameters from comparaf
.------------------------------------------------------------
HPLS0000  IF        IBCNMHOS=1
            PACK      KEY11,FILTHOSP,SP70
          ELSE
            PACK      KEY11,SP70
          ENDIF
.
          CALL      RSCMPAR1
HPLS1000  CALL      RKCMPAR1
          BRANCH    OVRCD,HPLS9999
.
          IF        IBCNMHOS=1
            MATCH     SP70,FILTHOSP
            IF        !@EQUAL
              MATCH     FILTHOSP,CMPAHOSP
              GOTO      HPLS9999 IF NOT EQUAL
            ENDIF
          ENDIF
.
          MATCH     "1",FILTSTAT
          IF        @EQUAL
            MATCH     "1",CMPAHOSP
            GOTO      HPLS1000 IF NOT EQUAL
          ENDIF
.
          CLEAR     HTMLLINK
          APPEND    "javascript:UpdateParameter('",HTMLLINK
          APPEND    CMPAHOSP,HTMLLINK
          APPEND    "','",HTMLLINK
          APPEND    CMPAPARM,HTMLLINK
          APPEND    "')",HTMLLINK
          RESET     HTMLLINK
.
          MOVE      SP70,DISPCNAM
          MATCH     SP70,CMPACUID
          IF        !@EQUAL
            PACK      KEY10,CMPACUID,SP70
            CALL      RDWBSE1
            IF        OVRCD=1
              MOVE      CMPACUID,WBSENAM   
            ENDIF
            MOVE    WBSENAM,DISPCNAM        * Created By
          ENDIF
.
          MOVE      SP70,DISPUNAM
          MATCH     SP70,CMPAUUID
          IF        !@EQUAL
            PACK      KEY10,CMPAUUID,SP70
            CALL      RDWBSE1
            IF        OVRCD=1
              MOVE      CMPAUUID,WBSENAM
            ENDIF
            MOVE    WBSENAM,DISPUNAM        * Updated By
          ENDIF
.
          MATCH     "1",CMPAACTV            * Active
          IF        @EQUAL
            MOVE      "No",DISPACTV
          ELSE
            MOVE      "Yes",DISPACTV
          ENDIF
.
          MOVE      "Default",PTHSNAME
          MATCH     SP70,CMPAHOSP
          IF        !@EQUAL
            PACK      KEY3,CMPAHOSP,SP70
            CALL      RDPTHSP1
            IF        OVRCD=1
              MOVE      CMPAHOSP,PTHSNAME
            ENDIF
          ENDIF
.
          PACK     DISPVALU,SP70,SP70,SP70,SP70       *Clear  Display Value
.
          MATCH    "  1",CMPATYPE           * Date
          IF       @EQUAL
            MATCH    SP70,CMPAVALU
            IF       !@EQUAL
              UNPACK    CMPAVALU,CCENT,CYEAR,CMON,CDAY
              MOVE      CMON,F2
              LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT:
                                   NOV,DEC
              PACK      DISPVALU,CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR
            ENDIF
            GOTO     HPLS2000
          ENDIF
.
          MATCH    "  3",CMPATYPE           * HCP
          IF       @EQUAL
            MATCH    SP70,CMPAVALU
            IF       !@EQUAL
              PACK      KEY10,CMPAVALU,SP70
              CALL      RDPMHCP1
              IF        OVRCD=1
                MOVE      CMPAVALU,DISPVALU
              ELSE
                MOVE      PMHCTITL,FMTTITLE
                MOVE      PMHCGNAM,FMTGNAME
                MOVE      PMHCSNAM,FMTSNAME
                CALL      DOCNM000
                MOVE      DOCFNAME,DISPVALU
              ENDIF
            ENDIF
            GOTO     HPLS2000
          ENDIF
.
          MATCH    "  4",CMPATYPE           * Category Code
          IF       @EQUAL
            MATCH    SP70,CMPAVALU
            IF       !@EQUAL
              UNPACK    CMPAVALU,D2,D3
              MATCH     SP70,D3
              IF        !@EQUAL
                PACK      KEY5,CMPAVALU,SP70
                CALL      RDCODE1
                IF        OVRCD=1
                  MOVE      CMPAVALU,TDESC
                ENDIF
                MOVE      TDESC,DISPVALU
              ENDIF
            ENDIF
            GOTO     HPLS2000
          ENDIF

.
          MATCH    "  5",CMPATYPE           * Yes/No
          IF       @EQUAL
            MATCH    "0",CMPAVALU
            IF       @EQUAL
              MOVE     "No",DISPVALU
            ENDIF
            MATCH    "1",CMPAVALU
            IF       @EQUAL
              MOVE     "Yes",DISPVALU
            ENDIF
            GOTO     HPLS2000
          ENDIF
.
          MOVE      CMPAVALU,DISPVALU
.
