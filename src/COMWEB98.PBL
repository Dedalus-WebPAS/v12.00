.------------------------------------------------------------
. Check IBA Controlled screen password and key
.------------------------------------------------------------
SECU0000  MATCH     SP70,UPDTTYPE
          GOTO      SECU8000 IF EQUAL
.
          MATCH     ANSV,UPDTTYPE
          GOTO      SECU1000 IF EQUAL            * Validate Passwork/Key
.
          GOTO      SECU9100
.
SECU1000  OPEN      CONTROLF,"controlf"
          READ      CONTROLF,TWO;*233,CSPASSWD
.
. ----- Password ----- 
.
          MATCH     PASSWORD,CSPASSWD
          GOTO      SECU9000 IF NOT EQUAL   * Not the correct password
.
. ----- Date -----
.
          MOVE      ZERO,FORM8
          MOVE      PASSWKEY,FORM8
          SUB       CDD,FORM8
          SUB       CMM,FORM8
          SUB       CYY,FORM8
          COMPARE   ZERO,FORM8
          GOTO      SECU9000 IF NOT EQUAL   * Not the correct date
.
SECU7000  MOVE      ZERO,EXIT
          MOVE      ONE,VALIDPAS            * A valid password has been entered
          GOTO      SECU9999
.
SECU8000  CLEAR     WEBTITLE
          MOVE      "Password Protection",WEBTITLE
          RESET     WEBTITLE
.
          CALL      WEBHED00   * Create Output File and Write HTML Page Header
          BRANCH    EXIT,SECU9999
          CALL      WEBBOD00   * Process Web Body
          CALL      WEBEND00   * Process End of HTML File
.
          MOVE      ONE,EXIT
          GOTO      SECU9999
.
SECU9000  MOVE      "Invalid Password/Key.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      SECU9999
.
SECU9100  MOVE      "Invalid Update Type.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      SECU9999
.
SECU9999  RETURN
.
