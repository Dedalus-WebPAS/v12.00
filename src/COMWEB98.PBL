.------------------------------------------------------------
. Casemix Generation Parameter Maintenance
.------------------------------------------------------------
CCON0000  MATCH     SP70,UPDTTYPE
          GOTO      CCON7000 IF EQUAL
.
          MATCH     "U",UPDTTYPE       * Update
          GOTO      CCON1000 IF EQUAL
.
          MATCH     "H",UPDTTYPE       * Update Hospital database locations
          GOTO      CCON2000 IF EQUAL
.
          GOTO      CCON9000
.
CCON1000  CALL      RPCIA000           * Read CIACONFD file
.
          CALL      MVCIA000           * Move cgi variables to file variables
.
          CALL      UPCIA000           * Update CIACONFD file     
.
          MOVE      ZERO,EXIT
          GOTO      CCON9999
.
CCON2000  MOVE     "1 ",KEY2
          CALL     RDCCHL1
          IF       OVRCD=1
            CALL     CLCSHC00          * Clear hospital database locations
            CALL     MVCSCH00          * Move cgi vars to file vars
            MOVE     "1 ",KEY2
            CALL     WRCCHL1
          ELSE
            CALL     MVCSCH00          * Move cgi vars to file vars
            CALL     UPCCHL1
          ENDIF
.
          MOVE     "1 ",CCHCHCD
          PACK      KEY2,CCHCHCD,SP70
          CALL      RDCCHC1
          IF        OVRCD=0
            MATCH     Z70,CSHCD001
            IF        !@EQUAL
              MOVE      CSHCD001,CCHCDES
            ENDIF
            CALL      UPCCHC1
          ELSE
            MATCH     Z70,CSHCD001
            IF        !@EQUAL
              MOVE      CSHCD001,CCHCDES
            ELSE
              MOVE      SP70,CCHCDES
            ENDIF
            MOVE      SP70,CCHCPREF
            MOVE      SP70,CCHCPUSE
            MOVE      SP70,CCHCSPA 
            CALL      WRCCHC1
          ENDIF
.
          MOVE      ZERO,EXIT
          GOTO      CCON9999
.
CCON7000  CALL      RPCIA000           * Read CIACONFD file 
.
          MOVE     "1 ",KEY2
          CALL     RDCCHL1
          IF       OVRCD=1
            CALL     CLCSHC00          * Clear hospital database locations
          ENDIF
.
          MOVE     "1 ",KEY2
          CALL     RDCCHC1
          IF       OVRCD=1
            MOVE      "1 ",CCHCHCD
            MOVE      SP70,CCHCDES
            MOVE      SP70,CCHCPREF
            MOVE      SP70,CCHCPUSE
            MOVE      SP70,CCHCSPA
          ENDIF
.
          CLEAR     WEBTITLE
          MOVE      "Casemix Generation Parameter Maintenance",WEBTITLE
          RESET     WEBTITLE
.
          CALL      WEBHED00   * Create Output File and Write HTML Page Header
          BRANCH    EXIT,CCON9999
          CALL      WEBBOD00   * Process Web Body
          CALL      WEBEND00   * Process End of HTML File
.
          MOVE      ONE,EXIT
          GOTO      CCON9999
.
CCON9000  MOVE      "Invalid Form Action.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      CCON9999
.
CCON9999  RETURN
