            IF        !@EQUAL
              MOVE      "Invalid Code",TDESC
              PACK      KEY5,CATPR,XPRTY
              CALL      RDCODE1
            ENDIF
          ENDIF
.
          PRINT     *1,TDESC,SP1,XRNG1,SP2,XRNG2,SP2,XRNG3:
