.  OPRCOMIO
.  --------
.
RSOPCOM1  RESET     KEY25
          READ      OPRCOMA1,KEY25;;
          RETURN
.
RAOPCOM1  RESET     KEY25
          MOVE      ZERO,OVRCD
          READ      OPRCOMA1,KEY25;ANS
          GOTO      OVERCOND IF OVER
          RETURN
.
RDOPCOM1  RESET     KEY25
          MOVE      ZERO,OVRCD
          READ      OPRCOMA1,KEY25;OPCMDATE,OPCMTIME,OPCMCLIN,DOPCMLIN:
                                  OPCMTEXT,OPCMHOSP,OPCMSPAR
          GOTO      OVERCOND IF OVER
          MOVE      DOPCMLIN,OPCMLINE
          RETURN
.
RKOPCOM1  MOVE      ZERO,OVRCD
          READKS    OPRCOMA1;OPCMDATE,OPCMTIME,OPCMCLIN,DOPCMLIN:
                                  OPCMTEXT,OPCMHOSP,OPCMSPAR
          GOTO      OVERCOND IF OVER
          MOVE      DOPCMLIN,OPCMLINE
          RETURN
.
RPOPCOM1  MOVE      ZERO,OVRCD
          READKP    OPRCOMA1;OPCMDATE,OPCMTIME,OPCMCLIN,DOPCMLIN:
                                  OPCMTEXT,OPCMHOSP,OPCMSPAR
          GOTO      OVERCOND IF OVER
          MOVE      DOPCMLIN,OPCMLINE
          RETURN
.
UPOPCOM1  MOVE      OPCMLINE,DOPCMLIN
          UPDATE    OPRCOMA1;OPCMDATE,OPCMTIME,OPCMCLIN,DOPCMLIN:
                                  OPCMTEXT,OPCMHOSP,OPCMSPAR
          RETURN
.
WROPCOM1  RESET     KEY25
          MOVE      OPCMLINE,DOPCMLIN
          WRITE     OPRCOMA1,KEY25;OPCMDATE,OPCMTIME,OPCMCLIN,DOPCMLIN:
                                  OPCMTEXT,OPCMHOSP,OPCMSPAR
          RETURN
.
DEOPCOM1  RESET     KEY25
          DELETE    OPRCOMA1,KEY25
          RETURN
