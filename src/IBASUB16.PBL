.----------------------------------------------------------------------
. Post Data
.----------------------------------------------------------------------
POST0000  MOVE      SP70,DISPMSG
          CALL      MESSAGE2
.
.---- check ranges ----
.
          MATCH     FROMCODE,TOCODE
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Cost Centre Range - ";
            CALL      HITENTER
            GOTO      POST9500
          ENDIF
.
          MATCH     FROMDATE,TODATE
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Date Range - ";
            CALL      HITENTER
            GOTO      POST9500
          ENDIF
.
.---- loop through requisitions ----
.
          MOVE      ZERO,CPAGENO                 * reset page no.
          CLOCK     TIME,CTIMEIS                 * get current time
          MOVE      "999",CLNO                   * init last line number
          MOVE      SP70,LASTCODE
          MOVE      ZERO,ADDFLAG
.
          PACK      KEY13,FROMCODE,SP70
.
POST1000  CALL      RSSIQC1
          CALL      RKSIQC1
          BRANCH    OVRCD,POST9000     * at end if processing range ?
          MATCH     SIQCCST,TOCODE
          GOTO      POST9000 IF LESS
.
          PACK      KEY13,SIQCCST,SIQCRID,SP70
          CALL      RDSIQA1            * check if outstanding
          IF        OVRCD<>1
            GOTO      POST1000         * skip if still outstanding
          ENDIF
          DISPLAY   *P1:24,"Processing : ",*V2LON,KEY13,*EL;
.
          MATCH     SIQCPDT,TODATE     * check date range
          GOTO      POST1000 IF LESS
          MATCH     FROMDATE,SIQCPDT
          GOTO      POST1000 IF LESS
.
          CALL      WRIA0000           * print report if required
          MOVE      ONE,ADDFLAG
          GOTO      POST1000
.
POST9000  MOVE      ZERO,EXIT          * quit program
          IF        ADDFLAG=0
            DISPLAY   *P1:24,*B,*EF,"No Requisitions Found - ";
            CALL      HITENTER
            GOTO      POST9500
          ELSE
            IF        PRINTREP=1
              CALL      UND132
            ENDIF
          ENDIF
.
          GOTO      POST9999
.
POST9500  MOVE      ONE,EXIT           * continue program
.
POST9999  CALL      MESSAGE3
          RETURN
