.********************************************************************
. WRIA - print data line                            Called by REPA
.  Requires : KEY13    (Requisition to purge)
.             PRINTREP (1=print report)rge)
.********************************************************************
WRIA0000  CALL      DESIQC1                      * delete data
.
          IF        PRINTREP=1
            MATCH     SIQCCST,LASTCODE
            IF        !@EQUAL
              MOVE      "999",CLNO               * init last line number
              MOVE      SIQCCST,LASTCODE
            ENDIF
.
            COMPARE   "55",CLNO                  * end of page ?
            CALL      HEAA0000 IF NOT LESS       * start new page
.
            UNPACK    SIQCDAT,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
            MOVE      SIQCQTY,F8
.
            ADD       "1",CLNO                   * add one to line number
            PRINT     "Entered ",SIQCRID,SP1,CPCDATE,SP1,SIQCTIM,SP1:
                      SIQCUID,SLASH,SIQCPOR,SP4,F8,SP1,SIQCUNT,SP1:
                      SIQCCAT,SP1,SIQCPD
.
            UNPACK    SIQCPDT,CCENT,CYEAR,CMON,CDAY
            CALL      PACDATE
.
            MOVE      "PRC",KEY3
            MOVE      SP70,SICDDESC
            MATCH     SP70,SIQCSTA
            IF        !@EQUAL
              PACK      KEY6,KEY3,SIQCSTA,SP70
              CALL      RDSICD1
            ENDIF
.
            MOVE      SP70,SIORNAME
            PACK      KEY3,SIQCORI,SP70
            CALL      RDSIOR1
.
            MOVE      SP70,KEY20
            IF        0=SIQCPTY
              MOVE      "Stock Order         ",KEY20
              PACK      KEY8,SIQCREQ,SP70
            ENDIF
            IF        1=SIQCPTY
              MOVE      "Purchase Order      ",KEY20
              PACK      KEY8,SIQCORD,SP70
            ENDIF
            IF        2=SIQCPTY
              MOVE      "Cancelled by C.C.   ",KEY20
              PACK      KEY8,SP70
            ENDIF
            IF        3=SIQCPTY
              MOVE      "Cancelled by Supply ",KEY20
              PACK      KEY8,SP70
            ENDIF
.
            ADD       "2",CLNO                   * add two to line number
            PRINT     "Process          ",CPCDATE,SP1,SIQCPTM,SP1:
                      SIQCPID,SLASH,SIQCPPO:
                      *44,"Type  : ",KEY20:
                          "  P/N    : ",SIQCPN:
                          "  Ord/Req : ",KEY8,*N:
                      *44,"Orig  : ",SIORNAME:
                          "  Reason : ",SIQCCAN
          ENDIF
.
          MOVE      "Notes : ",KEY9             * process notes
          PACK      KEY16,KEY13,SP70
.
WRIA2000  CALL      RSSIQB1
          CALL      RKSIQB1
          BRANCH    OVRCD,WRIA9000               * at end if processing range ?
          PACK      KEY16,SIQBCST,SIQBRID,SIQBLIN,SP70
          MATCH     KEY13,KEY16
          GOTO      WRIA9000 IF NOT EQUAL
.
          CALL      DESIQB1
.
          IF        PRINTREP=1
            COMPARE   "55",CLNO                  * end of page ?
            CALL      HEAA0000 IF NOT LESS       * start new page
            ADD       "1",CLNO                   * add one to line number
            PRINT     *44,KEY9,SIQBCOM
            MOVE      "         ",KEY9
          ENDIF
          GOTO      WRIA2000
.
WRIA9000  IF        PRINTREP=1
            ADD       "1",CLNO                   * add one to line number
            PRINT     SP1
          ENDIF
.
WRIA9999  RETURN
