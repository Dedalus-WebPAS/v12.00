.------------------------------------------------------------
. Coding Audit Detail Fields
.------------------------------------------------------------
MCAD0000  BRANCH    FLDITMNO,MCAD0010,MCAD0020,MCAD0030,MCAD0040,MCAD0050:
                             MCAD0060,MCAD0070,MCAD0080,MCAD0090,MCAD0100:
                             MCAD0110,MCAD0120,MCAD0130,MCAD0140,MCAD0150:
                             MCAD0160
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      MCAD9999
.
MCAD0010  WRITE     HTMLFILE;MRCDADMN;   * Admission Number
          GOTO      MCAD9999
.
MCAD0020  WRITE     HTMLFILE;MRCDADAT;   * Audit Date (ccyymmdd)
          GOTO      MCAD9999
.
MCAD0030  WRITE     HTMLFILE;MRCDATIM;   * Audit Time (hh:mm:ss) 
          GOTO      MCAD9999
.
MCAD0040  WRITE     HTMLFILE;MRCDCNTR;   * Counter
          GOTO      MCAD9999
.
MCAD0050  WRITE     HTMLFILE;MRCDRTYP;   * Record Type 
          GOTO      MCAD9999
.
MCAD0060  WRITE     HTMLFILE;MRCDPRFX;   * Prefix
          GOTO      MCAD9999
.
MCAD0070  WRITE     HTMLFILE;MRCDDCOD;   * Diagnosis Code
          GOTO      MCAD9999
.
MCAD0080  WRITE     HTMLFILE;MRCDDDES;   * Diagnosis Description 
          GOTO      MCAD9999
.
MCAD0090  WRITE     HTMLFILE;MRCDOFLG;   * Onset Flag 
          GOTO      MCAD9999
.
MCAD0100  WRITE     HTMLFILE;MRCDPCOD;   * Procedure Code
          GOTO      MCAD9999
.
MCAD0110  WRITE     HTMLFILE;MRCDPDES;   * Procedure Description
          GOTO      MCAD9999
.
MCAD0120  WRITE     HTMLFILE;MRCDODAT;   * Operation Date (ccyymmdd)
          GOTO      MCAD9999
.
MCAD0130  WRITE     HTMLFILE;MRCDOSTM;   * Operation State Time (hh:mm:ss)
          GOTO      MCAD9999
.
MCAD0140  WRITE     HTMLFILE;MRCDOETM;   * Operation End Time (hh:mm:ss)
          GOTO      MCAD9999
.
MCAD0150  WRITE     HTMLFILE;MRCDSURG;   * Operating Surgeon
          GOTO      MCAD9999
.
MCAD0160  WRITE     HTMLFILE;MRCDDRGD;   * DRG Driver
          GOTO      MCAD9999
.
MCAD9999  RETURN
.
