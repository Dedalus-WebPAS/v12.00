.******************************************************************************
.*                                 DOPT0000              Called By : ML0000   *
.*                         Print full description ?                           *
.******************************************************************************
DOPT0000  MOVE      ZERO,PDSFLAG
          MOVE      "N",ANS
          KEYIN     *P1:10,*EL,"Print full description (Y/N) ? ",*RV,*V2LON,ANS;
          REP       "yYnN",ANS
          CMATCH    "N",ANS
          GOTO      DOPT9999 IF EQUAL
          MOVE      ONE,PDSFLAG
          CMATCH    "Y",ANS
          GOTO      DOPT9999 IF EQUAL
