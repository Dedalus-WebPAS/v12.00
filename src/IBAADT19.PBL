                     *P1:14,*V2LON,ONE,*HOFF," = Claim Code Seq. ";
.
OPTN1000  KEYIN      *P1:17,*EL,"Please Select : ",*V2LON,OPTION;
          COMPARE    ZERO,OPTION
          GOTO       OPTN9000 IF EQUAL
.
          BRANCH     OPTION,OPTN9999
          BEEP
          GOTO       OPTN1000
.
OPTN9000  MOVE       TRUE,EXIT
