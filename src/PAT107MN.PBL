.------------------------------------------------------------------------------
. Online ICD10 Edition 7 Code Validation
.------------------------------------------------------------------------------
VAL7CD00  MOVE      SP70,VALDNAME
          MATCH     SP70,VALDCODE
          GOTO      VAL7CD90 IF EQUAL
.
          MOVE      "Invalid Code Entered",VALDNAME
          BRANCH    VALDTYPE,VAL7CD10,VAL7CD20
          MOVE      "Invalid Validation Type",VALDNAME
          GOTO      VAL7CD90
.
VAL7CD10  MOVE      VALDCODE,KEY9
          CALL      RDPT17O1
          IF        OVRCD=0
            MOVE      PT0ODESC,VALDNAME
          ENDIF
          GOTO      VAL7CD90
.
VAL7CD20  MOVE      VALDCODE,KEY9
          CALL      RDPT17D1
          IF        OVRCD=0
            MOVE      PT0DDESC,VALDNAME
          ENDIF
          GOTO      VAL7CD90
.
VAL7CD90  CALL      OPENHTML
          BRANCH    EXIT,VAL7CD99
          WRITE     HTMLFILE;"<h3 align=center>Checking Database</h3>":
                             "<script type=#"text/javascript#">":
                             "{ opener.ValidateName.value=#"",VALDNAME,"#";":
                             "  opener.ValidateCode.value=#"",VALDCODE,"#";"
.
          MATCH     "Invalid",VALDNAME
          IF        @EQUAL
            WRITE     HTMLFILE;"  alert(#"Invalid Code Entered#");";
          ENDIF
.
          WRITE     HTMLFILE;"  close(); } </script>"
          CALL      CLOSHTML
.
VAL7CD99  RETURN
