.------------------------------------------------------------------------------
. ICD10 Edition 7 Diagnosis Coding Maintenance: Display,Add,Update,Delete
.------------------------------------------------------------------------------
ICD7DI00  RESET     PTICD001
          MOVE      "7",ICD10EDN
.                                           * make sure files are open
          OPEN      PATI17D1,"pat10d7f"     * Open ICD10 Ed.7 Diseases
          OPEN      PATK7DA1,"patkcd7f"     * ICD10 Ed.7 Key Word Diag.
          OPEN      PATK7DA2,"patkcd7f"     * ICD10 Ed.7 Key Word Diag.
.
          RESET     UPDTTYPE
          MATCH     SP1,UPDTTYPE
          GOTO      ICD7DI40 IF EQUAL
.
          MATCH     "A",UPDTTYPE    * Add formaction
          GOTO      ICD7DI10 IF EQUAL
.
          MATCH     "U",UPDTTYPE    * Update formaction
          GOTO      ICD7DI20 IF EQUAL
.
          MATCH     "D",UPDTTYPE    * Delete formaction
          GOTO      ICD7DI30 IF EQUAL
.
          MATCH     "V",UPDTTYPE    * Validate Formaction
          IF        @EQUAL
            CALL      VAL7CD00
            GOTO      ICD7DI99
          ENDIF
.
          GOTO      ICD7DI93
.
ICD7DI10  CALL      CHKDIS00  * Checks mandatory fields & checks for blank
          BRANCH    EXIT,ICD7DI99
          PACK      KEY9,PTICD001,SP70  * ADD FORMACTION
          CALL      RDPT17D1
          COMPARE   ZERO,OVRCD
          GOTO      ICD7DI92 IF EQUAL
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      WRPT17D1      * Writes away the data
          PROC      PATK7DUP      * Update Keyword table
.
          CALL      ADDCAN00      * Add Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD7DI99
.
ICD7DI20  PACK      KEY9,PTICD001,SP70  * UPDATE FORMACTION
          CALL      RDPT17D1
          BRANCH    OVRCD,ICD7DI91
          CALL      CHKDIS00      * Checks mandatory fields
          BRANCH    EXIT,ICD7DI99
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      UPPT17D1      * Writes away the data
          PROC      PATK7DUP      * Update Key word Table
.
          CALL      UPDCAN00      * Update Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD7DI99
.
ICD7DI30  PACK      KEY9,PTICD001,SP70   * DELETE FORMACTION
          CALL      RDPT17D1
          BRANCH    OVRCD,ICD7DI91
          CALL      DEPT17D1      * Deletes the data
          PROC      PATK7DUP      * Update Key word Table
.
          CALL      DELCAN00      * Delete Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD7DI99
.
ICD7DI40  PACK      KEY9,PTICD001,SP70
          CALL      RDPT17D1
          IF        OVRCD=1
            CALL      CLRDIS00    * Clear all the file variables
          ENDIF
.
          MOVE      ONE,CANCERCD
          PACK      KEY9,PTICD001,SP70
          CALL      RDPTCAN1
          IF        OVRCD=1
            MOVE     ZERO,CANCERCD
            MOVE     ZERO,PTCCLATR
          ENDIF
.
          CLEAR     WEBTITLE
          MOVE      "ICD10 Edition 7 Diagnosis Maintenance",WEBTITLE
          RESET     WEBTITLE
.
          CALL      WEBHED00   * Create Output File and Write HTML Page Header
          BRANCH    EXIT,ICD7DI99
          CALL      WEBBOD00   * Process Web Body
          CALL      WEBEND00   * Process End of HTML File
          MOVE      ONE,EXIT
          GOTO      ICD7DI99
.
ICD7DI91  MOVE      "ICD10 Edition 7 Diagnosis Code does not exist",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD7DI99
.
ICD7DI92  MOVE      "ICD10 Edition 7 Diagnosis Code exists",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD7DI99
.
ICD7DI93  MOVE      "Invalid Form Action.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD7DI99
.
ICD7DI99  RETURN
+
