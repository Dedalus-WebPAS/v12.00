.*                   Keyin date for the change of send flag to no            *
.*****************************************************************************
KYDT0000  CALL       INDT0000
          MOVE       "18",CCOL
          MOVE       "7",CVERT
          MOVE       ZERO,CHIGHLT
          DISPLAY   *P1:7,*EF,*P1:7,"Effective Date : "
          CALL      KEYDATE
          BRANCH    OVRCD,KYDT0000
          PACK      KYDTE,CCENT,CYEAR,CMON,CDAY
          REPLACE   " 0",KYDTE
KYDT9999  RETURN
