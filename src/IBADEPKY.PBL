HIBDPA72  MOVE      ONE,OVRCD
HIBDPA75  RETURN
HIBDPA85
          LOAD      KEY5,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY5,PRXCATA                                               
          CALL      RDICOD1
HIBDPA90
.                                                                               
          BEEP
          GOTO      HIBDPA20
HIBDPA91
          MOVE      SP70,KEY5 
          MOVE      SP70,PRXCATA
          GOTO      HIBDPA96
HIBDPA92
