.---------------------------------------------------------------------------
.   System    :   IBA Software                       
.   Include   :   IBADEPKY
.   Routine   :   KIBDPA00 Keyin                     
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.   Mods      :   Adapted From IBAMENKY
.-------------------------------------------------------------------------------
KIBDPA00
          MOVE      UNDLN70,PRXCATA
          KEYIN     *PCCOL:CVERT,*DV,PRXCATA:                                  
                    *PCCOL:CVERT,*V2LON,PRXCATA:                               
                    *PCCOL:CVERT,*DV,PRXCATA
.
          ENDSET    PRXCATA
          APPEND    SP70,PRXCATA
          RESET     PRXCATA
.
          MATCH     SP70,PRXCATA
          GOTO      KIBDPA90 IF EQUAL
.
          CMATCH    EXITCHAR,PRXCATA
          GOTO      KIBDPA95 IF EQUAL
.
          CMATCH    QUEST,PRXCATA
          GOTO      KIBDPA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HIBDPA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    PRXCATA
          APPEND    SP70,PRXCATA
          RESET     PRXCATA
.
          MATCH     SP70,PRXCATA
          GOTO      KIBDPA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,PRXCATA                                
KIBDPA10
          PACK      KEY5,PRXCATA,SP5
          CALL      RDAICOD1
          BRANCH    OVRCD,KIBDPA80
          BRANCH    CKEYTYP,KIBDPA75
          CALL      RDICOD1
KIBDPA70
          MOVE      ZERO,EXIT
          GOTO      KIBDPA99
KIBDPA75
          DISPLAY   *P1:24,*EL,*B," Already Exist - ";                                                             
          CALL      HITENTER
          GOTO      KIBDPA00
KIBDPA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KIBDPA99,KIBDPA99
          DISPLAY   *P1:24,*EL,*B," Does Not Exist - ";                                                            
          CALL      HITENTER
          GOTO      KIBDPA00
KIBDPA90
          MOVE      ONE,EXIT
          GOTO      KIBDPA99
KIBDPA95
          MOVE      TWO,EXIT
KIBDPA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   IBA Software                       
.   Include   :   IBAMENKY
.   Routine   :   HIBDPA00 Help Description    
.   Return    :           
.-------------------------------------------------------------------------------
HIBDPA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
          GOTO      HIBDPA04 IF NOT EQUAL
          MOVE      "10",HTOP
          MOVE      "24",HBOT
          MOVE      "40",HLEF
          MOVE      "80",HRIG
          ENDIF
HIBDPA04
          CALL      GSCRN000
          BRANCH    EXIT,HIBDPA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Code"               
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 8",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "20",STDCOL03
HIBDPA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,PRXCATA
          PACK      KEY5,PRXCATA,SP70                                          
.
          CALL      RDSICOD1
HIBDPA10
          CALL      RDKICOD1
          BRANCH    OVRCD,HIBDPA15
.
          MATCH     SP3,PRXCODE
          GOTO      HIBDPA10 IF NOT EQUAL
.
          PACK      KEY5,PRXCATA,SP70                                          
.
          BRANCH    CSEARCH,HIBDPA12
          CALL      HIBDPA60
          BRANCH    OVRCD,HIBDPA10
.
HIBDPA12  ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY5,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,PRXCATA
          DISPLAY   *PSTDCOL02:STDROW,PRXDESC
          COMPARE   MAXITEM,XITEMNO
          GOTO      HIBDPA10 IF NOT EQUAL
.                                                                               
HIBDPA15  MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HIBDPA20
          CALL      HSCRI000
          BRANCH    EXIT,HIBDPA30,HIBDPA35,HIBDPA40,HIBDPA45,HIBDPA04:
                              HIBDPA91,HIBDPA50
          GOTO      HIBDPA92
HIBDPA30
          CALL      RDKICOD1
          BRANCH    OVRCD,HIBDPA85
.
          MATCH     SP3,PRXCODE
          GOTO      HIBDPA30 IF NOT EQUAL
.
          PACK      KEY5,PRXCATA,SP70                                          
.
          CALL      RDPICOD1
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBDPA10
HIBDPA35
          MOVE      ST1,KEY5 
          CALL      RDICOD1
          CALL      RDPICOD1
          BRANCH    OVRCD,HIBDPA85
          PACK      KEY5,PRXCATA,SP70                                          
.
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HIBDPA36
          CALL      RDPICOD1
          BRANCH    OVRCD,HIBDPA37
          PACK      KEY5,PRXCATA,SP70                                          
.
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HIBDPA36 IF NOT EQUAL
HIBDPA37
          MOVE      ZERO,XITEMNO
          GOTO      HIBDPA10
HIBDPA40
          MOVE      ST1,KEY5 
          CALL      RDICOD1
          CALL      RDPICOD1
          BRANCH    OVRCD,HIBDPA85
          PACK      KEY5,PRXCATA,SP70                                          
.
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBDPA08
HIBDPA45
          CALL      RDKICOD1
          BRANCH    OVRCD,HIBDPA85
.
          MATCH     SP3,PRXCODE
          GOTO      HIBDPA10 IF EQUAL
.
          PACK      KEY5,PRXCATA,SP70                                          
.
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY5,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RDSICOD1
HIBDPA46
          CALL      RDPICOD1
          BRANCH    OVRCD,HIBDPA47
          PACK      KEY5,PRXCATA,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HIBDPA46 IF NOT EQUAL
HIBDPA47
          MOVE      ZERO,XITEMNO
          GOTO      HIBDPA10
HIBDPA50  COMPARE   ZERO,CSEARCH
          GOTO      HIBDPA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBDPA08
HIBDPA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
