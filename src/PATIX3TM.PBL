SX3PRA00  MATCH     KEYWRD01,PTCOKWD
          GOTO      SX3PRA92 IF NOT EQUAL
          IF        KEYWRDNO>1
            PACK      SAVKEY24,PTCOKWD,PTCOITM
            CALL      SX3PRB00
            BRANCH    EXIT,SX3PRA91
            MOVE      SAVKEY24,KEY24
            CALL      RDPO13A2
          ENDIF
.
. Insert Other Check for Valid Record Here i.e. Suspended or In Active
.   eg    COMPARE    ONE,INACTIVE
.         GOTO       SX3PRA91 IF EQUAL
.
          GOTO      SX3PRA90
.
SX3PRA90  MOVE      ZERO,OVRCD
          GOTO      SX3PRA99
SX3PRA91  MOVE      ONE,OVRCD
          GOTO      SX3PRA99
SX3PRA92  MOVE      TWO,OVRCD
SX3PRA99  RETURN
+
.------------------------------------------------------------
. Check for Matching ICD10 Edition 13 Procedure Words
.------------------------------------------------------------
SX3PRB00  MOVE      ONE,F2
          MOVE      ZERO,EXIT
          PACK      SAVKEY9,PTCOITM,SP70
.
SX3PRB10  COMPARE   KEYWRDNO,F2           * Loop through each Key Word in Search
          GOTO      SX3PRB99 IF EQUAL     * Exit OK
          ADD       ONE,F2
          LOAD      KEYWRDXX,F2,KEYWRD01,KEYWRD02,KEYWRD03,KEYWRD04,KEYWRD05:
                             KEYWRD06,KEYWRD07,KEYWRD08,KEYWRD09,KEYWRD10
          PACK      KEY24,PTCOITM,KEYWRDXX
          CALL      RDPO13A1              * Check for Exact Match
          COMPARE   ZERO,OVRCD            * Ok so Continue Loop
          GOTO      SX3PRB10 IF EQUAL
.
          CALL      RKPO13A1              * Not Exact so Read Next Record
          BRANCH    OVRCD,SX3PRB90        * No Match So Exit No Match
          PACK      KEY9,PTCOITM,SP70
          MATCH     SAVKEY9,KEY9        * Match Key Fields
          GOTO      SX3PRB90 IF NOT EQUAL * No Match So Exit
          STRIP     KEYWRDXX
          MATCH     PTCOKWD,KEYWRDXX         * Check Key Word
          GOTO      SX3PRB90 IF NOT EQUAL * No Match So Exit
          GOTO      SX3PRB10              * Check Next Search Word
.
SX3PRB90  MOVE      ONE,EXIT
SX3PRB99  RETURN
