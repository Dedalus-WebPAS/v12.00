.------------------------------------------------------------------------------
. ICD10 Edition 13 Diagnosis Coding Maintenance: Display, Add, Update, Delete
.------------------------------------------------------------------------------
ICDX3D00  RESET     PTICD001
          MOVE      "13",ICD10EDN
.                                           * make sure files are open
          OPEN      PATD13A1,"patd13af"     * Open ICD10 Ed.13 Diseases
          OPEN      PATD13A2,"patd13af"     * Open ICD10 Ed.13 Diseases
          OPEN      PTDK13A1,"patdk13f"     * ICD10 Ed.13 Key Word Diag.
          OPEN      PTDK13A2,"patdk13f"     * ICD10 Ed.13 Key Word Diag.
.
          RESET     UPDTTYPE
          MATCH     SP1,UPDTTYPE
          GOTO      ICDX3D40 IF EQUAL
.
          MATCH     "A",UPDTTYPE    * Add formaction
          GOTO      ICDX3D10 IF EQUAL
.
          MATCH     "U",UPDTTYPE    * Update formaction
          GOTO      ICDX3D20 IF EQUAL
.
          MATCH     "D",UPDTTYPE    * Delete formaction
          GOTO      ICDX3D30 IF EQUAL
.
          MATCH     "V",UPDTTYPE    * Validate Formaction
          IF        @EQUAL
            CALL      VALX1C00
            GOTO      ICDX3D99
          ENDIF
.
          GOTO      ICDX3D93
.
ICDX3D10  CALL      CHKDIS00  * Checks mandatory fields & checks for blank
          BRANCH    EXIT,ICDX3D99
          PACK      KEY9,PTICD001,SP70  * ADD FORMACTION
          CALL      RDPD13A1
          COMPARE   ZERO,OVRCD
          GOTO      ICDX3D92 IF EQUAL
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      WRPD13A1      * Writes away the data
          PROC      PTDK13UP      * Update Keyword table
.
          CALL      ADDCAN00      * Add Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICDX3D99
.
ICDX3D20  PACK      KEY9,PTICD001,SP70  * UPDATE FORMACTION
          CALL      RDPD13A1
          BRANCH    OVRCD,ICDX3D91
          CALL      CHKDIS00      * Checks mandatory fields
          BRANCH    EXIT,ICDX3D99
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      UPPD13A1      * Writes away the data
          PROC      PTDK13UP      * Update Key word Table
.
          CALL      UPDCAN00      * Update Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICDX3D99
.
ICDX3D30  PACK      KEY9,PTICD001,SP70   * DELETE FORMACTION
          CALL      RDPD13A1
          BRANCH    OVRCD,ICDX3D91
          CALL      DEPD13A1      * Deletes the data
          PROC      PTDK13UP      * Update Key word Table
.
          CALL      DELCAN00      * Delete Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICDX3D99
.
ICDX3D40  PACK      KEY9,PTICD001,SP70
          CALL      RDPD13A1
          IF        OVRCD=1
            CALL      CLRDIS00    * Clear all the file variables
          ENDIF
.
          MOVE      ONE,CANCERCD
          PACK      KEY9,PTICD001,SP70
          CALL      RDPTCAN1
          IF        OVRCD=1
            MOVE     ZERO,CANCERCD
            MOVE     ZERO,PTCCLATR
          ENDIF
.
          CLEAR     WEBTITLE
          MOVE      "ICD10 Edition 13 Diagnosis Maintenance",WEBTITLE
          RESET     WEBTITLE
.
          CALL      WEBHED00   * Create Output File and Write HTML Page Header
          BRANCH    EXIT,ICDX3D99
          CALL      WEBBOD00   * Process Web Body
          CALL      WEBEND00   * Process End of HTML File
          MOVE      ONE,EXIT
          GOTO      ICDX3D99
.
ICDX3D91  MOVE      "ICD10 Edition 13 Diagnosis Code does not exist",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICDX3D99
.
ICDX3D92  MOVE      "ICD10 Edition 13 Diagnosis Code exists",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICDX3D99
.
ICDX3D93  MOVE      "Invalid Form Action.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICDX3D99
.
ICDX3D99  RETURN
