.------------------------------------------------------------------------------
. Online ICD10 Edition 13 Code Validation
.------------------------------------------------------------------------------
VALX3C00  MOVE      SP70,VALDNAME
          MATCH     SP70,VALDCODE
          GOTO      VALX3C90 IF EQUAL
.
          MOVE      "Invalid Code Entered",VALDNAME
          BRANCH    VALDTYPE,VALX3C10,VALX3C20
          MOVE      "Invalid Validation Type",VALDNAME
          GOTO      VALX3C90
.
VALX3C10  MOVE      VALDCODE,KEY9
          CALL      RDPO13A1
          IF        OVRCD=0
            MOVE      PT0ODSC2,VALDNAME
          ENDIF
          GOTO      VALX3C90
.
VALX3C20  MOVE      VALDCODE,KEY9
          CALL      RDPD13A1
          IF        OVRCD=0
            MOVE      PT0DDSC2,VALDNAME
          ENDIF
          GOTO      VALX3C90
.
VALX3C90  CALL      OPENHTML
          BRANCH    EXIT,VALX3C99
          WRITE     HTMLFILE;"<h3 align=center>Checking Database</h3>":
                             "<script type=#"text/javascript#">":
                             "{ opener.ValidateName.value=#"",VALDNAME,"#";":
                             "  opener.ValidateCode.value=#"",VALDCODE,"#";"
.
          MATCH     "Invalid",VALDNAME
          IF        @EQUAL
            WRITE     HTMLFILE;"  alert(#"Invalid Code Entered#");";
          ENDIF
.
          WRITE     HTMLFILE;"  close(); } </script>"
          CALL      CLOSHTML
.
VALX3C99  RETURN
