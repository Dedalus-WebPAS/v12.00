.*****************************************************************************
.*                                 CPBN0000                                  *
.*                        Print "Class Postage" in banner form               *
.*****************************************************************************
.
CPBN0000
          MOVE      ONE,COUNT
          WHILE     COUNT < 8
            PACK      BANNLINE,SP30,SP30
            LOAD      BANNLINE,COUNT,BCLASS1,BCLASS2,BCLASS3,BCLASS4:
                                     BCLASS5,BCLASS6,BCLASS7
            PRINT     BANNLINE
            ADD       ONE,COUNT
          DO
          MOVE      ONE,COUNT
          WHILE     COUNT < 8
            PACK      BANNLINE,SP30,SP30
            LOAD      BANNLINE,COUNT,BPOST1,BPOST2,BPOST3,BPOST4:
                                     BPOST5,BPOST6,BPOST7
            PRINT     BANNLINE
            ADD       ONE,COUNT
          DO
          PRINT     *N,*N
          CALL      UND80
          PRINT     *F
CPBN9999  RETURN
