.*****************************************************************************
.*                                 FCBN0000                                  *
.*                        Print "1st" in banner form                         *
.*****************************************************************************
.
FCBN0000
          CALL      UND80
          PRINT     *N,*N
          MOVE      ONE,COUNT
          WHILE     COUNT < 8
            PACK      BANNLINE,SP30,SP30
            LOAD      BANNLINE,COUNT,FIRSTC1,FIRSTC2,FIRSTC3,FIRSTC4,FIRSTC5:
                                     FIRSTC6,FIRSTC7
            PRINT     BANNLINE
            ADD       ONE,COUNT
          DO
FCBN9999  RETURN
.*****************************************************************************
.*                                 SCBN0000                                  *
.*                        Print "2nd" in banner form                         *
.*****************************************************************************
.
