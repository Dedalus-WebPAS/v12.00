          CALL      UND80
          PRINT     *N,*N
          MOVE      ONE,COUNT
          WHILE     COUNT < 8
            PACK      BANNLINE,SP30,SP30
            LOAD      BANNLINE,COUNT,SECONDC1,SECONDC2,SECONDC3,SECONDC4:
