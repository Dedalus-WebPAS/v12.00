                             OPCN1560,OPCN1570,OPCN1580,OPCN1590
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      OPCN9999
.
OPCN1010  WRITE     HTMLFILE;OPCNDORD;
          GOTO      OPCN9999
.
OPCN1020  WRITE     HTMLFILE;OPCNSCOM;
          GOTO      OPCN9999
.
OPCN1030  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      ZERO,F1
          MOVE      KEY1,F1
          BRANCH    F1,OPCN1032,OPCN1034
.
          MOVE      OPCNWHCD,KEY5
          CALL      RDSIWA1
          BRANCH    OVRCD,OPCN9999
          WRITE     HTMLFILE;SIWADES;
          GOTO      OPCN9999
.
OPCN1032  WRITE     HTMLFILE;OPCNWHCD;                                 
          GOTO      OPCN9999
.
OPCN1034  MOVE      SP10,KEY5
          CALL      RSSIWA1
OPCN1035  CALL      RKSIWA1
          BRANCH    OVRCD,OPCN9999
.
          PACK      KEY7,QUOTE,SIWAWAR,QUOTE
          MATCH     SIWAWAR,OPCNWHCD
          IF        @EQUAL
            WRITE     HTMLFILE;"<option value=",KEY7," selected>",SIWADES:
                               "</option>";
          ELSE
            WRITE     HTMLFILE;"<option value=",KEY7,">",SIWADES:
                               "</option>";
          ENDIF
          GOTO      OPCN1035
.
OPCN1040  WRITE     HTMLFILE;OPCNFTIM;      New Operation start time 
          GOTO      OPCN9999
.
OPCN1050  WRITE     HTMLFILE;OPCNTTIM;      New Operation end time    
          GOTO      OPCN9999
.
