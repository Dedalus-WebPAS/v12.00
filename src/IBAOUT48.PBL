PRLIN300  PRINT     *1,"|",*31,"|":
                    *61,"|",*63,CDAY,SLASH,CMON,SLASH,CCENT,CYEAR:
                    *74,"|",*76,OBASTRT,*82,"|",*84,BOOKNEW:
                    *90,"|",*92,BOOKSPC,*98,"|",*100,BOOKREV:
