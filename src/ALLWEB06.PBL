                                      "#"",ALGRNAME,"#",":              *1
                                      "#"",ALGRSEST,"#",":              *2
                                      "#"",DESCSEST,"#",":              *3
                                      "#"",ALGRLOCN,"#",":              *4
                                      "#"",DESCLOCN,"#",":              *5
                                      "#"",ALGRDAYI,"#",":              *6
                                      "#"",DAYNAM3,"#",":               *7
                                      "#"",ALGRSDAT,"#",":              *8
                                      "#"",ALGRSTIM,"#",":              *9 
                                      "#"",ALGRSDAT,ALGRSTIM,"#",":     *10
                                      "#"",ALGRDURN,"#",":              *11
                                      "#"",DESCACTV,"#",":              *12
                                      "#"",DESCDEPT,"#",":              *13
                                      "#"",ALGRDEPT,"#");"              *14
.
          GOTO      OTAB1000
.
