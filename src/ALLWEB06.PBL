          BRANCH    OVRCD,ETAB3400
          MATCH     ALGECONT,ALGCCONT
          GOTO      ETAB3400 IF NOT EQUAL
          ADD       ONE,F3A
          MATCH     SP3,ALGCHSTA
          GOTO      ETAB3200 IF EQUAL
          CALL      RDCODE1
          BRANCH    OVRCD,ETAB3200
          ADD       ONE,F3B                 * count HCPs with valid Status
          GOTO      ETAB3200
.
