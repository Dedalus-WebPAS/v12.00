          IF        OVRCD=1
            CALL     CGRO0000
          ENDIF
.
          PACK      KEY16,ALGRP001,ALGRP003,SP70
          CALL      RDALGRP1
          IF        OVRCD=1
            CALL     CGRP0000
          ENDIF
.
          PACK      KEY21,ALGRH001,ALGRH002,ALGRH003,SP70
          CALL      RDALGRH1
          IF        OVRCD=1
            CALL     CGRH0000
          ENDIF
.
          CLEAR     WEBTITLE
