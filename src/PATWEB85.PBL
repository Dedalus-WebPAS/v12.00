          MATCH     SP3,FILTHHOS
          IF        !@EQUAL
            MOVE      FILTHHOS,DFLTHHOS
          ENDIF
          MATCH     SP3,FILTHOSP
          IF        !@EQUAL
            MOVE      FILTHOSP,DFLTHOSP
