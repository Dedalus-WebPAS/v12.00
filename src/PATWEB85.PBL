          MATCH     SP70,WBSEHLOC
          IF        !@EQUAL
            MOVE      WBSEHHOS,DFLTHOSP
            MOVE      WBSEHLOC,DFLTHLOC
          ELSE
            IF        IBCNMHOS=1
              PACK      KEY3,WBSEHOSP,SP70
