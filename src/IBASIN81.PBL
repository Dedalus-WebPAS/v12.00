.**********************************************************************
.  NREA - Get next record                              Called By PRNA
.**********************************************************************
NREA0000
.
.---- print by type 1 ----
.
NREA0100  CALL      RKSIDS1
          BRANCH    OVRCD,NREA9500
.
          MATCH     SIDSWAR,ENDWAR
          GOTO      NREA9500 IF LESS             * out of range ?
.
. check warehouse security
.
          MATCH     SP70,SIDSWAR
          IF        !@EQUAL
            PACK      KEY9,PASSCODE,SIDSWAR,SP70
            CALL      RDSIWS1
            BRANCH    OVRCD,NREA0100
          ENDIF
.
          MATCH     SIDSWAR,LASTCODE             * at next warehouse ?
          IF        !@EQUAL
            CALL      WTOT0000                   * print warehouse totals
            PACK      LASTCODE,SIDSWAR,SP70
            MOVE      SP70,LASTCOD7
            MOVE      ZERO,ATOTAL01
            MOVE      ZERO,ATOTAL02
            MOVE      "599",CLNO
            PACK      KEY20,SIDSWAR,STRCAT,FROMDATE,SP70
            CALL      RSSIDS1
            CALL      RPSIDS1
            GOTO      NREA0100
          ENDIF
.
          MATCH     SIDSCAT,ENDCAT               * at end of catalog range ?
          IF        @LESS
            PACK      KEY20,SIDSWAR,Z70
            CALL      RSSIDS1
            CALL      RPSIDS1
            GOTO      NREA0100
          ENDIF
.
          MATCH     FROMDATE,SIDSDAT
          GOTO      NREA0100 IF LESS
.
          MATCH     SIDSDAT,TODATE               * at end of date range ?
          IF        @LESS
            PACK      KEY20,SIDSWAR,SIDSCAT,Z70
            CALL      RSSIDS1
            CALL      RPSIDS1
            GOTO      NREA0100
          ENDIF
.
          GOTO      NREA9000
.
NREA9000  MOVE      ZERO,OVRCD         * continue
          GOTO      NREA9999
.
