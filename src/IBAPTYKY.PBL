.---------------------------------------------------------------------------
.   System    :   Financial Management               
.   Include   :   IBAPTYKY
.   Routine   :   HIBPTA00 Print Type          
.   Return    :           
.-------------------------------------------------------------------------------
HIBPTA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HIBPTA04
          CALL      GSCRN000
          BRANCH    EXIT,HIBPTA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Type"               
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 5",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"         
.
          MOVE      STDCOL02,STDCOL03
          ADD       "21",STDCOL03
HIBPTA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,IBPTCODE
          PACK      KEY3,IBPTCODE,SP70                                          
.                                                                               
.                                                                               
          CALL      RSIBPT1 
HIBPTA10
          CALL      RKIBPT1 
          BRANCH    OVRCD,HIBPTA15
          PACK      KEY3,IBPTCODE,SP70                                          
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HIBPTA12
          CALL      HIBPTA60
          BRANCH    OVRCD,HIBPTA10
HIBPTA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,IBPTCODE
          DISPLAY   *PSTDCOL02:STDROW,IBPTDESC
          COMPARE   MAXITEM,XITEMNO
          GOTO      HIBPTA10 IF NOT EQUAL
HIBPTA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HIBPTA20
          CALL      HSCRI000
          BRANCH    EXIT,HIBPTA30,HIBPTA35,HIBPTA40,HIBPTA45,HIBPTA04:
                              HIBPTA91,HIBPTA50
          GOTO      HIBPTA92
HIBPTA30
          CALL      RKIBPT1 
          BRANCH    OVRCD,HIBPTA85
          PACK      KEY3,IBPTCODE,SP70                                          
.                                                                               
.                                                                               
          CALL      RPIBPT1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBPTA10
HIBPTA35
          MOVE      ST1,KEY3 
          CALL      RDIBPT1 
          CALL      RPIBPT1 
          BRANCH    OVRCD,HIBPTA85
          PACK      KEY3,IBPTCODE,SP70                                          
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HIBPTA36
          CALL      RPIBPT1 
          BRANCH    OVRCD,HIBPTA37
          PACK      KEY3,IBPTCODE,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HIBPTA36 IF NOT EQUAL
HIBPTA37
          MOVE      ZERO,XITEMNO
          GOTO      HIBPTA10
HIBPTA40
          MOVE      ST1,KEY3 
          CALL      RDIBPT1 
          CALL      RPIBPT1 
          BRANCH    OVRCD,HIBPTA85
          PACK      KEY3,IBPTCODE,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBPTA08
HIBPTA45
          CALL      RKIBPT1 
          BRANCH    OVRCD,HIBPTA85
          PACK      KEY3,IBPTCODE,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY3,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSIBPT1 
HIBPTA46
          CALL      RPIBPT1 
          BRANCH    OVRCD,HIBPTA47
          PACK      KEY3,IBPTCODE,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HIBPTA46 IF NOT EQUAL
HIBPTA47
          MOVE      ZERO,XITEMNO
          GOTO      HIBPTA10
HIBPTA50  COMPARE   ZERO,CSEARCH
          GOTO      HIBPTA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBPTA08
HIBPTA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBPTA08
HIBPTA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HIBPTA61 IF EQUAL
          MATCH     SCHFLD01,IBPTCODE
          GOTO      HIBPTA72 IF NOT EQUAL
HIBPTA61
          MATCH     SP70,SCHFLD02
          GOTO      HIBPTA62 IF EQUAL
          MATCH     SCHFLD02,IBPTDESC
          GOTO      HIBPTA72 IF NOT EQUAL
HIBPTA62
HIBPTA63
HIBPTA64
HIBPTA65
HIBPTA66
HIBPTA67
HIBPTA68
HIBPTA69
HIBPTA70
          GOTO      HIBPTA75
HIBPTA72  MOVE      ONE,OVRCD
HIBPTA75  RETURN
HIBPTA85
          LOAD      KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,IBPTCODE                                               
          CALL      RDIBPT1 
HIBPTA90
.                                                                               
          BEEP
          GOTO      HIBPTA20
HIBPTA91
          MOVE      SP70,IBPTDESC
          MOVE      SP70,IBPTSPAR
          MOVE      SP70,KEY3 
          MOVE      SP70,IBPTCODE
          GOTO      HIBPTA96
HIBPTA92
          MOVE      SP70,KEY3 
          MOVE      SP70,IBPTCODE
          LOAD      KEY3,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,IBPTCODE                                               
HIBPTA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HIBPTA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Financial Management               
.   Include   :   IBAPTYKY
.   Routine   :   KIBPTA00 Keyin Print Type          
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KIBPTA00 
          CLEAR     IBPTCODE
          RESET     IBPTCODE
          PACK      IBPTCODE,SP70
.
          KEYIN     *PCCOL:CVERT,*V2LON,IBPTCODE:                               
                    *PCCOL:CVERT,*DV,IBPTCODE
.
          PACK      IBPTCODE,IBPTCODE,SP70
          MATCH     IBPTCODE,SP70
          GOTO      KIBPTA90 IF EQUAL
.
          CMATCH    EXITCHAR,IBPTCODE
          GOTO      KIBPTA95 IF EQUAL
.
          CMATCH    QUEST,IBPTCODE
          GOTO      KIBPTA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HIBPTA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    IBPTCODE
          APPEND    SP70,IBPTCODE
          RESET     IBPTCODE
.
          MATCH     SP70,IBPTCODE
          GOTO      KIBPTA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,IBPTCODE                                
KIBPTA10
          PACK      KEY3,IBPTCODE                                               
          CALL      RAIBPT1 
          BRANCH    OVRCD,KIBPTA80
          BRANCH    CKEYTYP,KIBPTA75
          CALL      RDIBPT1 
KIBPTA70
          MOVE      ZERO,EXIT
          GOTO      KIBPTA99
KIBPTA75
          MOVE      "Print Type Already Exists - ",DISPMSG                                            
          CALL      MESSAGE1
          GOTO      KIBPTA00
KIBPTA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KIBPTA99,KIBPTA99
          MOVE      "Print Type Does Not Exist - ",DISPMSG                                           
          CALL      MESSAGE1
          GOTO      KIBPTA00
KIBPTA90
          MOVE      ONE,EXIT
          GOTO      KIBPTA99
KIBPTA95
          MOVE      TWO,EXIT
KIBPTA99 TOPIC    PRGID
          RETURN
