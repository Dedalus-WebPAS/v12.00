.*****************************************************************************
.*                               EXTR0000                                    *
.*        Extract the data into a file ready for sending to Medicare         *
.*****************************************************************************
.
. Build .properties file and move to B2B Device oecwecm claims directory
. -------------------------------------------------------------------
.
EXTR0000  PACK      PROPFLNM,WBOETRID,PROPLITR,SP70
.
          PREP      PROPFILE,PROPFLNM
          CLOSE     PROPFILE,DELETE
          PREP      PROPFILE,PROPFLNM
.
          WRITE     PROPFILE,SEQ;PRPLAB01,PRPVAL01
          WRITE     PROPFILE,SEQ;PRPLAB02,PRPVAL02          
          WRITE     PROPFILE,SEQ;PRPLAB03,PRPVAL03
.
          CLOSE     PROPFILE
.
.         Move .properties to B2B Device oecwecm claims directory 
.
          CLEAR     CMDLINE
          APPEND    "chmod 777 ",CMDLINE
          APPEND    PROPFLNM,CMDLINE
          RESET     CMDLINE
.
          EXECUTE   CMDLINE,TASKID
.
          MATCH     "0       ",TASKID
          IF        !@EQUAL
            MOVE      "Unable to chmod 777 .properties file.",ERRORDSC
            GOTO      EXTR9100
          ENDIF 
.
          CLEAR     CMDLINE
          APPEND    "mv ",CMDLINE
          APPEND    PROPFLNM,CMDLINE
          APPEND    SP1,CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/oecwecm/.",CMDLINE 
          RESET     CMDLINE
.
          EXECUTE   CMDLINE,TASKID
.
          MATCH     "0       ",TASKID
          IF        !@EQUAL
            MOVE      "Unable to move .properties file to claims dir.",ERRORDSC
            GOTO      EXTR9100
          ENDIF
.
. Build _oecwecm file and move to B2B Device oecw claims directory
. -------------------------------------------------------------------
.
EXTR1000  PACK      OECWFLNM,WBOETRID,OECWLITR,SP70
.
          PREP      OECWFILE,OECWFLNM
          CLOSE     OECWFILE,DELETE
          PREP      OECWFILE,OECWFLNM
.
          WRITE     OECWFILE,SEQ;LCBRAC
.
.         claim
.         -----
.
          WRITE     OECWFILE,SEQ;SP2,QUOTE,OECLAB01,QUOTE,COLON,SP2,LCBRAC
.
.         claim>medicalEvent
.         ------------------
.
          WRITE     OECWFILE,SEQ;SP4,QUOTE,OECLAB23,QUOTE,COLON,SP2,LSBRAC
.
          WRITE     OECWFILE,SEQ;SP6,LCBRAC
.
.         claim>medicalEvent>service
.         --------------------------
.
          MOVE      SP70,SERVINDC
          MOVE      ZERO,FORM2
          WHILE     FORM2 < 50
            ADD       ONE,FORM2
.
            MATCH     SP70,OECVAL28[FORM2]
            IF        !@EQUAL
              MOVE      "1",SERVINDC
              GOTO      EXTR5000
            ENDIF
          DO
.
EXTR5000  MATCH     "1",SERVINDC
          IF        @EQUAL
.
            WRITE     OECWFILE,SEQ;SP8,QUOTE,OECLAB27,QUOTE,COLON,LSBRAC
.
            MOVE      SP70,FRSTINDC
            MOVE      ZERO,FORM2
            WHILE     FORM2 < 50
              ADD       ONE,FORM2
.
              MATCH     SP70,OECVAL28[FORM2]
              IF        !@EQUAL
.
                MATCH     "1",FRSTINDC
                IF        @EQUAL
                  WRITE     OECWFILE,SEQ;SP8,COMMA
                ENDIF
.
                MOVE      "1",FRSTINDC
.
                WRITE     OECWFILE,SEQ;SP8,LCBRAC
.
.               claim>medicalEvent>service>timeDependantOverrideInd
.               ---------------------------------------------------
.
                MATCH     SP70,OECVAL38[FORM2]
                IF        !@EQUAL
                  WRITE     OECWFILE,SEQ;SP10,QUOTE,OECLAB38,QUOTE,COLON:
                                              QUOTE,OECVAL38[FORM2],QUOTE,COMMA
                ENDIF
.
.               claim>medicalEvent>service>serviceCodeTypeCode
.               ----------------------------------------------
.
                MATCH     SP70,OECVAL37[FORM2]
                IF        !@EQUAL
                  WRITE     OECWFILE,SEQ;SP10,QUOTE,OECLAB37,QUOTE,COLON:
                                              QUOTE,OECVAL37[FORM2],QUOTE,COMMA
                ENDIF
.
.               claim>medicalEvent>service>restrictionOverrideInd
.               -------------------------------------------------
.
                MATCH     SP70,OECVAL36[FORM2]
                IF        !@EQUAL
                  WRITE     OECWFILE,SEQ;SP10,QUOTE,OECLAB36,QUOTE,COLON:
                                              QUOTE,OECVAL36[FORM2],QUOTE,COMMA
                ENDIF
.
.               claim>medicalEvent>service>numberOfPatientsSeen
.               -----------------------------------------------
.
                MATCH     SP70,OECVAL35[FORM2]
                IF        !@EQUAL
.                 WRITE     OECWFILE,SEQ;SP10,QUOTE,OECLAB35,QUOTE,COLON:
.                                             QUOTE,OECVAL35[FORM2],QUOTE,COMMA
.
                  CLEAR     DIM100
                  APPEND    SP8,DIM100
                  APPEND    QUOTE,DIM100
                  APPEND    OECLAB35,DIM100
                  APPEND    QUOTE,DIM100
                  APPEND    COLON,DIM100
                  APPEND    QUOTE,DIM100
                  APPEND    OECVAL35[FORM2],DIM100
                  RESET     DIM100
                  STRIP     DIM100
                  ENDSET    DIM100
                  APPEND    QUOTE,DIM100
                  APPEND    COMMA,DIM100
                  RESET     DIM100
                  WRITE     OECWFILE,SEQ;DIM100
.
                ENDIF
.
.               claim>medicalEvent>service>itemNumber
.               -------------------------------------
.
.               WRITE     OECWFILE,SEQ;SP10,QUOTE,OECLAB34,QUOTE,COLON:
.                                           QUOTE,OECVAL34[FORM2],QUOTE,COMMA
.
                CLEAR     DIM100
                APPEND    SP10,DIM100
                APPEND    QUOTE,DIM100
                APPEND    OECLAB34,DIM100
                APPEND    QUOTE,DIM100
                APPEND    COLON,DIM100
                APPEND    QUOTE,DIM100
                APPEND    OECVAL34[FORM2],DIM100
                RESET     DIM100
                STRIP     DIM100
                ENDSET    DIM100
                APPEND    QUOTE,DIM100
                APPEND    COMMA,DIM100
                RESET     DIM100
                WRITE     OECWFILE,SEQ;DIM100
.
.               claim>medicalEvent>service>fieldQuantity
.               ----------------------------------------
.
                MATCH     SP70,OECVAL33[FORM2]
                IF        !@EQUAL
                  WRITE     OECWFILE,SEQ;SP10,QUOTE,OECLAB33,QUOTE,COLON:
                                              QUOTE,OECVAL33[FORM2],QUOTE,COMMA
                ENDIF
.
.
.               claim>medicalEvent>service>duplicateServiceOverrideInd
.               ------------------------------------------------------
.
                MATCH     SP70,OECVAL32[FORM2]
                IF        !@EQUAL
                  WRITE     OECWFILE,SEQ;SP10,QUOTE,OECLAB32,QUOTE,COLON:
                                              QUOTE,OECVAL32[FORM2],QUOTE,COMMA
                ENDIF
.
.               claim>medicalEvent>service>dateOfService
.               ----------------------------------------
.
                WRITE     OECWFILE,SEQ;SP10,QUOTE,OECLAB31,QUOTE,COLON:
                                            QUOTE,OECVAL31[FORM2],QUOTE,COMMA
.
.		claim>medicalEvent>service>chargeAmount
.               ---------------------------------------
.
.               WRITE     OECWFILE,SEQ;SP10,QUOTE,OECLAB30,QUOTE,COLON:
.                                           QUOTE,OECVAL30[FORM2],QUOTE,COMMA
.
                CLEAR     DIM100
                APPEND    SP10,DIM100
                APPEND    QUOTE,DIM100
                APPEND    OECLAB30,DIM100
                APPEND    QUOTE,DIM100
                APPEND    COLON,DIM100
                APPEND    QUOTE,DIM100
                APPEND    OECVAL30[FORM2],DIM100
                RESET     DIM100
                STRIP     DIM100
                ENDSET    DIM100
                APPEND    QUOTE,DIM100
                APPEND    COMMA,DIM100
                RESET     DIM100
                WRITE     OECWFILE,SEQ;DIM100
.
.               claim>medicalEvent>service>aftercareOverrideInd
.               -----------------------------------------------
.
                MATCH     SP70,OECVAL29[FORM2]
                IF        !@EQUAL
                  WRITE     OECWFILE,SEQ;SP10,QUOTE,OECLAB29,QUOTE,COLON:
                                              QUOTE,OECVAL29[FORM2],QUOTE,COMMA
                ENDIF
.
.               claim>medicalEvent>service>id
.               -----------------------------
.
                WRITE     OECWFILE,SEQ;SP10,QUOTE,OECLAB28,QUOTE,COLON:
