.------------------------------------------------------------
. Validate and Extract OECW-ECM files
. [TransactionId].properties
. [TransactionId]_oecwecm
.------------------------------------------------------------
OEMW0000  CALL      FFOEC000
.
          CALL      PROC0000               * process
          BRANCH    EXIT,OEMW9999          * error
.
          CALL      EXTR0000               * extract data 
          BRANCH    EXIT,OEMW9999          * error
.
          CALL      POST0000               * Post OECW-ECM JSON to Medicare 
          BRANCH    EXIT,OEMW9999          * error
.
OEMW9999  RETURN
+
.*****************************************************************************
.*                               PROC0000                                    *
.*        Load the OECW-ECM variables with the relevant data                 *
.* Returns: EXIT   0 = Ok to continue                                        *
.*                 1 = error(s) encountered                                  *
.*****************************************************************************
.
. Collect data from webPAS tables         
. ***************************************
.
PROC0000  CALL      CLOEVL00  
.
          IF        FFOECIND=ONE
.
.           Get the Free Format OEC 
.
            PACK      KEY8,WBOEVISN,SP70
            CALL      RDWBOMF1
            IF        OVRCD = 1
              PACK      KEY8,WBOEELEG,SP70
              CALL      RDWBOMF1
              IF        OVRCD = 1
                MOVE      "OEC.03-Free Format OECW record missing.",ERRORDSC
                GOTO      PROC9100
              ENDIF
            ENDIF
.
            MATCH     SP70,WBOEURNO
            IF        !@EQUAL
              MOVE      WBOEURNO,KEY8
              CALL      RDPMPX21
              IF        OVRCD = 1
                MOVE      "OEC.06-PMI Master extension record missing",ERRORDSC
                GOTO      PROC9100
              ENDIF
            ENDIF
.
          ELSE
.
            CALL      CLWEBOMF
.
.           Get the PMI master and master extension records
.
            MOVE      WBOEURNO,KEY8
            CALL      RDMAST1                    * patient master record found ?
            IF        OVRCD = 1
              MOVE      "OEC.05-PMI Master record missing",ERRORDSC
              GOTO      PROC9100
            ENDIF
.
            MOVE      WBOEURNO,KEY8
            CALL      RDPMPX21                   * pt. master ext record found ?
            IF        OVRCD = 1
              MOVE      "OEC.06-PMI Master extension record missing",ERRORDSC
              GOTO      PROC9100
            ENDIF
.
.           Get the visit extension
.
            MOVE      WBOEVISN,KEY8
            CALL      RDPMVX11                     * visit ext. record found ?
            IF        OVRCD = 1
              MOVE      "OEC.04-Visit extension record not found",ERRORDSC
              GOTO      PROC9100
            ENDIF
.
.           Get the admission record for the visit
.  
            MOVE      WBOEVISN,KEY8
            CALL      RDMISS1                      * patient adm. record found ?
            IF        OVRCD = 1
              MOVE      "OEC.07-Admission record not found",ERRORDSC
              GOTO      PROC9100
            ENDIF
.
            CALL      CLPATDSC
.
          ENDIF
.
. Collect date for .properties file
. ***************************************        
.
.         Load webpas.oececmTransactionId=
.         -----------------------------
.
          MATCH     SP70,WBOETRID
          IF        @EQUAL 
            MOVE      "OEC.01-weboemaf transactionid is blank",ERRORDSC
            GOTO      PROC9100
          ELSE
            MOVE      WBOETRID,PRPVAL01
          ENDIF
.
.         Load webpas.oececmMessageId=
.         -------------------------
.
          MATCH     SP70,WBOEMSID
          IF        @EQUAL
            MOVE      "OEC.02-weboemaf messageid is blank",ERRORDSC
            GOTO      PROC9100
          ELSE
            MOVE      WBOEMSID,PRPVAL02
          ENDIF
.
.         Load webpas.oececmSubjectId=
.         -------------------------
.
          IF        FFOECIND=ONE
            MOVE      WBOFMEDI,PRPVAL03
          ELSE
            MOVE      PMEDI,PRPVAL03
          ENDIF
.
          MATCH     SP70,PRPVAL03
          IF        @EQUAL
            MOVE      "N/A",PRPVAL03
            PACK      PRPVAL03,PRPVAL03,SP70
          ENDIF
.
. Collect Data for _oecwecm file
. ***************************************
.
.         Load claim>typeCode
.         -------------------
.
          MOVE      "ECM",OECVAL06 
.
.         Load claim>serviceTypeCode
.         --------------------------
.
          MOVE      "O",OECVAL04
.
          MATCH     SP70,WBOFSTCD
          IF        !@EQUAL
            PACK      KEY5,CATst,WBOFSTCD,SP70
            CALL      RDCODE1                      * code on file ?
            IF        OVRCD=0
              MOVE      TCINDC1,OECVAL04
            ENDIF
          ENDIF
.
.
.         Load claim>accountReferenceId
.         -----------------------------
.
          IF        FFOECIND=ONE
            MOVE      WBOEARID,OECVAL02
            SQUEEZE   OECVAL02
            PACK      OECVAL02,OECVAL02,SP20   
          ELSE
            MOVE      WBOEARID,OECVAL02
            SQUEEZE   OECVAL02
            PACK      OECVAL02,OECVAL02,SP20  
          ENDIF
.
.         Load claim>patient>identity>dateOfBirth
.         ---------------------------------------
.
          IF        FFOECIND=ONE
.
            MATCH     SP70,WBOFBDAT
            IF        @EQUAL
              MOVE      "OEC.08-PatientDateOfBirth is blank",ERRORDSC
              GOTO      PROC9100
            ELSE
              UNPACK    WBOFBDAT,CCENT,CYEAR,CMON,CDAY
              PACK      OECVAL08,CCENT,CYEAR,DASH,CMON,DASH,CDAY
            ENDIF 
.
          ELSE
.
            MATCH     SP70,PBDATE
            IF        @EQUAL
              MOVE      "OEC.08-PatientDateOfBirth is blank",ERRORDSC
              GOTO      PROC9100
            ELSE
              UNPACK    PBDATE,CCENT,CYEAR,CMON,CDAY
              PACK      OECVAL08,CCENT,CYEAR,DASH,CMON,DASH,CDAY
            ENDIF
.
          ENDIF
.
.         Load claim>patient>identity>familyName
.         --------------------------------------
.
          IF        FFOECIND=ONE
.
            PACK      OECVAL09,WBOFSNAM,SP70
            MATCH     SP70,OECVAL09
            IF        @EQUAL
              MOVE      "OEC.09-PatientFamilyName is blank",ERRORDSC
              GOTO      PROC9100
            ENDIF
.
          ELSE
.
            PACK      OECVAL09,PSNAME,SP70
            MATCH     SP70,OECVAL09
            IF        @EQUAL
              MOVE      "OEC.09-PatientFamilyName is blank",ERRORDSC
              GOTO      PROC9100
            ENDIF
.
         ENDIF
.
.         Load claim>patient>identity>givenName
.         -------------------------------------
.
          IF        FFOECIND=ONE
.
            PACK      OECVAL10,WBOFFNAM,SP70
            MATCH     SP70,OECVAL10
            IF        @EQUAL
              MOVE      "OEC.10-PatientGivenName is blank",ERRORDSC
              GOTO      PROC9100
            ENDIF
.
          ELSE
.
            PACK      OECVAL10,PMPXFNAM,SP70
            MATCH     SP70,OECVAL10
            IF        @EQUAL
              MOVE      "OEC.10-PatientGivenName is blank",ERRORDSC
              GOTO      PROC9100
            ENDIF
.
          ENDIF
.
.         Load claim>patient>identity>secondInitial
.         -----------------------------------------
.
          IF        FFOECIND=ONE
.
            PACK      OECVAL11,SP70 
.
            MATCH     SP70,WBOEURNO
            IF        !@EQUAL
              PACK      OECVAL11,PMPXSNAM,SP70
            ENDIF
.
          ELSE
.
            PACK      OECVAL11,PMPXSNAM,SP70
.
          ENDIF
.
.         Load claim>patient>identity>sex
.         -------------------------------
.
          IF        FFOECIND=ONE
.
            MOVE      NINE,OECVAL12
            MATCH     ANSF,WBOFGEND
            IF        @EQUAL
              MOVE      TWO,OECVAL12
            ELSE
              MATCH     ANSM,WBOFGEND
              IF        @EQUAL
                MOVE      ONE,OECVAL12
              ELSE
                MATCH     SP70,WBOFGEND
                IF        !@EQUAL & !@EOS
                  MOVE      THREE,OECVAL12
                ENDIF
              ENDIF
            ENDIF
.
          ELSE
.
            MOVE      NINE,OECVAL12
            MATCH     ANSF,PSEX
            IF        @EQUAL
              MOVE      TWO,OECVAL12
            ELSE
              MATCH     ANSM,PSEX
              IF        @EQUAL
                MOVE      ONE,OECVAL12
              ELSE
                MATCH     SP70,PSEX
                IF        !@EQUAL & !@EOS
                  MOVE      THREE,OECVAL12
                ENDIF
              ENDIF
            ENDIF
.
          ENDIF
.
.         Load claim>patient>medicare>memberNumber
.         ----------------------------------------
.
          IF        FFOECIND=ONE
            MOVE      WBOFMEDI,OECVAL14
          ELSE
            MOVE      PMEDI,OECVAL14
          ENDIF
.
.         Load claim>patient>medicare>memberRefNumber
.         ----------------------------------------
.
          IF        FFOECIND=ONE
            MOVE      WBOFMEDR,OECVAL15
          ELSE
            MOVE      PTMXMCCD,OECVAL15
          ENDIF
.
.         Load claim>principalProvider>providerNumber
.         ----------------------------------------
.
          IF        FFOECIND=ONE
.
            MATCH     SP70,WBOFPRPR
            IF        !@EQUAL & !@EOS
.
              PACK      KEY10,WBOFPRPR,SP70
              CALL      RDPMHCP1
              IF        OVRCD=0
.
                MATCH     SP70,PMHCPRV1
                IF        !@EQUAL
                  MOVE      PMHCPRV1,OECVAL21
                ENDIF
.
              ENDIF           
.
            ENDIF
.
          ELSE
.
            MATCH     SP70,ADOCTA
            IF        !@EQUAL & !@EOS
.
              PACK      KEY10,ADOCTA,SP70
              CALL      RDPMHCP1
              IF        OVRCD=0
.
                MATCH     SP70,PMHCPRV1
                IF        !@EQUAL
                  MOVE      PMHCPRV1,OECVAL21
                ENDIF
.
              ENDIF
.
            ENDIF
.
          ENDIF
.
.         Load claim>serviceProvider>providerNumber
.         ----------------------------------------
.
          IF        FFOECIND=ONE 
. 
            MATCH     SP70,WBOFSVPR
            IF        !@EQUAL & !@EOS
.
              PACK      KEY10,WBOFSVPR,SP70
              CALL      RDPMHCP1
              IF        OVRCD=0
.
                MATCH     SP70,PMHCPRV1
                IF        !@EQUAL
                  MOVE      PMHCPRV1,OECVAL26
                ENDIF
.
              ENDIF
.
            ENDIF
.
          ELSE
.
            MATCH     SP70,ADOCTA
            IF        !@EQUAL & !@EOS
.
              PACK      KEY10,ADOCTA,SP70
              CALL      RDPMHCP1
              IF        OVRCD=0
.
                MATCH     SP70,PMHCPRV1
                IF        !@EQUAL
                  MOVE      PMHCPRV1,OECVAL26
                ENDIF
.
              ENDIF
.
            ENDIF
.
          ENDIF
.
.         Load claim>senderContact>emailAddress
.         -------------------------------------
.
PROC3100  MATCH     SP80,OECVAL17              * blank sender contact email ?
          IF        @EQUAL
            MOVE      PTCNSCEM,OECVAL17          * no
          ENDIF
.
.         Load claim>senderContact>name
.         -----------------------------
.
          MATCH     SP40,OECVAL18                * blank sender contact name ?
          IF        @EQUAL
            MOVE      PTCNSCPN,OECVAL18          * no
          ENDIF
.
.         Load claim>senderContact>phoneNumber
.         ------------------------------------
.
          MATCH     SP20,OECVAL19               * blank sender contact phone ?
          IF        @EQUAL
            MOVE      PTCNSCPP,OECVAL19         * no
          ENDIF
.
.         Load claim>facilityId
.         ---------------------
.
          IF        IBCNMHOS = 1
.
            IF        FFOECIND=ONE
.
              PACK      KEY10,WBOFCUID,SP70
              CALL      RDWBSE1
              IF        OVRCD=ONE
                MOVE      "Created By User record missing",ERRORDSC
                GOTO      PROC9100
              ENDIF
.
              MATCH     SP3,WBSEHOSP                 * hospital id blank ?
              IF        @EQUAL
                MOVE      "Hospital id blank",ERRORDSC
                GOTO      PROC9100
              ENDIF
.
              MOVE      WBSEHOSP,KEY3
              CALL      RDPTHSP1                     * hospital code on file ?
              IF        OVRCD = 1
                MOVE      "Hospital code (",ERRORDSC
                ENDSET    ERRORDSC
                APPEND    WBSEHOSP,ERRORDSC
                APPEND    ") not on file",ERRORDSC
                RESET     ERRORDSC
                GOTO      PROC9100
              ENDIF
.
            ELSE
.
              MATCH     SP3,PMVXMHOS                 * hospital id blank ?
              IF        @EQUAL
                MOVE      "Hospital id blank",ERRORDSC
                GOTO      PROC9100
              ENDIF
.
              MOVE      PMVXMHOS,KEY3
              CALL      RDPTHSP1                     * hospital code on file ?
              IF        OVRCD = 1
                MOVE      "Hospital code (",ERRORDSC
                ENDSET    ERRORDSC
                APPEND    PMVXMHOS,ERRORDSC
                APPEND    ") not on file",ERRORDSC
                RESET     ERRORDSC
                GOTO      PROC9100
              ENDIF
.
            ENDIF
.
            MOVE      PTHSAPPR,OECVAL03
          ELSE
            MOVE      CAPPRVNO,OECVAL03
          ENDIF
.
.         Load claim>submissionAuthorityInd
.
PROC6000  MOVE      "Y",OECVAL05
.
.         Load      claim>medicalEvent>id
.
          MOVE      "01",OECVAL24
.
.         Load      claim>medicalEvent>service>
.
          IF        FFOECIND=ONE
            CALL      FOMS0000                     * write services segment
            BRANCH    EXIT,PROC9100                * error occured
          ELSE
            CALL      WOMS0000                     * write services segment
            BRANCH    EXIT,PROC9100                * error occured
          ENDIF
.
          MOVE      ZERO,EXIT
          GOTO      PROC9999
.
PROC9100  MOVE      "1",ERRORIND
          MOVE      ONE,EXIT
.
PROC9999  RETURN
.
.
