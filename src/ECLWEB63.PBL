.******************************************************************************
.*                             WOMS0000            Called by: PROC0000        *
.*                  Populate the OES (services) variables                     *
.* Returns: EXIT   0 = no errors occurred                                     *
.*                 1 = one or more errors occurred                            *
.******************************************************************************
WOMS0000  MOVE      ZERO,OMSCOUNT                * initialise oes counter
.
          PACK      KEY14,WBOEVISN,WBOECNTR,SP20
          CALL      RSWBOMS1                     * position on visit/report
WOMS0500  CALL      RKWBOMS1                     * read next record
          BRANCH    OVRCD,WOMS9000               * eof - finished
.
          MATCH     WBOEVISN,WBOMVISN            * same visit still ?
          GOTO      WOMS9000 IF NOT EQUAL        * no - finished
.
          MATCH     WBOECNTR,WBOMCNTR            * same report count still ?
          GOTO      WOMS9000 IF NOT EQUAL        * no - finished
.
          ADD       ONE,OMSCOUNT                 * increment oes count
.
.         Load claim>medicalEvent>service>id
.
          MOVE      OMSCOUNT,OECVAL28[OMSCOUNT]
          RJUSTIFY  OECVAL28[OMSCOUNT]
          REP       " 0",OECVAL28[OMSCOUNT] 
.
.         Load the ServiceCode
.
          MATCH     "7",WBOMRTYP                 * MBS charge ?
          IF        !@EQUAL
            MOVE      "Service Record Type not valid",ERRORDSC
            GOTO      WOMS9100
          ENDIF
.
.         Load      claim>medicalEvent>service>dateOfService
.
          UNPACK    WBOMPDTE,CCENT,CYEAR,CMON,CDAY
          PACK      OECVAL31[OMSCOUNT],CCENT,CYEAR,DASH,CMON,DASH,CDAY 
.                                                  * set DateOfService
.
.         Load      claim>medicalEvent>service>timeDependantOverrideInd
.
          MATCH     "1",WBOMTDOI
          IF        @EQUAL
            MOVE      "Y",OECVAL38[OMSCOUNT]   * set timeDependantOverrideInd
          ENDIF
.
.
.         Load      claim>medicalEvent>service>serviceCodeTypeCode
.
          MOVE      "C",OECVAL37[OMSCOUNT]     * set serviceCodeTypeCode
.
.
.         Load      claim>medicalEvent>service>restrictionOverrideInd
.
          MATCH     "1",WBOMRSOI
          IF        @EQUAL
            MOVE      "Y",OECVAL36[OMSCOUNT]     * set restrictionOverrideInd
          ENDIF
.
.         
.         Load      claim>medicalEvent>service>numberOfPatientsSeen
.
          MOVE      WBOMNOPT,OECVAL35[OMSCOUNT]     * set numberOfPatientsSeen
.
.
.         Load      claim>medicalEvent>service>itemNumber
.
          MOVE      WBOMITMN,OECVAL34[OMSCOUNT]     * set numberOfPatientsSeen
.
.
.         Load      claim>medicalEvent>service>fieldQuantity
.
          MOVE      WBOMFQNT,OECVAL33[OMSCOUNT]     * set fieldQuantity
.
.
.         Load      claim>medicalEvent>service>duplicateServiceOverrideInd
.
          MATCH     "1",WBOMDSOI
          IF        @EQUAL
            MOVE      "Y",OECVAL32[OMSCOUNT]     * set duplicateServiceOver
          ENDIF
.
.
.         Load      claim>medicalEvent>service>chargeAmount        
.
          PACK      DIM9,WBOMITMN,SP70
.         
          PACK      KEY17,DIM9,WBOMPDTE,SP70
          CALL      PATITMRD
          IF        OVRCD<>1
            ASSIGN    QIAMT,FORM12P2
            MOVE      FORM12P2,DIM15
            UNPACK    DIM15,KEY5,KEY7,ANS,KEY2
            PACK      KEY9,KEY7,KEY2
.           REP       " 0",KEY9
            LJUSTIFY  KEY9
            PACK      KEY9,KEY9,SP70
            MOVE      KEY9,OECVAL30[OMSCOUNT]      * set chargeAmount
          ELSE
            MOVE      "Charge Amount not valid.",ERRORDSC
            GOTO      WOMS9100
          ENDIF 
.
.
.         Load      claim>medicalEvent>service>aftercareOverrideInd
.
          MATCH     "1",WBOMACOI
          IF        @EQUAL
            MOVE      "Y",OECVAL29[OMSCOUNT]     * set aftercareOverrideInd
          ENDIF
.
.
          GOTO      WOMS0500                     * get next service
.
WOMS9000  MOVE      ZERO,EXIT
          GOTO      WOMS9999
.
WOMS9100  MOVE      ONE,EXIT
.
WOMS9999  RETURN
