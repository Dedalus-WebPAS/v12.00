.---------------------------------------------------------------
. Get Free Format Indicator
.---------------------------------------------------------------
FFOEC000  MOVE      ZERO,FFOECIND                * Determine if Free Format OEC
          MOVE      SP70,DIM1
          MOVE      WBOEVISN,DIM1
          REP       REPALPHA,DIM1
          MATCH     "~",DIM1
          IF        @EQUAL
            MOVE      ONE,FFOECIND
          ELSE
            PACK      KEY8,WBOEVISN,SP70
            CALL      RAPTMIS1
            IF        OVRCD=ONE
              PACK      KEY8,WBOEVISN,SP70
              CALL      RAWBOMF1
              IF        OVRCD=ZERO
                MOVE      ONE,FFOECIND
                GOTO      FFOEC999
              ENDIF
.
              MATCH   SP70,WBOEELEG
              IF      !@EQUAL & !@EOS
                PACK      KEY8,WBOEELEG,SP70
                CALL      RAWBOMF1
                IF        OVRCD=ZERO
                  MOVE      ONE,FFOECIND
                  GOTO      FFOEC999
                ENDIF
              ENDIF
.
            ENDIF
.
            MATCH   SP70,WBOEELEG
            IF      !@EQUAL & !@EOS
              PACK      KEY8,WBOEELEG,SP70
              CALL      RAWBOMF1
              IF        OVRCD=ZERO
                MOVE      ONE,FFOECIND
                GOTO      FFOEC999
              ENDIF
            ENDIF
.
          ENDIF
.
FFOEC999  RETURN
