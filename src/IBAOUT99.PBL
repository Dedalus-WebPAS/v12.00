.*********************************************************************
. Keyin new value for comments     
.*********************************************************************
KCOM0000  DISPLAY   *P1:3,*V2LON,"Enter New Values"
.
          KEYIN     *P1:4,*EF:
                    *P1:4,"Clinic Comments. Retain Original Value : ":
                    " (",*V2LON,"Y",*HOFF,"/":
                    *V2LON,"N",*HOFF,") ? ",*V2LON,RETAIN01

          REP       UPPLOW,RETAIN01
          MATCH     ANSY,RETAIN01
          IF        @EQUAL
            GOTO      KCOM1000
          ENDIF
.
          MATCH     ANSN,RETAIN01
          IF        @EQUAL
            GOTO      KCOM1000
          ENDIF
.
          BEEP
          GOTO      KCOM0000
.
