.
          MATCH     SAVPPOST,PPOST
          IF        !@EQUAL
            MOVE      "10",P2PMUNIQ
            MOVE      PPOST,P2PMVALU
            CALL      WPRSP000
          ENDIF
          MATCH     SAVPMEDI,PMEDI
          IF        !@EQUAL
