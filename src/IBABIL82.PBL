          MOVE      FROMDATE,ICDRDDTE
          CALL      RDPTICO1
          IF        OVRCD=1 & ICDRDVER>1
            CALL      RD10T9O1
          ENDIF
          BRANCH    OVRCD,OPCO3200
          MOVE      ODESC,OPFRDESC
