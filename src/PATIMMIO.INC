.
.PATIMMIO
.--------
.
RSPTIMM1  RESET     KEY19
          READ      PATIMMA1,KEY19;;
          RETURN
.
RAPTIMM1  RESET     KEY19
          MOVE      ZERO,OVRCD
          READ      PATIMMA1,KEY19;ANS
          GOTO      OVERCOND IF OVER
          RETURN
.
RDPTIMM1  RESET     KEY19
          MOVE      ZERO,OVRCD
          READ      PATIMMA1,KEY19;PTIMURNO,PTIMDATE,PTIMTYPE,PTIMPROV:
                                  PTIMPVTY,PTIMDUED,PTIMSPAR
          GOTO      OVERCOND IF OVER
          RETURN
.
RKPTIMM1  MOVE      ZERO,OVRCD
          READKS    PATIMMA1;PTIMURNO,PTIMDATE,PTIMTYPE,PTIMPROV:
                                  PTIMPVTY,PTIMDUED,PTIMSPAR
          GOTO      OVERCOND IF OVER
          RETURN
.
RPPTIMM1  MOVE      ZERO,OVRCD
          READKP    PATIMMA1;PTIMURNO,PTIMDATE,PTIMTYPE,PTIMPROV:
                                  PTIMPVTY,PTIMDUED,PTIMSPAR
          GOTO      OVERCOND IF OVER
          RETURN
.
UPPTIMM1  UPDATE    PATIMMA1;PTIMURNO,PTIMDATE,PTIMTYPE,PTIMPROV:
                                  PTIMPVTY,PTIMDUED,PTIMSPAR
          RETURN
.
WRPTIMM1  RESET     KEY19
          WRITE     PATIMMA1,KEY19;PTIMURNO,PTIMDATE,PTIMTYPE,PTIMPROV:
                                  PTIMPVTY,PTIMDUED,PTIMSPAR
          RETURN
.
DEPTIMM1  RESET     KEY19
          DELETE    PATIMMA1,KEY19
          RETURN
.
