          ELSE
            MOVE      SP3,KEYSTARR[COUNT]
          ENDIF
.
          ADD       ONE,COUNT
          MOVE      ONE,KEYSTARR[COUNT]    * extract pcs (1)
.
.         ADD       ONE,COUNT
.         MOVE      ONE,OPTNFLAG
.
.         MATCH     "2",PRECEETP
.         IF        @EQUAL
.           MOVE       TWO,OPTNFLAG
.         ENDIF
.         MOVE      OPTNFLAG,KEYSTARR[COUNT]    * extract by fund(1)/med(2)
.
.         COMPARE   TWO,OPTNFLAG
.         GOTO      SPCIX500 IF EQUAL
.
.         ADD       ONE,COUNT
