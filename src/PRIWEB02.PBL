          UNPACK    QRYNAME,KEY5,KEY3
          MOVE      KEY3,F3
          ASSIGN    F3-100,F3
          IF        F3>0 & F3<100
            RESET     QRYDATA
            MATCH     SP70,QRYDATA
            GOTO      STMTIM99 IF EQUAL
            PACK      MTIME[F3],QRYDATA,SP70
          ENDIF
STMTIM99  RETURN
