SPTXOA00  WRITE     HTMLFILE;"<tr align=center>":
                             "<td class=HeadingCell colspan=2>":
                             "<b>You Searched for ":
                               KEYWORDS,"</td>":
                             "</tr>":
                             "<tr>":
                             "<td class=HeadingCell width=15%><b>Code</b></td>":
                             "<td class=HeadingCell><b>Description</b></td>":
                             "</tr>"
.
          CALL      GETWRD00
.
          UNPACK    DIM127,D1,LINKPRG,DIM127
          UNPACK    DIM127,D1,LINKREP,DIM127
          UNPACK    DIM127,D1,LINKTEM,DIM127
.
          IF        NORECORD=0
            MOVE      PTCNNRTD,NORECORD     * Set Default No Records to Display
          ENDIF
          MOVE      ZERO,RECNUMB
          MOVE      ZERO,DISNUMB
.
....      UNPACK    PTCNGDVX,KEY4,KEY5
....      MATCH     "9999",KEY4          * is ICD10 Ed.10 in use yet?
....      GOTO      SPTXOA90 IF EQUAL
.
          STRIP     KEYWRD01
          PACK      KEY24,KEYWRD01,SP70
          CALL      RSPTXO2
SPTXOA10  CALL      RKPTXO2
          BRANCH    OVRCD,SPTXOA90
.
          CALL      SPTXOB00
          BRANCH    OVRCD,SPTXOA10,SPTXOA90
.
          MOVE      PTCOITM,KEY9
          CALL      RDPT1XO1
          BRANCH    OVRCD,SPTXOA10
.
          ADD       ONE,RECNUMB
          IF        STARTNUM>=RECNUMB
            GOTO      SPTXOA10
          ENDIF
.
          BRANCH    LNK2PRT,SPTXOA20,SPTXOA30
          GOTO      SPTXOA99
.
SPTXOA20  WRITE     HTMLFILE;"<tr><td nowrap><A HREF='javascript:showICD02":
                             "(#"",LINKPRG,"#.pbl":
                             "?reportno=",LINKREP:
                             "&template=",LINKTEM:
                             "&ptico001=",OPCODE,"#")'>":
                             "<img src=../images/MaintenanceIcon.gif hspace=4":
                             " border=0 align=absmiddle></a>":
                             OPCODE,"</td>":
                             "<td>",PT0ODSC2,"</td>":
                             "</tr>"
.
          GOTO      SPTXOA80
.
SPTXOA30  WRITE     HTMLFILE;"<tr valign=top>":
                              "<td><a href=#"#" ":
                              "OnClick=#"updateParent('",PT0ODSC2:
                              "','",OPCODE,"')#">":
                              "<img src=../images/MaintenanceIcon.gif hspace=4":
                              " border=0 align=absmiddle></a>":
                              OPCODE,"</td>":
                              "<td>",PT0ODSC2,"</td>":
                             "</tr>"
.
SPTXOA80  ADD       ONE,DISNUMB
          COMPARE   NORECORD,DISNUMB
          GOTO      SPTXOA10 IF NOT EQUAL
          GOTO      SPTXOA99
.
SPTXOA90  CALL      NOMORE00        * Display No More Records Message
.
SPTXOA99  RETURN
.------------------------------------------------------------
. Check ICD10 Edition 10 Procedure Record
.------------------------------------------------------------
SPTXOB00  MATCH     KEYWRD01,PTCOKWD
          GOTO      SPTXOB92 IF NOT EQUAL
          IF        KEYWRDNO>1
            PACK      SAVKEY24,PTCOKWD,PTCOITM
            CALL      SPTXOC00
            BRANCH    EXIT,SPTXOB91
            MOVE      SAVKEY24,KEY24
            CALL      RDPTXO2
          ENDIF
.
. Insert Other Check for Valid Record Here i.e. Suspended or In Active
.   eg    COMPARE    ONE,INACTIVE
.         GOTO       SPTXOB91 IF EQUAL
.
          GOTO      SPTXOB90
.
SPTXOB90  MOVE      ZERO,OVRCD
          GOTO      SPTXOB99
SPTXOB91  MOVE      ONE,OVRCD
          GOTO      SPTXOB99
SPTXOB92  MOVE      TWO,OVRCD
SPTXOB99  RETURN
.------------------------------------------------------------
. Check for Matching ICD10 Edition 10 Procedure Words
.------------------------------------------------------------
SPTXOC00  MOVE      ONE,F2
          MOVE      ZERO,EXIT
          PACK      SAVKEY9,PTCOITM,SP70
.
SPTXOC10  COMPARE   KEYWRDNO,F2           * Loop through each Key Word in Search
          GOTO      SPTXOC99 IF EQUAL     * Exit OK
          ADD       ONE,F2
          LOAD      KEYWRDXX,F2,KEYWRD01,KEYWRD02,KEYWRD03,KEYWRD04,KEYWRD05:
                             KEYWRD06,KEYWRD07,KEYWRD08,KEYWRD09,KEYWRD10
          PACK      KEY24,PTCOITM,KEYWRDXX
          CALL      RDPTXO1               * Check for Exact Match
          COMPARE   ZERO,OVRCD            * Ok so Continue Loop
          GOTO      SPTXOC10 IF EQUAL
.
          CALL      RKPTXO1               * Not Exact so Read Next Record
          BRANCH    OVRCD,SPTXOC90        * No Match So Exit No Match
          PACK      KEY9,PTCOITM,SP70
          MATCH     SAVKEY9,KEY9        * Match Key Fields
          GOTO      SPTXOC90 IF NOT EQUAL * No Match So Exit
          STRIP     KEYWRDXX
          MATCH     PTCOKWD,KEYWRDXX         * Check Key Word
          GOTO      SPTXOC90 IF NOT EQUAL * No Match So Exit
          GOTO      SPTXOC10              * Check Next Search Word
.
SPTXOC90  MOVE      ONE,EXIT
SPTXOC99  RETURN
