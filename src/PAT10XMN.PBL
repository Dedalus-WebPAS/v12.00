.------------------------------------------------------------------------------
. Online ICD10 Edition 10 Code Validation
.------------------------------------------------------------------------------
VALXCD00  MOVE      SP70,VALDNAME
          MATCH     SP70,VALDCODE
          GOTO      VALXCD90 IF EQUAL
.
          MOVE      "Invalid Code Entered",VALDNAME
          BRANCH    VALDTYPE,VALXCD10,VALXCD20
          MOVE      "Invalid Validation Type",VALDNAME
          GOTO      VALXCD90
.
VALXCD10  MOVE      VALDCODE,KEY9
          CALL      RDPT1XO1
          IF        OVRCD=0
            MOVE      PT0ODSC2,VALDNAME
          ENDIF
          GOTO      VALXCD90
.
VALXCD20  MOVE      VALDCODE,KEY9
          CALL      RDPT1XD1
          IF        OVRCD=0
            MOVE      PT0DDSC2,VALDNAME
          ENDIF
          GOTO      VALXCD90
.
VALXCD90  CALL      OPENHTML
          BRANCH    EXIT,VALXCD99
          WRITE     HTMLFILE;"<h3 align=center>Checking Database</h3>":
                             "<script type=#"text/javascript#">":
                             "{ opener.ValidateName.value=#"",VALDNAME,"#";":
                             "  opener.ValidateCode.value=#"",VALDCODE,"#";"
.
          MATCH     "Invalid",VALDNAME
          IF        @EQUAL
            WRITE     HTMLFILE;"  alert(#"Invalid Code Entered#");";
          ENDIF
.
          WRITE     HTMLFILE;"  close(); } </script>"
          CALL      CLOSHTML
.
VALXCD99  RETURN
