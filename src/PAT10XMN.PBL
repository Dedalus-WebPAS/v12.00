.------------------------------------------------------------------------------
. ICD10 Edition 10 Diagnosis Coding Maintenance: Display,Add,Update,Delete
.------------------------------------------------------------------------------
ICDXDI00  RESET     PTICD001
          MOVE      "10",ICD10EDN
.                                           * make sure files are open
          OPEN      PATI1XD1,"pat10dxf"     * Open ICD10 Ed.10 Diseases
          OPEN      PATKXDA1,"patkcdxf"     * ICD10 Ed.10 Key Word Diag.
          OPEN      PATKXDA2,"patkcdxf"     * ICD10 Ed.10 Key Word Diag.
.
          RESET     UPDTTYPE
          MATCH     SP1,UPDTTYPE
          GOTO      ICDXDI40 IF EQUAL
.
          MATCH     "A",UPDTTYPE    * Add formaction
          GOTO      ICDXDI10 IF EQUAL
.
          MATCH     "U",UPDTTYPE    * Update formaction
          GOTO      ICDXDI20 IF EQUAL
.
          MATCH     "D",UPDTTYPE    * Delete formaction
          GOTO      ICDXDI30 IF EQUAL
.
          MATCH     "V",UPDTTYPE    * Validate Formaction
          IF        @EQUAL
            CALL      VALXCD00
            GOTO      ICDXDI99
          ENDIF
.
          GOTO      ICDXDI93
.
ICDXDI10  CALL      CHKDIS00  * Checks mandatory fields & checks for blank
          BRANCH    EXIT,ICDXDI99
          PACK      KEY9,PTICD001,SP70  * ADD FORMACTION
          CALL      RDPT1XD1
          COMPARE   ZERO,OVRCD
          GOTO      ICDXDI92 IF EQUAL
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      WRPT1XD1      * Writes away the data
          PROC      PATKXDUP      * Update Keyword table
.
          CALL      ADDCAN00      * Add Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICDXDI99
.
ICDXDI20  PACK      KEY9,PTICD001,SP70  * UPDATE FORMACTION
          CALL      RDPT1XD1
          BRANCH    OVRCD,ICDXDI91
          CALL      CHKDIS00      * Checks mandatory fields
          BRANCH    EXIT,ICDXDI99
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      UPPT1XD1      * Writes away the data
          PROC      PATKXDUP      * Update Key word Table
.
          CALL      UPDCAN00      * Update Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICDXDI99
.
ICDXDI30  PACK      KEY9,PTICD001,SP70   * DELETE FORMACTION
          CALL      RDPT1XD1
          BRANCH    OVRCD,ICDXDI91
          CALL      DEPT1XD1      * Deletes the data
          PROC      PATKXDUP      * Update Key word Table
.
          CALL      DELCAN00      * Delete Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICDXDI99
.
ICDXDI40  PACK      KEY9,PTICD001,SP70
          CALL      RDPT1XD1
          IF        OVRCD=1
            CALL      CLRDIS00    * Clear all the file variables
          ENDIF
.
          MOVE      ONE,CANCERCD
          PACK      KEY9,PTICD001,SP70
          CALL      RDPTCAN1
          IF        OVRCD=1
            MOVE     ZERO,CANCERCD
            MOVE     ZERO,PTCCLATR
          ENDIF
.
          CLEAR     WEBTITLE
          MOVE      "ICD10 Edition 10 Diagnosis Maintenance",WEBTITLE
          RESET     WEBTITLE
.
          CALL      WEBHED00   * Create Output File and Write HTML Page Header
          BRANCH    EXIT,ICDXDI99
          CALL      WEBBOD00   * Process Web Body
          CALL      WEBEND00   * Process End of HTML File
          MOVE      ONE,EXIT
          GOTO      ICDXDI99
.
ICDXDI91  MOVE      "ICD10 Edition 10 Diagnosis Code does not exist",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICDXDI99
.
ICDXDI92  MOVE      "ICD10 Edition 10 Diagnosis Code exists",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICDXDI99
.
ICDXDI93  MOVE      "Invalid Form Action.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICDXDI99
.
ICDXDI99  RETURN
+
