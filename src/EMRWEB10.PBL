SUBNUR00  MATCH    "1",EMIPNCOM
          IF       @EQUAL
            MOVE      "2",DIM1
            MOVE      EMIPNURS,EMISCODE
            PACK      KEY11,DIM1,EMISCODE,SP70
            CALL      RDEMISM1        * Reading Incomplete summary table
            IF        OVRCD=0
              IF        EMISNOIN=1
                 CALL      DEEMISM1
              ELSE
                 SUB       ONE,EMISNOIN
                 CALL      UPEMISM1           * Updating summary table
              ENDIF
            ENDIF
          ENDIF
.
SUBNUR99  RETURN
.------------------------------------------------------------
