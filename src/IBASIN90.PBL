.********************************************************************
. WRIT - print data line                            Called by PRNT
.********************************************************************
WRIT0000  MATCH     LASTCODE,SIBACST             * next cost centre
          IF        !@EQUAL
            PACK      LASTCOD7,SP70
            PACK      LASTCODE,SIBACST,SP70
            MOVE      "999",CLNO
          ENDIF
.
          MATCH     LASTWAR,SIBAWAR              * next warehouse
          IF        !@EQUAL
            PACK      LASTCOD7,SP70
            PACK      LASTWAR,SIBAWAR,SP70
            MOVE      "999",CLNO
          ENDIF
.
          COMPARE   "55",CLNO                    * end of page ?
          CALL      HEAD0000 IF NOT LESS         * start new page
.
WRIT0100  PACK      SIIADES,SP70
          PACK      SIIAISS,SP70
          PACK      SIIASUB,SP70
          PACK      KEY7,SIBACAT,SP70
          CALL      RDSIIA1
          UNPACK    SIIADES,KEY60
.
          PACK      SIWADES,SP70
          PACK      KEY5,SIBAWAR,SP70
          CALL      RDSIWA1
.
          CALL      BKLIN000
.
          UNPACK    SIBADAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          PACK      REQDATE,CPCDATE,SP70
          MOVE      SIBAQTY,BACKQUAN
.
          MATCH     LASTCOD7,SIBACAT             * next catalog ?
          IF        !@EQUAL
            PACK      SISADESC,SP70
            PACK      KEY5,SIIASUB,SP70
            CALL      RDSISA1
.
            PACK      LASTCOD7,SIBACAT,SP70
            CALL      PLAST000                   * print end of last description
            PRINT     "|",SIBACAT,"-",SIIASUB,SP1,KEY30," | ",SIBAWAR," | ":
                      SIIAISS," |",BACKQUAN," ",SIBAREQ," ",CPCDATE," |";
            CALL      CORDR000                   * calc on order
            PRINT     F9P2," ";
            CALL      PREV0000
            PACK      DELDATE,CPCDATE,SP70
            PRINT     CPCDATE,*132,"|"
            PACK      SKEY30,KEY60,SP70
          ELSE
            PRINT     "|         ",SKEY30,"      |       |":
                      "                 |":
                      BACKQUAN," ",SIBAREQ," ",CPCDATE," |":
                      *132,"|"
            MOVE      SP70,SKEY30
          ENDIF
          CALL        WRTEXP00
.
WRIT9999  RETURN
