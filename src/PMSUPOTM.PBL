.------------------------------------------------------------
. Patient OP Program Management
.------------------------------------------------------------
PMUO0000  BRANCH    FLDITMNO,PMUO0100,PMUO0200,PMUO0300,PMUO0400,PMUO0500:
                             PMUO0600,PMUO0700,PMUO0800,PMUO0900,PMUO1000:
                             PMUO1100,PMUO1200,PMUO1300,PMUO1400,PMUO1500:
                             PMUO1600
          GOTO      PMUO9999
.
PMUO0100  WRITE     HTMLFILE;PMUOURNO;
          GOTO      PMUO9999
.
PMUO0200  MOVE      "A ",CATEGORY
          MOVE      PMUOATYP,CODE
          CALL      CODITM00
          GOTO      PMUO9999
.
PMUO0300  MOVE      "CL",CATEGORY
          MOVE      PMUOCLAM,CODE
          CALL      CODITM00
          GOTO      PMUO9999
.
PMUO0400  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      ZERO,F1
          MOVE      KEY1,F1
          BRANCH    F1,PMUO0410
          WRITE     HTMLFILE;PMUOFUND;
          GOTO      PMUO9999
.
PMUO0410  PACK      KEY14,PMUOFUND,ZERO,ZERO,ZERO,ZERO,SP10
          CALL      RDFUND1
          IF        OVRCD=0
            WRITE     HTMLFILE;HFNAME;
          ELSE
            WRITE     HTMLFILE;PMUOFUND;
          ENDIF
          GOTO      PMUO9999
.
PMUO0500  MOVE      "HT",CATEGORY
          MOVE      PMUOTABT,CODE
          CALL      CODITM00
          GOTO      PMUO9999
.
PMUO0600  MATCH     PMUOEDAT,SP70
          GOTO      PMUO9999 IF EQUAL
.
          UNPACK    PMUOEDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUO9999
.
PMUO0700  MOVE      "CV",CATEGORY
          MOVE      PMUOVTYP,CODE
          CALL      CODITM00
          GOTO      PMUO9999
.
PMUO0800  WRITE     HTMLFILE;PMUOMAXO;
          GOTO      PMUO9999
.
PMUO0900  WRITE     HTMLFILE;PMUOWARO;
          GOTO      PMUO9999
.
PMUO1000  WRITE     HTMLFILE;PMUOMANO;
          GOTO      PMUO9999
.
PMUO1100  MATCH     PMUOCDAT,SP70
          GOTO      PMUO9999 IF EQUAL
.
          UNPACK    PMUOCDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUO9999
.
PMUO1200  WRITE     HTMLFILE;PMUOCTIM;
          GOTO      PMUO9999
.
PMUO1300  MATCH     SP70,PMUOCUID
          GOTO      PMUO9999 IF EQUAL
.
          PACK      KEY10,PMUOCUID,SP70
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      PMUOCUID,WBSENAM
          ENDIF
          WRITE     HTMLFILE;WBSENAM;
          GOTO      PMUO9999
.
PMUO1400  MATCH     PMUOUDAT,SP70
          GOTO      PMUO9999 IF EQUAL
.
          UNPACK    PMUOUDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUO9999
.
PMUO1500  WRITE     HTMLFILE;PMUOUTIM;
          GOTO      PMUO9999
.
PMUO1600  MATCH     SP70,PMUOUUID
          GOTO      PMUO9999 IF EQUAL
.
          PACK      KEY10,PMUOUUID,SP70
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      PMUOUUID,WBSENAM
          ENDIF
          WRITE     HTMLFILE;WBSENAM;
          GOTO      PMUO9999
.
PMUO9999  RETURN
