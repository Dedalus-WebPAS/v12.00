          KEYIN     *P26:8,*EL,*V2LON,*RV,DIM1;
          GOTO      KCDI9999 IF EOS
.
          ENDSET    DIM1
          APPEND    SP70,DIM1
          RESET     DIM1
.
          MATCH     SP70,DIM1
          GOTO      KCDI9999 IF EQUAL
.
          REP       UPPLOW,DIM1
          MATCH     "Y",DIM1
          GOTO      KCDI1000 IF EQUAL
.
          MATCH     "N",DIM1
          GOTO      KCDI1000 IF EQUAL
.
          BEEP
          GOTO      KCDI0000
.
