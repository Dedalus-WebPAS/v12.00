.         WRITE     HTMLFILE;"<RETURN_VALUE TYPE=SIMPLE>":
.                   SP20,"|",DIM11,"|",DIM40,"|",SP30,"|",SP20,"|":
.                   SP30,"|",DIM36,"|",SP30,"|",SP3,"|":
.                   SP10,"|",SP20,"|",SP30,"|",SP30,"|":
.                   SP20,"|",SP30,"|",DIM12,"|",SP1: 
