            WRITE     HTMLFILE;"<RETURN_VALUE TYPE=SIMPLE>": * Silent
                               SP1:                          * No errors
                               "</RETURN_VALUE>"
          ELSE
            WRITE     HTMLFILE;"<RETURN_VALUE TYPE=ERROR>":
                               "Invalid Clinic Type - ":
                                KEY12,"</RETURN_VALUE>"
