            PACK      DIM11,CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR
.
            REP       " 0",PTDEEDAT
            UNPACK    PTDEEDAT,CCENT,CYEAR,CMON,CDAY
            MOVE      CMON,F2
            LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
            PACK      DIM11A,CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR
