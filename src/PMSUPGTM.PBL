PMUG2800  WRITE     HTMLFILE;PMUGUTIM;
          GOTO      PMUG9999
.
PMUG2900  MATCH     SP70,PMUGUUID
          GOTO      PMUG9999 IF EQUAL
.
          PACK      KEY10,PMUGUUID,SP70
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      PMUGUUID,WBSENAM
          ENDIF
          WRITE     HTMLFILE;WBSENAM;
          GOTO      PMUG9999
.
PMUG3000  WRITE     HTMLFILE;PMUGEDAT;
          GOTO      PMUG9999
.
PMUG9999  RETURN
.
