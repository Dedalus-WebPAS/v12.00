.------------------------------------------------------------
. OP Program Management
.------------------------------------------------------------
PMUG0000  BRANCH    FLDITMNO,PMUG0100,PMUG0200,PMUG0300,PMUG0400,PMUG0500:
                             PMUG0600,PMUG0700,PMUG0800,PMUG0900,PMUG1000:
                             PMUG1100,PMUG1200,PMUG1300,PMUG1400,PMUG1500:
                             PMUG1600,PMUG1700,PMUG1800,PMUG1900,PMUG2000:
                             PMUG2100,PMUG2200,PMUG2300,PMUG2400,PMUG2500:
                             PMUG2600,PMUG2700,PMUG2800,PMUG2900,PMUG3000
          GOTO      PMUG9999
.
PMUG0100  WRITE     HTMLFILE;PMUGURNO;
          GOTO      PMUG9999
.
PMUG0200  MOVE      "A ",CATEGORY
          MOVE      PMUGATYP,CODE
          CALL      CODITM00
          GOTO      PMUG9999
.
PMUG0300  MOVE      "CL",CATEGORY
          MOVE      PMUGCLAM,CODE
          CALL      CODITM00
          GOTO      PMUG9999
.
PMUG0400  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      ZERO,F1
          MOVE      KEY1,F1
          BRANCH    F1,PMUG0410
          WRITE     HTMLFILE;PMUGFUND;
          GOTO      PMUG9999
.
PMUG0410  PACK      KEY14,PMUGFUND,ZERO,ZERO,ZERO,ZERO,SP10
          CALL      RDFUND1
          IF        OVRCD=0
            WRITE     HTMLFILE;HFNAME;
          ELSE
            WRITE     HTMLFILE;PMUGFUND;
          ENDIF
          GOTO      PMUG9999
.
PMUG0500  MOVE      "HT",CATEGORY
          MOVE      PMUGTABT,CODE
          CALL      CODITM00
          GOTO      PMUG9999
.
PMUG0600  MATCH     PMUGEDAT,SP70
          GOTO      PMUG9999 IF EQUAL
.
          UNPACK    PMUGEDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUG9999
.
PMUG0700  WRITE     HTMLFILE;PMUGMAXI;
          GOTO      PMUG9999
.
PMUG0800  WRITE     HTMLFILE;PMUGWARI;
          GOTO      PMUG9999
.
PMUG0900  WRITE     HTMLFILE;PMUGMANI;
          GOTO      PMUG9999
.
PMUG1000  WRITE     HTMLFILE;PMUGMAXO;
          GOTO      PMUG9999
.
PMUG1100  WRITE     HTMLFILE;PMUGWARO;
          GOTO      PMUG9999
.
PMUG1200  WRITE     HTMLFILE;PMUGMANO;
          GOTO      PMUG9999
.
PMUG1300  WRITE     HTMLFILE;PMUGBRKO;
          GOTO      PMUG9999
.
PMUG1400  MATCH     PMUGEXTD,SP70
          GOTO      PMUG9999 IF EQUAL
.
          UNPACK    PMUGEXTD,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUG9999
.
PMUG1500  MATCH     PMUGSDAT,SP70
          GOTO      PMUG9999 IF EQUAL
.
          UNPACK    PMUGSDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUG9999
.
PMUG1600  WRITE     HTMLFILE;PMUGPNAM;
          GOTO      PMUG9999
.
PMUG1700  WRITE     HTMLFILE;PMUGUDSI;
          GOTO      PMUG9999
.
PMUG1800  WRITE     HTMLFILE;PMUGSNAP;
          GOTO      PMUG9999
.
PMUG1900  MATCH     PMUGENDD,SP70
          GOTO      PMUG9999 IF EQUAL
.
          UNPACK    PMUGENDD,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUG9999
.
PMUG2000  WRITE     HTMLFILE;PMUGPROG;
          GOTO      PMUG9999
.
PMUG2100  WRITE     HTMLFILE;PMUGPINT;
          GOTO      PMUG9999
.
PMUG2200  MATCH     PMUGRPDT,SP70
          GOTO      PMUG9999 IF EQUAL
.
          UNPACK    PMUGRPDT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUG9999
.
PMUG2300  MATCH     PMUGRDDT,SP70
          GOTO      PMUG9999 IF EQUAL
.
          UNPACK    PMUGRDDT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUG9999
.
PMUG2400  MATCH     PMUGCDAT,SP70
          GOTO      PMUG9999 IF EQUAL
.
          UNPACK    PMUGCDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUG9999
.
PMUG2500  WRITE     HTMLFILE;PMUGCTIM;
          GOTO      PMUG9999
.
PMUG2600  MATCH     SP70,PMUGCUID
          GOTO      PMUG9999 IF EQUAL
.
          PACK      KEY10,PMUGCUID,SP70
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      PMUGCUID,WBSENAM
          ENDIF
          WRITE     HTMLFILE;WBSENAM;
          GOTO      PMUG9999
PMUG2700  MATCH     PMUGUDAT,SP70
          GOTO      PMUG9999 IF EQUAL
.
          UNPACK    PMUGUDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUG9999
