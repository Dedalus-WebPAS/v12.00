          ENDIF
          MATCH     Z70,OBDET034
          IF        !@EQUAL
            SQUEEZE   OBDET034
            MOVE      OBDET034,OBDEUN1   *    User Def Num 1 
          ENDIF
          MATCH     Z70,OBDET035
          IF        !@EQUAL
            SQUEEZE   OBDET035
            MOVE      OBDET035,OBDEUN2   *    User Def Bum 2
          ENDIF
          MATCH     Z70,OBDET036
