.------------------------------------------------------------
. Check for Abnormal Obs.
.------------------------------------------------------------
CHKABN00  MOVE      ZERO,OBSFLAG
          IF        PSAGEYRS<16
            GOTO      CHKABN80
          ENDIF
.
          IF        OBDEPULS<>0
            IF        (OBDEPULS<60) | (OBDEPULS>120)
              GOTO      CHKABN90
            ENDIF
          ENDIF
.
          IF        OBDETEMP<>0
            IF        (OBDETEMP<35.5) | (OBDETEMP>38)
              GOTO      CHKABN90
            ENDIF
          ENDIF
.
          IF        OBDERESP<>0
            IF        (OBDERESP<14) | (OBDERESP>26)
              GOTO      CHKABN90
            ENDIF
          ENDIF
.
          IF        OBDEBPSY<>0
            IF        (OBDEBPSY<100) | (OBDEBPSY>160)
              GOTO      CHKABN90
            ENDIF
          ENDIF
.
          IF        OBDEBPDI<>0
            IF        OBDEBPDI>110
              GOTO      CHKABN90
            ENDIF
          ENDIF
.
          IF        OBDEPOX<>0
            IF        OBDEPOX<90
              GOTO      CHKABN90
            ENDIF
          ENDIF
.
          MOVE      ZERO,F2
          MOVE      ZERO,F1
          MOVE      OBDEEYEO,F1
          ADD       F1,F2
.
          MOVE      ZERO,F1
          MOVE      OBDEVERB,F1
          ADD       F1,F2
.
          MOVE      ZERO,F1
          MOVE      OBDEMOTO,F1
          ADD       F1,F2
.
          IF        F2<>0
            IF        F2<13
              GOTO      CHKABN90
            ENDIF
          ENDIF
.
CHKABN80  MOVE      ONE,OBSFLAG
          GOTO      CHKABN99
.
CHKABN90  MOVE      ZERO,OBSFLAG
CHKABN99  RETURN
