.*                               PRINTERR              Called by: VALD0000*
.*                          Print error report                            *
.**************************************************************************
.
PRINTERR  COMPARE   ZERO,ERRFND
          IF        @EQUAL
            CLOCK     TIME,CTIMEIS
            MOVE      ONE,ERRFND
            MOVE      ONE,ERRORS
            CALL      HEAD80
            CALL      UND80
            PRINT     *1,"|","U/R Number",*12,"|","Adm Number",*23,"| ":
                      " Error Description",*80,"|"
            CALL      UND80
            MOVE      "6",CLNO
          ENDIF
.
          COMPARE   FIFTY5,CLNO
          IF        !@LESS
            CALL      HEAD80
          ENDIF
.
          PRINT     *1,"| ",PURNO,*12,"| ",SAVADMN,*23,"| ",ERRDESC,*80,"|"
          ADD       ONE,CLNO
.
          RETURN
