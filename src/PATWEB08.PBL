CMAN0000  PACK      KEY5,SP70,SP70
          CALL      RDPMWFM1
CMAN1000  CALL      RKPMWFM1
          BRANCH    OVRCD,CMAN9999
.
          MOVE      "1",DIM1
          MATCH     "0",PMWFMAND
          IF        @EQUAL
            MOVE      "0",DIM1
          ENDIF

CMAN2000  WRITE       HTMLFILE;"mandatoryFields.push({field:'",PMWFSCDE:
                               "',name:'",PMWFSDES,"',isMandatory:'",DIM1,"'});"
          GOTO      CMAN1000
.
CMAN9999  RETURN
