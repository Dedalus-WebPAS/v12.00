. Overnight additional table
.------------------------------------------------------------
PTFE0000  BRANCH    FLDITMNO,PTFE0100,PTFE0200,PTFE0300,PTFE0400,PTFE0500:
                             PTFE0600,PTFE0700,PTFE0800,PTFE0900,PTFE1000:
                             PTFE1100,PTFE1200,PTFE1300,PTFE1400
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      PTFE9999
.
PTFE0100  MOVE      "CL",CATEGORY            * Claim Code
          MOVE      PTFECLMN,CODE
          CALL      CODITM00
          GOTO      PTFE9999
.
PTFE0200  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      KEY1,F1
          BRANCH    F1,PTFE0210
          WRITE     HTMLFILE;PTFEHFND;       * Health Fund
          GOTO      PTFE9999
.
PTFE0210  MATCH     SP70,PTFEHFND
          GOTO      PTFE9999 IF EQUAL
.
          PACK      KEY14,PTFEHFND,SP70
          CALL      RDSFUND1
          CALL      RDKFUND1
          BRANCH    OVRCD,PTFE9999
          WRITE     HTMLFILE;HFNAME;        * Health Fund Name
          GOTO      PTFE9999
.
PTFE0300  MOVE      "HT",CATEGORY           * HF Table Type
          MOVE      PTFETABT,CODE
          CALL      CODITM00
          GOTO      PTFE9999
.
PTFE0400  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      KEY1,F1
          BRANCH    F1,PTFE0410
          WRITE     HTMLFILE;PTFECASM;      * Casemix Code
          GOTO      PTFE9999
.
PTFE0410  MATCH     SP70,PTFECASM
          GOTO      PTFE9999 IF EQUAL
.
          PACK      KEY9,PTFECASM,SP70
          CALL      RDPTCMC1
          BRANCH    OVRCD,PTFE9999
          WRITE     HTMLFILE;PTCADES1;      * Casemix Code Desc
          GOTO      PTFE9999
.
PTFE0500  WRITE     HTMLFILE;PTFEDES1;      * Description
          GOTO      PTFE9999
.
PTFE0600  WRITE     HTMLFILE;PTFEDES2;      * Description 2
          GOTO      PTFE9999
.
PTFE0700  WRITE     HTMLFILE;PTFEDREB;      * Additional rebate portion
          GOTO      PTFE9999
.
PTFE0800  WRITE     HTMLFILE;PTFEDPAT;      * Additional patient portion
          GOTO      PTFE9999
.
PTFE0900  MOVE      "BT",CATEGORY           * Bed Type
          MOVE      PTFEBTYP,CODE
          CALL      CODITM00
          GOTO      PTFE9999
.
PTFE1000  WRITE     HTMLFILE;DPTFEEDA;      * Ending Day
          GOTO      PTFE9999
.
PTFE1100  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      ZERO,F1
          MOVE      KEY1,F1
          BRANCH    F1,PTFE1110,PTFE1120
.
          REP       " 0",PTFEACIN
          WRITE     HTMLFILE;PTFEACIN;
          GOTO      PTFE9999
.
PTFE1110  MOVE      "No                   ",KEY21
          MATCH     "1",PTFEACIN
          IF        @EQUAL
            MOVE      "Yes.Non-Accum.Inliers",KEY21
          ELSE
            MATCH     "2",PTFEACIN
            IF        @EQUAL
              MOVE      "No.ICU/CCU Rules    ",KEY21
            ENDIF
          ENDIF
          WRITE     HTMLFILE;KEY21;
          GOTO      PTFE9999
.
PTFE1120  MOVE      ZERO,F1
          PACK      KEY5,ANSB,ANST,PTFEBTYP
          CALL      RDCODE1
          BRANCH    OVRCD,PTFE1140
          MATCH     "1",TCINDC9
          GOTO      PTFE1130 IF EQUAL     * Intensive care
          MATCH     "3",TCINDC9
          GOTO      PTFE1140 IF NOT EQUAL * Not Critical Care
.
PTFE1130  MOVE      ONE,F1                * ICU/CCU
PTFE1140  MATCH     "1",PTFEACIN
          IF        @EQUAL
            WRITE     HTMLFILE;"<option value=#"0#">":
                               "No</option>":
                               "<option value=#"1#" selected>":
                               "Yes.Non-Accum.Inliers</option>";
.
            IF        F1=1
              WRITE     HTMLFILE;"<option value=#"2#">":
                                 "No.ICU/CCU Rules</option>"
            ENDIF
          ELSE
            MATCH     "2",PTFEACIN
            IF        @EQUAL
              WRITE     HTMLFILE;"<option value=#"0#">":
                                 "No</option>":
                                 "<option value=#"1#">":
                                 "Yes.Non-Accum.Inliers</option>";
.
              IF        F1=1
                WRITE     HTMLFILE;"<option value=#"2#" selected>":
                                   "No.ICU/CCU Rules</option>"
              ENDIF
            ELSE
              WRITE     HTMLFILE;"<option value=#"0#" selected>":
                                 "No</option>":
                                 "<option value=#"1#">":
                                 "Yes.Non-Accum.Inliers</option>";
.
              IF        F1=1
                WRITE     HTMLFILE;"<option value=#"2#">":
                                    "No.ICU/CCU Rules</option>"
              ENDIF
            ENDIF
          ENDIF
          GOTO      PTFE9999
.
PTFE1200  WRITE     HTMLFILE;PTFECNID;      * Contract ID
          GOTO      PTFE9999
.
PTFE1300  MOVE      "FI",CATEGORY           * Fees Invoiced Group
          MOVE      PTFEFIGR,CODE
          CALL      CODITM00
          GOTO      PTFE9999
.
PTFE1400  MATCH     "1",PTFENPDY
          IF        !@EQUAL
            MOVE      "0",PTFENPDY
          ENDIF
          WRITE     HTMLFILE;PTFENPDY;
          GOTO      PTFE9999
.
PTFE9999  RETURN
+
.------------------------------------------------------------
. Check for ICU/CCU bed type
.------------------------------------------------------------
ICBT0000  MOVE      ZERO,F1
          PACK      KEY5,ANSB,ANST,PTFEBTYP
          CALL      RDCODE1
          BRANCH    OVRCD,ICBT9999
          MATCH     "1",TCINDC9
          GOTO      ICBT9000 IF EQUAL     * ICU
          MATCH     "3",TCINDC9
          GOTO      ICBT9999 IF NOT EQUAL * Not CCU
.
ICBT9000  MOVE      ONE,F1                * ICU/CCU bed type
ICBT9999  RETURN
