.------------------------------------------------------------
. Overnight Tables
.
. Inputs  -  Valid record from the pathlfaf or patafeaf
. Outputs -  Field specified by FLDITMNO & DIM127 for any parameters
.
. Include's needed - PATLSDFD
.                    PATCODFD
.
. Modifications :
.                 V10.10.02 17/05/2017 J.Tan  TSK 0297904
.                 Changed desc. for ICU/CCU Rules
.                 V10.10.01 28/03/2017 J.Tan  TSK 0297904
.                 Added ICU/CCU Rules to Non-Accumulative inlier field
.                 V10.07.01 30/10/2015 J.Tan  CAR 290448
.                 Added 'Charge on No Pay Day' to Additional fee
.                 V10.01.01 09/12/2010  Ebon Clements  CAR 233149
.                 Added fees invoiced group
.                 V9.12.01  07/09/09 J.Tan    CAR 205303
.                 Added Contract ID
.                 V9.09.01  10/08/07 J.Tan    CAR 144656
.                 Added Non-Accumulative Inliers to additional charge
.                 V9.04.02  14/09/05 J.Tan    CAR 76252
.                 Added day type to overnight inlier/outlier
.                 V9.04.01  03/08/05 J.Tan    CAR 62750
.                 Removed redefined amount variables
.                 V9.03.01 07/05/04 J.Tan   CAR 49644
.                 Fixed updating lumpsum amount
.------------------------------------------------------------
.------------------------------------------------------------
. Overnight lumpsum table
.------------------------------------------------------------
PTHL0000  BRANCH    FLDITMNO,PTHL0100,PTHL0200,PTHL0300,PTHL0400,PTHL0500:
                             PTHL0600,PTHL0700,PTHL0800,PTHL0900,PTHL1000:
                             PTHL1100,PTHL1200,PTHL1300
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      PTHL9999
.
PTHL0100  MOVE      "CL",CATEGORY            * Claim Code
          MOVE      PTHLCLMN,CODE
          CALL      CODITM00
          GOTO      PTHL9999
.
PTHL0200  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      KEY1,F1
          BRANCH    F1,PTHL0210
          WRITE     HTMLFILE;PTHLHFND;       * Health Fund
          GOTO      PTHL9999
.
PTHL0210  MATCH     SP70,PTHLHFND
          GOTO      PTHL9999 IF EQUAL
.
          PACK      KEY14,PTHLHFND,SP70
          CALL      RDSFUND1
          CALL      RDKFUND1
          BRANCH    OVRCD,PTHL9999
          WRITE     HTMLFILE;HFNAME;        * Health Fund Name
          GOTO      PTHL9999
.
PTHL0300  MOVE      "HT",CATEGORY           * HF Table Type
          MOVE      PTHLTABT,CODE
          CALL      CODITM00
          GOTO      PTHL9999
.
PTHL0400  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      KEY1,F1
          BRANCH    F1,PTHL0410
          WRITE     HTMLFILE;PTHLCASM;      * Casemix Code
          GOTO      PTHL9999
.
PTHL0410  MATCH     SP70,PTHLCASM
          GOTO      PTHL9999 IF EQUAL
.
          PACK      KEY9,PTHLCASM,SP70
          CALL      RDPTCMC1
          BRANCH    OVRCD,PTHL9999
          WRITE     HTMLFILE;PTCADES1;      * Casemix Code Desc
          GOTO      PTHL9999
.
PTHL0500  WRITE     HTMLFILE;PTHLDES1;      * Description
          GOTO      PTHL9999
.
PTHL0600  WRITE     HTMLFILE;PTHLDES2;      * Description 2
          GOTO      PTHL9999
.
PTHL0700  WRITE     HTMLFILE;PTHLLREB;      * Lumpsum rebate portion
          GOTO      PTHL9999
.
PTHL0800  WRITE     HTMLFILE;PTHLLPAT;      * Lumpsum patient portion
          GOTO      PTHL9999
.
PTHL0900  WRITE     HTMLFILE;PTHLINVB;      * Revenue breakdown
          GOTO      PTHL9999
.
PTHL1000  WRITE     HTMLFILE;PTHLINVI;      * Invoice breakdown
          GOTO      PTHL9999
.
PTHL1100  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      KEY1,F1
          BRANCH    F1,PTHL1110
          WRITE     HTMLFILE;PTHLNINV;      * Number of invoices
          GOTO      PTHL9999
.
PTHL1110  COMPARE   TWO,PTHLNINV
          IF        @EQUAL
            WRITE     HTMLFILE;"<option value=1>1</option> ":
                               "<option value=2 selected>2</option> ";
          ELSE
            WRITE     HTMLFILE;"<option value=1 selected>1</option> ":
                               "<option value=2>2</option> ";
          ENDIF
          GOTO      PTHL9999
.
PTHL1200  WRITE     HTMLFILE;PTHLCOFF;      * Low outlier cutoff
          GOTO      PTHL9999
.
PTHL1300  WRITE     HTMLFILE;PTHLCNID;      * Contract ID
          GOTO      PTHL9999
.
PTHL9999  RETURN
.
