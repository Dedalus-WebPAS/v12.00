          APPEND    "<p style='color:",TEMPVAR3
          APPEND    BEDRFNTR,TEMPVAR3
          APPEND    ";margin:0'>",TEMPVAR3
          APPEND    DISPBRDT,TEMPVAR3
          APPEND    "</p>",TEMPVAR3
          RESET     TEMPVAR3
