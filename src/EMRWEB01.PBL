            IF        @EQUAL
              APPEND    TWO,OTHRCLIN
            ELSE
              APPEND    ONE,OTHRCLIN
            ENDIF
            GOTO      OCLN1000
          ENDIF
.
          MATCH     "1",EMCIPR01                 * Returned Field 1
          IF        @EQUAL
            APPEND    TWO,OTHRCLIN
            GOTO      OCLN1000
          ENDIF
          APPEND    ZERO,OTHRCLIN
.
