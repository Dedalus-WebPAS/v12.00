          CALL      GETCOD00
          MOVE      TCINDC1,SRCHTYPE
.
          MATCH     "All",SRCHCODE
          IF        @EQUAL
            MOVE      ZERO,SRCHTYPE
          ENDIF
.
          PACK      KEY12,WBSEESC,TWO,SP70
          CALL      RSEMVIS5
INCVIS10  CALL      RKEMVIS5
          BRANCH    OVRCD,INCVIS99
.
          MATCH     EMVISITE,WBSEESC
          GOTO      INCVIS99 IF NOT EQUAL
.
          COMPARE   EMVISTAT,TWO
          GOTO      INCVIS99 IF NOT EQUAL
.
          MOVE      SP70,RESPNAME
          MOVE      SP70,RESPNURE
.  
          PACK      FORMATNM,SP70,SP70
.
          PACK      KEY8,EMVIURNO
          CALL      RDMAST1
          IF        OVRCD=0
            PACK      KEY8,EMVIURNO
            CALL      RDPMPX21
            IF        OVRCD=1
              CALL      CLPMSPX2
            ENDIF
            GOTO      INCVIS30
          ELSE
            CALL      CLPATMAS
