          MOVE      EMVIUNIT,CODE
          CALL      GETCOD00
          MOVE      TDESC,UNITDISP
          MOVE      SP70,TDESC
.
PATVIS24  MOVE      "el",CATEGORY
          MOVE      EMVIUC20,CODE
          CALL      GETCOD00
          MOVE      TDESC,COMPDISP
          MOVE      SP70,TDESC 
.
          PACK      KEY8,EMVIURNO
          CALL      RDMAST1
          IF        OVRCD=0
            PACK      KEY8,EMVIURNO
            CALL      RDPMPX21
            IF        OVRCD=1
              CALL      CLPMSPX2
            ENDIF
.
            PACK      AGEDATE,CCC,CYY,CMM,CDD
