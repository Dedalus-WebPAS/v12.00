          IF        @EQUAL
            CALL      DOCT0000
            CALL      NURS0000
            GOTO      INCVIS80
          ELSE
            GOTO      INCVIS10
          ENDIF
.
INCVIS70  MATCH     "0",EMIPNCOM
          IF        @EQUAL
            CALL      NURS0000
            CALL      DOCT0000  
            GOTO      INCVIS80
          ELSE
            GOTO      INCVIS10
          ENDIF
.
INCVIS80  CALL      OUTPUT00
          GOTO      INCVIS10
.
