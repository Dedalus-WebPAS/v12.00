.
          MATCH     ZEROUR,EMVIURNO      * Exit zero U/R
          GOTO      IPOP9999 IF EQUAL
.
          MATCH     "1",EMCNPTTI         * Any Patient Recent Visit IP/OP
          GOTO      IPOP0010 IF EQUAL
.
          MATCH     "2",EMCNPTTI         * Only IP Visits prior to Emerg Visit
          GOTO      IPOP0010 IF EQUAL
          GOTO      IPOP9999
.
