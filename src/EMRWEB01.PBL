              MATCH     EMVISITE,FILTSITE
              GOTO      PATVIS10 IF NOT EQUAL
            ENDIF
          ENDIF
.
          COMPARE   ONE,LNK2PRT
          GOTO      PATVIS20 IF NOT EQUAL
