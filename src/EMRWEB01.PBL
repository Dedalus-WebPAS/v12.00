          GOTO      BEDR9999 IF EQUAL
.
          IF        READYADM<>1
            MOVE      "B",KEY1              * Ward allocated (blue)
          ENDIF
.  
          MOVE      PMBREWRD,KEY3
          MOVE      PMBRRDAT,DISPWRDT       * Ward bed request ready date
          MOVE      PMBRRTIM,DISPWRTM       * Ward bed request ready time
          GOTO      BEDR9999
