          IF        @EQUAL
            MATCH     "1",EMCIPR02
            IF        @EQUAL
              APPEND    TWO,OTHRCLIN
            ELSE
              APPEND    ONE,OTHRCLIN
            ENDIF
            GOTO      OCLN2000
          ENDIF
.
