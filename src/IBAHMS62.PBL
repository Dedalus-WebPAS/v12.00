.*******************************************************************************
.         Print Zero invoice option on header
.*******************************************************************************
PROPT000  MOVE      "(Include Zero Invoice)",KEY25
          MATCH     "1",ZEROINVC
          IF        @EQUAL
            MOVE      "(Exclude Zero Invoice)",KEY25
          ELSE
            MATCH     "2",ZEROINVC
            IF        @EQUAL
              MOVE      "(Zero Invoice Only)",KEY25
            ENDIF
          ENDIF
          PRINT     *40,KEY25
          ADD       ONE,CLNO
PROPT999  RETURN
