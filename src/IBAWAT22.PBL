          ADD       DELT1,XDELET1
          ADD       DELT2,XDELET2
          ADD       DELT3,XDELET3
          ADD       DELT4,XDELET4
          ADD       DELT5,XDELET5
          ADD       DELT6,XDELET6
.
          BRANCH    OPTION,CUDC3000
.
.         For Option 2 (Pending) deletions are being subtracted
.
          SUB       XDELET1,XTOTAL
          SUB       XDELET2,XTOTAL
          SUB       XDELET3,XTOTAL
          SUB       XDELET4,XTOTAL
          SUB       XDELET5,XTOTAL
          SUB       XDELET6,XTOTAL
