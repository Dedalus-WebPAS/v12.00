          BRANCH    OPTION,CUDC5000
          SUB       XDELET1,XTOTAL
          SUB       XDELET2,XTOTAL
          SUB       XDELET3,XTOTAL
          SUB       XDELET4,XTOTAL
          SUB       XDELET5,XTOTAL
          SUB       XDELET6,XTOTAL
