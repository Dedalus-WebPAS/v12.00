          OPEN      APMX2FL2,APMX2FNM
          PACK      KEY35,APMACRD,SP70
          MOVE      KEY35,SAVKEY80
          SETLPTR   SAVKEY80,5 
          CALL      RDAPMX2 
          IF        OVRCD=0
            CALL      RPAPMX2 
          ENDIF
LM01L100
