          BRANCH    OVRCD,LM01L105
          PACK      KEY35,APMXCRD,APMXREF
          MATCH     SAVKEY80,KEY35   
          GOTO      LM01L105 IF NOT EQUAL
          RESET     KEY35,6
          MOVE      KEY35,KEY30
          MOVE      SP70,SCTMROW
          MOVE      SP70,SCTMCOL
          CALL      TAAPMX22
          IF        OVRCD=0
