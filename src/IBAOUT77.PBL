KEMP0000    MOVE       ZERO,PRINTEMP
.
            MOVE       "N",ANS
            KEYIN      *P1:14,*EL,"Print Empty Slots : ",*DV,*RV,ANS:
                       *P21:14,*V2LON,ANS;
            RESET      ANS
.
            CMATCH     "Y",ANS
            GOTO       KEMP9999 IF NOT EQUAL
.
            MOVE       ONE,PRINTEMP
.
