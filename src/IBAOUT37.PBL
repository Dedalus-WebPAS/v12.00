.*********************************************************************
SUBH0000  MATCH     Z70,SAVECONT
          IF        !@EQUAL
            CALL      UND132
            PRINT     *1,"| No. of Patients : ",TOTALPAT,*132,"|"
          ENDIF
.
          MOVE      ZERO,TOTALPAT
.
          CALL      HEAD0000                * Start on new page
          CALL      UND132
.
          MOVE      TEMPCONT,SAVECONT       * Save contractor
.
          MOVE      SP70,TDESC
          MATCH     SP70,SAVECONT
          IF        !@EQUAL
            PACK      KEY5,ANSO,ANSB,TEMPCONT
            CALL      RDCODE1
          ENDIF

          PRINT     *1,"| Transport Contractor : ",TDESC,*132,"|"
