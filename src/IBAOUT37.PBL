.
          MATCH     "2",INPTSTAT
          IF        @EQUAL
            MOVE      "Yes",INPTDESC
            DISPLAY   *P20:16,INPTDESC
            GOTO      KCUR9000
          ENDIF
.
          BEEP
          GOTO      KCUR0000
.
KCUR9000  MOVE      ZERO,EXIT
          GOTO      KCUR9999
.
KCUR9500  MOVE      ONE,EXIT
.
KCUR9999  RETURN
