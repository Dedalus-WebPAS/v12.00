.
	  PRINT     *1,"|Appointment Date/Time",*23,"| Patient Name":
                    *46,"| Pick Up Address",*83,"| Mobility":
                    *95,"| Aides",*110,"| Special Arrangements",*132,"|"
.
	  PRINT     *1,"|Return Time",*23,"|":
