                    *95,"|",*110,"|",*132,"|"
.
	  PRINT     *1,"|",*23,"|":
                    *46,"| Return Address",*83,"|":
                    *95,"|",*110,"|",*132,"|"
.
	  PRINT     *1,"|Claim Details",*23,"|":
                    *46,"| Comments",*83,"|":
                    *95,"|",*110,"|",*132,"|"
