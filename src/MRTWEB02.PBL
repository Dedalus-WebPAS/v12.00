            GOTO      VALICD80
          ENDIF
          MATCH     SP70,PT0DDESC
          IF        !@EQUAL
            WRITE     HTMLFILE;"<RETURN_VALUE TYPE=SIMPLE>":
                                PT0DDESC,"|",PT0DACRQ,"|";
            GOTO      VALICD80
