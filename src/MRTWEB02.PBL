          CALL      RPPTECD1
          IF        OVRCD=1
            MOVE      ONE,ECDFLAG
            GOTO      CODFLG10
          ENDIF
.
          MATCH     ADMISSNO,DPTEDADM
          IF        !@EQUAL
            MOVE      ONE,ECDFLAG
            GOTO      CODFLG10
          ENDIF
.
.         Check episode operation coding
.
CODFLG10  PACK      KEY13,ADMISSNO,EPSCD001,Z70 
          CALL      RSPTECO1
          CALL      RPPTECO1
          IF        OVRCD=1
            IF        ECDFLAG=1  
              GOTO      CODFLG90
            ENDIF
          ENDIF
.
          MATCH     ADMISSNO,DPTEOADM
