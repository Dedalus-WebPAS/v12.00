          MOVE      PTEOCCFL,DIM2
          CALL      GTCCFL00
.
          COMPARE   ONE,VIEWCCFL
          IF        @EQUAL
            WRITE     HTMLFILE;"<td class=",KEY9,">",DIM50,"</td>"
