            WRITE     HTMLFILE;"Patient is not Discharged";
            GOTO      WRDDIS99
          ENDIF
          PACK      KEY6,PTDSDWRD,SP70 
          CALL      RDWARD1
          BRANCH    OVRCD,WRDDIS90
.
          WRITE     HTMLFILE;WBDESC;    * description 
          GOTO      WRDDIS99
.
