                        PGTLYRMT,*56,PGTLYRDT,*68,"!":
                    *69,PGTCYRMT,*81,PGTCYRDT,*92,"!":
                    *93,VARIAN1A,*113,VARIAN1B,*132,"!"
          MOVE      SIIASUB,LASTCODE
          MOVE      "99",CLNO
