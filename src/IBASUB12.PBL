. Range Check
.------------------------------------------------------------
RANGE000  MOVE      ZERO,EXIT
          IF        !(REPTSEQ=1)
            MATCH     FCATALOG,SIIACAT
            GOTO      RANGE900 IF LESS
            MATCH     SIIACAT,TCATALOG
            GOTO      RANGE900 IF LESS
          ENDIF
.
          IF        !(REPTSEQ=2)
            MATCH     FDESCRP,SIIADES
            GOTO      RANGE900 IF LESS
            MATCH     SIIADES,TDESCRP
            GOTO      RANGE900 IF LESS
          ENDIF
.
          IF        !(REPTSEQ=3)
            MATCH     FGROUP,SIIASUB
            GOTO      RANGE900 IF LESS
            MATCH     SIIASUB,TGROUP
            GOTO      RANGE900 IF LESS
          ENDIF
.
          MOVE      SP70,SIIESID
.                                           * srf 17617 - begin
          COMPARE   ONE,SIIANON             * for stock catlg, goto range070
          GOTO      RANGE070 IF NOT EQUAL
.                                           * for non-stock catlg,read sinpocaf
          PACK      KEY23,SIIACAT,SIPEYEAR,SIPEPER,SP70
          CALL      RSSIPC3
RANGE050  CALL      RKSIPC3
          BRANCH    OVRCD,RANGE900          * no catalog purchased, dont display
.
          MATCH     SIPCCAT,SIIACAT
          GOTO      RANGE900 IF NOT EQUAL
.
          RESET     SIPCDAT                 * compare first 4 chars
          SETLPTR   SIPCDAT,4               * with the year
          MATCH     SIPCDAT,SIPEYEAR
          GOTO      RANGE900 IF NOT EQUAL
.
          RESET     SIPCDAT,5               * compare last 2 chars
          SETLPTR   SIPCDAT                 * with the period
          MATCH     SIPCDAT,SIPEPER
          RESET     SIPCDAT
          GOTO      RANGE900 IF NOT EQUAL
.
          MATCH     SP70,SIWAWAR            * for all warehouses, dont compare
          GOTO      RANGE060 IF EQUAL       * w/h with the one in sinpocaf
.
          MATCH     SIPCWAR,SIWAWAR
          GOTO      RANGE900 IF NOT EQUAL
.
          MOVE      SIPCWAR,WSACWAR
          CALL      WSAC0000                * check warehouse securit
          BRANCH    EXIT,RANGE050
.                                           * if correct record found
RANGE060  COMPARE   ONE,HNONUSAG
          GOTO      RANGE300 IF NOT EQUAL   * If Ack at purchase, display rec
.
          PACK      KEY12,SIPCPON,SIPCITM,SP70 * if non-stock catalog received
          CALL      RSSIPF1                 * print record
          CALL      RKSIPF1
          BRANCH    OVRCD,RANGE050          * else, chk for next Catlg record
.
          GOTO      RANGE300                           * was RANGE999  *C-17617
.
RANGE070  MATCH     SP70,SIWAWAR            * multiple warehouses selected
.                                           * srf 17617 - end
          IF        @EQUAL
            PACK      KEY12,SIIACAT,SP70
            CALL      RSSIIE1
RANGE100    CALL      RKSIIE1
            BRANCH    OVRCD,RANGE900
.
            MATCH     SIIECAT,SIIACAT
            GOTO      RANGE900 IF NOT EQUAL
.
            MOVE      SIIEWAR,WSACWAR
            CALL      WSAC0000                * check warehouse securit
            BRANCH    EXIT,RANGE100
.
          ELSE
            PACK      KEY12,SIIACAT,SIWAWAR
            CALL      RDSIIE1
            BRANCH    OVRCD,RANGE900
          ENDIF
.
RANGE300  BRANCH    INCLTYPE,RANGE510,RANGE520,RANGE530,RANGE530,RANGE530:
                             RANGE530,RANGE530,RANGE530
          GOTO      RANGE999
.
. SIIANON   FORM      1      Non-Stock Item 0=No
.------------------------------------------------
RANGE510  COMPARE   ONE,SIIANON
          GOTO      RANGE900 IF EQUAL   * Stock Only
          GOTO      RANGE999
.
RANGE520  COMPARE   ZERO,SIIANON
          GOTO      RANGE900 IF EQUAL   * Non-Stock Only
          GOTO      RANGE999
.
RANGE530  ASSIGN    INCLTYPE-2,F2
.
          MATCH     SP70,SIIESID
          IF        @EQUAL
            RESET     SIIASTI,F2
            CMATCH    "1",SIIASTI
            GOTO      RANGE900 IF NOT EQUAL
            GOTO      RANGE999
          ELSE
            RESET     SIIESID,F2
            CMATCH    "1",SIIESID
            GOTO      RANGE900 IF NOT EQUAL
            GOTO      RANGE999
          ENDIF
.
RANGE900  MOVE      ONE,EXIT
.
RANGE999  RETURN
