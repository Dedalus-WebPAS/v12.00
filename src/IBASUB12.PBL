. REPT - Print Out Report
.------------------------------------------------------------------------------
REPT0000  CALL      MAKEXP00
          PACK      DISPMSG,SP70,SP70                 * save line 24
          CALL      MESSAGE2
.
          MATCH     FCATALOG,TCATALOG
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Catalog Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
          MATCH     FGROUP,TGROUP
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Product Group Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
          MATCH     FDESCRP,TDESCRP
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Description Range - ";
            CALL      HITENTER
            GOTO      REPT9500
          ENDIF
.
.
          DISPLAY   *P1:24,*EL,"Processing - ";
          MOVE      "599",CLNO                        * Set up print values
          MOVE      "0",CPAGENO
          CLOCK     TIME,CTIMEIS
.
          MOVE      SIPEYEAR,SIPEYF4
          SUB       ONE,SIPEYF4
          MOVE      SIPEYF4,LASTYEAR
          REP       " 0",LASTYEAR
.
          MOVE      ZERO,TOTLYRMT
          MOVE      ZERO,TOTLYRDT
          MOVE      ZERO,TOTCYRMT
          MOVE      ZERO,TOTCYRDT
          BRANCH    REPTSEQ,REPT1000,REPT2000,REPT3000
.
. Catalog Seq
.-------------------------
