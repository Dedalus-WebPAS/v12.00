            APPEND      INCLDESC,VAR
            APPEND      " in ",VAR
            APPEND      SEQDESC,VAR
            APPEND      " Sequence for ",VAR
            APPEND      WAREDESC,VAR
            RESET       VAR
            PRINT       "Catalog Code  Range : ",FROMCATA," to  ",TOCATA:
                        *56,"Usage Period : ",SIPEPER,"/",SIPEYEAR:
                        *N,"Product Group Range : ",FROMCODE,"   to  ",TOCODE:
                        *N,"Catalog Name  Range : ",FROMDESC," to  ",TODESC:
                        *N,VAR
            MOVE      "12",CLNO
          ELSE
            PRINT       VAR
            MOVE      "8",CLNO
          ENDIF
          IF        REPTSEQ=3
            MOVE      SP70,SISADESC
            PACK      KEY5,SIIASUB,SP70
            CALL      RDSISA1
            PRINT     "Product Group : ",SIIASUB,SP1,SISADESC
          ENDIF
          CALL      UND132:
          PRINT     *1,"!",*3,"Catalog",*43,"!",*52,"Last Year":
                    *68,"!",*76,"Current Year",*92,"!",*107,"Variance":
                    *132,"!":
                    *N,*1,"!",*3,"Code  Description",*43,"!":
                    *50,"Month",*61,"Y.T.D.",*68,"!",*75,"Month":
                    *86,"Y.T.D.",*92,"!",*99,"Month",*118,"Y.T.D.",*132,"!"
          CALL      UND132
          RETURN
.----------------------------------------------------------
. Calculate Totals
.----------------------------------------------------------
CALC0000  MOVE      ZERO,LASTYRMT
          MOVE      ZERO,LASTYRDT
          MOVE      ZERO,CURRYRMT
          MOVE      ZERO,CURRYRDT
          MOVE      ZERO,LQTYYRMT
          MOVE      ZERO,LQTYYRDT
          MOVE      ZERO,CQTYYRMT
          MOVE      ZERO,CQTYYRDT
          MATCH     SP70,SIWAWAR          * All Warehouse Option
          GOTO      CALC5000 IF EQUAL
.
. Current Month Single Warehouse
.------------------------------
          PACK      KEY18,SIIACAT,SIWAWAR,SIPEYEAR,SIPEPER,SP70
          CALL      RDSIIH1
          BRANCH    OVRCD,CALC1000
          IF        ANALTYPE=1
             MOVE      SIIHUAM,CURRYRMT
             MOVE      SIIHUQT,CQTYYRMT
          ENDIF
          IF        ANALTYPE=2
             MOVE      SIIHPAM,CURRYRMT
             MOVE      SIIHPQT,CQTYYRMT
          ENDIF
          IF        ANALTYPE=3
             MOVE      SIIHRAM,CURRYRMT
             MOVE      SIIHRQT,CQTYYRMT
          ENDIF
.
. Current Year Single Warehouse YTD
.-----------------------------------
CALC1000  PACK      KEY18,SIIACAT,SIWAWAR,SIPEYEAR,SP70
          PACK      KEY16,SIIACAT,SIWAWAR,SIPEYEAR,SP70
          PACK      KEY6,SIPEYEAR,SIPEPER
          CALL      RSSIIH1
          CALL      RPSIIH1
.
CALC1100  CALL      RKSIIH1
          BRANCH    OVRCD,CALC1900
          PACK      KEY18,SIIHCAT,SIIHWAR,SIIHDAT,SP70
          MATCH     KEY16,KEY18
          GOTO      CALC1900 IF NOT EQUAL
          MATCH     SIIHDAT,KEY6
          GOTO      CALC1900 IF LESS
.
          IF        ANALTYPE=1
             ADD       SIIHUAM,CURRYRDT
             ADD       SIIHUQT,CQTYYRDT
          ENDIF
          IF        ANALTYPE=2
             ADD       SIIHPAM,CURRYRDT
             ADD       SIIHPQT,CQTYYRDT
          ENDIF
          IF        ANALTYPE=3
             ADD       SIIHRAM,CURRYRDT
             ADD       SIIHRQT,CQTYYRDT
          ENDIF
          GOTO      CALC1100
.
. Last Year Single Warehouse Current Month
.-------------------------------------------
CALC1900  PACK      KEY18,SIIACAT,SIWAWAR,LASTYEAR,SIPEPER
          CALL      RDSIIH1
          BRANCH    OVRCD,CALC2000
.
          IF        ANALTYPE=1
             MOVE      SIIHUAM,LASTYRMT
             MOVE      SIIHUQT,LQTYYRMT
          ENDIF
          IF        ANALTYPE=2
             MOVE      SIIHPAM,LASTYRMT
             MOVE      SIIHPQT,LQTYYRMT
          ENDIF
          IF        ANALTYPE=3
             MOVE      SIIHRAM,LASTYRMT
             MOVE      SIIHRQT,LQTYYRMT
          ENDIF
.
. Last Year Single Warehouse Current YTD
.-------------------------------------------
CALC2000  PACK      KEY18,SIIACAT,SIWAWAR,LASTYEAR,SP70
          PACK      KEY16,SIIACAT,SIWAWAR,LASTYEAR,SP70
          PACK      KEY6,LASTYEAR,SIPEPER
          CALL      RSSIIH1
          CALL      RPSIIH1
.
CALC2100  CALL      RKSIIH1
          BRANCH    OVRCD,CALC9999
          PACK      KEY18,SIIHCAT,SIIHWAR,SIIHDAT,SP70
          MATCH     KEY16,KEY18
          GOTO      CALC9999 IF NOT EQUAL
