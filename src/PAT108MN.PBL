.------------------------------------------------------------------------------
. Online ICD10 Edition 8 Code Validation
.------------------------------------------------------------------------------
VAL8CD00  MOVE      SP70,VALDNAME
          MATCH     SP70,VALDCODE
          GOTO      VAL8CD90 IF EQUAL
.
          MOVE      "Invalid Code Entered",VALDNAME
          BRANCH    VALDTYPE,VAL8CD10,VAL8CD20
          MOVE      "Invalid Validation Type",VALDNAME
          GOTO      VAL8CD90
.
VAL8CD10  MOVE      VALDCODE,KEY9
          CALL      RDPT18O1
          IF        OVRCD=0
            MOVE      PT0ODSC2,VALDNAME
          ENDIF
          GOTO      VAL8CD90
.
VAL8CD20  MOVE      VALDCODE,KEY9
          CALL      RDPT18D1
          IF        OVRCD=0
            MOVE      PT0DDSC2,VALDNAME
          ENDIF
          GOTO      VAL8CD90
.
VAL8CD90  CALL      OPENHTML
          BRANCH    EXIT,VAL8CD99
          WRITE     HTMLFILE;"<h3 align=center>Checking Database</h3>":
                             "<script type=#"text/javascript#">":
                             "{ opener.ValidateName.value=#"",VALDNAME,"#";":
                             "  opener.ValidateCode.value=#"",VALDCODE,"#";"
.
          MATCH     "Invalid",VALDNAME
          IF        @EQUAL
            WRITE     HTMLFILE;"  alert(#"Invalid Code Entered#");";
          ENDIF
.
          WRITE     HTMLFILE;"  close(); } </script>"
          CALL      CLOSHTML
.
VAL8CD99  RETURN
