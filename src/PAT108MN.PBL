.------------------------------------------------------------------------------
. ICD10 Edition 8 Diagnosis Coding Maintenance: Display,Add,Update,Delete
.------------------------------------------------------------------------------
ICD8DI00  RESET     PTICD001
          MOVE      "8",ICD10EDN
.                                           * make sure files are open
          OPEN      PATI18D1,"pat10d8f"     * Open ICD10 Ed.8 Diseases
          OPEN      PATK8DA1,"patkcd8f"     * ICD10 Ed.8 Key Word Diag.
          OPEN      PATK8DA2,"patkcd8f"     * ICD10 Ed.8 Key Word Diag.
.
          RESET     UPDTTYPE
          MATCH     SP1,UPDTTYPE
          GOTO      ICD8DI40 IF EQUAL
.
          MATCH     "A",UPDTTYPE    * Add formaction
          GOTO      ICD8DI10 IF EQUAL
.
          MATCH     "U",UPDTTYPE    * Update formaction
          GOTO      ICD8DI20 IF EQUAL
.
          MATCH     "D",UPDTTYPE    * Delete formaction
          GOTO      ICD8DI30 IF EQUAL
.
          MATCH     "V",UPDTTYPE    * Validate Formaction
          IF        @EQUAL
            CALL      VAL8CD00
            GOTO      ICD8DI99
          ENDIF
.
          GOTO      ICD8DI93
.
ICD8DI10  CALL      CHKDIS00  * Checks mandatory fields & checks for blank
          BRANCH    EXIT,ICD8DI99
          PACK      KEY9,PTICD001,SP70  * ADD FORMACTION
          CALL      RDPT18D1
          COMPARE   ZERO,OVRCD
          GOTO      ICD8DI92 IF EQUAL
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      WRPT18D1      * Writes away the data
          PROC      PATK8DUP      * Update Keyword table
.
          CALL      ADDCAN00      * Add Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD8DI99
.
ICD8DI20  PACK      KEY9,PTICD001,SP70  * UPDATE FORMACTION
          CALL      RDPT18D1
          BRANCH    OVRCD,ICD8DI91
          CALL      CHKDIS00      * Checks mandatory fields
          BRANCH    EXIT,ICD8DI99
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      UPPT18D1      * Writes away the data
          PROC      PATK8DUP      * Update Key word Table
.
          CALL      UPDCAN00      * Update Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD8DI99
.
ICD8DI30  PACK      KEY9,PTICD001,SP70   * DELETE FORMACTION
          CALL      RDPT18D1
          BRANCH    OVRCD,ICD8DI91
          CALL      DEPT18D1      * Deletes the data
          PROC      PATK8DUP      * Update Key word Table
.
          CALL      DELCAN00      * Delete Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD8DI99
.
ICD8DI40  PACK      KEY9,PTICD001,SP70
          CALL      RDPT18D1
          IF        OVRCD=1
            CALL      CLRDIS00    * Clear all the file variables
          ENDIF
.
          MOVE      ONE,CANCERCD
          PACK      KEY9,PTICD001,SP70
          CALL      RDPTCAN1
          IF        OVRCD=1
            MOVE     ZERO,CANCERCD
            MOVE     ZERO,PTCCLATR
          ENDIF
.
          CLEAR     WEBTITLE
          MOVE      "ICD10 Edition 8 Diagnosis Maintenance",WEBTITLE
          RESET     WEBTITLE
.
          CALL      WEBHED00   * Create Output File and Write HTML Page Header
          BRANCH    EXIT,ICD8DI99
          CALL      WEBBOD00   * Process Web Body
          CALL      WEBEND00   * Process End of HTML File
          MOVE      ONE,EXIT
          GOTO      ICD8DI99
.
ICD8DI91  MOVE      "ICD10 Edition 8 Diagnosis Code does not exist",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD8DI99
.
ICD8DI92  MOVE      "ICD10 Edition 8 Diagnosis Code exists",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD8DI99
.
ICD8DI93  MOVE      "Invalid Form Action.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD8DI99
.
ICD8DI99  RETURN
+
