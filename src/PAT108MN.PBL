. ICD10 Edition 8 Procedure Code: Search Table
.-----------------------------------------------------------
SPT8OA00  WRITE     HTMLFILE;"<tr align=center>":
                             "<td class=HeadingCell colspan=2>":
                             "<b>You Searched for ":
                               KEYWORDS,"</td>":
                             "</tr>":
                             "<tr>":
                             "<td class=HeadingCell width=15%><b>Code</b></td>":
                             "<td class=HeadingCell><b>Description</b></td>":
                             "</tr>"
.
          CALL      GETWRD00
.
          UNPACK    DIM127,D1,LINKPRG,DIM127
          UNPACK    DIM127,D1,LINKREP,DIM127
          UNPACK    DIM127,D1,LINKTEM,DIM127
.
          IF        NORECORD=0
            MOVE      PTCNNRTD,NORECORD      * Set Default No Records to Display
          ENDIF
          MOVE      ZERO,RECNUMB
          MOVE      ZERO,DISNUMB
.
....      UNPACK    PTCNGDV8,KEY4,KEY5
....      MATCH     "9999",KEY4          * is ICD10 Ed.8 in use yet?
....      GOTO      SPT8OA90 IF EQUAL
.
          STRIP     KEYWRD01
          PACK      KEY24,KEYWRD01,SP70
          CALL      RSPT8O2
SPT8OA10  CALL      RKPT8O2
          BRANCH    OVRCD,SPT8OA90
.
          CALL      SPT8OB00
          BRANCH    OVRCD,SPT8OA10,SPT8OA90
.
          MOVE      PTCOITM,KEY9
          CALL      RDPT18O1
          BRANCH    OVRCD,SPT8OA10
.
          ADD       ONE,RECNUMB
          IF        STARTNUM>=RECNUMB
            GOTO      SPT8OA10
          ENDIF
.
          BRANCH    LNK2PRT,SPT8OA20,SPT8OA30
          GOTO      SPT8OA99
.
SPT8OA20  WRITE     HTMLFILE;"<tr><td nowrap><A HREF='javascript:showICD02":
                             "(#"",LINKPRG,"#.pbl":
                             "?reportno=",LINKREP:
                             "&template=",LINKTEM:
                             "&ptico001=",OPCODE,"#")'>":
                             "<img src=../images/MaintenanceIcon.gif hspace=4":
                             " border=0 align=absmiddle></a>":
                             OPCODE,"</td>":
                             "<td>",PT0ODSC2,"</td>":
                             "</tr>"
.
          GOTO      SPT8OA80
.
SPT8OA30  WRITE     HTMLFILE;"<tr valign=top>":
                              "<td><a href=#"#" ":
                              "OnClick=#"updateParent('",PT0ODSC2:
                              "','",OPCODE,"')#">":
                              "<img src=../images/MaintenanceIcon.gif hspace=4":
                              " border=0 align=absmiddle></a>":
                              OPCODE,"</td>":
                              "<td>",PT0ODSC2,"</td>":
                             "</tr>"
.
SPT8OA80  ADD       ONE,DISNUMB
          COMPARE   NORECORD,DISNUMB
          GOTO      SPT8OA10 IF NOT EQUAL
          GOTO      SPT8OA99
.
SPT8OA90  CALL      NOMORE00        * Display No More Records Message
.
SPT8OA99  RETURN
.------------------------------------------------------------
. Check ICD10 Edition 8 Procedure Record
.------------------------------------------------------------
SPT8OB00  MATCH     KEYWRD01,PTCOKWD
          GOTO      SPT8OB92 IF NOT EQUAL
          IF        KEYWRDNO>1
            PACK      SAVKEY24,PTCOKWD,PTCOITM
            CALL      SPT8OC00
            BRANCH    EXIT,SPT8OB91
            MOVE      SAVKEY24,KEY24
            CALL      RDPT8O2
          ENDIF
.
. Insert Other Check for Valid Record Here i.e. Suspended or In Active
.   eg    COMPARE    ONE,INACTIVE
.         GOTO       SPT8OB91 IF EQUAL
.
          GOTO      SPT8OB90
.
SPT8OB90  MOVE      ZERO,OVRCD
          GOTO      SPT8OB99
SPT8OB91  MOVE      ONE,OVRCD
          GOTO      SPT8OB99
SPT8OB92  MOVE      TWO,OVRCD
SPT8OB99  RETURN
.------------------------------------------------------------
. Check for Matching ICD10 Edition 8 Procedure Words
.------------------------------------------------------------
SPT8OC00  MOVE      ONE,F2
          MOVE      ZERO,EXIT
          PACK      SAVKEY9,PTCOITM,SP70
.
SPT8OC10  COMPARE   KEYWRDNO,F2           * Loop through each Key Word in Search
          GOTO      SPT8OC99 IF EQUAL     * Exit OK
          ADD       ONE,F2
          LOAD      KEYWRDXX,F2,KEYWRD01,KEYWRD02,KEYWRD03,KEYWRD04,KEYWRD05:
                             KEYWRD06,KEYWRD07,KEYWRD08,KEYWRD09,KEYWRD10
          PACK      KEY24,PTCOITM,KEYWRDXX
          CALL      RDPT8O1               * Check for Exact Match
          COMPARE   ZERO,OVRCD            * Ok so Continue Loop
          GOTO      SPT8OC10 IF EQUAL
.
          CALL      RKPT8O1               * Not Exact so Read Next Record
          BRANCH    OVRCD,SPT8OC90        * No Match So Exit No Match
          PACK      KEY9,PTCOITM,SP70
          MATCH     SAVKEY9,KEY9        * Match Key Fields
          GOTO      SPT8OC90 IF NOT EQUAL * No Match So Exit
          STRIP     KEYWRDXX
          MATCH     PTCOKWD,KEYWRDXX         * Check Key Word
          GOTO      SPT8OC90 IF NOT EQUAL * No Match So Exit
          GOTO      SPT8OC10              * Check Next Search Word
.
SPT8OC90  MOVE      ONE,EXIT
SPT8OC99  RETURN
