.******************************************************************************
.         Update GST revenue breakdown file
.******************************************************************************
URFG0000  IF        PROGFLAG=3 & IBCNUGST=2
            MOVE      FGSTFLAG,FORM1     * Save
            MOVE      THREE,FGSTFLAG
            MOVE      FINAMT,GSTWORK
            MULT      GSTPCEN,GSTWORK
            DIV       "100.00",GSTWORK
            MOVE      GSTWORK,FINAMT
            CALL      PATCALF
            MOVE      FORM1,FGSTFLAG     * restore
          ENDIF
