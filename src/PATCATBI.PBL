          UNPACK    SP70,FININVN,FINADMN,FINURNO
          IF        PROGFLAG=3
            PACK      FININVN,CNEXTINV,SP70
            MOVE      AADMNO,FINADMN
            MOVE      AURNO,FINURNO
          ENDIF
.
          PACK      PINVDTE WITH CCC,CYY,CMM,CDD
          REP       " 0",PINVDTE
.
          MOVE      PINVDTE,FINDATE
          REP       " 0",FINDATE
          MOVE      THREE,FINSYS
          MOVE      SP6,FINSITE
          MOVE      PMVXMHOS,FINHOSP
          CALL      PATCALF
