          APPEND    "@",KEY25
          APPEND    XLSTRATE,KEY25
          APPEND    " for",KEY25
          APPEND    FORM41,KEY25
          APPEND    DAYFORMT,KEY25
          APPEND    SP1,KEY25
          APPEND    SP70,KEY25
