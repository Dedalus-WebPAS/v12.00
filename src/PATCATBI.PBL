.******************************************************************************
PRIG0000  COMPARE   "3",SAVRTYPE 
          GOTO      PRIG9999 IF NOT EQUAL   * Print Doctor charges later
.
          COMPARE   MAXLINE,COUNT
          CALL      TAIL IF NOT LESS
          MOVE      MISCTOT,FORM7P2
          IF        INVTYPE=1
            PRINT     *3,ESCCHR,CHARG,TDESC,ESCCHR,CHARH:
                      *58,FORM7P2
          ELSE
            PRINT     *3,ESCCHR,CHARG,TDESC,ESCCHR,CHARH:
                      *71,FORM7P2
          ENDIF
          ADD       ONE,COUNT
          MOVE      ZERO,MISCTOT
          MOVE      ZERO,MISCGST
