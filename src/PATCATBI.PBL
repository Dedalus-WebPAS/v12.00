            OPEN      CONTROLF,"controlf"
            READ      CONTROLF,HUND10;*229,PTCNMVCP
            IF        PTCNMVCP=1
              CALL      CMVDY000            * calculate mv days
              CALL      CLDAY000
              PACK      LDATE,CC,YY,MM,DD
              REP       " 0",LDATE          * end date to calculate LOS
.
              IF        MVBFLAG=1
