            MOVE      JULDAY,JWKDAY
            MOVE      JULYR,JWKYER
            MOVE      JULCC,JWKCC
            CALL      JULCON
            PACK      TDATE,CC,YY,MM,DD
            REP       " 0",TDATE
          ENDIF
.
          ASSIGN    FORM4/SIXTY,FORM4
          ASSIGN    FORM4*SIXTY,FORM4
          SUB       FORM4,FORM4A
          MOVE      FORM4A,F2
