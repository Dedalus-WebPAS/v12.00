             MOVE    ZERO,EXIT
             PROC    CETT0000         *certificat type
             IF        EXIT=1
               WRITE    HTMLFILE;"<font color=red>",BEDDESC,"</font>";
             ELSE
               WRITE    HTMLFILE;"<font color=green>",BEDDESC,"</font>";
             ENDIF
          ENDIF
.
          MATCH     SP70,PTDTDES2
