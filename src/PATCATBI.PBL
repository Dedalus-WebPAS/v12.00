          MOVE      MAXLINE,FORM2
          SUB       TWO,FORM2
          COMPARE   FORM2,COUNT
          CALL      TAIL IF NOT LESS
          PRINT     *N,*70,"----------":
                    *N,*3,"Total Doctor Charges",*70,FORM7P2
          ADD       THREE,COUNT
