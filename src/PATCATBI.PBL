JHED2000 MOVE      NETTOT,FORM12P2
         CALL      SRND0000                 * Work out the rounding adjustment
         WRITE      HTMLFILE;"<tr bgcolor=#CCCCCC><td>&nbsp;</td>":
                               "<td><b>Balance Payable</b></td>";
