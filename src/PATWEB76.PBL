          CALL      RDCMDEP1
          COMPARE   ZERO,OVRCD
          GOTO      CPND2200 IF EQUAL       * payment released
.
          PACK      KEY12,RCDTRECN,SP70
          CALL      RDRCBNK1
          BRANCH    OVRCD,CPND2200
          MATCH     "0",RCBNSTAT               * Cash Not banked?
          GOTO      CPND2200 IF NOT EQUAL
          MOVE      "*",CPNDFLAG               * Flag for cash pending 
.
          IF        RCDTDISC<>0
            ADD       ONE,DSCNFLAG
          ENDIF
