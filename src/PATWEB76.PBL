.
          MATCH      "2",PTCNIRAI
          IF         @EQUAL
            WRITE     HTMLFILE;"<td>",CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR:
                               " at ",PTINCTIM,"</td>";
          ELSE
            WRITE     HTMLFILE;"<td>",CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR:
                               "</td>";
