              WRITE     HTMLFILE;"<td>&nbsp;</td>";
            ENDIF
.
            MATCH     "2",PTCNIRAI
            IF        @EQUAL
              WRITE     HTMLFILE;"<td>&nbsp;</td>";
            ENDIF
.
            WRITE     HTMLFILE;"<td>&nbsp;</td>":
                               "<td>&nbsp;</td>":
                               "<td align=right><b>",SUMINV,"</b></td>":
                               "<td align=right><b>",SUMPAID,"</b></td>":
                               "<td align=right><b>",SUMJR,"</b></td>":
