.
          ADD       TRATEF,PTTRADPT
          ADD       TRATEH,PTTRADRB  * patient portion
.
          WRITE     HTMLFILE;"<tr>":
                             "<td>",CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR:
                             " at ",TTIME,DSTEP,"</td>":
                             "<td>&nbsp;",TWARD,"</td>":
                             "<td>&nbsp;",TBED,"</td>":
                             "<td align=right>&nbsp;",PTTRADPT,"</td>":
                             "<td align=right>&nbsp;",TDISC,"</td>":
                             "<td align=right>&nbsp;",PTTRADRB,"</td>":
                             "<td align=right>&nbsp;",FORM82,"</td>":
                             "</tr>"
