          IF        PINVTYP = 2 | (PINVAMT<>0 & PINVREB=0)
            WRITE     HTMLFILE;"<font color=#0000FF><b>",PINVNO:
                               "</font></b></td>";
          ELSE
            WRITE     HTMLFILE;PINVNO,"</td>";
