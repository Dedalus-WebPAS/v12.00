          ENDIF
.
          WRITE     HTMLFILE;"<td><b>Visit Type</b></td>":
                            "<td><b>Health Fund</b></td>":
                            "</tr>"
.
          IF        NORECORD=0
            MOVE    "10",NORECORD           * Numb of records=0 Default to 10
          ENDIF
