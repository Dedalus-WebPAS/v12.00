.
          CALL      IRBY0000      * Display Invoice Raised By ?
.
          WRITE     HTMLFILE;"<td>",VISTDESC,"</td>";
.
          MATCH     "&nbsp",HFNDESC
          IF        @EQUAL
            WRITE     HTMLFILE;"<td>&nbsp;</td></tr>"
          ELSE
            WRITE     HTMLFILE;"<td>",HFNDESC,"/",HFNTABL,"</td></tr>"
          ENDIF
.
          ADD       ONE,ROWCOUNT
          COMPARE   NORECORD,ROWCOUNT
          GOTO      PMTINV10 IF NOT EQUAL
.
