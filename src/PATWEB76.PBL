.
          COMPARE   "2",REGIBACD
          GOTO      CPND5000 IF EQUAL       * Private Practice
          COMPARE   "96",REGIBACD
          GOTO      CPND5000 IF EQUAL       * PRI Deposit
.
          COMPARE   ZERO,F1
          GOTO      CPND8000 IF EQUAL       * F1=0 for Patients
          GOTO      CPND2200
.
CPND5000  BRANCH    F1,CPND8000             * F1=1 for Private practice
          GOTO      CPND2200
.
