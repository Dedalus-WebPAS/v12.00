            IF        FORM72<>0 | FORM7P2<>0
              PACK      KEY22 WITH ADMISSNO,PINVNO,TRANSNUM[COUNTER],SP70
              CALL      RDDTR000
              IF        OVRCD=0
.
                IF        CFEETYP=5 & IBCNUGST=3
                  MOVE      CREDAMNT[COUNTER],TGSTEXCV
