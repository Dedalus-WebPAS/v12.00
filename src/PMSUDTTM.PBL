.-------------------------------------------------------------------------------
. Display Program Comments
.-------------------------------------------------------------------------------
PMUT0000  BRANCH    FLDITMNO,PMUT0100,PMUT0200,PMUT0300,PMUT0400,PMUT0500:
                             PMUT0600,PMUT0700,PMUT0800,PMUT0900,PMUT1000:
                             PMUT1100,PMUT1200,PMUT1300,PMUT1400,PMUT1500:
                             PMUT1600,PMUT1700,PMUT1800 
.
          WRITE     HTMLFILE;"Invalid PMUT IBA Tag";
          GOTO      PMUT9999
.
PMUT0100  MATCH     SP70,PMUTURNO
          GOTO      PMUT9999 IF EQUAL
          WRITE     HTMLFILE;PMUTURNO;
          GOTO      PMUT9999
.
PMUT0200  MATCH     SP70,PMUTATYP
          GOTO      PMUT9999 IF EQUAL
          WRITE     HTMLFILE;PMUTATYP;
          GOTO      PMUT9999
.
PMUT0300  MATCH     SP70,PMUTCTYP
          GOTO      PMUT9999 IF EQUAL
          WRITE     HTMLFILE;PMUTCTYP;
          GOTO      PMUT9999
.
PMUT0400  MATCH     SP70,PMUTHFND
          GOTO      PMUT9999 IF EQUAL
          WRITE     HTMLFILE;PMUTHFND;
          GOTO      PMUT9999
.
PMUT0500  MATCH     SP70,PMUTHTAB
          GOTO      PMUT9999 IF EQUAL
          WRITE     HTMLFILE;PMUTHTAB;
          GOTO      PMUT9999
.
PMUT0600  MATCH     PMUTEXPD,SP70
          GOTO      PMUT9999 IF EQUAL
          WRITE     HTMLFILE;PMUTEXPD;
          GOTO      PMUT9999
.
PMUT0700  MATCH     SP70,PMUTCOMT
          GOTO      PMUT9999 IF EQUAL
          WRITE     HTMLFILE;PMUTCOMT;
          GOTO      PMUT9999
.
PMUT0800  MATCH     SP70,PMUTNNUM
          GOTO      PMUT9999 IF EQUAL
          WRITE     HTMLFILE;PMUTNNUM;
          GOTO      PMUT9999
.
PMUT0900  MATCH     PMUTIDAT,SP70
          GOTO      PMUT9999 IF EQUAL
.
          UNPACK    PMUTIDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUT9999

PMUT1000  MATCH     SP70,PMUTITIM
          GOTO      PMUT9999 IF EQUAL
          WRITE     HTMLFILE;PMUTITIM;
          GOTO      PMUT9999
.
PMUT1100  MATCH     SP70,PMUTIUID
          GOTO      PMUT9999 IF EQUAL
.
          PACK      KEY10,PMUTIUID,SP70
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      PMUTIUID,WBSENAM
          ENDIF
          WRITE     HTMLFILE;WBSENAM;
          GOTO      PMUT9999
.
PMUT1200  MATCH     SP70,PMUTDIND
          GOTO      PMUT9999 IF EQUAL
          WRITE     HTMLFILE;PMUTDIND;
          GOTO      PMUT9999
.
PMUT1300  MATCH     PMUTDDAT,SP70
          GOTO      PMUT9999 IF EQUAL
.
          UNPACK    PMUTDDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      PMUT9999

PMUT1400  MATCH     SP70,PMUTDTIM
          GOTO      PMUT9999 IF EQUAL
          WRITE     HTMLFILE;PMUTDTIM;
          GOTO      PMUT9999
.
PMUT1500  MATCH     SP70,PMUTDUID
          GOTO      PMUT9999 IF EQUAL
.
          PACK      KEY10,PMUTDUID,SP70
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      PMUTDUID,WBSENAM
          ENDIF
          WRITE     HTMLFILE;WBSENAM;
          GOTO      PMUT9999
.
PMUT1600  PACK      KEY43,PMUTURNO,PMUTATYP,PMUTCTYP,PMUTHFND,PMUTHTAB:
                          PMUTEXPD,PMUTCOMT,PMUTNNUM,SP70
          CALL      RSPMUTX1
PMUT1610  CALL      RKPMUTX1
          BRANCH    OVRCD,PMUT9999
.
          MATCH     PMUTURNO,PMUXURNO
          GOTO      PMUT9999 IF NOT EQUAL
.
          MATCH     PMUTATYP,PMUXATYP
          GOTO      PMUT9999 IF NOT EQUAL
.
          MATCH     PMUTCTYP,PMUXCTYP
          GOTO      PMUT9999 IF NOT EQUAL
.
          MATCH     PMUTHFND,PMUXHFND
          GOTO      PMUT9999 IF NOT EQUAL
.
          MATCH     PMUTHTAB,PMUXHTAB
          GOTO      PMUT9999 IF NOT EQUAL
.
          MATCH     PMUTEXPD,PMUXEXPD
          GOTO      PMUT9999 IF NOT EQUAL
.
          MATCH     PMUTCOMT,PMUXCOMT
          GOTO      PMUT9999 IF NOT EQUAL
.
          MATCH     PMUTNNUM,PMUXNNUM
          GOTO      PMUT9999 IF NOT EQUAL
.
          STRIP     PMUXCOMM
          MOVELPTR  PMUXCOMM,LENGTH
.
          COMPARE   LENGTH,ZERO
          IF        @EQUAL
            MOVE      ONE,LENGTH
          ENDIF
.
          SFORMAT   VAR,LENGTH
          MOVE      PMUXCOMM,VAR
          WRITE     HTMLFILE;VAR
          SFORMAT   VAR,127
.          
          GOTO      PMUT1610
.
PMUT1700  CALL      PRGCTB00
          GOTO      PMUT9999
.
PMUT1800  PACK      KEY40,PMUPG001,PMUPG002,PMUPG003,PMUPG004,PMUPG005:
                          PMUPG006,SP70
          CALL      RSPMUDT1
          CALL      RKPMUDT1
          BRANCH    OVRCD,PMUT1890
.
          MATCH     PMUTURNO,PMUPG001
          GOTO      PMUT1890 IF NOT EQUAL
.
          MATCH     PMUTATYP,PMUPG002
          GOTO      PMUT1890 IF NOT EQUAL
.
          MATCH     PMUTCTYP,PMUPG003
          GOTO      PMUT1890 IF NOT EQUAL
.
          MATCH     PMUTHFND,PMUPG004
          GOTO      PMUT1890 IF NOT EQUAL
.
          MATCH     PMUTHTAB,PMUPG005
          GOTO      PMUT1890 IF NOT EQUAL
.
          MATCH     PMUTEXPD,PMUPG006
          GOTO      PMUT1890 IF NOT EQUAL
.
          WRITE     HTMLFILE;ONE;
          GOTO      PMUT9999
.
PMUT1890  WRITE     HTMLFILE;ZERO;
          GOTO      PMUT9999
.
PMUT9999  RETURN
.
