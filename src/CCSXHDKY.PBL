.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSXHDKY
.   Routine   :   KCCXHA00 Keyin Casemix Code        
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCXHA00 TOPIC     "CCXHKEY "
          MOVE      UNDLN70,CCXHKEY 
          KEYIN     *PCCOL:CVERT,*DV,CCXHKEY:                                   
                    *PCCOL:CVERT,*V2LON,CCXHKEY:                                
                    *PCCOL:CVERT,*DV,CCXHKEY 
.
          ENDSET    CCXHKEY 
          APPEND    SP70,CCXHKEY 
          RESET     CCXHKEY 
.
          MATCH     SP70,CCXHKEY 
          GOTO      KCCXHA90 IF EQUAL
.
          CMATCH    EXITCHAR,CCXHKEY 
          GOTO      KCCXHA95 IF EQUAL
.
          CMATCH    QUEST,CCXHKEY 
          GOTO      KCCXHA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCCXHA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCXHKEY 
          APPEND    SP70,CCXHKEY 
          RESET     CCXHKEY 
.
          MATCH     SP70,CCXHKEY 
          GOTO      KCCXHA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CCXHKEY                                 
KCCXHA10
          PACK      KEY13,CCXHSTY,CCXHKEY                                       
          CALL      RACCXH1 
          BRANCH    OVRCD,KCCXHA80
          BRANCH    CKEYTYP,KCCXHA75
          CALL      RDCCXH1 
KCCXHA70
          MOVE      ZERO,EXIT
          GOTO      KCCXHA99
KCCXHA75
          MOVE      "Casemix Code Already Exist - ",DISPMSG                                          
          CALL      MESSAGE1
          GOTO      KCCXHA00
KCCXHA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCXHA99,KCCXHA99
          MOVE      "Casemix Code Does Not Exist - ",DISPMSG                                         
          CALL      MESSAGE1
          GOTO      KCCXHA00
KCCXHA90
          MOVE      ONE,EXIT
          GOTO      KCCXHA99
KCCXHA95
          MOVE      TWO,EXIT
KCCXHA99 TOPIC    PRGID
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSXHDKY
.   Routine   :   HCCXHA00 Casemix Codes       
.   Return    :   CCXHKEY 
.-------------------------------------------------------------------------------
HCCXHA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "30",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCXHA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCXHA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Casemix Code"       
.
          MOVE      STDCOL01,STDCOL02
          ADD       "21",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "41",STDCOL03
HCCXHA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCXHKEY 
          PACK      KEY13,CCXHSTY,CCXHKEY,SP70                                  
          PACK      SAVKEY80,CCXHSTY,CCXHKEY,SP70                               
          SETLPTR   SAVKEY80,3                                                  
          CALL      RSCCXH1 
HCCXHA10
          CALL      RKCCXH1 
          BRANCH    OVRCD,HCCXHA15
          PACK      KEY13,CCXHSTY,CCXHKEY,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HCCXHA15 IF NOT EQUAL                                       
          BRANCH    CSEARCH,HCCXHA12
          CALL      HCCXHA60
          BRANCH    OVRCD,HCCXHA10
HCCXHA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY13,XITEMNO,ST1,ST2,ST3,ST4,ST5:                          
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCXHKEY 
          DISPLAY   *PSTDCOL02:STDROW,CCXHDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCXHA10 IF NOT EQUAL
HCCXHA15
          UNPACK    SAVKEY80,CCXHSTY,CCXHKEY                                    
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCXHA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCXHA30,HCCXHA35,HCCXHA40,HCCXHA45,HCCXHA04:
                              HCCXHA91,HCCXHA50
          GOTO      HCCXHA92
HCCXHA30
          CALL      RKCCXH1 
          BRANCH    OVRCD,HCCXHA85
          PACK      KEY13,CCXHSTY,CCXHKEY,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HCCXHA85 IF NOT EQUAL                                       
          CALL      RPCCXH1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCXHA10
HCCXHA35
          MOVE      ST1,KEY13
          CALL      RDCCXH1 
          CALL      RPCCXH1 
          BRANCH    OVRCD,HCCXHA85
          PACK      KEY13,CCXHSTY,CCXHKEY,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HCCXHA85 IF NOT EQUAL                                       
          MATCH     SP70,CCXHKEY                                                
          GOTO      HCCXHA85 IF EQUAL                                           
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCXHA36
          CALL      RPCCXH1 
          BRANCH    OVRCD,HCCXHA37
          PACK      KEY13,CCXHSTY,CCXHKEY,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HCCXHA37 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCXHA36 IF NOT EQUAL
HCCXHA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCXHA10
HCCXHA40
          MOVE      ST1,KEY13
          CALL      RDCCXH1 
          CALL      RPCCXH1 
          BRANCH    OVRCD,HCCXHA85
          PACK      KEY13,CCXHSTY,CCXHKEY,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HCCXHA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCXHA08
HCCXHA45
          CALL      RKCCXH1 
          BRANCH    OVRCD,HCCXHA85
          PACK      KEY13,CCXHSTY,CCXHKEY,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HCCXHA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY13,SAVKEY80,Z70                                          
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCXH1 
HCCXHA46
          CALL      RPCCXH1 
          BRANCH    OVRCD,HCCXHA47
          PACK      KEY13,CCXHSTY,CCXHKEY,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HCCXHA47 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCXHA46 IF NOT EQUAL
HCCXHA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCXHA10
HCCXHA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCXHA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCXHA08
HCCXHA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCXHA08
HCCXHA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCXHA61 IF EQUAL
          MATCH     SCHFLD01,CCXHKEY 
          GOTO      HCCXHA72 IF NOT EQUAL
HCCXHA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCXHA62 IF EQUAL
          MATCH     SCHFLD02,CCXHDES 
          GOTO      HCCXHA72 IF NOT EQUAL
HCCXHA62
HCCXHA63
HCCXHA64
HCCXHA65
HCCXHA66
HCCXHA67
HCCXHA68
HCCXHA69
HCCXHA70
          GOTO      HCCXHA75
HCCXHA72  MOVE      ONE,OVRCD
HCCXHA75  RETURN
HCCXHA85
          LOAD      KEY13,XITEMNO,ST1,ST2,ST3,ST4,ST5:                          
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY13,CCXHSTY,CCXHKEY                                       
          CALL      RDCCXH1 
HCCXHA90
          UNPACK    SAVKEY80,CCXHSTY,CCXHKEY                                    
          BEEP
          GOTO      HCCXHA20
HCCXHA91
          MOVE      SP70,CCXHDES 
          MOVE      SP70,CCXHSUM 
          MOVE      SP70,CCXHSPA 
          MOVE      SP70,KEY13
          MOVE      SP70,CCXHKEY 
          GOTO      HCCXHA96
HCCXHA92
          MOVE      SP70,KEY13
          MOVE      SP70,CCXHKEY 
          LOAD      KEY13,CCITEM,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY13,CCXHSTY,CCXHKEY                                       
HCCXHA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCXHA99
          RETURN
