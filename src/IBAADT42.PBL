.******************************************************************************
.*                                  GFND0000                                  *
.*                        Generate record of a health fund                    *
.******************************************************************************
GFND0000  MOVE      ZERO,LINECNTR
          MOVE      ONE,HEADFLAG
.
          OPEN      UMPROTXT,FNAMEI
.
GFND1000  READ      UMPROTXT,SEQ;XFLD0001,XFLD0002,XFLD0003,XFLD0004,XFLD0005:
                                 XFLD0006,XFLD0007,XFLD0008,XFLD0009
          GOTO      GFND9000 IF OVER
.
          MATCH     SP70,XFLD0007
          GOTO      GFND1000 IF EQUAL       * Skip blank charge type
.
          ADD       ONE,LINECNTR            * Text file line counter for error
.
          PACK      XFLD0001,XFLD0001,SP70
          PACK      XFLD0002,XFLD0002,SP70
          PACK      XFLD0003,XFLD0003,SP70
          PACK      XFLD0004,XFLD0004,SP70
          PACK      XFLD0005,XFLD0005,SP70
          PACK      XFLD0006,XFLD0006,SP70
          PACK      XFLD0007,XFLD0007,SP70
          PACK      XFLD0008,XFLD0008,SP70
          PACK      XFLD0009,XFLD0009,SP70
.
          IF        VALDPASS=1
            CALL      VFLD0000                * Validation pass so
            BRANCH    EXIT,GFND1000           * validate fields
          ENDIF
.
          PACK      XCLMCD,XFLD0001,SP70
          MATCH     XCLMCD,KYCLAIM
          IF        !@EQUAL
            MOVE      "Claim Code Mismatch",ERRORMSG
            CALL      PERR0000
            GOTO      GFND1000
          ENDIF
.
          PACK      XHFCOD,XFLD0002,SP70
.
          BRANCH    OPTION,GFND2100,GFND2200,GFND2300
          GOTO      GFND1000
.
GFND2100  PACK      XHFCOD,KYHFUND,SP70
          PACK      XHFTYP,XFLD0003,SP70
          GOTO      GFND3000
.
GFND2200  MATCH     XHFCOD,KYHFUND
          IF        !@EQUAL
            MOVE      "Health Fund Mismatch",ERRORMSG
            CALL      PERR0000
            GOTO      GFND1000
          ENDIF
.
          PACK      XHFTYP,XFLD0003,SP70
          GOTO      GFND3000
.
GFND2300  MOVE      SP70,XHFCOD
          MOVE      SP70,XHFTYP
          GOTO      GFND3000
.
GFND3000  PACK      XCCMIX,XFLD0004,SP70
          PACK      XCPMBS,XFLD0005,SP70
          PACK      XCATYP,XFLD0006,SP70
          PACK      XCCTYP,XFLD0007,SP70
.
          MOVE      ZERO,XCRAMT
          SQUEEZE   XFLD0008
          MOVE      XFLD0008,XCRAMT
.
          MOVE      ZERO,XCPAMT
          SQUEEZE   XFLD0009
          MOVE      XFLD0009,XCPAMT
.
          COMPARE   THREE,OPTION
          GOTO      GFND3800 IF EQUAL       * Default blank HF/table
.
          BRANCH    KYHFTAB,GFND4000        * All Health fund table
.
.         as per Upload file
.
GFND3800  CALL      LIFC0000
          GOTO      GFND1000
.
.         All health fund table type of the health fund
.
GFND4000  PACK      KEY14,KYHFUND,SP70
          CALL      RDSFUND1
GFND5000  CALL      RDKFUND1
          BRANCH    OVRCD,GFND1000
          MATCH     KYHFUND,HCODE
          GOTO      GFND1000 IF NOT EQUAL
.
          MATCH     "0000    ",HFTABL
          GOTO      GFND5000 IF EQUAL  
.
          BRANCH    PHFTACT,GFND5000        * Inactive
.
          MOVE      PTHFBCAT,XHFTYP         * Health fund Table type
          CALL      LIFC0000
          GOTO      GFND5000                * get next record
.
GFND9000  CLOSE     UMPROTXT
.
GFND9999  RETURN
+
.******************************************************************************
.*                                  LIFC0000                                  *
.*                             Write ICF Charge record                        *
.******************************************************************************
LIFC0000  UNPACK    SP70,PTFCCLTY,PTFCFUND,PTFCHTAB,PTFCRTYP,PTFCCMAT,PTFCCTYP
.
          MATCH     SP70,XCCMIX
          IF        !@EQUAL
            MOVE      ONE,PTFCRTYP                 * Casemix code
            PACK      PTFCCMAT,XCCMIX,SP70
          ELSE
            MATCH     SP70,XCPMBS
            IF        !@EQUAL
              MOVE      TWO,PTFCRTYP               * MBS Code
              PACK      PTFCCMAT,XCPMBS,SP70
            ELSE
              MATCH     SP70,XCATYP
              IF        !@EQUAL
                MOVE      THREE,PTFCRTYP           * Adm.type
                PACK      PTFCCMAT,XCATYP,SP70
              ENDIF
            ENDIF
          ENDIF
          PACK      KEY25,XCLMCD,XHFCOD,XHFTYP,PTFCRTYP,PTFCCMAT,XCCTYP,SP70
          CALL      RDPTIFC1
          IF        OVRCD=1
            UNPACK   KEY25,PTFCCLTY,PTFCFUND,PTFCHTAB,PTFCRTYP,PTFCCMAT,PTFCCTYP
            MOVE      XCRAMT,PTFCRAMT
            MOVE      XCPAMT,PTFCPAMT
            UNPACK    SP70,PTFCSPRE
            IF        VALDPASS=0
              ADD       ONE,ADDCOUNT        * Total records added
              CALL      WRPTIFC1            * Write if not validation pass
            ENDIF
          ELSE
            MOVE      XCRAMT,PTFCRAMT
            MOVE      XCPAMT,PTFCPAMT
            IF        VALDPASS=0
              ADD       ONE,UPDCOUNT        * Total records updated
              CALL      UPPTIFC1            * Update if not validation pass
            ENDIF                         
          ENDIF
LIFC9999  RETURN
+
.******************************************************************************
.         Remove ICF Charge record
.******************************************************************************
RMMP0000  BRANCH    OPTION,RMMP2000         * by health fund group
          IF        OPTION=3
            MOVE      SP70,KYHFUND          * blank health fund
          ENDIF
          CALL      DREC0000                * Deleting existing records
          GOTO      RMMP9999
.
.         Get health funds for the HF group
.
RMMP2000  MOVE      SP70,SAVKEY20
          PACK      KEY20,KYHFGRP,SP70
          CALL      RDSFUND3
RMMP4000  CALL      RDKFUND3
          BRANCH    OVRCD,RMMP9999
          MATCH     KYHFGRP,PTHFHGRP    * Same group?
          GOTO      RMMP9999 IF NOT EQUAL
          MATCH     "0000    ",HFTABL
          GOTO      RMMP4000 IF NOT EQUAL
.
          MOVE      HCODE,KYHFUND
          PACK      SAVKEY20,PTHFHGRP,HCODE,HFTABL,SP70
.
          CALL      DREC0000                * Deleting existing records
.
          MOVE      SAVKEY20,KEY20
          CALL      RDFUND3                 * Reposition
          GOTO      RMMP4000
.
RMMP9999  RETURN
+
.******************************************************************************
.         Deleting existing records
.******************************************************************************
DREC0000  PACK      KEY25,KYCLAIM,KYHFUND,SP70
          CALL      RSPTIFC1
          CALL      RKPTIFC1
          BRANCH    OVRCD,DREC9999
.
          MATCH     KYCLAIM,PTFCCLTY
          GOTO      DREC9999 IF NOT EQUAL
.
          MATCH     KYHFUND,PTFCFUND
          GOTO      DREC9999 IF NOT EQUAL
.
	  PACK      KEY25,PTFCCLTY,PTFCFUND,PTFCHTAB,PTFCRTYP,PTFCCMAT:
                          PTFCCTYP,SP70
          MATCH     SP70,KEY25
          GOTO      DREC9999 IF EQUAL
          CALL      DEPTIFC1
          GOTO      DREC0000
.
DREC9999  RETURN
+
.******************************************************************************

.         Execute us1 command before processing
.******************************************************************************
EXCUS000  CLEAR     CMDLINE
          APPEND    "ibaadt42.us1 ",CMDLINE
          APPEND    FNAMEI,CMDLINE
          APPEND    SP70,CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
EXCUS999  RETURN
+
.******************************************************************************
. Validate bed fee input text file fields
.******************************************************************************
VFLD0000  MOVE      ZERO,EXIT
.
          MATCH     SP70,XFLD0001
          IF        !@EQUAL
            PACK      KEY5,ANSC,ANSL,XFLD0001,SP70
            CALL      RDCODE1      
            IF        OVRCD=1
              MOVE      "Invalid Claim Code",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.
          MATCH     SP70,XFLD0002
          IF        !@EQUAL
            PACK      KEY6,XFLD0002,SP70
            PACK      KEY14,KEY6,ZERO8,SP70
            CALL      RDFUND1
            IF        OVRCD=1
              MOVE      "Invalid Health Fund",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.
          MATCH     SP70,XFLD0003
          IF        !@EQUAL
            PACK      KEY5,ANSH,ANST,XFLD0003,SP70
            CALL      RDCODE1
            IF        OVRCD=1
              MOVE      "Invalid Health Fund Table Type",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.
          MOVE      ZERO,F1
          MATCH     SP70,XFLD0004
          IF        !@EQUAL
            MOVE      ONE,F1
            PACK      KEY9,XFLD0004,SP70
            CALL      RDPTCMC1
            IF        OVRCD=1
              MOVE      "Invalid Casemix code",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.
          MATCH     SP70,XFLD0005
          IF        !@EQUAL
            MOVE      ONE,F1
            PACK      KEY17,XFLD0005,SP70
            CALL      PATITMRD
            IF        OVRCD=1
              MOVE      "Invalid MBS Item code",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.
          MATCH     SP70,XFLD0006
          IF        !@EQUAL
            MOVE      ONE,F1
            MOVE      "A ",KEY2
            PACK      KEY5,KEY2,XFLD0006,SP70
            CALL      RDCODE1
            IF        OVRCD=1
              MOVE      "Invalid Admission Type",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
          IF        F1=0
            CLEAR     ERRORMSG
            APPEND    "Blank Adm.Typ/MBS/Casemix code",ERRORMSG
            APPEND    SP70,ERRORMSG
            RESET     ERRORMSG
            CALL      PERR0000
            MOVE      ONE,EXIT
          ENDIF
.
          MATCH     SP70,XFLD0007
          IF        @EQUAL
            MOVE      "Blank Charge Type",ERRORMSG
            CALL      PERR0000
            MOVE      ONE,EXIT
          ENDIF
.
          PACK      XFLD0008,XFLD0008,SP70
          MATCH     SP70,XFLD0008
          IF        !@EQUAL
            SQUEEZE   XFLD0008
            TYPE      XFLD0008
            IF        !@EQUAL | @EOS
              MOVE      "Invalid Rebate Amount",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.      
          PACK      XFLD0009,XFLD0009,SP70
          MATCH     SP70,XFLD0009
          IF        !@EQUAL
            SQUEEZE   XFLD0009
            TYPE      XFLD0009
            IF        !@EQUAL | @EOS
              MOVE      "Invalid Patient Amount",ERRORMSG
              CALL      PERR0000
              MOVE      ONE,EXIT
            ENDIF
          ENDIF
.      
VFLD9999  RETURN
+
.******************************************************************************
. Print error line
.******************************************************************************
PERR0000  ADD       ONE,ERRCOUNT            * Total error count
.
          UNPACK    XFLD0001,KEY3
          UNPACK    XFLD0002,KEY6
          UNPACK    XFLD0003,DIM3
.
          PRINT     *1,"|",LINECNTR,*12,"|",ERRORMSG,*44,"|":
                    KEY3,SP2,KEY6,SP2,DIM3,SP2;
.
          UNPACK    XFLD0004,DIM9
          UNPACK    XFLD0005,KEY9
          UNPACK    XFLD0006,KEY3
.
          PRINT     DIM9,SP2,KEY9,SP2,KEY3,SP2;
.
          UNPACK    XFLD0007,KEY3
          UNPACK    XFLD0008,DIM15
          UNPACK    XFLD0009,KEY15
.
          PRINT     KEY3,SP2,DIM15,SP2,KEY15:
                    *132,"|"
.
PERR9999  RETURN
