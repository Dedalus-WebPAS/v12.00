.------------------------------------------------------------------------
. Open Nato X-Ref File if required
.------------------------------------------------------------------------
ONAT0000  OPEN      CONTROLF,"controlf"
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF FORMAT
          READ      CONTROLF,SIXTY3;*143,HNONUSAG
          TRAPCLR   FORMAT
          IF        OVRCD=1
            MOVE      ONE,HNONUSAG
            WRITAB    CONTROLF,SIXTY3;*143,HNONUSAG
          ENDIF
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF FORMAT
          READ      CONTROLF,SIXTY3;*141,INCNNATO
          TRAPCLR   FORMAT
          IF        OVRCD=1
            MOVE      ZERO,INCNNATO
            WRITAB    CONTROLF,SIXTY3;*141,INCNNATO
          ENDIF
.
          IF        INCNNATO=1
            MOVE      ZERO,OVRCD
            TRAP      OVERCOND IF IO
            OPEN      SINNATA1,"sinnataf"
            TRAPCLR   IO
            IF        OVRCD=1
              DISPLAY   *P1:24,*EL,*B,"Nato file does not exist. Run fixit.  ";
              CALL      HITENTER
              CHAIN     PGM
            ENDIF
.
            OPEN      SINNATA2,"sinnataf"
          ENDIF
.
ONAT9999  RETURN
UPDTOT00   MOVE     "   28",FLDID
           CALL     GETPOSIT
           IF       EXIT=0
             CALL     ADDBUD00
             MOVE     BUDPERXX,VAR
             CALL     DISPITEM
           ENDIF
           CALL     SELECTED
           RETURN
ADDBUD00  MOVE      ZERO,BUDPERXX
          ASSIGN    BUDPERXX+BUDPER01+BUDPER02+BUDPER03+BUDPER04,BUDPERXX
          ASSIGN    BUDPERXX+BUDPER05+BUDPER06+BUDPER07+BUDPER08,BUDPERXX
          ASSIGN    BUDPERXX+BUDPER09+BUDPER10+BUDPER11+BUDPER12,BUDPERXX
          IF        SIPENUM=13
            ADD       BUDPER13,BUDPERXX
          ENDIF
          RETURN
UPDCST00   MOVE     "   45",FLDID
           CALL     GETPOSIT
           IF       EXIT=0
             CALL     ADDCST00
             MOVE     CSTPERXX,VAR
             CALL     DISPITEM
           ENDIF
           CALL     SELECTED
           RETURN
ADDCST00  MOVE      ZERO,CSTPERXX
          ASSIGN    CSTPERXX+CSTPER01+CSTPER02+CSTPER03+CSTPER04,CSTPERXX
          ASSIGN    CSTPERXX+CSTPER05+CSTPER06+CSTPER07+CSTPER08,CSTPERXX
          ASSIGN    CSTPERXX+CSTPER09+CSTPER10+CSTPER11+CSTPER12,CSTPERXX
          IF        SIPENUM=13
            ADD       CSTPER13,CSTPERXX
          ENDIF
          RETURN
EPYR0000  CALL      RINPECON
          MOVE      SIPECYR,FINYEAR
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,*JR,FINYEAR
          PACK      FINYEAR,FINYEAR,SP70
.
          MATCH     SP70,FINYEAR
          GOTO      EPYR0990 IF EQUAL
          PACK      KEY4,SP3,EXITCHAR * exitchar entered ?
          MATCH     KEY4,FINYEAR
          GOTO      EPYR0990 IF EQUAL
.
          TYPE      FINYEAR           * valid numeric ?
          GOTO      EPYR0000 IF NOT EQUAL
.
          MOVE      FINYEAR,SIPEYF4
          IF        SIPEYF4<100
            SQUEEZE   FINYEAR
            MOVE      FINYEAR,FORM2
            PACK      FINYEAR,CCC,FORM2,SP70
          ENDIF
.
          REP       " 0",FINYEAR
          MOVE      ZERO,BUDPERXX
          MOVE      ZERO,CSTPERXX
          MOVE      ONE,ACCEPT
.
          MOVE      ZERO,F2
EPYR0100  ADD       ONE,F2
          IF        F2>SIPENUM
            GOTO      EPYR0900
          ENDIF
          PACK      SIIHDAT,FINYEAR,F2
          REP       " 0",SIIHDAT
          PACK      KEY18,SIIACAT,SIWAWAR,SIIHDAT
          CALL      RDSIIH1
          IF        OVRCD=0
            MOVE      ZERO,ACCEPT
          ELSE
            MOVE      ZERO,SIIHPQT
            MOVE      ZERO,SIIHPAM
          ENDIF
.
          STORE     SIIHPQT,F2,BUDPER01,BUDPER02,BUDPER03,BUDPER04:
                               BUDPER05,BUDPER06,BUDPER07,BUDPER08:
                               BUDPER09,BUDPER10,BUDPER11,BUDPER12,BUDPER13
          STORE     SIIHPAM,F2,CSTPER01,CSTPER02,CSTPER03,CSTPER04:
                               CSTPER05,CSTPER06,CSTPER07,CSTPER08:
                               CSTPER09,CSTPER10,CSTPER11,CSTPER12,CSTPER13
          ADD       SIIHPQT,BUDPERXX
          ADD       SIIHPAM,CSTPERXX
          GOTO      EPYR0100
.
EPYR0900  MOVE      ACCEPT,OVRCD
          IF        ACCEPT=1
            MOVE      ZERO,ACCEPT
            CALL      DF00L000
            MOVE      "    3",FLDID
            CALL      GETPOSIT
            MOVE      ONE,ACCEPT
            MOVE      ONE,OVRCD
          ENDIF
          GOTO      EPYR0999
