.------------------------------------------------------------
. Add Billing Table Fields
.------------------------------------------------------------
ALER0000  BRANCH    FLDITMNO,ALER0100,ALER0200,ALER0300,ALER0400,ALER0500:
                             ALER0600,ALER0700,ALER0800,ALER0900,ALER1000:
                             ALER1100,ALER1200,ALER1300,ALER1400,ALER1500:
                             ALER1600,ALER1700,ALER1800,ALER1900,ALER2000:
                             ALER2100,ALER2200
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      ALER9999
.
.
ALER0100  WRITE     HTMLFILE;ALERPHOT;
          GOTO      ALER9999
.
ALER0200  WRITE     HTMLFILE;ALERLASR;
          GOTO      ALER9999
.
ALER0300  WRITE     HTMLFILE;ALERPOSS;
          GOTO      ALER9999
.
ALER0400  WRITE     HTMLFILE;ALERCAR;
          GOTO      ALER9999
.
ALER0500  WRITE     HTMLFILE;ALERCARH;
          GOTO      ALER9999
.
ALER0600  WRITE     HTMLFILE;ALERNVIE;
          GOTO      ALER9999
.
ALER0700  WRITE     HTMLFILE;ALERNTRA; 
          GOTO      ALER9999
.
ALER0800  WRITE     HTMLFILE;ALERNNEG;
          GOTO      ALER9999
.
ALER0900  WRITE     HTMLFILE;ALERNBWP;
          GOTO      ALER9999
.
ALER1000  WRITE     HTMLFILE;ALERNCLP;
          GOTO      ALER9999
.
ALER1100  WRITE     HTMLFILE;ALERNVID;
          GOTO      ALER9999
.
ALER1200  WRITE     HTMLFILE;ALERVISN;
          GOTO      ALER9999
.
ALER1300  WRITE     HTMLFILE;ALERENCT;
          GOTO      ALER9999
.
ALER1400  MATCH     ALERCDAT,SP70
          GOTO      ALER9999 IF EQUAL
          UNPACK    ALERCDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR
          GOTO      ALER9999
.
ALER1500  WRITE     HTMLFILE;ALERCTIM;
          GOTO      ALER9999
.
ALER1600  MATCH     SP70,ALERCUID
          GOTO      ALER9999 IF EQUAL
          PACK      KEY10,ALERCUID
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      ALERCUID,WBSENAM
          ENDIF
          WRITE     HTMLFILE;WBSENAM;
          GOTO      ALER9999
.
ALER1700  MATCH     ALERUDAT,SP70 
          GOTO      ALER9999 IF EQUAL
          UNPACK    ALERUDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR
          GOTO      ALER9999
.
ALER1800  WRITE     HTMLFILE;ALERUTIM; 
          GOTO      ALER9999
.
ALER1900  MATCH     SP70,ALERUUID
          GOTO      ALER9999 IF EQUAL
          PACK      KEY10,ALERUUID
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      ALERUUID,WBSENAM
          ENDIF
          WRITE     HTMLFILE;WBSENAM;
          GOTO      ALER9999
.
ALER2000  MATCH     ALERDSNT,SP70
          GOTO      ALER9999 IF EQUAL
          UNPACK    ALERDSNT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR
          GOTO      ALER9999
.
ALER2100  MATCH     SP70,ALERSTFF
          GOTO      ALER9999 IF EQUAL
          WRITE     HTMLFILE;ALERSTFF; 
          GOTO      ALER9999
.
ALER2200  WRITE     HTMLFILE;ALERNDIG;
          GOTO      ALER9999
.
ALER9999  RETURN
.
.------------------------------------------------------------
.   Set Parameters for waiting list extension file
.------------------------------------------------------------
SPALER00  MOVE      ZERO,EXIT
          UNPACK    QRYNAME,KEY5,KEY3
          MOVE      KEY3,F3
          BRANCH    F3,SPALER01,SPALER02,SPALER03,SPALER04,SPALER05:
                       SPALER06,SPALER07,SPALER08,SPALER09,SPALER10:
                       SPALER11,SPALER12,SPALER13,SPALER14,SPALER15:
                       SPALER16,SPALER17,SPALER18,SPALER19,SPALER20:
                       SPALER21,SPALER22
.
