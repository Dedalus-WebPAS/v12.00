.   System    :   Supply and Inventory               
.   Include   :   SINCODKY
.   Routine   :   KSICDA00 Keyin Category            
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KSICDA00 TOPIC     "SICDCATA"
          MOVE      UNDLN70,SICDCATA
          KEYIN     *PCCOL:CVERT,*DV,SICDCATA:                                  
                    *PCCOL:CVERT,*V2LON,SICDCATA:                               
                    *PCCOL:CVERT,*DV,SICDCATA
.
          ENDSET    SICDCATA
          APPEND    SP70,SICDCATA
          RESET     SICDCATA
.
          MATCH     SP70,SICDCATA
          GOTO      KSICDA90 IF EQUAL
.
          CMATCH    EXITCHAR,SICDCATA
          GOTO      KSICDA95 IF EQUAL
.
          CMATCH    QUEST,SICDCATA
          GOTO      KSICDA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HSICDA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    SICDCATA
          APPEND    SP70,SICDCATA
          RESET     SICDCATA
.
          MATCH     SP70,SICDCATA
          GOTO      KSICDA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,SICDCATA                                
KSICDA10
          PACK      KEY6,SICDCODE,SICDCATA                                      
          CALL      RASICD2 
          BRANCH    OVRCD,KSICDA80
          BRANCH    CKEYTYP,KSICDA75
          CALL      RDSICD2 
KSICDA70
          MOVE      ZERO,EXIT
          GOTO      KSICDA99
KSICDA75
          MOVE      "Category Already Exist - ",DISPMSG                                              
          CALL      MESSAGE1
          GOTO      KSICDA00
KSICDA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KSICDA99,KSICDA99
          MOVE      "Category Does Not Exist - ",DISPMSG                                             
          CALL      MESSAGE1
          GOTO      KSICDA00
KSICDA90
          MOVE      ONE,EXIT
          GOTO      KSICDA99
KSICDA95
          MOVE      TWO,EXIT
KSICDA99 TOPIC    PRGID
          RETURN
