          GOTO      HSICDA72 IF NOT EQUAL
HSICDA62
HSICDA63
HSICDA64
HSICDA65
HSICDA66
HSICDA67
HSICDA68
HSICDA69
HSICDA70
          GOTO      HSICDA75
HSICDA72  MOVE      ONE,OVRCD
HSICDA75  RETURN
HSICDA85
          LOAD      KEY6,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY6,SICDCODE,SICDCATA                                      
          CALL      RDSICD2 
HSICDA90
          UNPACK    SAVKEY80,SICDCODE,SICDCATA                                  
          BEEP
          GOTO      HSICDA20
HSICDA91
          MOVE      SP70,SICDDESC
          MOVE      SP70,SICDOTHR
          MOVE      SP70,SICDFILL
          MOVE      SP70,KEY6 
          MOVE      SP70,SICDCATA
          GOTO      HSICDA96
HSICDA92
          MOVE      SP70,KEY6 
          MOVE      SP70,SICDCATA
          LOAD      KEY6,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY6,SICDCODE,SICDCATA                                      
HSICDA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HSICDA99
          RETURN
