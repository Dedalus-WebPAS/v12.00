.******************************************************************************
.*                                  M16CH000              Called by: PATWIS16 *
.*                Calculate Mechanical Ventialtion Co-payment                 *
.******************************************************************************
M16CH000  MOVE      ZERO,ADJMVDAY
          MOVE      ZERO,MVCOPAY
.
          PACK      KEY8,AADMNO
          CALL      RDPTICU1                * get mech. vent. hours
          BRANCH    OVRCD,M16CH999
.
          MATCH     "D",PT16MVEL            * check mech. vent. eligibility
          IF        @EQUAL
            IF        PTICUMEC > 6
              ASSIGN    ((PTICUMEC+12)/24),FORM12P4    * days receiving copay.
              MOVE      FORM12P4,ADJMVDAY
              ASSIGN    ((ADJMVDAY*0.7729)+0.6980),MVCOPAY  * mech. vent. copay.
            ENDIF
            GOTO      M16CH999
          ENDIF
.
          MATCH     "4",PT16MVEL
          IF        @EQUAL
            IF        PTICUMEC > 96
              ASSIGN    (((PTICUMEC+12)/24)-4),FORM12P4   * days recving copay.
              MOVE      FORM12P4,ADJMVDAY
              ASSIGN    ((ADJMVDAY*0.7729)+0.6980),MVCOPAY  * mech. vent. copay.
            ENDIF
          ENDIF
.
M16CH999  RETURN
+
.******************************************************************************
.*                                  T16AL000              Called by: PATWIS16 *
.*                      Calculate Thalessaemia Co-payment                     *
.******************************************************************************
T16AL000  MOVE      ZERO,THCOPAY
          MATCH     "T",PT16CPAY            * check WIES16 weights file
          IF        @EQUAL
            PACK      KEY16,AADMNO,SP20
            CALL      RSPTECD2              * Position on & read a patient
T16AL100    CALL      RKPTECD2                episode disease file record
            BRANCH    OVRCD,T16AL999
.
            MATCH     AADMNO,PTEDADMN
            GOTO      T16AL999 IF NOT EQUAL
.
            MATCH     "D56",PTEDCODE        * check ICD-10 code
            IF        !@EQUAL
              MATCH     "D57.2",PTEDCODE    * check ICD-10 code
              GOTO      T16AL100 IF NOT EQUAL
            ENDIF
            MOVE      "0.2648",THCOPAY      * eligible for thal. copayment
          ENDIF
T16AL999  RETURN
