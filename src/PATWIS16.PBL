.******************************************************************************
.*                                  B16SW000              Called by: PATWIS16 *
.*                            Calculate Base WIES16                           *
.******************************************************************************
B16SW000  CALL      D16SC000                * length of stay category
          CALL      C16ST000                * calculate inlier status
.
          MATCH     "L",W16NLIER            * low outlier
          CALL      BW16L000 IF EQUAL       * calculate base WIES16
.
          MATCH     "I",W16NLIER            * inlier
          CALL      BW16N000 IF EQUAL       * calculate base WIES16
.
          MATCH     "H",W16NLIER            * high outlier
          CALL      BW16H000 IF EQUAL       * calculate base WIES16
.
B16SW999  RETURN
+
.******************************************************************************
.*                                  D16SC000              Called by: B16SW000 *
.*                      Determine Length Of Stay Category                     *
.******************************************************************************
D16SC000  MOVE      NBRDAYS,WIS16LOS        * length of stay from NHOSPDAY
          MOVE      "M",LOSCAT              * set LOS category to multiday
          MATCH     ADATE,DDATE             * check for sameday
          IF        @EQUAL
            MOVE      ONE,SAMEDAY           * sameday
            MOVE      ONE,WIS16LOS          * LOS = 1
            MOVE      "S",LOSCAT            * set LOS category to sameday
            GOTO      D16SC999
          ELSE
            MOVE      ZERO,SAMEDAY          * not sameday
          ENDIF
.
          IF        SAMEDAY = 0 & WIS16LOS = 1
            MOVE      "O",LOSCAT            * set LOS category to oneday
            GOTO      D16SC999
          ENDIF
.
          IF        WIS16LOS > 1825
            MOVE      "1825",WIS16LOS       * maximum LOS is 5 years
          ENDIF
.
D16SC999  RETURN
+
.******************************************************************************
.*                                  C16ST000              Called by: B16SW000 *
.*                           Calculate Inlier Status                          *
.******************************************************************************
C16ST000  MOVE      "I",W16NLIER            * set status to inlier
          IF        WIS16LOS < PT16LIBP
            MOVE      "L",W16NLIER          * set status to low outlier
            GOTO      C16ST999
          ENDIF
.
          ASSIGN    (PT16HIBP+ADJMVDAY),FORM12P4   * high bound. + mech.v days
          IF        WIS16LOS > FORM12P4
            MOVE      "H",W16NLIER          * set status to high outlier
          ENDIF
C16ST999  RETURN
+
.******************************************************************************
.*                                  BW16L000              Called by: B16SW000 *
.*                     Calculate Base WIES16 for Low Outlier                  *
.******************************************************************************
BW16L000  MATCH     "S",LOSCAT            * sameday
          IF        @EQUAL
            MOVE      PT16TSDW,TOTBWIES             * base WIES16
            ASSIGN    (TOTBWIES/PT16TIMW),TOTIES16  * IES16
.
            MOVE      PT16TSDW,WIS16WGT             * for display
            MOVE      "Same Day Wgt ",WIS16DSC
            GOTO      BW16L999
          ENDIF
.
          MATCH     "O",LOSCAT            * oneday
          IF        @EQUAL
            MOVE      PT16TODW,TOTBWIES             * base WIES16
            ASSIGN    (TOTBWIES/PT16TIMW),TOTIES16  * IES16
.
            MOVE      PT16TODW,WIS16WGT             * for display
            MOVE      "One Day Wgt  ",WIS16DSC
            GOTO      BW16L999
          ENDIF
.
          MATCH     "M",LOSCAT            * multiday
          IF        @EQUAL
            ASSIGN    (PT16TODW+((WIS16LOS-1)*PT16TLOM)),TOTBWIES
.                                                    * base WIES16
            ASSIGN    (TOTBWIES/PT16TIMW),TOTIES16   * IES16
.
            MOVE      PT16TLOM,WIS16WGT             * for display
            MOVE      "Low Outlr Wgt",WIS16DSC
            GOTO      BW16L999
          ENDIF
BW16L999  RETURN
+
.******************************************************************************
.*                                  BW16N000              Called by: B16SW000 *
.*                      Calculate Base WIES16 for Inlier                      *
.******************************************************************************
BW16N000  MOVE      ONE,TOTIES16          * IES16 = 1
          MATCH     "S",LOSCAT            * sameday
          IF        @EQUAL
            MOVE      PT16TSDW,TOTBWIES   * base WIES16
.
            MOVE      PT16TSDW,WIS16WGT             * for display
            MOVE      "Same Day Wgt ",WIS16DSC
            GOTO      BW16N999
          ENDIF
.
          MATCH     "O",LOSCAT            * oneday
          IF        @EQUAL
            MOVE      PT16TODW,TOTBWIES   * base WIES16
.
            MOVE      PT16TODW,WIS16WGT             * for display
            MOVE      "One Day Wgt  ",WIS16DSC
            GOTO      BW16N999
          ENDIF
.
          MATCH     "M",LOSCAT            * multiday
          IF        @EQUAL
            MOVE      PT16TIMW,TOTBWIES   * base WIES16
.
            MOVE      PT16TIMW,WIS16WGT             * for display
            MOVE      "Inlier Wgt   ",WIS16DSC
            GOTO      BW16N999
          ENDIF
BW16N999  RETURN
+
.******************************************************************************
.*                                  BW16H000              Called by: B16SW000 *
.*                    Calculate Base WIES16 for High Outlier                  *
.******************************************************************************
BW16H000  ASSIGN    ((WIS16LOS-PT16HIBP)-ADJMVDAY),HIGHDAYS  * high outlr days
          COMPARE   ZERO,HIGHDAYS
          IF        @LESS
            MOVE      ZERO,HIGHDAYS         * highdays must be >= zero
          ENDIF
.
          COMPARE   HIGHDAYS,HIHLOS
          IF        @LESS
            MOVE      HIHLOS,HITHDAYS       * get lesser of highdays and
          ELSE                              *  HIHLOS
            MOVE      HIGHDAYS,HITHDAYS
          ENDIF
.
          ASSIGN    (HIGHDAYS-HITHDAYS)*PT16THOM,TOTBWIES    *high WIES
          ASSIGN    (HITHDAYS*PT16THIH)+TOTBWIES,TOTBWIES    *HITH WIES
          ASSIGN    (PT16TIMW+TOTBWIES),TOTBWIES             *base WIES16
          ASSIGN    (TOTBWIES/PT16TIMW),TOTIES16             * IES16
.
          MOVE      PT16THOM,WIS16WGT                        * for display
          MOVE      "High Outl Wgt",WIS16DSC
BW16H999  RETURN
+
.******************************************************************************
.*                                  C16AB000              Called by: PATWIS16 *
.*                          Check for Aboriginality                           *
.******************************************************************************
C16AB000  MOVE      ZERO,ATSIWIES                * set ATSI WIES to zero
          IF        PNSWRACE<1 | PNSWRACE>14
            GOTO      C16AB999              * Invalid parameter
          ENDIF
.
          LOAD      TCODE,PNSWRACE,CATP1,CATP2,CATP3,CATP4,CATP5:
                                   CATP6,CATA1,CATA2,CATA3,CATA4:
                                   CATA5,CATA6,CATA7,CATVA
          LOAD      ACODE,PNSWRACE,PUSR1,PUSR2,PUSR3,PUSR4,PUSR5:
                                   PUSR6,AUSR1,AUSR2,AUSR3,AUSR4:
                                   AUSR5,AUSR6,AUSR7,PMPXABRG
.
          MATCH     ACODE,SP3
          GOTO      C16AB999 IF EQUAL
.
          PACK      KEY5,TCODE,ACODE
          CALL      RDCODE1                 * Read a code file record
          BRANCH    OVRCD,C16AB999
.
          UNPACK    THCSCOD,DIM1,KEY1       * use 2nd char of HDP Equiv
.>>>>     MOVE      THCSCOD,KEY1
          TYPE      KEY1
          GOTO      C16AB999 IF NOT EQUAL   * Non numeric, error
.
          MOVE      KEY1,FORM1
.
. ---- Calculate ATSI Loading if aboriginality = 1, 2, or 3 ----
.
          IF        FORM1=1 | FORM1=2 | FORM1=3
            MOVE      TOTBWIES,FORM12P4       * base WIES
            ADD       MVCOPAY,FORM12P4        * mechv copayment
            ADD       THCOPAY,FORM12P4        * thal  copayment
            ADD       AAACOPAY,FORM12P4       * AAA   copayment
            ADD       ASDCOPAY,FORM12P4       * ASD   copayment
            ASSIGN    (0.3*FORM12P4),ATSIWIES
          ENDIF
.
C16AB999  RETURN
+
