.******************************************************************************
.*                                  B19SW000              Called by: PATWIS19 *
.*                            Calculate Base WIES19                           *
.******************************************************************************
B19SW000  CALL      D19SC000                * length of stay category
          CALL      C19ST000                * calculate inlier status
.
          MATCH     "L",W19NLIER            * low outlier
          CALL      BW19L000 IF EQUAL       * calculate base WIES19
.
          MATCH     "I",W19NLIER            * inlier
          CALL      BW19N000 IF EQUAL       * calculate base WIES19
.
          MATCH     "H",W19NLIER            * high outlier
          CALL      BW19H000 IF EQUAL       * calculate base WIES19
.
B19SW999  RETURN
+
.******************************************************************************
.*                                  D19SC000              Called by: B19SW000 *
.*                      Determine Length Of Stay Category                     *
.******************************************************************************
D19SC000  MOVE      NBRDAYS,WIS19LOS        * length of stay from NHOSPDAY
          MOVE      "M",LOSCAT              * set LOS category to multiday
          MATCH     ADATE,DDATE             * check for sameday
          IF        @EQUAL
            MOVE      ONE,SAMEDAY           * sameday
            MOVE      ONE,WIS19LOS          * LOS = 1
            MOVE      "S",LOSCAT            * set LOS category to sameday
            GOTO      D19SC999
          ELSE
            MOVE      ZERO,SAMEDAY          * not sameday
          ENDIF
.
          IF        SAMEDAY = 0 & WIS19LOS = 1
            MOVE      "O",LOSCAT            * set LOS category to oneday
            GOTO      D19SC999
          ENDIF
.
          IF        WIS19LOS > 1825
            MOVE      "1825",WIS19LOS       * maximum LOS is 5 years
          ENDIF
.
D19SC999  RETURN
+
.******************************************************************************
.*                                  C19ST000              Called by: B19SW000 *
.*                           Calculate Inlier Status                          *
.******************************************************************************
C19ST000  MOVE      "I",W19NLIER            * set status to inlier
          IF        WIS19LOS < PT19LIBP
            MOVE      "L",W19NLIER          * set status to low outlier
            GOTO      C19ST999
          ENDIF
.
          ASSIGN    (PT19HIBP+ADJMVDAY),FORM12P4   * high bound. + mech.v days
          IF        WIS19LOS > FORM12P4
            MOVE      "H",W19NLIER          * set status to high outlier
          ENDIF
C19ST999  RETURN
+
.******************************************************************************
.*                                  BW19L000              Called by: B19SW000 *
.*                     Calculate Base WIES19 for Low Outlier                  *
.******************************************************************************
BW19L000  MATCH     "S",LOSCAT            * sameday
          IF        @EQUAL
            MOVE      PT19TSDW,TOTBWIES             * base WIES19
            ASSIGN    (TOTBWIES/PT19TIMW),TOTIES19  * IES19
.
            MOVE      PT19TSDW,WIS19WGT             * for display
            MOVE      "Same Day Wgt ",WIS19DSC
            GOTO      BW19L999
          ENDIF
.
          MATCH     "O",LOSCAT            * oneday
          IF        @EQUAL
            MOVE      PT19TODW,TOTBWIES             * base WIES19
            ASSIGN    (TOTBWIES/PT19TIMW),TOTIES19  * IES19
.
            MOVE      PT19TODW,WIS19WGT             * for display
            MOVE      "One Day Wgt  ",WIS19DSC
            GOTO      BW19L999
          ENDIF
.
          MATCH     "M",LOSCAT            * multiday
          IF        @EQUAL
            ASSIGN    (PT19TODW+((WIS19LOS-1)*PT19TLOM)),TOTBWIES
.                                                    * base WIES19
            ASSIGN    (TOTBWIES/PT19TIMW),TOTIES19   * IES19
.
            MOVE      PT19TLOM,WIS19WGT             * for display
            MOVE      "Low Outlr Wgt",WIS19DSC
            GOTO      BW19L999
          ENDIF
BW19L999  RETURN
+
.******************************************************************************
.*                                  BW19N000              Called by: B19SW000 *
.*                      Calculate Base WIES19 for Inlier                      *
.******************************************************************************
BW19N000  MOVE      ONE,TOTIES19          * IES19 = 1
          MATCH     "S",LOSCAT            * sameday
          IF        @EQUAL
            MOVE      PT19TSDW,TOTBWIES   * base WIES19
.
            MOVE      PT19TSDW,WIS19WGT             * for display
            MOVE      "Same Day Wgt ",WIS19DSC
            GOTO      BW19N999
          ENDIF
.
          MATCH     "O",LOSCAT            * oneday
          IF        @EQUAL
            MOVE      PT19TODW,TOTBWIES   * base WIES19
.
            MOVE      PT19TODW,WIS19WGT             * for display
            MOVE      "One Day Wgt  ",WIS19DSC
            GOTO      BW19N999
          ENDIF
.
          MATCH     "M",LOSCAT            * multiday
          IF        @EQUAL
            MOVE      PT19TIMW,TOTBWIES   * base WIES19
.
            MOVE      PT19TIMW,WIS19WGT             * for display
            MOVE      "Inlier Wgt   ",WIS19DSC
            GOTO      BW19N999
          ENDIF
BW19N999  RETURN
+
.******************************************************************************
.*                                  BW19H000              Called by: B19SW000 *
.*                    Calculate Base WIES19 for High Outlier                  *
.******************************************************************************
BW19H000  ASSIGN    ((WIS19LOS-PT19HIBP)-ADJMVDAY),HIGHDAYS  * high outlr days
          COMPARE   ZERO,HIGHDAYS
          IF        @LESS
            MOVE      ZERO,HIGHDAYS         * highdays must be >= zero
          ENDIF
.
          COMPARE   HIGHDAYS,HIHLOS
          IF        @LESS
            MOVE      HIHLOS,HITHDAYS       * get lesser of highdays and
          ELSE                              *  HIHLOS
            MOVE      HIGHDAYS,HITHDAYS
          ENDIF
.
          ASSIGN    (HIGHDAYS-HITHDAYS)*PT19THOM,TOTBWIES    *high WIES
          ASSIGN    (HITHDAYS*PT19THIH)+TOTBWIES,TOTBWIES    *HITH WIES
          ASSIGN    (PT19TIMW+TOTBWIES),TOTBWIES             *base WIES19
          ASSIGN    (TOTBWIES/PT19TIMW),TOTIES19             * IES19
.
          MOVE      PT19THOM,WIS19WGT                        * for display
          MOVE      "High Outl Wgt",WIS19DSC
BW19H999  RETURN
+
.******************************************************************************
.*                                  C19AB000              Called by: PATWIS19 *
.*                          Check for Aboriginality                           *
.******************************************************************************
C19AB000  MOVE      ZERO,ATSIWIES                * set ATSI WIES to zero
          IF        PNSWRACE<1 | PNSWRACE>14
            GOTO      C19AB999              * Invalid parameter
          ENDIF
.
          LOAD      TCODE,PNSWRACE,CATP1,CATP2,CATP3,CATP4,CATP5:
                                   CATP6,CATA1,CATA2,CATA3,CATA4:
                                   CATA5,CATA6,CATA7,CATVA
          LOAD      ACODE,PNSWRACE,PUSR1,PUSR2,PUSR3,PUSR4,PUSR5:
                                   PUSR6,AUSR1,AUSR2,AUSR3,AUSR4:
                                   AUSR5,AUSR6,AUSR7,PMPXABRG
.
          MATCH     ACODE,SP3
          GOTO      C19AB999 IF EQUAL
.
          PACK      KEY5,TCODE,ACODE
          CALL      RDCODE1                 * Read a code file record
          BRANCH    OVRCD,C19AB999
.
          UNPACK    THCSCOD,DIM1,KEY1       * use 2nd char of HDP Equiv
.>>>>     MOVE      THCSCOD,KEY1
          TYPE      KEY1
          GOTO      C19AB999 IF NOT EQUAL   * Non numeric, error
.
          MOVE      KEY1,FORM1
.
. ---- Calculate ATSI Loading if aboriginality = 1, 2, or 3 ----
.
          IF        FORM1=1 | FORM1=2 | FORM1=3
            MOVE      TOTBWIES,FORM12P4       * base WIES
            ADD       MVCOPAY,FORM12P4        * mechv copayment
            ADD       THCOPAY,FORM12P4        * thal  copayment
            ADD       AAACOPAY,FORM12P4       * AAA   copayment
            ADD       ASDCOPAY,FORM12P4       * ASD   copayment
            ASSIGN    (0.3*FORM12P4),ATSIWIES
          ENDIF
.
C19AB999  RETURN
+
