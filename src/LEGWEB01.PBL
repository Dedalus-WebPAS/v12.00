          MOVE      OVRCD,MASTFLAG
          MOVE      PURNO,KEY8
          CALL      RDPMPX21
          IF        OVRCD=1
            CALL      CLPMSPX2
          ENDIF
.
          IF        PSTAT=1 | PSTAT=2
            MOVE      PURNO,SAVEURNO
            MOVE      PPSNAME,URNUMBER
            MOVE      ONE,MRGEFLAG
            GOTO      AAEPAT00
          ENDIF
.
          IF        MASTFLAG=1
            GOTO      AAEPAT95
          ENDIF
.
          MOVE      LEGADMNO,KEY8
          CALL      RDLGDEA1
          BRANCH    OVRCD,AAEPAT95
.
          CALL      RDLGDIS1
          BRANCH    OVRCD,AAEPAT95
