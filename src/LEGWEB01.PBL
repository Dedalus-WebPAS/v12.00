            CALL      RKLGPGR1
            IF        OVRCD=0
              PACK      DIM10,LPTPGADM,LPTPGEPN
              MATCH     KEY10,DIM10
              GOTO      DRGS0010 IF EQUAL
            ENDIF
            CALL      CLRGRP00
          ENDIF
.
.  Now start normal branch on flditmno section
.
