          ADD       PRINHAMT,OSTDAMNT
          ADD       PRINIAMT,OSTDAMNT
          ADD       PRINMAMT,OSTDAMNT
          ADD       PRINVAMT,OSTDAMNT
          ADD       PRINOTHA,OSTDAMNT
          ADD       PRINJAMT,OSTDAMNT
          ADD       PRINCNTT,OSTDAMNT
