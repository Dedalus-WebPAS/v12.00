                               DAYNAM,SP1,CDAY,SP1,MTHNAM,SP1,CCYY,"</option>"
          ENDIF
          MOVE      "1",ADJDAYS
          CALL      ADJDAT00
          PACK      STRTDATE,CCENT,CYEAR,CMON,CDAY
          MATCH     ENDDATE,STRTDATE
          GOTO      SELC1000 IF NOT EQUAL
