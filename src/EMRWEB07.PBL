          ELSE
            WRITE     HTMLFILE;"<option value=",STRTDATE,">Week of ":
                               CDAY,SP1,MTHNAM,SP1,CCENT,CYEAR,"</option>";
          ENDIF
          MOVE      "7",ADJDAYS
