          REP       " 0",EMDVDTRC
          PACK      EMDVTMRC,CTIMEIS
          REP       " 0",EMDVTMRC
.
          PACK      KEY19,EMDVP001,EMDVP002,EMDVP003,SP70
          CALL      WREMDVP1      * Writes away the data
.
          MOVE      ZERO,EXIT
          GOTO      EDVP9999
.
