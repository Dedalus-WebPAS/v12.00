          WRITE     PRECOMAF,SEQ;QRYDATA
          MOVE      ONE,PRECOMFL
.
GETPRE10  READ      CONFFILE,SEQ;QRYNAME,QRYDATA
          GOTO      GETPRE90 IF OVER
          MATCH     SP70,QRYNAME
          GOTO      GETPRE80 IF NOT EQUAL
          MATCH     SP70,QRYDATA
          GOTO      GETPRE20 IF EQUAL
          GOTO      GETPRE20 IF EOS
       
GETPRE20  ADD       ONE,F3
          WRITE     PRECOMAF,SEQ;QRYDATA
          GOTO      GETPRE10
.
