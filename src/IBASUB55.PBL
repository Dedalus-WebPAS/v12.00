. PRNA - print report for SICO                      Called by ML
.         Returns  : EXIT            (1=illegal range entered)
.********************************************************************
PRNA0000  PACK      DISPMSG,SP70,SP70            * save line 24
          CALL      MESSAGE2
.
          CALL      COPY0000
          BRANCH    EXIT,PRNA9800
.
          OPEN      IMPORTFL,IMPORTFN
.
          CLEAR     FILENAME
          APPEND    "sub55a",FILENAME
          APPEND    PORT,FILENAME
          RESET     FILENAME
          REP       " 0",FILENAME
          PREP      TEMPFILE,FILENAME
.
          DISPLAY   *P1:24,"Printing :   ";
          MOVE      ZERO,CPAGENO                 * reset page no.
          CLOCK     TIME,CTIMEIS                 * get current time
          MOVE      "60",CLNO                   * init last line number
          MOVE      ZERO,TOTREC
          MOVE      ZERO,TOTUPD
          MOVE      "Print Only          ",CPHDROPT
          IF        EXPORTYN=1
            MOVE      "Update and Print    ",CPHDROPT
          ENDIF
.
PRNA1000  READ      IMPORTFL,SEQ;PARTNUMB,SUPPRICE
          GOTO      PRNA3000 IF OVER
.
          DISPLAY   *P12:24,*V2LON,PARTNUMB,*EL;
.
          ADD       ONE,TOTREC
          PACK      KEY57,FROMSUPP,PARTNUMB,SP70
          CALL      RSSIIC4
.
          CALL      RKSIIC4
          BRANCH    OVRCD,PRNA2000
.
          MATCH     FROMSUPP,SIICSUP
          GOTO      PRNA2000 IF NOT EQUAL
.
          MATCH     PARTNUMB,SIICPNO
          GOTO      PRNA2000 IF NOT EQUAL
.
          CALL      WRIA0000
          GOTO      PRNA1000                     * get next record
.
PRNA2000  WRITE     TEMPFILE,SEQ;PARTNUMB,SUPPRICE
          GOTO      PRNA1000
.
PRNA3000  COMPARE   ZERO,CPAGENO
          CALL      HEAD0000 IF EQUAL            * print head if nothing printed
          CALL      UND132                       * print line
          MOVE      SIXTY,CLNO
.
          WEOF      TEMPFILE,SEQ
          CLOSE     TEMPFILE
.
          TRAP      OVERCOND IF IO
          OPEN      TEMPFILE,FILENAME
          TRAPCLR   IO
.
          IF        OVRCD=1
            CLEAR    DISPMSG
            APPEND   "ERROR : File is Empty/Missing",DISPMSG
            APPEND   " - ",DISPMSG
            APPEND   FILENAME,DISPMSG
            RESET    DISPMSG
            BEEP
            CALL     MESSAGE1
            MOVE     ONE,EXIT
            GOTO     PRNA9800
          ENDIF
.
PRNA3200  TRAP      OVERCOND IF FORMAT
          READ      TEMPFILE,SEQ;PARTNUMB,SUPPRICE
          GOTO      PRNA3800 IF OVER
          TRAPCLR   FORMAT
          BRANCH    OVRCD,PRNA8100
.
          COMPARE   "54",CLNO
          CALL      HEAD0000 IF NOT LESS
          PRINT     *1,"| Part Number ",PARTNUMB:
                    " Does Not Exist for Supplier":
                    *132,"|"
          ADD       ONE,CLNO
          GOTO      PRNA3200
.
PRNA3800  CALL      UND132
.
          PRINT     *N,*N,"Total Records Read    : ",TOTREC:
                    *N,"Total Records Updated : ",TOTUPD:
                    *N,*N,"****   End of Report   ****"
.
          CLOSE     TEMPFILE,DELETE
          CLOSE     IMPORTFL,DELETE
.
          GOTO      PRNA9000
.
PRNA8100  CLEAR    DISPMSG
          APPEND   "ERROR : Format Error at Line",DISPMSG
          APPEND   SECTOR,DISPMSG
          APPEND   " - ",DISPMSG
          RESET    DISPMSG
          BEEP
          CALL     MESSAGE1
          MOVE     ONE,EXIT
          GOTO     PRNA3200
.
PRNA9000  MOVE      ZERO,EXIT                    * exit screen
          GOTO      PRNA9999
.
PRNA9800  MOVE      TWO,EXIT                     * continue
          GOTO      PRNA9999
.
PRNA9999  CALL      MESSAGE3                     * restore line 24
          RETURN
