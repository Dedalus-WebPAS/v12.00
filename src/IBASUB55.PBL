.********************************************************************
. WRIA - print data line                            Called by PRNA
.********************************************************************
WRIA0000  COMPARE   "55",CLNO                    * end of page ?
          CALL      HEAD0000 IF NOT LESS         * start new page
.
          MOVE      SP70,SIIADES
          PACK      KEY7,SIICCAT,SP70
          CALL      RDSIIA1
.
          MOVE      SUPPRICE,FORM12P4
          SUB       SIICLCT,FORM12P4
.
          PRINT     "| ",SIICCAT,SP2,SIIADES,SP5,SIICLCT,SP2,SUPPRICE,SP1:
                    FORM12P4,*132,"|":
                    *N,"|",*46,SIICPNO,SP6:
                    SIICSUT,*132,"|":
                    *N,"|",*132,"|"
          ADD       "3",CLNO           * add one to line number
.
          IF        EXPORTYN=1
            IF        UPDTCURR=1
              MOVE      SUPPRICE,SIICCCT
            ENDIF
            MOVE      SUPPRICE,SIICLCT
            CALL      UPSIIC4
            ADD       ONE,TOTUPD
          ENDIF
.
WRIA9999  RETURN
