GETDET00  UNPACK    PBDATE,CDAY,CMON,CYEAR,CCENT
          CALL      IBACLOCK
          IF        PCEASE=1
            MOVE      PDECDTE,AGEDATE            * Date of Death
          ELSE
            PACK      AGEDATE,CCC,CYY,CMM,CDD    * Today's date
          ENDIF
          REP       " 0",AGEDATE
          CALL      CALCAGE
.
          CALL      PATLN000
.
          CLEAR     KEY3
          PACK      KEY5,CATFS,PMPXFLDR,SP70
          CALL      RDCODE1
          IF        OVRCD=0
            REP       " 0",TCINDC1
            APPEND    TCINDC1,KEY3
          ELSE
            APPEND    ZERO,KEY3
          ENDIF
.
          PACK      KEY5,CATT,PTYPE,SP70
          CALL      RDCODE1
          IF        OVRCD=0
            REP       " 0",TCINDC1
            APPEND    TCINDC1,KEY3
          ELSE
            APPEND    ZERO,KEY3
          ENDIF
.
          PACK      KEY5,CATPV,PMPXPRVI,SP70
          CALL      RDCODE1
          IF        OVRCD=0
            REP       " 0",TCINDC1
            APPEND    TCINDC1,KEY3
          ELSE
            APPEND    ZERO,KEY3
          ENDIF
          RESET     KEY3
          RETURN
