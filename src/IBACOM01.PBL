.**************************************************************************
.* Include current inpatients                                             *
.**************************************************************************
KCUR0000  KEYIN     *P1:15,*EF:
                    *P1:15,"Include Current Inpatients. ":
                    " (",*V2LON,"Y",*HOFF,"/":
                    *V2LON,"N",*HOFF,") ? ",*V2LON,INCLCURR

          REP       UPPLOW,INCLCURR
          MATCH     ANSY,INCLCURR
          IF        @EQUAL
            GOTO      KCUR8000
          ENDIF
.
          MATCH     ANSN,INCLCURR
          IF        @EQUAL
            GOTO      KCUR8000
          ENDIF
.
          MATCH     SP70,INCLCURR
          GOTO      KCUR9000 IF EQUAL
.
          BEEP
          GOTO      KCUR0000
.
KCUR8000  MOVE      ZERO,EXIT
          GOTO      KCUR9999
.
KCUR9000  MOVE      ONE,EXIT
          GOTO      KCUR9999
.
KCUR9999  RETURN
