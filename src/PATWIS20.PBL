.******************************************************************************
.*                                  B20SW000              Called by: PATWIS20 *
.*                            Calculate Base WIES20                           *
.******************************************************************************
B20SW000  CALL      D20SC000                * length of stay category
          CALL      C20ST000                * calculate inlier status
.
          MATCH     "L",W20NLIER            * low outlier
          CALL      BW20L000 IF EQUAL       * calculate base WIES20
.
          MATCH     "I",W20NLIER            * inlier
          CALL      BW20N000 IF EQUAL       * calculate base WIES20
.
          MATCH     "H",W20NLIER            * high outlier
          CALL      BW20H000 IF EQUAL       * calculate base WIES20
.
B20SW999  RETURN
+
.******************************************************************************
.*                                  D20SC000              Called by: B20SW000 *
.*                      Determine Length Of Stay Category                     *
.******************************************************************************
D20SC000  MOVE      NBRDAYS,WIS20LOS        * length of stay from NHOSPDAY
          MOVE      "M",LOSCAT              * set LOS category to multiday
          MATCH     ADATE,DDATE             * check for sameday
          IF        @EQUAL
            MOVE      ONE,SAMEDAY           * sameday
            MOVE      ONE,WIS20LOS          * LOS = 1
            MOVE      "S",LOSCAT            * set LOS category to sameday
            GOTO      D20SC999
          ELSE
            MOVE      ZERO,SAMEDAY          * not sameday
          ENDIF
.
          IF        SAMEDAY = 0 & WIS20LOS = 1
            MOVE      "O",LOSCAT            * set LOS category to oneday
            GOTO      D20SC999
          ENDIF
.
          IF        WIS20LOS > 1825
            MOVE      "1825",WIS20LOS       * maximum LOS is 5 years
          ENDIF
.
D20SC999  RETURN
+
.******************************************************************************
.*                                  C20ST000              Called by: B20SW000 *
.*                           Calculate Inlier Status                          *
.******************************************************************************
C20ST000  MOVE      "I",W20NLIER            * set status to inlier
          IF        WIS20LOS < PT20LIBP
            MOVE      "L",W20NLIER          * set status to low outlier
            GOTO      C20ST999
          ENDIF
.
          ASSIGN    (PT20HIBP+ADJMVDAY),FORM12P4   * high bound. + mech.v days
          IF        WIS20LOS > FORM12P4
            MOVE      "H",W20NLIER          * set status to high outlier
          ENDIF
C20ST999  RETURN
+
.******************************************************************************
.*                                  BW20L000              Called by: B20SW000 *
.*                     Calculate Base WIES20 for Low Outlier                  *
.******************************************************************************
BW20L000  MATCH     "S",LOSCAT            * sameday
          IF        @EQUAL
            MOVE      PT20TSDW,TOTBWIES             * base WIES20
            ASSIGN    (TOTBWIES/PT20TIMW),TOTIES20  * IES20
.
            MOVE      PT20TSDW,WIS20WGT             * for display
            MOVE      "Same Day Wgt ",WIS20DSC
            GOTO      BW20L999
          ENDIF
.
          MATCH     "O",LOSCAT            * oneday
          IF        @EQUAL
            MOVE      PT20TODW,TOTBWIES             * base WIES20
            ASSIGN    (TOTBWIES/PT20TIMW),TOTIES20  * IES20
.
            MOVE      PT20TODW,WIS20WGT             * for display
            MOVE      "One Day Wgt  ",WIS20DSC
            GOTO      BW20L999
          ENDIF
.
          MATCH     "M",LOSCAT            * multiday
          IF        @EQUAL
            ASSIGN    (PT20TODW+((WIS20LOS-1)*PT20TLOM)),TOTBWIES
.                                                    * base WIES20
            ASSIGN    (TOTBWIES/PT20TIMW),TOTIES20   * IES20
.
            MOVE      PT20TLOM,WIS20WGT             * for display
            MOVE      "Low Outlr Wgt",WIS20DSC
            GOTO      BW20L999
          ENDIF
BW20L999  RETURN
+
.******************************************************************************
.*                                  BW20N000              Called by: B20SW000 *
.*                      Calculate Base WIES20 for Inlier                      *
.******************************************************************************
BW20N000  MOVE      ONE,TOTIES20          * IES20 = 1
          MATCH     "S",LOSCAT            * sameday
          IF        @EQUAL
            MOVE      PT20TSDW,TOTBWIES   * base WIES20
.
            MOVE      PT20TSDW,WIS20WGT             * for display
            MOVE      "Same Day Wgt ",WIS20DSC
            GOTO      BW20N999
          ENDIF
.
          MATCH     "O",LOSCAT            * oneday
          IF        @EQUAL
            MOVE      PT20TODW,TOTBWIES   * base WIES20
.
            MOVE      PT20TODW,WIS20WGT             * for display
            MOVE      "One Day Wgt  ",WIS20DSC
            GOTO      BW20N999
          ENDIF
.
          MATCH     "M",LOSCAT            * multiday
          IF        @EQUAL
            MOVE      PT20TIMW,TOTBWIES   * base WIES20
.
            MOVE      PT20TIMW,WIS20WGT             * for display
            MOVE      "Inlier Wgt   ",WIS20DSC
            GOTO      BW20N999
          ENDIF
BW20N999  RETURN
+
.******************************************************************************
.*                                  BW20H000              Called by: B20SW000 *
.*                    Calculate Base WIES20 for High Outlier                  *
.******************************************************************************
BW20H000  ASSIGN    ((WIS20LOS-PT20HIBP)-ADJMVDAY),HIGHDAYS  * high outlr days
          COMPARE   ZERO,HIGHDAYS
          IF        @LESS
            MOVE      ZERO,HIGHDAYS         * highdays must be >= zero
          ENDIF
.
          COMPARE   HIGHDAYS,HIHLOS
          IF        @LESS
            MOVE      HIHLOS,HITHDAYS       * get lesser of highdays and
          ELSE                              *  HIHLOS
            MOVE      HIGHDAYS,HITHDAYS
          ENDIF
.
          ASSIGN    (HIGHDAYS-HITHDAYS)*PT20THOM,TOTBWIES    *high WIES
          ASSIGN    (HITHDAYS*PT20THIH)+TOTBWIES,TOTBWIES    *HITH WIES
          ASSIGN    (PT20TIMW+TOTBWIES),TOTBWIES             *base WIES20
          ASSIGN    (TOTBWIES/PT20TIMW),TOTIES20             * IES20
.
          MOVE      PT20THOM,WIS20WGT                        * for display
          MOVE      "High Outl Wgt",WIS20DSC
BW20H999  RETURN
+
.******************************************************************************
.*                                  C20AB000              Called by: PATWIS20 *
.*                          Check for Aboriginality                           *
.******************************************************************************
C20AB000  MOVE      ZERO,ATSIWIES                * set ATSI WIES to zero
          IF        PNSWRACE<1 | PNSWRACE>14
            GOTO      C20AB999              * Invalid parameter
          ENDIF
.
          LOAD      TCODE,PNSWRACE,CATP1,CATP2,CATP3,CATP4,CATP5:
                                   CATP6,CATA1,CATA2,CATA3,CATA4:
                                   CATA5,CATA6,CATA7,CATVA
          LOAD      ACODE,PNSWRACE,PUSR1,PUSR2,PUSR3,PUSR4,PUSR5:
                                   PUSR6,AUSR1,AUSR2,AUSR3,AUSR4:
                                   AUSR5,AUSR6,AUSR7,PMPXABRG
.
          MATCH     ACODE,SP3
          GOTO      C20AB999 IF EQUAL
.
          PACK      KEY5,TCODE,ACODE
          CALL      RDCODE1                 * Read a code file record
          BRANCH    OVRCD,C20AB999
.
          UNPACK    THCSCOD,DIM1,KEY1       * use 2nd char of HDP Equiv
.>>>>     MOVE      THCSCOD,KEY1
          TYPE      KEY1
          GOTO      C20AB999 IF NOT EQUAL   * Non numeric, error
.
          MOVE      KEY1,FORM1
.
. ---- Calculate ATSI Loading if aboriginality = 1, 2, or 3 ----
.
          IF        FORM1=1 | FORM1=2 | FORM1=3
            MOVE      TOTBWIES,FORM12P4       * base WIES
            ADD       MVCOPAY,FORM12P4        * mechv copayment
            ADD       THCOPAY,FORM12P4        * thal  copayment
            ADD       AAACOPAY,FORM12P4       * AAA   copayment
            ADD       ASDCOPAY,FORM12P4       * ASD   copayment
            ASSIGN    (0.3*FORM12P4),ATSIWIES
          ENDIF
.
C20AB999  RETURN
+
