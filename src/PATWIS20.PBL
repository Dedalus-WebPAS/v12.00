.******************************************************************************
.*                                  M20CH000              Called by: PATWIS20 *
.*                Calculate Mechanical Ventialtion Co-payment                 *
.******************************************************************************
M20CH000  MOVE      ZERO,ADJMVDAY
          MOVE      ZERO,MVCOPAY
.
          PACK      KEY8,AADMNO
          CALL      RDPTICU1                * get mech. vent. hours
          BRANCH    OVRCD,M20CH999
.
          MATCH     "D",PT20MVEL            * check mech. vent. eligibility
          IF        @EQUAL
            IF        PTICUMEC > 6
              ASSIGN    ((PTICUMEC+12)/24),FORM12P4    * days receiving copay.
              MOVE      FORM12P4,ADJMVDAY
              ASSIGN    ((ADJMVDAY*0.7659)+0.6980),MVCOPAY  * mech. vent. copay.
            ENDIF
            GOTO      M20CH999
          ENDIF
.
          MATCH     "4",PT20MVEL
          IF        @EQUAL
            IF        PTICUMEC > 96
              ASSIGN    (((PTICUMEC+12)/24)-4),FORM12P4   * days recving copay.
              MOVE      FORM12P4,ADJMVDAY
              ASSIGN    ((ADJMVDAY*0.7659)+0.6980),MVCOPAY  * mech. vent. copay.
            ENDIF
          ENDIF
.
M20CH999  RETURN
+
.******************************************************************************
.*                                  T20AL000              Called by: PATWIS20 *
.*                      Calculate Thalessaemia Co-payment                     *
.******************************************************************************
T20AL000  MOVE      ZERO,THCOPAY
          MATCH     "T",PT20CPAY            * check WIES20 weights file
          IF        @EQUAL
            PACK      KEY16,AADMNO,SP20
            CALL      RSPTECD2              * Position on & read a patient
T20AL100    CALL      RKPTECD2                episode disease file record
            BRANCH    OVRCD,T20AL999
.
            MATCH     AADMNO,PTEDADMN
            GOTO      T20AL999 IF NOT EQUAL
.
            MATCH     "D56",PTEDCODE        * check ICD-10 code
            IF        !@EQUAL
              MATCH     "D57.2",PTEDCODE    * check ICD-10 code
              GOTO      T20AL100 IF NOT EQUAL
            ENDIF
            MOVE      "0.2648",THCOPAY      * eligible for thal. copayment
          ENDIF
T20AL999  RETURN
