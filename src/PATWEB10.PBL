.
          MATCH     FRSTDATE,LASTDATE
          IF        @EQUAL
            WRITE     HTMLFILE;"<td>"
          ELSE
            WRITE     HTMLFILE;"<td width=13%>"
          ENDIF
.
          WRITE     HTMLFILE;"<table cellspacing=0 cellpadding=1 border=1 ":
                             "width=100% align=center ><tr height=26>";
.
          MOVE      ONE,COUNT
          WHILE     COUNT <= 23
.
            MOVE      SP1,DISPCLAS
            UNPACK    DISPLDAY[COUNT][DDAYFLAG],D1:
                      DISPLDAY[COUNT][DDAYFLAG]
            MOVE      ZERO,F1
            MOVE      D1,F1
