.
.         INC       OUTATAIO.INC
.
.         FILE  I/O REQUESTS FOR ATTENDANCE FILE
.
.OVRCD    FORM      1
.KEY24    DIM       24
.
RDAATTA1  RESET     KEY24
          MOVE      ZERO,OVRCD
          READ      OUTATTA1,KEY24;ANS
          GOTO      OVERCOND IF OVER
          RETURN  
.
RDSATTA1  RESET     KEY24
          READ      OUTATTA1,KEY24;;
          RETURN  
.
RDATTA1   MOVE      ZERO,OVRCD
          RESET     KEY24
          READ      OUTATTA1,KEY24;OATSITE,OATGRUP,OATCTYP,OATREFR,OATDATE:
                                   OATNREF,OATNNON,OATNBKD,OTATDSCH,OATSPAR
          GOTO      OVERCOND IF OVER
          RETURN  
.
RDKATTA1  MOVE      ZERO,OVRCD
          READKS    OUTATTA1;OATSITE,OATGRUP,OATCTYP,OATREFR,OATDATE:
                             OATNREF,OATNNON,OATNBKD,OTATDSCH,OATSPAR
          GOTO      OVERCOND IF OVER
          RETURN  
.
WRATTA1   RESET     KEY24
          WRITE     OUTATTA1,KEY24;KEY24,OATNREF,OATNNON,OATNBKD,OTATDSCH:
                                         OATSPAR
          RETURN  
.
UPATTA1   UPDATE    OUTATTA1;OATSITE,OATGRUP,OATCTYP,OATREFR,OATDATE:
                             OATNREF,OATNNON,OATNBKD,OTATDSCH,OATSPAR
          RETURN  
.
DEATTA1   RESET     KEY24
          DELETE    OUTATTA1,KEY24
          RETURN
.
.
