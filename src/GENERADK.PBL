.
.----------------------------------------------------------------------
.  data includes
.----------------------------------------------------------------------
.
          INC       STDGENDF
          INC       ALLDIAFD/INC
          INC       ALLDKIFD/INC
.
.----------------------------------------------------------------------
.  local variables
.----------------------------------------------------------------------
.
.
PRGID     INIT      "GENERADK"
PRGNAM    INIT      "Fix alldkiaf"
VERSION   INIT      "V12.00.00"
.
.----------------------------------------------------------------------
.  program code
.----------------------------------------------------------------------
.
.---- initialise screen and confirm desire to execute program ----
.
ML0000    CALL      DISPHEAD
.
          DISPLAY   *P1:10,"Generate A/H Diagnosis Keyword records."
          CALL      CONTQST
          SUB       ONE,CEXIT
          BRANCH    CEXIT,ML9999,ML9999
.
.---- open files ----
.
          OPEN      ALLDIAA1,"alldiaaf"
          OPEN      ALLDKIA1,"alldkiaf"
          OPEN      ALLDKIA2,"alldkiaf"
.
.---- loop through alldiaaf ----
.
          MOVE      ZERO,SECTOR
.
          PACK      KEY12,SP70
ML1000    CALL      RSALDIA1           * read next record
ML1100    CALL      RKALDIA1
          BRANCH    OVRCD,ML9999
.
          ADD       ONE,SECTOR
          IF        SECTOR%100=1
            DISPLAY   *P1:24,*V2LON,SECTOR," - ",ALDIDIAG,*EF;
          ENDIF
.
          PROC      ALLADKUP
          GOTO      ML1100
.
.---- exit program ----
.
ML9999    CHAIN     PGM
.----------------------------------------------------------------------
.  code includes
.----------------------------------------------------------------------
.
          INC       ALLDIAIO/INC
          INC       ALLDKIIO/INC
          INC       ALLADKPR
          INC       STDGENCD
.
