. Modifications :          
.------------------------------------------------------------
HCCL0000  BRANCH    FLDITMNO,HCCL0100,HCCL0200,HCCL0300,HCCL0400,HCCL0500:
                       HCCL0600,HCCL0700,HCCL0800,HCCL0900,HCCL1000:
                       HCCL1100,HCCL1200,HCCL1300,HCCL1400,HCCL1500:
                       HCCL1600,HCCL1700,HCCL1800,HCCL1900
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      HCCL9999
.
HCCL0100  WRITE     HTMLFILE;HCCLCLMN;
          GOTO      HCCL9999
.
HCCL0200  UNPACK    DIM127,D1,ANS,DIM127
          MOVE      ANS,F1
          BRANCH    F1,HCCL0210,HCCL0220
          WRITE     HTMLFILE;HCCLSTAT;
          GOTO      HCCL9999
.
HCCL0210  MATCH     " 0",HCCLSTAT
          IF        @EQUAL
            WRITE     HTMLFILE;"Printed";
          ELSE
            MATCH     " 1",HCCLSTAT
            IF        @EQUAL
              WRITE     HTMLFILE;"Batched";
            ELSE
              MATCH     " 2",HCCLSTAT
              IF        @EQUAL
                WRITE     HTMLFILE;"Hold";
              ENDIF
            ENDIF
          ENDIF
          GOTO      HCCL9999
.
HCCL0220  MATCH     " 1",HCCLSTAT
          GOTO      HCCL0222 IF EQUAL       * Batched
          MATCH     " 2",HCCLSTAT
          GOTO      HCCL0223 IF EQUAL       * Hold
.
          WRITE     HTMLFILE;"<option value=0>Printed</option>":
                             "<option value=2>Hold</option>";
          GOTO      HCCL9999
.
HCCL0222  WRITE     HTMLFILE;"<option value=1 selected>Batched</option>"
          GOTO      HCCL9999
.
HCCL0223  WRITE     HTMLFILE;"<option value=0>Printed</option>":
                             "<option value=2 selected>Hold</option>"
          GOTO      HCCL9999
.
HCCL0300  WRITE     HTMLFILE;HCCLBTCH;
          GOTO      HCCL9999
.
HCCL0400  WRITE     HTMLFILE;HCCLPMCI;
          GOTO      HCCL9999
.
HCCL0500  WRITE     HTMLFILE;HCCLPYEE;
          GOTO      HCCL9999
.
HCCL0600  WRITE     HTMLFILE;HCCLPSRV;
          GOTO      HCCL9999
.
HCCL0700  WRITE     HTMLFILE;HCCLMPRA;
          GOTO      HCCL9999
.
HCCL0800  WRITE     HTMLFILE;HCCLCAMN;
          GOTO      HCCL9999
.
HCCL0900  WRITE     HTMLFILE;HCCLRCVA;
          GOTO      HCCL9999
.
HCCL1000  WRITE     HTMLFILE;HCCLREJA;
          GOTO      HCCL9999
.
HCCL1100  WRITE     HTMLFILE;HCCLWOFF;
          GOTO      HCCL9999
.
HCCL1200  WRITE     HTMLFILE;HCCLSITE;
          GOTO      HCCL9999
.
HCCL1300  WRITE     HTMLFILE;HCCLADOC;
          GOTO      HCCL9999
.
HCCL1400  WRITE     HTMLFILE;HCCLCTYP;
          GOTO      HCCL9999
.
HCCL1500  WRITE     HTMLFILE;HCCLCLID;
          GOTO      HCCL9999
.
HCCL1600  MATCH     SP70,HCCLSENT
          GOTO      HCCL9999 IF EQUAL 
.
          UNPACK    HCCLSENT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR
          GOTO      HCCL9999
.
HCCL1700  CALL      VCMHTM00                * Claim Comments
          GOTO      HCCL9999
.
HCCL1800  WRITE     HTMLFILE;HCCLVISN;
          GOTO      HCCL9999
.
HCCL1900  MATCH     SP70,HCCLDATE
          GOTO      HCCL9999 IF EQUAL 
.
          UNPACK    HCCLDATE,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR
          GOTO      HCCL9999
.
HCCL9999  RETURN
