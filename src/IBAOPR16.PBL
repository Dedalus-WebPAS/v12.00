EVNT0000  KEYIN     *P1:CVERT,*EF,*DV,EVNTD,*V2LON:
                    EVENT
.
          ENDSET    EVENT
          APPEND    SP70,EVENT
          RESET     EVENT
.
          MATCH     SP70,EVENT            * Test for spaces
          IF        @EQUAL
            PACK      EVENT,SP70
            DISPLAY   *P1:CVERT,*EF,EVNTD,*V2LON,"Y"

            MOVE      "1",EVENT
            GOTO      EVNT9999
          ENDIF
.
          REP       "yYnN",EVENT
.
          MATCH     EVENT,ANSY
          IF        @EQUAL
            MOVE      "1",EVENT
            GOTO      EVNT9999
          ENDIF
.
          MOVE      "0",EVENT
.
EVNT9999  ADD       ONE,CVERT
          RETURN
