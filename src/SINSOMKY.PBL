.---------------------------------------------------------------------------
.   System    :   Supply and Inventory               
.   Include   :   SINSOMKY
.   Routine   :   HSIOMA00 Suggested Order ID  
.   Return    :           
.-------------------------------------------------------------------------------
HSIOMA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "30",HLEF
            MOVE      "80",HRIG
          ENDIF
HSIOMA04
          CALL      GSCRN000
          BRANCH    EXIT,HSIOMA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Code"               
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 5",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Orig"               
.
          MOVE      STDCOL02,STDCOL03
          ADD       " 5",STDCOL03
          DISPLAY   *PSTDCOL03:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL03,STDCOL04
          ADD       "41",STDCOL04
HSIOMA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,SIOMSID 
          PACK      KEY3,SIOMSID,SP70                                           
.                                                                               
.                                                                               
          CALL      RSSIOM1 
HSIOMA10
          CALL      RKSIOM1 
          BRANCH    OVRCD,HSIOMA15
          PACK      KEY3,SIOMSID,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HSIOMA12
          CALL      HSIOMA60
          BRANCH    OVRCD,HSIOMA10
HSIOMA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,SIOMSID 
          DISPLAY   *PSTDCOL02:STDROW,SIOMORG 
          DISPLAY   *PSTDCOL03:STDROW,SIOMDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HSIOMA10 IF NOT EQUAL
HSIOMA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HSIOMA20
          CALL      HSCRI000
          BRANCH    EXIT,HSIOMA30,HSIOMA35,HSIOMA40,HSIOMA45,HSIOMA04:
                              HSIOMA91,HSIOMA50
          GOTO      HSIOMA92
HSIOMA30
          CALL      RKSIOM1 
          BRANCH    OVRCD,HSIOMA85
          PACK      KEY3,SIOMSID,SP70                                           
.                                                                               
.                                                                               
          CALL      RPSIOM1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HSIOMA10
HSIOMA35
          MOVE      ST1,KEY3 
          CALL      RDSIOM1 
          CALL      RPSIOM1 
          BRANCH    OVRCD,HSIOMA85
          PACK      KEY3,SIOMSID,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HSIOMA36
          CALL      RPSIOM1 
          BRANCH    OVRCD,HSIOMA37
          PACK      KEY3,SIOMSID,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HSIOMA36 IF NOT EQUAL
HSIOMA37
          MOVE      ZERO,XITEMNO
          GOTO      HSIOMA10
HSIOMA40
          MOVE      ST1,KEY3 
          CALL      RDSIOM1 
          CALL      RPSIOM1 
          BRANCH    OVRCD,HSIOMA85
          PACK      KEY3,SIOMSID,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HSIOMA08
HSIOMA45
          CALL      RKSIOM1 
          BRANCH    OVRCD,HSIOMA85
          PACK      KEY3,SIOMSID,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY3,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSSIOM1 
HSIOMA46
          CALL      RPSIOM1 
          BRANCH    OVRCD,HSIOMA47
          PACK      KEY3,SIOMSID,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HSIOMA46 IF NOT EQUAL
HSIOMA47
          MOVE      ZERO,XITEMNO
          GOTO      HSIOMA10
HSIOMA50  COMPARE   ZERO,CSEARCH
          GOTO      HSIOMA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          KEYIN     *PSTDCOL03:STDROW,*V2LON,SCHFLD03
          RESET     SCHFLD03
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HSIOMA08
HSIOMA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HSIOMA08
HSIOMA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HSIOMA61 IF EQUAL
          MATCH     SCHFLD01,SIOMSID 
          GOTO      HSIOMA72 IF NOT EQUAL
HSIOMA61
          MATCH     SP70,SCHFLD02
          GOTO      HSIOMA62 IF EQUAL
          MATCH     SCHFLD02,SIOMORG 
          GOTO      HSIOMA72 IF NOT EQUAL
HSIOMA62
          MATCH     SP70,SCHFLD03
          GOTO      HSIOMA63 IF EQUAL
          MATCH     SCHFLD03,SIOMDES 
          GOTO      HSIOMA72 IF NOT EQUAL
HSIOMA63
HSIOMA64
HSIOMA65
HSIOMA66
HSIOMA67
HSIOMA68
HSIOMA69
HSIOMA70
          GOTO      HSIOMA75
HSIOMA72  MOVE      ONE,OVRCD
HSIOMA75  RETURN
HSIOMA85
          LOAD      KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,SIOMSID                                                
          CALL      RDSIOM1 
HSIOMA90
.                                                                               
          BEEP
          GOTO      HSIOMA20
HSIOMA91
          MOVE      SP70,SIOMDES 
          MOVE      SP70,SIOMORG 
          MOVE      SP70,SIOMDEL 
          MOVE      SP70,SIOMMES 
          MOVE      ZERO,SIOMPER 
          MOVE      SP70,SIOMWAR 
          MOVE      ZERO,SIOMINC 
          MOVE      SP70,SIOMFCO 
          MOVE      SP70,SIOMTCO 
          MOVE      SP70,SIOMFPG 
          MOVE      SP70,SIOMTPG 
          MOVE      SP70,SIOMFSU 
          MOVE      SP70,SIOMTSU 
          MOVE      SP70,SIOMUID 
          MOVE      SP70,SIOMPOR 
          MOVE      SP70,SIOMTIM 
          MOVE      SP70,SIOMDAT 
          MOVE      SP70,SIOMSOQ 
          MOVE      SP70,SIOMFIA 
          MOVE      SP70,SIOMTIA 
          MOVE      SP70,SIOMSPA 
          MOVE      SP70,KEY3 
          MOVE      SP70,SIOMSID 
          GOTO      HSIOMA96
HSIOMA92
          MOVE      SP70,KEY3 
          MOVE      SP70,SIOMSID 
          LOAD      KEY3,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,SIOMSID                                                
HSIOMA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HSIOMA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Supply and Inventory               
.   Include   :   SINSOMKY
.   Routine   :   KSIOMA00 Keyin Suggested Order Code
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KSIOMA00 TOPIC     "SIOMSID "
          MOVE      UNDLN70,SIOMSID 
          KEYIN     *PCCOL:CVERT,*DV,SIOMSID:                                   
                    *PCCOL:CVERT,*V2LON,SIOMSID:                                
                    *PCCOL:CVERT,*DV,SIOMSID 
.
          ENDSET    SIOMSID 
          APPEND    SP70,SIOMSID 
          RESET     SIOMSID 
.
          MATCH     SP70,SIOMSID 
          GOTO      KSIOMA90 IF EQUAL
.
          CMATCH    EXITCHAR,SIOMSID 
          GOTO      KSIOMA95 IF EQUAL
.
          CMATCH    QUEST,SIOMSID 
          GOTO      KSIOMA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HSIOMA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    SIOMSID 
          APPEND    SP70,SIOMSID 
          RESET     SIOMSID 
.
          MATCH     SP70,SIOMSID 
          GOTO      KSIOMA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,SIOMSID                                 
KSIOMA10
          PACK      KEY3,SIOMSID                                                
          CALL      RASIOM1 
          BRANCH    OVRCD,KSIOMA80
          BRANCH    CKEYTYP,KSIOMA75
          CALL      RDSIOM1 
KSIOMA70
          MOVE      ZERO,EXIT
          GOTO      KSIOMA99
KSIOMA75
          MOVE      "Suggested Order Code Already Exist - ",DISPMSG                                  
          CALL      MESSAGE1
          GOTO      KSIOMA00
KSIOMA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KSIOMA99,KSIOMA99
          MOVE      "Suggested Order Code Does Not Exist - ",DISPMSG                                 
          CALL      MESSAGE1
          GOTO      KSIOMA00
KSIOMA90
          MOVE      ONE,EXIT
          GOTO      KSIOMA99
KSIOMA95
          MOVE      TWO,EXIT
KSIOMA99 TOPIC    PRGID
          RETURN
