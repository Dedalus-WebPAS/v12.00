.----------------------------------------------------------------------
. Program       : GENEMDKI
.
. Function      : Generate Emergency Doctor KeyWord Table
.
. Modifications :
.----------------------------------------------------------------------
. V10.11.01 08/08/2017  Tracey Nguyen TSK 0299206
.           Recompiled for EMRDKIPR - Added Sound-Ex key for Emergency
.           Doctor display name
. V9.03.00  13/05/2003  Lina Vo     CAR 38501
.           Create new              
.----------------------------------------------------------------------
.  data includes
.----------------------------------------------------------------------
.
          INC       STDGENDF
          INC       EMRDOCFD/INC
          INC       EMRDKIFD/INC
.
.----------------------------------------------------------------------
.  local variables
.----------------------------------------------------------------------
.
.
PRGID     INIT      "GENEMDKI"
PRGNAM    INIT      "Generate emrdkiaf"
VERSION   INIT      "V12.00.00"
.
.----------------------------------------------------------------------
.  program code
.----------------------------------------------------------------------
.
.---- initialise screen and confirm desire to execute program ----
.
ML0000    CALL      DISPHEAD
.
          DISPLAY   *P1:10,"This file generate emrdkiaf Keyword records."
          CALL      CONTQST
          SUB       ONE,CEXIT
          BRANCH    CEXIT,ML9999,ML9999
.
.---- open files ----
.
          OPEN      EMRDOCA1,"emrdocaf"
          OPEN      EMRDKIA1,"emrdkiaf"
          OPEN      EMRDKIA2,"emrdkiaf"
.
.---- loop through emrkedaf ----
.
          PACK      KEY10,SP70
          MOVE      ZERO,SECTOR
.
ML1000    CALL      RSEMDO1           * read next record
.
ML1100    CALL      RKEMDO1
          BRANCH    OVRCD,ML9999
.
          ADD       ONE,SECTOR         * display every 100th record
          IF        SECTOR%100=1
            DISPLAY   *P1:24,*V2LON,SECTOR," - ",EMDODOC,*EF;
          ENDIF
.
          PROC      EMRDKIUP
          GOTO      ML1100
.
.---- exit program ----
.
ML9999    CHAIN     PGM
.----------------------------------------------------------------------
.  code includes
.----------------------------------------------------------------------
.
          INC       EMRDOCIO/INC
          INC       EMRDKIIO/INC
          INC       EMRDKIPR
          INC       STDGENCD
.
