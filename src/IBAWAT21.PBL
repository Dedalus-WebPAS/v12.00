          MOVE      "19",EVERT
          MOVE      SP6,CKYIDEF6                  * no default
          MOVE      ZERO,CKYIMAND                 * not mandatory
.
          CALL      PATDOCKY
.
          IF        EXIT=2
            MOVE      ONE,EXIT                    * EXITCHAR input
            GOTO      CRNG9999
          ENDIF
.
