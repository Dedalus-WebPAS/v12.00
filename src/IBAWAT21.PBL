            MOVE      ZERO,EXIT                   * Nothing Input
            MOVE      "zzzzzz",ENDCONS
            DISPLAY   *P19:19,*V2LON,"Finish",*HOFF;
            GOTO      CRNG9000
          ENDIF
.
          MOVE      DOCCODE,ENDCONS
.
