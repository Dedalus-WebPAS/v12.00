          IF        KEYWRDNO>1
            PACK      SAVKEY24,PTCMKWD,PTCMITM
            CALL      SPTCMC00
            BRANCH    EXIT,SPTCMB91
            MOVE      SAVKEY24,KEY24
            CALL      RDPTKCM1
          ENDIF
