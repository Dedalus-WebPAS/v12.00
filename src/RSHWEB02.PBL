          REP       " 0",ACAUDATE
          MOVE      CTIMEIS,ACAUTIME                * Current Time
          MOVE      USERID,ACAUWUID                 * Web UserId
.
          MOVE      "A",ACAUAUTY                    * Audit Type for Add
          MOVE      "P",ACAUTREC                    * ACC Type of Record
.
          MATCH     STATNCOD,SP70
          IF        !@EQUAL
            MOVE      STATNCOD,ACAUSATC          * Stationery Code (ibatmhaf)   
          ENDIF
.
