.
          MATCH     PTCLM011,Z70
          IF        !@EQUAL
            MOVE      PTCLM011,ACAUCLMN             * ACC Claim Number
          ENDIF
.
          MATCH     SP70,ACAUCLMN                   * Blank ACC Number
          GOTO      MVACCP99 IF EQUAL               * so exit
          GOTO      MVACCP99 IF EOS
.
          MATCH     ADMISSNO,Z70
          IF        !@EQUAL
            MOVE      ADMISSNO,ACAUADMN             * Visit Number
          ENDIF
.
          CALL      IBACLOCK
