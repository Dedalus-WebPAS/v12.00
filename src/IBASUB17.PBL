.----------------------------------------------------------------------
. Process Data
.----------------------------------------------------------------------
POST0000  MATCH     FROMDATE,TODATE              * check Date range
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Date Range - ";
            CALL      HITENTER
            GOTO      POST9500
          ENDIF
.
          MATCH     FROMCOD3,TOCOD3              * check Status range
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Status Range - ";
            CALL      HITENTER
            GOTO      POST9500
          ENDIF
.
          MATCH     FROMCODE,TOCODE              * check Cost Centre range
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Cost Centre Range - ";
            CALL      HITENTER
            GOTO      POST9500
          ENDIF
.
.---- process cost centres ----
.
          MOVE      ZERO,ABORTNOW
          MOVE      ZERO,COUNTDAT
          PACK      KEY5,FROMCODE,SP70
          CALL      RSSICA1
          CALL      RPSICA1
.
POST1000  CALL      RKSICA1
          BRANCH    OVRCD,POST8000
          MATCH     SICACODE,TOCODE
          GOTO      POST8000 IF LESS
.
          CALL      CREAD000                * read data
          IF        EXIT=0
            MOVE      ONE,COUNTDAT          * data found
            CALL      SC01L000              * execute sub screen
          ENDIF
.
          COMPARE   ONE,ABORTNOW            * abort ?
          GOTO      POST1000 IF NOT EQUAL
.
POST8000  IF        COUNTDAT=0
            CLEAR     DISPMSG
            APPEND    "No Data Found - ",DISPMSG
            CALL      MESSAGE1
            GOTO      POST9500
          ENDIF
.
POST9000  MOVE      ZERO,EXIT
          GOTO      POST9999
.
POST9500  MOVE      ONE,EXIT
.
POST9999  RETURN
.----------------------------------------------------------------------
. Read Data For Cost Centre
.----------------------------------------------------------------------
.
.---- get next requisition id ----
.
CREAD000  MOVE      SP70,SIRATYP
          PACK      KEY8,REQTYPE,Z70
          CALL      RSSIRA1
          CALL      RPSIRA1
.
          MOVE      ZERO,F7
          MATCH     REQTYPE,SIRATYP
          IF        @EQUAL
            MOVE      ZERO,F7
            MOVE      SIRAREQ,F7
          ENDIF
.
          MOVE      ZERO,SIRAIND
          PACK      SIRACST,SICACODE,SP70
          PACK      SIRADAT,CCC,CYY,CMM,CDD,SP70
          REP       " 0",SIRADAT
          PACK      SIRADEL,SP70
          MOVE      ZERO,SIRATOT
          PACK      SIRASPA,SP70
.
          REPEAT
            ADD       ONE,F7
            MOVE      F7,REQTNUM
            PACK      KEY8,REQTYPE,F7,SP70
            UNPACK    KEY8,SIRATYP,SIRAREQ
            CALL      RASIRA1
          UNTIL     OVRCD=1
.
          CALL      WRSIRA1
          CALL      RLSIRA1
.
.---- read in data ----
.
          MOVE      ONE,EXIT                * default to nothing found
          PACK      KEY13,SIRACST,SP70
          CALL      RSSIQA1
.
CREAD100  CALL      RKSIQA1                 * read through outstanding records
          BRANCH    OVRCD,CREAD900
.
          MATCH     SIRACST,SIQACST         * check cost centre range
          GOTO      CREAD900 IF NOT EQUAL
.
          MOVE      ONE,SIIANON             * check if stocked item
          PACK      KEY7,SIQACAT,SP70
          CALL      RDSIIA1
          BRANCH    SIIANON,CREAD100
.
          MATCH     FROMDATE,SIQADAT        * check date range
          GOTO      CREAD100 IF LESS
          MATCH     SIQADAT,TODATE
          GOTO      CREAD100 IF LESS
.
          MATCH     FROMCOD3,SIQASTA        * check status range
          GOTO      CREAD100 IF LESS
          MATCH     SIQASTA,TOCOD3
          GOTO      CREAD100 IF LESS
.
          PACK      KEY12,SIQACAT,WARECODE
          CALL      RDSIIE1
          BRANCH    OVRCD,CREAD100          * Not in warehouse
.
          PACK      KEY13,SIQACST,SIQARID,SP70
          CALL      RLSIQA1                 * check if history record exists
          BRANCH    OVRCD,CREAD100,CREAD100 * 0=ok, 1=no more, 2=locked
.
          CALL      DESIQC1                 * delete history rec (this should
.                                           * not exist if SIQA not locked)
          UNPACK    KEY13,SIQCCST,SIQCRID
          MOVE      SIQACAT,SIQCCAT
          MOVE      SIQAUNT,SIQCUNT
          MOVE      SIQAPD,SIQCPD
          MOVE      SIQAPN,SIQCPN
          MOVE      SIQAQTY,SIQCQTY
          MOVE      SIQASTA,SIQCSTA
          MOVE      SIQAUID,SIQCUID
          MOVE      SIQADAT,SIQCDAT
          MOVE      SIQATIM,SIQCTIM
          MOVE      SIQAPOR,SIQCPOR
          PACK      SIQCPID,PASSCODE,SP70
          PACK      SIQCPDT,CCC,CYY,CMM,CDD,SP70
          REP       " 0",SIQCPDT
          CLOCK     TIME,SIQCPTM
          PACK      SIQCPPO,PORT,SP70
          MOVE      SP70,SIQCORI
          MOVE      "0",SIQCPTY
          MOVE      SIQAQTY,SIQCPQT
          MOVE      ZERO,SIQCECT
          MOVE      ZERO,SIQCGSTA
          MOVE      SP70,SIQCORD
          PACK      SIQCREQ,SIRATYP,SIRAREQ,SP70
          MOVE      SP70,SIQCEDT
          MOVE      SP70,SIQCSUP
          MOVE      SP70,SIQCSUT
          MOVE      SP70,SIQCPGP
          MOVE      SP70,SIQCCON
          MOVE      SP70,SIQCCDT
          MOVE      SP70,SIQCGST
          MOVE      SP70,SIQCSPA
.
          CALL      WRSIQC1       * Add current record
          MOVE      ZERO,EXIT
          GOTO      CREAD100
.
CREAD900  IF        EXIT=1
            PACK      KEY8,SIRATYP,SIRAREQ,SP70
            CALL      RDSIRA1     * delete requisition header
            CALL      DESIRA1
          ENDIF
.
CREAD999  RETURN
EREQT000  CALL      VCATRT00        * Verify Category Ok
          BRANCH    EXIT,EREQT800,EREQT900
.
          CALL      REQTYP00
          BRANCH    EXIT,EREQT999,EREQT999
          MOVE      SICDDESC,REQTDESC
          GOTO      EREQT999
.
EREQT800  MOVE      ZERO,EXIT       * Only One Requisition Type
          GOTO      EREQT999
.
EREQT900  MOVE      "Requisition Type Codes Missing. (Cat. RT ) - ",DISPMSG
          CALL      MESSAGE1
          MOVE      TWO,EXIT
          GOTO      EREQT999
.
EREQT999  RETURN
.
VCATRT00  MOVE      "RT    ",KEY6
          CALL      RDSICD1
          BRANCH    OVRCD,VCATRT90
          CALL      RKSICD1
          BRANCH    OVRCD,VCATRT90
          MATCH     "RT ",SICDCATA
          GOTO      VCATRT90 IF NOT EQUAL
.
VCATRT10
          CALL      RKSICD1
          BRANCH    OVRCD,VCATRT80
          MATCH     "RT ",SICDCATA
          GOTO      VCATRT80 IF NOT EQUAL
          MATCH     "R  ",SICDCODE
          GOTO      VCATRT10 IF EQUAL
          MOVE      ZERO,EXIT
          GOTO      VCATRT99
.
VCATRT80  CALL      RPSICD1
          MOVE      SICDCODE,REQTYPE
          MATCH     ANSR,REQTYPE         * Type "R" is Reserved for Returns
          GOTO      VCATRT90 IF EQUAL
          MOVE      SICDDESC,REQTDESC
          MOVE      ONE,EXIT
          GOTO      VCATRT99
.
VCATRT90  MOVE      TWO,EXIT   * Nothing Setup
          GOTO      VCATRT99
.
VCATRT99  RETURN
.
REQTYP00  KEYIN     *PCCOL:CVERT,*V2LON,REQTYPE
          MOVE      REQTYPE,SICDCODE
          ENDSET    SICDCODE
          APPEND    SP70,SICDCODE
          RESET     SICDCODE
.
          MATCH     SP70,SICDCODE
          GOTO      REQTYP90 IF EQUAL
.
          CMATCH    EXITCHAR,SICDCODE
          GOTO      REQTYP95 IF EQUAL
.
          CMATCH    QUEST,SICDCODE
          GOTO      REQTYP10 IF NOT EQUAL
.
          MOVE      "RT ",SICDCATA
          MOVE      CCITEM,SCCITEM
          CALL      HSICDB00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    SICDCODE
          APPEND    SP70,SICDCODE
          RESET     SICDCODE
.
          MATCH     SP70,SICDCODE
          GOTO      REQTYP00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,SICDCODE
REQTYP10
          MOVE      "RT ",SICDCATA
          PACK      KEY6,SICDCATA,SICDCODE
          CALL      RDSICD1
          BRANCH    OVRCD,REQTYP80
.
          MATCH     "1",SICDCANC         * check if type has been cancelled
          IF        @EQUAL
            MOVE      SP70,SICDCODE
            BEEP
            CLEAR     DISPMSG
            APPEND    "Requisition Type Has Been Disabled - ",DISPMSG
            CALL      MESSAGE1
            GOTO      REQTYP00
          ENDIF
.
          MOVE      SICDCODE,REQTYPE
          MATCH     ANSR,REQTYPE         * Type "R" is Reserved for Returns
          GOTO      REQTYP70 IF EQUAL
          MATCH     ANSB,REQTYPE         * Type "B" is Reserved for Backorders
          GOTO      REQTYP71 IF EQUAL
          MATCH     ANSI,REQTYPE         * Type "I" is Reserved for Imprest
          GOTO      REQTYP72 IF EQUAL
          MOVE      ZERO,EXIT
          GOTO      REQTYP99
.
REQTYP70  MOVE      "Requisition Type is for Return of Stock - ",DISPMSG
          CALL      MESSAGE1
          GOTO      REQTYP00
.
REQTYP71  MOVE      "Requisition Type is for Backorders - ",DISPMSG
          CALL      MESSAGE1
          GOTO      REQTYP00
.
REQTYP72  MOVE      "Requisition Type is for Imprest - ",DISPMSG
          CALL      MESSAGE1
          GOTO      REQTYP00
.
REQTYP80  MOVE      "Requisition Type Does Not Exist - ",DISPMSG
          CALL      MESSAGE1
          GOTO      REQTYP00
REQTYP90  MOVE      ONE,EXIT
          GOTO      REQTYP99
REQTYP95  MOVE      TWO,EXIT
REQTYP99  RETURN
.
.----------------------------------------------------------------------
. Keyin Date
. Returns  : FROMDATE (Date entered)
.            FROMDATD (Date description)
.----------------------------------------------------------------------
KDATF000
          PACK      FROMDATE,CCC,CYY,CMM,SP70         * default to blank date
          UNPACK    FROMDATE,CCENT,CYEAR,CMON,CDAY    * set up date parameters
          CALL      KEYDATE                           * keyin date
.
          MATCH     SP70,CDAY
          IF        !@EQUAL & OVRCD=0
            PACK      FROMDATE,CCENT,CYEAR,CMON,CDAY  * pack desired date
            CALL      PACDATE                         * get date description
            PACK      FROMDATD,CPCDATE,SP70
          ELSE
            PACK      FROMDATE,SP70                   * default to start date
            PACK      FROMDATD,START,SP70             * get date description
          ENDIF
.
          MOVE      ZERO,EXIT
.
KDATF999  RETURN
.----------------------------------------------------------------------
. Keyin To Date
. Returns  : TODATE (Date entered)
.            TODATD (Date description)
.----------------------------------------------------------------------
KDATT000
          PACK      TODATE,CCC,CYY,CMM,SP70           * default to blank date
          UNPACK    TODATE,CCENT,CYEAR,CMON,CDAY      * set up date parameters
          CALL      KEYDATE                           * keyin date
.
          MATCH     SP70,CDAY
          IF        !@EQUAL & OVRCD=0
            PACK      TODATE,CCENT,CYEAR,CMON,CDAY    * pack desired date
            CALL      PACDATE                         * get date description
            PACK      TODATD,CPCDATE,SP70
          ELSE
            PACK      TODATE,Z70                      * default to end date
            PACK      TODATD,END,SP70                 * get date description
          ENDIF
.
          MOVE      ZERO,EXIT
.
KDATT999  RETURN
