.----------------------------------------------------------------------
. Read in line item data
.----------------------------------------------------------------------
REDITM00
          MOVE      ZERO,SIIAAVC
          MOVE      SP70,SIIADES
          PACK      KEY7,SIQCCAT,SP70
          CALL      RDSIIA1
          ASSIGN    SIQCQTY*SIIAAVC,TOTCOST
.
REDITM99  RETURN
.----------------------------------------------------------------------
. Post Screen 01 Data
.----------------------------------------------------------------------
POSTA000
          DISPLAY   *P1:24,*EL,"Posting Requisition : "
          MOVE      ZERO,SECTOR
          PACK      KEY8,REQTYPE,REQTNUM,SP70
          CALL      RDSIRA1
.
          PACK      KEY21,KEY8,SP70
          CALL      RSSIQC2
.
POSTA100
          CALL      RKSIQC2
          BRANCH    OVRCD,POSTA900          * any more records ?
.
          PACK      KEY21,SIQCREQ,SIQCCST,SIQCRID,SP70
          MATCH     SIQCREQ,KEY8            * any more records ?
          GOTO      POSTA900 IF NOT EQUAL
.
          ADD       ONE,SECTOR
          DISPLAY   *P23:24,*V2LON,SIRAREQ,*HOFF,"/",*V2LON,SECTOR
.
. Lock Catalog/Warehouse Record
.
POSTA110  PACK       KEY12,SIQCCAT,SIWAWAR
          CALL       RLSIIE1   * Lock
          BRANCH     OVRCD,POSTA500,POSTA400
          CALL       SISIWK1
          COMPARE    ZERO,OVRCD
          GOTO       POSTA410 IF EQUAL
.
          PACK       KEY7,SIQCCAT
          CALL       RDSIIA1   * Read Catalog File
          BRANCH     OVRCD,POSTA500
.
          MOVE       SIQCQTY,TOTALREC  * set total issued
          MOVE       SIQCQTY,TOTALOUT  * set total requested
.
          CALL       UPSOH000  * Update Stock on Hand & Backorders if Required
          CALL       UPPIC000  * Update Picking Sheet Details
          CALL       UPREB000  * Update Requisition Details
          CALL       UPITM000  * Update Item History
.
          PACK       KEY5,SIRACST
          CALL       RDSICA1   * Read Cost Centre
          ASSIGN     TOTALREC*SIIAAVC,SIUSCST
          MOVE       TOTALREC,SIUSQTY
          CALL       SINUSAGE  * Update Usage
.
          ADD        SIUSCST,SIRATOT
          CALL       UPSIRA1   * Update Requisition Total
.
          CALL       UUSIIE1   * Unlock
.
          PACK       KEY13,SIQCCST,SIQCRID,SP70
          CALL       RDSIQA1   * delete outstanding record
          CALL       DESIQA1
          GOTO       POSTA100
.
.---- lock cat/ware ----
.
POSTA400  MOVE       ZERO,COUNT
          REPEAT
            ADD        ONE,COUNT
            DISPLAY    *P1:24,*W
            PACK       KEY12,SIQCCAT,WARECODE
            CALL       RLSIIE1
          UNTIL     (OVRCD=0)|(COUNT=5)
.
          COMPARE   ZERO,OVRCD
          GOTO      POSTA110 IF EQUAL
          BEEP
          DISPLAY    *P1:24,*EL,"Catalog/Warehouse Locked : ":
                     *V2LON,SIQCCAT,*HOFF,"/",*V2LON,WARECODE,*HOFF:
                     " - Hit <Enter> to Retry ";
          KEYIN      ANS
          DISPLAY    *P1:24,*EL,"Posting Requisition : "
          DISPLAY    *P23:24,*V2LON,SIRAREQ,*HOFF,"/",*V2LON,SECTOR
          GOTO       POSTA110
.
POSTA410  DISPLAY    *P1:24,*EL,"Catalog/Warehouse Deleted. STOCKTAKE : ":
                     *V2LON,SIQCCAT,*HOFF,"/",*V2LON,WARECODE,*HOFF;
          CALL       HITENTER
          DISPLAY    *P1:24,*EL,"Posting Requisition : "
          DISPLAY    *P23:24,*V2LON,SIRAREQ,*HOFF,"/",*V2LON,SECTOR
          GOTO       POSTA100
.
POSTA500  DISPLAY    *P1:24,*EL,"Catalog/Warehouse Deleted : ":
                     *V2LON,SIQCCAT,*HOFF,"/",*V2LON,WARECODE,*HOFF;
          CALL       HITENTER
          DISPLAY    *P1:24,*EL,"Posting Requisition : "
          DISPLAY    *P23:24,*V2LON,SIRAREQ,*HOFF,"/",*V2LON,SECTOR
          GOTO       POSTA100
.
POSTA900
          CALL      UUSIRA1    * Unlock Requisition
.
POSTA999  RETURN
.--------------------------------------------
. Update Stock on hand and Backorder File
.--------------------------------------------
UPSOH000  MOVE      WARECODE,KEY5
          CALL      RDSIWA1
          BRANCH    SIWAPIC,UPSOH500
.
. Requisition Input After Picking.
.
          SUB       TOTALREC,SIIESOH
          GOTO      UPSOH600
.
. Requisition Input Before Picking.
.
UPSOH500 IF        SIIESOH<TOTALOUT
            MOVE      SIIESOH,TOTALREC
            MOVE      ZERO,SIIESOH
          ELSE
            MOVE      TOTALOUT,TOTALREC
            SUB       TOTALREC,SIIESOH
          ENDIF
.
UPSOH600  CALL      UPSIIE1
          MOVE      ZERO,SIBAQTY
          IF        TOTALREC<TOTALOUT
            CALL      UPBAC000
          ENDIF
          GOTO      UPSOH999
UPSOH999  RETURN
.-----------------------------------
. Update Backorder File
.-----------------------------------
UPBAC000
          READ      CONTROLF,TWENTY3;*120,HBACK
          COMPARE   ONE,HBACK
          GOTO      UPBAC999 IF NOT EQUAL
.
          MOVE      SIRACST,SIBACST
          PACK      SIBAREQ,SIRATYP,SIRAREQ
          MOVE      SIQCCAT,SIBACAT
          MOVE      WARECODE,SIBAWAR
          PACK      KEY25,SIBACST,SIBACAT,SIBAREQ,SIBAWAR
          CALL      RDSIBA1
          IF        OVRCD=0
            MOVE      TWO,AUDTTYPE
            CALL      WASIBA00                 * Before Audit Change
            ASSIGN    SIBAQTY+TOTALOUT-TOTALREC,SIBAQTY
            CALL      UPSIBA1
            MOVE      THREE,AUDTTYPE
            CALL      WASIBA00                 * After Audit Change
          ELSE
            ASSIGN    TOTALOUT-TOTALREC,SIBAQTY
            MOVE      SIRADAT,SIBADAT
            MOVE      SP70,SIBARESI
            CALL      WRSIBA1
            MOVE      ONE,AUDTTYPE
            CALL      WASIBA00                 * Audit Added
          ENDIF
UPBAC999  RETURN
.-----------------------------------
. Update Picking Sheets File
.-----------------------------------
UPPIC000  MOVE      WARECODE,SIPRWAR * Warehouse
          MOVE      SIRACST,SIPRCST  * Cost Centre
          MOVE      SIRADEL,SIPRDEL  * Delivery Details
          MOVE      SIIERAS,SIPRRAC  * Rack&Shelf
          MOVE      SIQCCAT,SIPRCAT  * Catalog
          MOVE      SIRATYP,SIPRTYP  * Req Type
          MOVE      SIRAREQ,SIPRREQ  * Req No
UPPIC100
          PACK     KEY83,SIPRWAR,SIPRCST,SIPRDEL,SIPRRAC,SIPRCAT,SIPRTYP,SIPRREQ
          CALL      RDSIPR1
          BRANCH    OVRCD,UPPIC500
          MATCH     SP70,SIPRPFL
          GOTO      UPPIC400 IF EQUAL
.
. Can't update a picking sheet record if it has already been printed.
.
          MATCH     SP70,SIPRDEL
          IF        @EQUAL
            MOVE      "Part 2",SIPRDEL
            GOTO      UPPIC100
          ENDIF
          ENDSET    SIPRDEL
          APPEND    " - 2",SIPRDEL
          RESET     SIPRDEL
          GOTO      UPPIC100 * If all that doesn't work there is a problem.
.
UPPIC400  ADD       TOTALREC,SIPRAQT * Quantity to Issue
          ADD       TOTALOUT,SIPRRQT * Quantity Requisted
          MOVE      SIBAQTY,SIPRBQT
          MOVE      SP70,SIPRPFL     * Print Flag
          MOVE      SP70,SIPRSPA
          CALL      UPSIPR1
          GOTO      UPPIC999
.
UPPIC500  MOVE      TOTALREC,SIPRAQT * Quantity to Issue
          MOVE      TOTALOUT,SIPRRQT * Quantity Requisted
          MOVE      SIBAQTY,SIPRBQT
          MOVE      SP70,SIPRPFL     * Print Flag
          MOVE      SP70,SIPRSPA
          CALL      WRSIPR1
.
UPPIC999  RETURN
.-----------------------------------
. Update Requisition Details File
.-----------------------------------
UPREB000  MOVE      SIRATYP,SIRBTYP
          MOVE      SIRAREQ,SIRBREQ
          MOVE      WARECODE,SIRBWAR
          MOVE      SIQCCAT,SIRBCAT
          PACK      KEY20,SIRBTYP,SIRBREQ,SIRBWAR,SIRBCAT
          CALL      RDSIRB1
          IF        OVRCD=0
            ADD       TOTALOUT,SIRBRQT
            ADD       TOTALREC,SIRBIQT
            ASSIGN    SIIAAVC*SIRBIQT,SIRBAMT
            CALL      UPSIRB1
          ELSE
            MOVE      TOTALOUT,SIRBRQT
            MOVE      TOTALREC,SIRBIQT
            ASSIGN    SIIAAVC*SIRBIQT,SIRBAMT
            MOVE      SIRADAT,SIRBDAT
            CALL      WRSIRB1
          ENDIF
          RETURN
.-----------------------------------
. Update Item History
.-----------------------------------
UPITM000
          MOVE      SIRADAT,SIPEDATE
          CALL      RSIPE000                       * Get Usage Period
          PACK      SIITPER,SIPEYEAR,SIPEPER
.
          MOVE      WARECODE,SIITWAR               * Warehouse
          MOVE      SIQCCAT,SIITCAT                * Catalog
          ASSIGN    TOTALREC*SEQ,SIITQTY           * Quantity
          ASSIGN    SEQ*TOTALREC*SIIAAVC,SIITAMT
          MOVE      SIRACST,SIITCST                * Cost Centre
          PACK      SIITREQ,SIRATYP,SIRAREQ
          MOVE      "5",SIITTYP                    * Requisition
          MOVE      SP70,SIITPUR
          MOVE      SP70,SIITSPA
          MOVE      SIIESOH,SIITSOH            * Stock on Hand
          MOVE      SP70,SIITREF
.
          MOVE      PRGID,SIITPRG
          MOVE      PORT,SIITPOR
          MOVE      PASSCODE,SIITOPR
          PACK      SIITDAT,CCC,CYY,CMM,CDD
          REP       " 0",SIITDAT
          REPEAT
            CLOCK     TIME,SIITTIM
            PACK      KEY28,SIITWAR,SIITCAT,SIITDAT,SIITTIM,SP70
            CALL      RASIIT1
          UNTIL     OVRCD=1
          CALL      WRSIIT1
          RETURN
.----------------------------------------------------------------------
. Call screen 02
.----------------------------------------------------------------------
SCR02000
          MOVE      SIQC2MKY,KEY21
          CALL      RDSIQC2
          IF        OVRCD=1
            MOVE      "Invalid Selection - ",DISPMSG
            BEEP
            CALL      MESSAGE1
          ELSE
            CALL      REDITM00         * read in line item data
            CALL      SC02L000
          ENDIF
.
SCR02999  RETURN
.********************************************************************
. POSTB - Post screen 01 data
.********************************************************************
POSTB000  CALL      POSTA000      * post data
          MOVE      ONE,ABORTNOW  * abort
.
POSTB999  RETURN
.----------------------------------------------------------------------
. Skip current cost centre
.----------------------------------------------------------------------
.
.---- clear outstanding data ----
.
SKICC000  PACK      KEY8,REQTYPE,REQTNUM,SP70
          PACK      KEY21,KEY8,SP70
.
SKICC100  CALL      RSSIQC2
          CALL      RKSIQC2
          BRANCH    OVRCD,SKICC900          * any more records ?
.
          PACK      KEY21,SIQCREQ,SIQCCST,SIQCRID,SP70
          MATCH     SIQCREQ,KEY8            * any more records ?
          GOTO      SKICC900 IF NOT EQUAL
.
          CALL      DESIQC2                 * delete history record
.
          PACK      KEY13,SIQCCST,SIQCRID,SP70
          CALL      RDSIQA1                 * unlock outstanding record
          CALL      UUSIQA1
          GOTO      SKICC100
.
SKICC900  PACK      KEY8,SIRATYP,SIRAREQ,SP70
          CALL      RDSIRA1                 * delete requisition header
          CALL      DESIRA1
.
SKICC999  RETURN
.********************************************************************
. ABORT - Abort Program
.********************************************************************
ABORT000  CALL      SKICC000      * Skip current Cost Centre
          MOVE      ONE,ABORTNOW  * abort
.
ABORT999  RETURN
.********************************************************************
. IGNOR - Ignore record
.********************************************************************
IGNOR000  CALL      DESIQC2       * skip current record
          PACK      KEY13,SIQCCST,SIQCRID,SP70
          CALL      RDSIQA1       * unlock outstanding record
          CALL      UUSIQA1
.
IGNOR999  RETURN
.********************************************************************
. CANCL - Cancel record
.********************************************************************
CANCL000
          PACK      KEY13,SIQCCST,SIQCRID,SP70
          CALL      RDSIQA1
          IF        OVRCD=1
            CLEAR     DISPMSG
            APPEND    "Someone else has already updated this req. - ",DISPMSG
            CALL      MESSAGE1
            GOTO      CANCL900
          ENDIF
.
.---- enter cancellation reason ----
.
          PACK      DISPMSG,SP70,SP70
          CALL      MESSAGE2
          KEYIN     *P1:24,"Enter Reason For Cancellation : ":
                    *MASK,*V2LON,SIQCCAN;
          CALL      MESSAGE3
          PACK      SIQCCAN,SIQCCAN,SP70
.
.---- check if ok to cancel ----
.
          MOVE      "Ok to Cancel This Item ? ",DISPMSG
          CALL      MESSAGE4
          BRANCH    EXIT,CANCL950
.
.---- cancel data ----
.
          MOVE      SIQACAT,SIQCCAT
          MOVE      SIQAUNT,SIQCUNT
          MOVE      SIQAPD,SIQCPD
          MOVE      SIQAPN,SIQCPN
          MOVE      SIQAQTY,SIQCQTY
          MOVE      SIQASTA,SIQCSTA
          MOVE      SIQAUID,SIQCUID
          MOVE      SIQADAT,SIQCDAT
          MOVE      SIQATIM,SIQCTIM
          MOVE      SIQAPOR,SIQCPOR
          PACK      SIQCPID,PASSCODE,SP70
          PACK      SIQCPDT,CCC,CYY,CMM,CDD,SP70
          REP       " 0",SIQCPDT
          CLOCK     TIME,SIQCPTM
          PACK      SIQCPPO,PORT,SP70
          MOVE      SP70,SIQCORI
          MOVE      "3",SIQCPTY
          MOVE      ZERO,SIQCPQT
          MOVE      ZERO,SIQCECT
          MOVE      ZERO,SIQCGSTA
          MOVE      SP70,SIQCORD
          MOVE      SP70,SIQCREQ
          MOVE      SP70,SIQCEDT
          MOVE      SP70,SIQCSUP
          MOVE      SP70,SIQCSUT
          MOVE      SP70,SIQCPGP
          MOVE      SP70,SIQCCON
          MOVE      SP70,SIQCCDT
          MOVE      SP70,SIQCGST
          MOVE      SP70,SIQCSPA
.
          CALL      UPSIQC2       * Update current record
          CALL      DESIQA1
.
CANCL900  MOVE      ZERO,EXIT
          GOTO      CANCL999
.
CANCL950  MOVE      ONE,EXIT
.
CANCL999  RETURN
