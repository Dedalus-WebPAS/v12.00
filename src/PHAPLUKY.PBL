.---------------------------------------------------------------------------
.   System    :   Supply and Inventory               
.   Include   :   PHAPLUKY
.   Routine   :   KPHPLA00 Keyin Plural Unit          
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KPHPLA00 TOPIC     "PLDISP"
          MOVE      UNDLN70,PLDISP
          KEYIN     *PCCOL:CVERT,*DV,PLDISP:                                  
                    *PCCOL:CVERT,*V2LON,PLDISP:                               
                    *PCCOL:CVERT,*DV,PLDISP
.
          ENDSET    PLDISP
          APPEND    SP70,PLDISP
          RESET     PLDISP
.
          MATCH     SP70,PLDISP
          GOTO      KPHPLA90 IF EQUAL
.
          CMATCH    EXITCHAR,PLDISP
          GOTO      KPHPLA95 IF EQUAL
.
          CMATCH    QUEST,PLDISP
          GOTO      KPHPLA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HPHPLA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    PLDISP
          APPEND    SP70,PLDISP
          RESET     PLDISP
.
          MATCH     SP70,PLDISP
          GOTO      KPHPLA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,PLDISP                                
KPHPLA10
          PACK      KEY15,PLDISP                                     
          CALL      RAPHPL1 
          BRANCH    OVRCD,KPHPLA80
          BRANCH    CKEYTYP,KPHPLA75
          CALL      RDPHPL1 
KPHPLA70
          MOVE      ZERO,EXIT
          GOTO      KPHPLA99
KPHPLA75
          MOVE      "PLural Code Already Exists - ",DISPMSG                                            
          CALL      MESSAGE1
          GOTO      KPHPLA00
KPHPLA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KPHPLA99,KPHPLA99
          MOVE      "PLural Code Does Not Exist - ",DISPMSG                                           
          CALL      MESSAGE1
          GOTO      KPHPLA00
KPHPLA90
          MOVE      ONE,EXIT
          GOTO      KPHPLA99
KPHPLA95
          MOVE      TWO,EXIT
KPHPLA99 TOPIC    PRGID
          RETURN
.---------------------------------------------------------------------------
.   System    :   Supply and Inventory               
.   Include   :   PHAPLUKY
.   Routine   :   HPHPLA00 Issue/Supply Units  
.   Return    :   PLDISP  
.-------------------------------------------------------------------------------
HPHPLA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "35",HLEF
            MOVE      "80",HRIG
          ENDIF
HPHPLA04
          CALL      GSCRN000
          BRANCH    EXIT,HPHPLA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Dispensing Unit"
.
          MOVE      STDCOL01,STDCOL02
          ADD       "16",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Plural Unit      "
.
          MOVE      STDCOL02,STDCOL03
          ADD       "18",STDCOL03
HPHPLA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,PLDISP
          PACK      KEY15,PLDISP,SP70                                
          CALL      RSPHPL1 
HPHPLA10
          CALL      RKPHPL1 
          BRANCH    OVRCD,HPHPLA15
          PACK      KEY15,PLDISP,SP70                                
          BRANCH    CSEARCH,HPHPLA12
          CALL      HPHPLA60
          BRANCH    OVRCD,HPHPLA10
HPHPLA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY15,XITEMNO,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,PLDISP
          DISPLAY   *PSTDCOL02:STDROW,PLUNIT  
          COMPARE   MAXITEM,XITEMNO
          GOTO      HPHPLA10 IF NOT EQUAL
HPHPLA15
          UNPACK    SP70,PLDISP                                  
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HPHPLA20
          CALL      HSCRI000
          BRANCH    EXIT,HPHPLA30,HPHPLA35,HPHPLA40,HPHPLA45,HPHPLA04:
                              HPHPLA91,HPHPLA50
          GOTO      HPHPLA92
HPHPLA30
          CALL      RKPHPL1 
          BRANCH    OVRCD,HPHPLA85
          PACK      KEY15,PLDISP,SP70                                
          CALL      RPPHPL1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HPHPLA10
HPHPLA35
          MOVE      ST1,KEY15
          CALL      RDPHPL1 
          CALL      RPPHPL1 
          BRANCH    OVRCD,HPHPLA85
          PACK      KEY15,PLDISP,SP70                                
          MATCH     SP70,PLDISP                                               
          GOTO      HPHPLA85 IF EQUAL                                           
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HPHPLA36
          CALL      RPPHPL1 
          BRANCH    OVRCD,HPHPLA37
          PACK      KEY15,PLDISP,SP70                                
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HPHPLA36 IF NOT EQUAL
HPHPLA37
          MOVE      ZERO,XITEMNO
          GOTO      HPHPLA10
HPHPLA40
          MOVE      ST1,KEY15
          CALL      RDPHPL1 
          CALL      RPPHPL1 
          BRANCH    OVRCD,HPHPLA85
          PACK      KEY15,PLDISP,SP70                                
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HPHPLA08
HPHPLA45
          CALL      RKPHPL1 
          BRANCH    OVRCD,HPHPLA85
          PACK      KEY15,PLDISP,SP70                                
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY15,Z70                                          
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSPHPL1 
HPHPLA46
          CALL      RPPHPL1 
          BRANCH    OVRCD,HPHPLA47
          PACK      KEY15,PLDISP,SP70                                
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HPHPLA46 IF NOT EQUAL
HPHPLA47
          MOVE      ZERO,XITEMNO
          GOTO      HPHPLA10
HPHPLA50  COMPARE   ZERO,CSEARCH
          GOTO      HPHPLA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HPHPLA08
HPHPLA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HPHPLA08
HPHPLA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HPHPLA61 IF EQUAL
          MATCH     SCHFLD01,PLDISP
          GOTO      HPHPLA72 IF NOT EQUAL
HPHPLA61
HPHPLA62
HPHPLA63
HPHPLA64
HPHPLA65
HPHPLA66
HPHPLA67
HPHPLA68
HPHPLA69
HPHPLA70
          GOTO      HPHPLA75
HPHPLA72  MOVE      ONE,OVRCD
HPHPLA75  RETURN
HPHPLA85
          LOAD      KEY15,XITEMNO,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY15,PLDISP                                     
          CALL      RDPHPL1 
HPHPLA90
          UNPACK    SP70,PLDISP                                  
          BEEP
          GOTO      HPHPLA20
HPHPLA91
          MOVE      SP70,KEY15
          MOVE      SP70,PLDISP
          GOTO      HPHPLA96
HPHPLA92
          MOVE      SP70,KEY15
          MOVE      SP70,PLDISP
          LOAD      KEY15,CCITEM,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY15,PLDISP                                     
HPHPLA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HPHPLA99
          RETURN
