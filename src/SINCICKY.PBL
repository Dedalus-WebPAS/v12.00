.------------------------------------------------------------
. Routine   : GSIIC000
.
. Function  : Initialisation for Help Screen Display.
.              - Save Screen
.              - Display Box Details
.
. Called by : All Computer Generated Help Screens
.------------------------------------------------------------
GSIIC000  
          MOVE      ZERO,EXIT
          MOVE      ONE,CSEARCH
.
          CALL      GETSCR00
          BRANCH    EXIT,GSIIC999
.
          MOVE      HTOP,CTOP
          MOVE      HLEF,CLEF
          MOVE      HBOT,CBOT
          MOVE      HRIG,CRIG
.
          ADD       TWO,CTOP       * Cxxx Clip Box Size
          ADD       ONE,CLEF
          SUB       THREE,CBOT
          SUB       ONE,CRIG
          MOVE      HBOT,MAXITEM
          SUB       HTOP,MAXITEM
          SUB       "4",MAXITEM
.
          SUSPEND
.
          BOXCLR    HLEF,HTOP,HRIG,HBOT
          BOX       16,HLEF,HTOP,HRIG,HBOT
          ADD       ONE,CBOT
          DISPLAY   *PHRIG:CBOT,*G34,*PHLEF:CBOT,*G33;
          HLINE     *G37,CBOT,CLEF,CRIG
          ADD       ONE,CBOT
.
          CLIP      1,1,CRIG,24    * Start Inside of Box Displays
.
          DISPLAY   *PCLEF:CBOT,"e",*V2LON,"X",*HOFF,"it, ex":
                                    *V2LON,"P",*HOFF,"and, ":
                                    *V2LON,"U",*HOFF,"p, ":
                                    *V2LON,"D",*HOFF,"own, ":
                                    *V2LON,"S",*HOFF,"earch, ":
                                    *V2LON,"H",*HOFF,"ome, ":
                                    *V2LON,"E",*HOFF,"nd, ":
                                    *V2LON,"R",*HOFF,"educe, ":
                                    *V2LON,"F",*HOFF,"ull Catalog Search ";
.exit, expand, up, down, search, home, end, reduce, view, more
          SUB       TWO,CBOT
.
          MOVE      HTOP,MINITEM
          ADD       ONE,MINITEM
          MOVE      MINITEM,STDROW
.
          MOVE      HLEF,STDCOL00
          ADD       ONE,STDCOL00
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*ULON,"Item"
.
GSIIC999  RETURN
.------------------------------------------------------------
. Function    : Select Option Function for Help Screens
.
. Called by   : All Computer Generated Help Screens
.
. Exit Values : 0 = Item Selected      
.               1 = Page Up
.               2 = Page Down            
.               3 = Home
.               4 = End      
.               5 = Display Screen from start
.               6 = Exit
.               7 = Search
.               8 = View
.------------------------------------------------------------
SSIIC000  MOVE      ZERO,CCITEM
          MOVE      SP2,CCITEMD
.
          KEYIN     *PCLEF:CBOT,*NOEDIT,*+,*EOFF,ANS
          GOTO      SSIIC300 IF PAGEDOWN
          GOTO      SSIIC350 IF PAGEUP
          GOTO      SSIIC400 IF HOME
          GOTO      SSIIC450 IF END
          GOTO      SSIIC500 IF RIGHT
          GOTO      SSIIC550 IF LEFT 
          GOTO      SSIIC600 IF UP
          GOTO      SSIIC650 IF DOWN
          RESET     ANS
          GOTO      SSIIC250 IF EOS
          TYPE      ANS
          GOTO      SSIIC220 IF NOT EQUAL
          MOVE      ANS,CCITEM
.
          MOVE      XITEMNO,F3
          DIV       TEN,F3
          COMPARE   CCITEM,F3          * Less than ten items on screen 
          GOTO      SSIIC210 IF LESS   * so no need to enter second character
.
          KEYIN     *PCLEF:CBOT,*NOEDIT,*+,*EOFF,ANS
          RESET     ANS
          GOTO      SSIIC210 IF EOS
          MULT      TEN,CCITEM
          MOVE      ANS,F1
          ADD       F1,CCITEM
.
SSIIC210  
          COMPARE   ONE,CCITEM
          GOTO      SSIIC900 IF LESS       * Invalid Entry
          COMPARE   CCITEM,XITEMNO
          GOTO      SSIIC900 IF LESS       * Invalid Entry
          MOVE      ZERO,EXIT
          GOTO      SSIIC999               * Item Selected
.
SSIIC220 
          REP       UPPLOW,ANS
          MATCH     SP70,ANS           * Next Item
          GOTO      SSIIC650 IF EQUAL  
          MATCH     "X",ANS             * Exit
          GOTO      SSIIC910 IF EQUAL
          MATCH     "D",ANS             * Page Down
          GOTO      SSIIC300 IF EQUAL
          MATCH     "U",ANS             * Page Up
          GOTO      SSIIC350 IF EQUAL
          MATCH     "H",ANS             * Home
          GOTO      SSIIC400 IF EQUAL
          MATCH     "E",ANS             * End
          GOTO      SSIIC450 IF EQUAL
          MATCH     "S",ANS             * Search
          GOTO      SSIIC480 IF EQUAL
          MATCH     "P",ANS             * Left  (Expand Display)
          GOTO      SSIIC500 IF EQUAL
          MATCH     "R",ANS             * Right (Reduce Display)
          GOTO      SSIIC550 IF EQUAL
          MATCH     "F",ANS             * Full Catalog Search
          GOTO      SSIIC700 IF EQUAL
          GOTO      SSIIC900               * Invalid Entry
.
SSIIC250  
          MOVE      DITEM,CCITEM
          COMPARE   CCITEM,XITEMNO
          GOTO      SSIIC900 IF LESS       * Invalid Entry
          MOVE      ZERO,EXIT
          GOTO      SSIIC999               * Item Selected
.         
. Page Down
.
SSIIC300  MOVE      ONE,EXIT
          GOTO      SSIIC999
.
. Page Up
.
SSIIC350  MOVE      TWO,EXIT
          GOTO      SSIIC999
.
. Home
.
SSIIC400  MOVE      THREE,EXIT
          GOTO      SSIIC999
.
. End
.
SSIIC450  MOVE      FOUR,EXIT
          GOTO      SSIIC999
.
. Search
.
SSIIC480  MOVE      SEVEN,EXIT
          GOTO      SSIIC999
.
. Expand Display
.
SSIIC500  BRANCH    CSCRSIZ,SSIIC900
          MOVE      ONE,CSCRSIZ
          SUSPEND
          CALL      PUTSCR00
          MOVE      HTOP,STOP     * Save Screen Size
          MOVE      HLEF,SLEF
          MOVE      HBOT,SBOT
          MOVE      HRIG,SRIG
          MOVE      "3",HTOP      * Full Screen Display Size
          MOVE      "1",HLEF
          MOVE      "24",HBOT
          MOVE      "80",HRIG
          MOVE      FIVE,EXIT     * Display Help Screen from Start
          GOTO      SSIIC999
.
.
. Reduce Display
.
SSIIC550  COMPARE   ZERO,CSCRSIZ
          GOTO      SSIIC900 IF EQUAL
          MOVE      ZERO,CSCRSIZ
          CALL      PUTSCR00
          MOVE      STOP,HTOP     * Restore Saved Screen Size
          MOVE      SLEF,HLEF
          MOVE      SBOT,HBOT
          MOVE      SRIG,HRIG
          MOVE      FIVE,EXIT     * Display Help Screen from Start
          GOTO      SSIIC999
.
. Up Arrow. Highlight Last Item
.
SSIIC600  DISPLAY   *PSTDCOL00:DROW,*V2LON,SP1,DITEM,DOT
          COMPARE   DITEM,ONE
          GOTO      SSIIC610 IF NOT EQUAL
          COMPARE   XITEMNO,ZERO
          GOTO      SSIIC620 IF EQUAL
          MOVE      XITEMNO,DITEM
          MOVE      CTOP,DROW
          ADD       XITEMNO,DROW  
          ADD       ONE,DITEM
.
SSIIC610  SUB       ONE,DITEM
          SUB       ONE,DROW
SSIIC620  DISPLAY   *PSTDCOL00:DROW,*HON,*V2LON,SP1,DITEM,DOT
          GOTO      SSIIC000
.
. Down Arrow. Highlight Next Item
.
SSIIC650  DISPLAY   *PSTDCOL00:DROW,*V2LON,SP1,DITEM,DOT
          COMPARE   DITEM,XITEMNO
          GOTO      SSIIC660 IF NOT EQUAL
          MOVE      ZERO,DITEM
          MOVE      CTOP,DROW
          SUB       ONE,DROW
.
SSIIC660  COMPARE   XITEMNO,ZERO
          GOTO      SSIIC670 IF EQUAL
          ADD       ONE,DITEM
          ADD       ONE,DROW
SSIIC670  DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          GOTO      SSIIC000
.
SSIIC700  MOVE      EIGHT,EXIT
          GOTO      SSIIC999               * Item Selected
.
SSIIC900  BEEP
          GOTO      SSIIC000
.
SSIIC910  MOVE      SIX,EXIT
.
SSIIC999  RETURN
.
