.*******************************************************************************
.         Get the default charging claim code for multi hospital
.            Routine required for PATCATBI, PATGNFEE, et al.
.*******************************************************************************
PATDEFCL
DCLM0000  READ      CONTROLF,ZERO;*43,IBCNMHOS
          READ      CONTROLF,TWENTY;*193,PTCNDCLM     * standard default
.
          IF        IBCNMHOS=1
            OPEN      PATHSPA1,"pathspaf"
            MOVE      PMVXMHOS,KEY3
            CALL      RDPTHSP1
            IF        OVRCD=0
              MOVE      PTHSDCLM,PTCNDCLM     * default claim code charging
            ENDIF
          ENDIF
.
          MATCH     SP70,PTCNDCLM
          IF        @EQUAL
            MOVE      "0  ",PTCNDCLM          * Use zero claim code
          ENDIF
.
DCLM9999  RETURN
.
.*******************************************************************************
