. Description     : EMR Diagnosis Region codes file
.
. ******* INDEX ONE *******
.
RSEMGRC1  RESET     KEY2
          READ      EMRGRCA1,KEY2;;
          RETURN
.
RLEMGRC1  RESET     KEY2
          MOVE      ZERO,OVRCD
          READL     EMRGRCA1,KEY2;EMRCCODE,EMRCDESC,EMRCFLAG,EMRCSPAR
          GOTO      OVERLOCK IF LOCKED
          GOTO      OVERCOND IF OVER
          RETURN
.
REMRGRC1  RESET     KEY2
          MOVE      ZERO,OVRCD
          READ      EMRGRCA1,KEY2;ANS
          GOTO      OVERCOND IF OVER
          RETURN
.
RDEMGRC1  RESET     KEY2
          MOVE      ZERO,OVRCD
          READ      EMRGRCA1,KEY2;EMRCCODE,EMRCDESC,EMRCFLAG,EMRCSPAR
          GOTO      OVERCOND IF OVER
          RETURN
.
RKEMGRC1  MOVE      ZERO,OVRCD
          READKS    EMRGRCA1;EMRCCODE,EMRCDESC,EMRCFLAG,EMRCSPAR
          GOTO      OVERCOND IF OVER
          RETURN
.
RPEMGRC1  MOVE      ZERO,OVRCD
          READKP    EMRGRCA1;EMRCCODE,EMRCDESC,EMRCFLAG,EMRCSPAR
          GOTO      OVERCOND IF OVER
          RETURN
.
UPEMGRC1  UPDATE    EMRGRCA1;EMRCCODE,EMRCDESC,EMRCFLAG,EMRCSPAR
          RETURN
.
WREMGRC1  RESET     KEY2
          WRITE     EMRGRCA1,KEY2;EMRCCODE,EMRCDESC,EMRCFLAG,EMRCSPAR
          RETURN
.
DEEMGRC1  RESET     KEY2
          DELETE    EMRGRCA1,KEY2
          RETURN
.
UUEMGRC1  UNLOCK    EMRGRCA1
          RETURN
