                        "#"",WBOPDATE,WBOPTIME,"#",":
                        "#"",WBOPSTAT,"#",":
                        "#"",WBOPEROR,"#",":
                        "#"<font color=red>",WBOPTRID,"</font>#",":
                        "#"",SP1,"#")"
.
          GOTO      OPVTAB10
.
OPVTAB50  MOVE      SAVKEY26,KEY26
