.******************************************************************************
.*     PROGRAM NAME:    RCHLABEL                                              *
.*     FUNCTION:        Include of routines to print RCH Labels               *
.*     AUTHOR:          Tonii Tang                                            *
.*     DATE WRITTEN:    05/09/2001                                            *
.******************************************************************************
.*     MODIFICATIONS:                                                         *
.*       V10.06.01  31/03/2013  Don Nguyen       CAR 303612                   *
.*                  Added WLINEZ19 (Aboriginality) to SINGLE00 and DOUBLE00   *
.******************************************************************************
.*        V9.02.02  30/01/2002  Tonii                                         *
.*                  RCH LABELS - new layout for PMI labels                    * 
.*                  - mods to print new fields added in PATSETLB              *
.*        V9.02.01  03/11/2001  Bronko                                        *
.*                  Inserted "*" after the barcode CMDLINE                    *
.******************************************************************************
.
.--------- Single Label
.
SINGLE00  IF        LINE = 9
            PRINT     *1,RESET,MARGIN    
            MOVE      ONE,LINE
          ENDIF
          PRINT     *1,ESC,"&l6D":
                    *1,HPA10FNT,HPLINE1A,WLINEZ2:
                    *1,HPA14FNT,HPLINE4A,WRCHURNO,HPA10FNT,WRCHFLAG:
                    *1,HPA10FNT 
          PRINT     *1,HPLINE1A,WLINEZ3 
          PRINT     *1,ESC,"*p-32Y":
                    *1,HPLINE4A,HPBARCOD,WRCHURNT 
          PRINT     *1,HPA10FNT,ESC,"*p-12Y": 
                    *1,ESC,"&l8D":
                    *1,ESC,"*p+10Y":
                    *1,HPLINE4A,HPBARCOD,WRCHURNT:
                    *1,HPA07FNT,HPLINE1A,WLINEZ4:
                    *1,ESC,"*p+34Y":
                    *1,HPLINE1A,WLINEZ5:
                    *1,HPLINE4A,HPA08FNT,WLINEZ8,HPA07FNT:
                    *1,HPLINE7A,WLINEZ17:
                    *1,ESC,"*p+30Y":
                    *1,HPLINE1A,WLINEZ6:
                    *1,HPLINE4A,WLINEZ9:
                    *1,ESC,"*p+30Y":
                    *1,HPLINE1A,WLINEZ7:
                    *1,HPLINE4A,WLINEZ19
          PRINT     *1,ESC,"*p+10Y":
                    *1,HPLINE1A,WLINEZ18:
                    *1,HPLINE4A,WLINEZ10:
                    *1,ESC,"*p+30Y":
                    *1,HPLINE1A,WLINEZ11:
                    *1,HPLINE4A,WLINEZ12:
                    *1,ESC,"*p+30Y":
                    *1,HPLINE1A,WLINEZ13:
                    *1,HPLINE4A,WLINEZ14:
                    *1,HPLINE6A,WLINEZ16
          ADD       ONE,LINE
SINGLE99  RETURN
.
.-------- Double Labels
.
DOUBLE00  IF        LINE = 9
            PRINT     *1,RESET,MARGIN    
            MOVE      ONE,LINE
          ENDIF
.
          PRINT     *1,ESC,"&l6D":
                    *1,HPA10FNT,HPLINE1A,WLINEZ2:
                    *1,HPA14FNT,HPLINE4A,WRCHURNO,HPA10FNT,WRCHFLAG:
                    *1,HPA10FNT,HPLINE1B,WLINEZ2:
                    *1,HPA14FNT,HPLINE4B,WRCHURNO,HPA10FNT,WRCHFLAG:
                    *1,HPA10FNT  
          PRINT     *1,HPLINE1A,WLINEZ3:
                    *1,HPLINE1B,WLINEZ3
          PRINT     *1,ESC,"*p-32Y":
                    *1,HPLINE4A,HPBARCOD,WRCHURNT:
                    *1,HPLINE4B,HPBARCOD,WRCHURNT 
          PRINT     *1,HPA10FNT,ESC,"*p-12Y":
                    *1,ESC,"&l8D",HPA07FNT:
                    *1,ESC,"*p+10Y":
                    *1,HPLINE4A,HPBARCOD,WRCHURNT:
                    *1,HPLINE4B,HPBARCOD,WRCHURNT:
                    *1,HPA07FNT,HPLINE1A,WLINEZ4:
                    *1,HPLINE1B,WLINEZ4:
                    *1,ESC,"*p+34Y":
                    *1,HPLINE1A,WLINEZ5:
                    *1,HPLINE1B,WLINEZ5:
                    *1,HPLINE4A,HPA08FNT,WLINEZ8,HPA07FNT:
                    *1,HPLINE4B,HPA08FNT,WLINEZ8,HPA07FNT:
                    *1,HPLINE7A,WLINEZ17:
                    *1,HPLINE7B,WLINEZ17:
                    *1,ESC,"*p+30Y":
                    *1,HPLINE1A,WLINEZ6:
                    *1,HPLINE1B,WLINEZ6:
                    *1,HPLINE4A,WLINEZ9:
                    *1,HPLINE4B,WLINEZ9:
                    *1,ESC,"*p+30Y":
                    *1,HPLINE1A,WLINEZ7:
                    *1,HPLINE1B,WLINEZ7:
                    *1,HPLINE4A,WLINEZ19:
                    *1,HPLINE4B,WLINEZ19
          PRINT     *1,ESC,"*p+10Y":
                    *1,HPLINE1A,WLINEZ18:
                    *1,HPLINE1B,WLINEZ18:
                    *1,HPLINE4A,WLINEZ10:
                    *1,HPLINE4B,WLINEZ10:
                    *1,ESC,"*p+30Y":
                    *1,HPLINE1A,WLINEZ11:
                    *1,HPLINE1B,WLINEZ11:
                    *1,HPLINE4A,WLINEZ12:
                    *1,HPLINE4B,WLINEZ12:
                    *1,ESC,"*p+30Y":
                    *1,HPLINE1A,WLINEZ13:
                    *1,HPLINE1B,WLINEZ13:
                    *1,HPLINE4A,WLINEZ14:
                    *1,HPLINE4B,WLINEZ14:
                    *1,HPLINE6A,WLINEZ16:
                    *1,HPLINE6B,WLINEZ16
          ADD       ONE,LINE
DOUBLE99  RETURN
               
