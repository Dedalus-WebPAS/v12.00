              CALL      RDCODE1
              IF        OVRCD<>1
                MATCH     SP70,TCINDC7
                IF        !@EQUAL
                  MATCH     TCINDC7,PTVADTYP
                  GOTO      VADKY100 IF NOT EQUAL
                ENDIF
              ENDIF
            ENDIF
          ENDIF  
