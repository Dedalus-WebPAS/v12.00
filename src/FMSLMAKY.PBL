.----------------------------------
. Old Keyin and Accept Routine
.----------------------------------
KLMAFM00  MOVE      ZERO,CKEYTYP
          CALL      KFMLAA00
          RETURN
.
ALMAFM00  MOVE      ONE,CKEYTYP
          CALL      KFMLAA00
          RETURN
.
.---------------------------------------------------------------------------
.   System    :   Financial Management System        
.   Include   :   FMSLMAKY
.   Routine   :   HFMLAA00 Ledger              
.   Return    :           
.-------------------------------------------------------------------------------
HFMLAA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "12",HTOP
            MOVE      "24",HBOT
            MOVE      "50",HLEF
            MOVE      "80",HRIG
          ENDIF
HFMLAA04
          CALL      GSCRN000
          BRANCH    EXIT,HFMLAA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Ledger"             
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 7",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "36",STDCOL03
HFMLAA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,FMLALEDG
          PACK      KEY2,FMLALEDG,SP70                                          
.                                                                               
.                                                                               
          CALL      RSFMLA1 
HFMLAA10
          CALL      RKFMLA1 
          BRANCH    OVRCD,HFMLAA15
          PACK      KEY2,FMLALEDG,SP70                                          
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HFMLAA12
          CALL      HFMLAA60
          BRANCH    OVRCD,HFMLAA10
HFMLAA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY2,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,FMLALEDG
          DISPLAY   *PSTDCOL02:STDROW,FMLADESC
          COMPARE   MAXITEM,XITEMNO
          GOTO      HFMLAA10 IF NOT EQUAL
HFMLAA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HFMLAA20
          CALL      HSCRI000
          BRANCH    EXIT,HFMLAA30,HFMLAA35,HFMLAA40,HFMLAA45,HFMLAA04:
                              HFMLAA91,HFMLAA50
          GOTO      HFMLAA92
HFMLAA30
          CALL      RKFMLA1 
          BRANCH    OVRCD,HFMLAA85
          PACK      KEY2,FMLALEDG,SP70                                          
.                                                                               
.                                                                               
          CALL      RPFMLA1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMLAA10
HFMLAA35
          MOVE      ST1,KEY2 
          CALL      RDFMLA1 
          CALL      RPFMLA1 
          BRANCH    OVRCD,HFMLAA85
          PACK      KEY2,FMLALEDG,SP70                                          
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HFMLAA36
          CALL      RPFMLA1 
          BRANCH    OVRCD,HFMLAA37
          PACK      KEY2,FMLALEDG,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HFMLAA36 IF NOT EQUAL
HFMLAA37
          MOVE      ZERO,XITEMNO
          GOTO      HFMLAA10
HFMLAA40
          MOVE      ST1,KEY2 
          CALL      RDFMLA1 
          CALL      RPFMLA1 
          BRANCH    OVRCD,HFMLAA85
          PACK      KEY2,FMLALEDG,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMLAA08
HFMLAA45
          CALL      RKFMLA1 
          BRANCH    OVRCD,HFMLAA85
          PACK      KEY2,FMLALEDG,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY2,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSFMLA1 
HFMLAA46
          CALL      RPFMLA1 
          BRANCH    OVRCD,HFMLAA47
          PACK      KEY2,FMLALEDG,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HFMLAA46 IF NOT EQUAL
HFMLAA47
          MOVE      ZERO,XITEMNO
          GOTO      HFMLAA10
HFMLAA50  COMPARE   ZERO,CSEARCH
          GOTO      HFMLAA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMLAA08
HFMLAA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMLAA08
HFMLAA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HFMLAA61 IF EQUAL
          MATCH     SCHFLD01,FMLALEDG
          GOTO      HFMLAA72 IF NOT EQUAL
HFMLAA61
          MATCH     SP70,SCHFLD02
          GOTO      HFMLAA62 IF EQUAL
          MATCH     SCHFLD02,FMLADESC
          GOTO      HFMLAA72 IF NOT EQUAL
HFMLAA62
HFMLAA63
HFMLAA64
HFMLAA65
HFMLAA66
HFMLAA67
HFMLAA68
HFMLAA69
HFMLAA70
          GOTO      HFMLAA75
HFMLAA72  MOVE      ONE,OVRCD
HFMLAA75  RETURN
HFMLAA85
          LOAD      KEY2,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY2,FMLALEDG                                               
          CALL      RDFMLA1 
HFMLAA90
.                                                                               
          BEEP
          GOTO      HFMLAA20
HFMLAA91
          MOVE      SP70,FMLADESC
          MOVE      SP70,FMLACYRR
          MOVE      ZERO,FMLAPERS
          MOVE      SP70,FMLABNK 
          MOVE      SP70,FMLACRD 
          MOVE      SP70,FMLADIS 
          MOVE      SP70,FMLADEB 
          MOVE      SP70,FMLASPAR
          MOVE      SP70,KEY2 
          MOVE      SP70,FMLALEDG
          GOTO      HFMLAA96
HFMLAA92
          MOVE      SP70,KEY2 
          MOVE      SP70,FMLALEDG
          LOAD      KEY2,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY2,FMLALEDG                                               
HFMLAA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HFMLAA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Financial Management System        
.   Include   :   FMSLMAKY
.   Routine   :   KFMLAA00 Keyin Ledger              
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KFMLAA00 TOPIC     "FMLALEDG"
          MOVE      UNDLN70,FMLALEDG
          KEYIN     *PCCOL:CVERT,*DV,FMLALEDG:                                  
                    *PCCOL:CVERT,*V2LON,*JR,*ZF,FMLALEDG:                       
                    *PCCOL:CVERT,*DV,FMLALEDG
.
          ENDSET    FMLALEDG
          APPEND    SP70,FMLALEDG
          RESET     FMLALEDG
.
          MATCH     SP70,FMLALEDG
          GOTO      KFMLAA90 IF EQUAL
.
          ENDSET    FMLALEDG
          CMATCH    EXITCHAR,FMLALEDG
          GOTO      KFMLAA95 IF EQUAL
.
          CMATCH    QUEST,FMLALEDG
          GOTO      KFMLAA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HFMLAA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    FMLALEDG
          APPEND    SP70,FMLALEDG
          RESET     FMLALEDG
.
          MATCH     SP70,FMLALEDG
          GOTO      KFMLAA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,FMLALEDG                                
KFMLAA10
          RESET     FMLALEDG
          PACK      KEY2,FMLALEDG                                               
          CALL      RAFMLA1 
          BRANCH    OVRCD,KFMLAA80
          BRANCH    CKEYTYP,KFMLAA75
          CALL      RDFMLA1 
KFMLAA70
          MOVE      ZERO,EXIT
          GOTO      KFMLAA99
KFMLAA75
          MOVE      "Ledger Already Exist - ",DISPMSG                                                
          CALL      MESSAGE1
          GOTO      KFMLAA00
KFMLAA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KFMLAA99,KFMLAA99
          MOVE      "Ledger Does Not Exist - ",DISPMSG                                               
          CALL      MESSAGE1
          GOTO      KFMLAA00
KFMLAA90
          MOVE      ONE,EXIT
          GOTO      KFMLAA99
KFMLAA95
          MOVE      TWO,EXIT
KFMLAA99 TOPIC    PRGID
          RETURN
