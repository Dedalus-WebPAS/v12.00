.---------------------------------------------------------------------------
.   System    :   Supply and Inventory               
.   Include   :   SINCCAKY
.   Routine   :   KSICAA00 Keyin Cost Centre         
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KSICAA00 TOPIC     "SICACODE"
          MOVE      UNDLN70,SICACODE
          KEYIN     *PCCOL:CVERT,*DV,SICACODE:                                  
                    *PCCOL:CVERT,*V2LON,SICACODE:                               
                    *PCCOL:CVERT,*DV,SICACODE
.
          ENDSET    SICACODE
          APPEND    SP70,SICACODE
          RESET     SICACODE
.
          MATCH     SP70,SICACODE
          GOTO      KSICAA90 IF EQUAL
.
          CMATCH    EXITCHAR,SICACODE
          GOTO      KSICAA95 IF EQUAL
.
          CMATCH    QUEST,SICACODE
          GOTO      KSICAA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HSICAA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    SICACODE
          APPEND    SP70,SICACODE
          RESET     SICACODE
.
          MATCH     SP70,SICACODE
          GOTO      KSICAA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,SICACODE                                
KSICAA10
          PACK      KEY5,SICACODE                                               
          CALL      RASICA1 
          BRANCH    OVRCD,KSICAA80
          BRANCH    CKEYTYP,KSICAA75
          CALL      RDSICA1 
KSICAA70
          MOVE      ZERO,EXIT
          GOTO      KSICAA99
KSICAA75
          MOVE      "Cost Centre Already Exist - ",DISPMSG                                           
          CALL      MESSAGE1
          GOTO      KSICAA00
KSICAA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KSICAA99,KSICAA99
          MOVE      "Cost Centre Does Not Exist - ",DISPMSG                                          
          CALL      MESSAGE1
          GOTO      KSICAA00
KSICAA90
          MOVE      ONE,EXIT
          GOTO      KSICAA99
KSICAA95
          MOVE      TWO,EXIT
KSICAA99 TOPIC    PRGID
          RETURN
