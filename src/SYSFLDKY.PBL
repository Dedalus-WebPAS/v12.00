.   System    :   System Control                     
.   Include   :   SYSFLDKY
.   Routine   :   KSYFLA00 Keyin Field               
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KSYFLA00 TOPIC     "SYFLFLD "
          MOVE      UNDLN70,SYFLFLD 
          KEYIN     *PCCOL:CVERT,*DV,SYFLFLD:                                   
                    *PCCOL:CVERT,*V2LON,SYFLFLD:                                
                    *PCCOL:CVERT,*DV,SYFLFLD 
.
          ENDSET    SYFLFLD 
          APPEND    SP70,SYFLFLD 
          RESET     SYFLFLD 
          REP       UPPLOW,SYFLFLD
.
          MATCH     SP70,SYFLFLD 
          GOTO      KSYFLA90 IF EQUAL
.
          CMATCH    EXITCHAR,SYFLFLD 
          GOTO      KSYFLA95 IF EQUAL
.
          CMATCH    QUEST,SYFLFLD 
          GOTO      KSYFLA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HSYFLA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    SYFLFLD 
          APPEND    SP70,SYFLFLD 
          RESET     SYFLFLD 
.
          MATCH     SP70,SYFLFLD 
          GOTO      KSYFLA00 IF EQUAL
.
KSYFLA10
          DISPLAY   *PCCOL:CVERT,*V2LON,SYFLFLD                                 
          PACK      KEY8,SYFLSYS,SYFLFIL,SYFLFLD                                
          CALL      RASYFL2 
          BRANCH    OVRCD,KSYFLA80
          BRANCH    CKEYTYP,KSYFLA75
          CALL      RDSYFL2 
KSYFLA70
          MOVE      ZERO,EXIT
          GOTO      KSYFLA99
KSYFLA75
          MOVE      "Field Already Exist - ",DISPMSG                                                 
          CALL      MESSAGE1
          GOTO      KSYFLA00
KSYFLA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KSYFLA99,KSYFLA99
          MOVE      "Field Does Not Exist - ",DISPMSG                                                
          CALL      MESSAGE1
          GOTO      KSYFLA00
KSYFLA90
          MOVE      ONE,EXIT
          GOTO      KSYFLA99
KSYFLA95
          MOVE      TWO,EXIT
KSYFLA99 TOPIC    PRGID
          RETURN
