.****************************************************************************
.*                                OPTN0000             Called by: ML0000    *
.*                        get user options or exit                          *
.*                                                                          *
.*    Returns:  EXIT    = FALSE (0)  Run clean up                           *
.*                        TRUE  (1)  Exit option selected                   *
.****************************************************************************
.
OPTN0000  DISPLAY   *P1:3,*EF,*P1:4,*V2LON,ZERO,*HOFF,". Exit":
                    *P1:5,*V2LON,ONE,*HOFF,". Run"
.
OPTN0500  KEYIN     *P1:7,*EL,"Select Option : ",*DV,UNDLN1:
                    *P17:7,*V2LON,COPTION
.
          COMPARE   ZERO,COPTION                 * exit selected ?
          GOTO      OPTN9500 IF EQUAL            * yes
.
          BRANCH    COPTION,OPTN9000             * run clean-up
.
          BEEP
          GOTO      OPTN0500
.
OPTN9000  MOVE      ZERO,EXIT
          GOTO      OPTN9999
.
OPTN9500  MOVE      ONE,EXIT
.
OPTN9999  RETURN
+
.**************************************************************************
.*                               PROC0000              Called by: ML0000  *
.*                                                                        *
.**************************************************************************
PROC0000  CALL      HEAD0000                * Print heading
          MOVE      ZERO,RECCOUNT
.
          PACK      KEY9,ONE,SP70
          CALL      RDSMISS2
PROC1000  CALL      RDKMISS2
          BRANCH    OVRCD,PROC9000
.
          COMPARE   "1",ASTAT
          GOTO      PROC9000 IF NOT EQUAL
.
          MATCH     ADATE,CALCDATE
          GOTO      PROC1000 IF LESS
.
          ADD       ONE,RECCOUNT
          IF        RECCOUNT>999
            PRINT     *1,"*********** Limit of 1000 records reached *******"
            GOTO      PROC9000
          ENDIF
.
          MOVE      DAADMNO,ADMISSNO
          MATCH     ANSN,REPTONLY
          IF        @EQUAL
            MOVE      ZERO,ERRORFLG
            OPEN      BOKRC1A1,"bokrc1af"
            CALL      CPADM000
            IF        ERRORFLG=1
.   there was an error, so this record has already been printed 
              GOTO      PROC1000
            ENDIF
          ENDIF
.
          CALL      PRIN0000                * Print report
          GOTO      PROC1000
.
PROC9000  CALL      UND132
          PRINT     *1,"Records : ",RECCOUNT
          PRINT     *1,"***** End of Report ****"
.
PROC9999  RETURN
