            PRINT     *40,"Hospital                    :",*71,PTHSNAME
          ENDIF
          UNPACK    CALCDATE,CCENT,CYEAR,CMON,CDAY
          PRINT     *40,"Prior Pre-Admission Date      :",*71:
                     CDAY,SLASH,CMON,SLASH,CCENT,CYEAR
          PRINT     *40,"Cancellation Reason           :",*71,TDESC
