                MOVE      SP70,FILTHOSP
                GOTO      HOSP9400
              ELSE
                MOVE      KEY3,FILTHOSP
                DISPLAY   *P29:10,*EL,PTHSNAME,*W2;
              ENDIF
            ENDIF
         ENDIF
