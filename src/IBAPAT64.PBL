.**********************************************************************
.* DAYX0000 - Number of days prior to cancel                          *
.**********************************************************************
DAYX0000  DISPLAY   *P1:11,*EL:
                    *P1:11,"Days Prior to cancel : ";
.
DAYX0500  MOVE      TWENTY,DAYPRIOR
          DISPLAY   *P25:11,UNDLN1;
          KEYIN     *P25:11,*V2LON,DAYPRIOR;
.
          CALL      IBACLOCK
          PACK      CURRDATE,CCC,CYY,CMM,CDD
          REP       " 0",CURRDATE
.
          SUB       DAYPRIOR,CURRDATE
          MOVE      CURRDATE,CALCDATE
          CALL      IBACLOCK
          PACK      CURRDATE,CCC,CYY,CMM,CDD
          REP       " 0",CURRDATE
.
          DISPLAY   *P25:16,"Date From ",CALCDATE;
