.**********************************************************************
.* KREP0000 - Keyin report only                                       *
.**********************************************************************
KREP0000  DISPLAY   *P1:13,*EL:
                    *P1:13,"Report Only : ";
.
.-------- keyin slot type to delete  -----
.
KREP0500  MOVE      SP70,REPTONLY
          DISPLAY   *P25:13,UNDLN1;
          KEYIN     *P25:13,*V2LON,REPTONLY;
.
          PACK      REPTONLY,REPTONLY,SP70
.
          MATCH     SP70,REPTONLY
          IF        @EQUAL
            MOVE      "Y",REPTONLY * Default to report only
          ENDIF
.
          MATCH     "N",REPTONLY   * Run and delete
          IF        @EQUAL
            MOVE      "No ",REPONLYD
            DISPLAY   *P29:13,*EL,REPONLYD;
            GOTO      KREP9999
          ENDIF

          MATCH     "Y",REPTONLY   * Report only
          IF        @EQUAL
            MOVE      "Yes",REPONLYD
            DISPLAY   *P29:13,*EL,REPONLYD;
            GOTO      KREP9999
          ENDIF
.
          DISPLAY   *P1:13,*EL,*B,"Invalid report only option - Hit <":
                              *V2LON,"ENTER",*HOFF,"> To Continue ";
          KEYIN     ANS;
          DISPLAY  *P1:13,*EL;
          GOTO      KREP0500
.
KREP9999  RETURN
