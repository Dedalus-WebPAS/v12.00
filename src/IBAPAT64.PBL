          PRINT     *1," "
          CALL      UND132
          PRINT     *1,"| UR NO",*12,"| Admit Date ":
                    *30,"|Visit No",*40,"| Error Conditions",*132,"|"
          CALL      UND132
          IF        IBCNMHOS = 1
            MOVE      11,CLNO
          ELSE
            MOVE      10,CLNO
          ENDIF
