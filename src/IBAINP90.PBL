          MOVE      SP3,CLAMCODE
          MOVE      SP20,DIM16
          MOVE      SP20,TDESC
          MOVE      "57",ECOL
          MOVE      "15",EVERT
          MOVE      "CL",CODE
          MOVE      SP3,CKYIDEF3
