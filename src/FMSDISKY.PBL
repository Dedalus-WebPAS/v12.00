.---------------------------------------------------------------------------
.   System    :   Financial Management System        
.   Include   :   FMSDISKY
.   Routine   :   HFMDSA00 Dissection          
.   Return    :           
.-------------------------------------------------------------------------------
HFMDSA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "40",HLEF
            MOVE      "80",HRIG
          ENDIF
HFMDSA04
          CALL      GSCRN000
          BRANCH    EXIT,HFMDSA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Dissection"         
.
          MOVE      STDCOL01,STDCOL02
          ADD       "11",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "35",STDCOL03
HFMDSA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,FMDSCODE
          PACK      KEY5,FMDSCODE,SP70                                          
.                                                                               
.                                                                               
          CALL      RSFMDS1 
HFMDSA10
          CALL      RKFMDS1 
          BRANCH    OVRCD,HFMDSA15
          PACK      KEY5,FMDSCODE,SP70                                          
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HFMDSA12
          CALL      HFMDSA60
          BRANCH    OVRCD,HFMDSA10
HFMDSA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY5,XITEMNO,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,FMDSCODE
          DISPLAY   *PSTDCOL02:STDROW,FMDSDESC
          COMPARE   MAXITEM,XITEMNO
          GOTO      HFMDSA10 IF NOT EQUAL
HFMDSA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HFMDSA20
          CALL      HSCRI000
          BRANCH    EXIT,HFMDSA30,HFMDSA35,HFMDSA40,HFMDSA45,HFMDSA04:
                              HFMDSA91,HFMDSA50
          GOTO      HFMDSA92
HFMDSA30
          CALL      RKFMDS1 
          BRANCH    OVRCD,HFMDSA85
          PACK      KEY5,FMDSCODE,SP70                                          
.                                                                               
.                                                                               
          CALL      RPFMDS1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMDSA10
HFMDSA35
          MOVE      ST1,KEY5 
          CALL      RDFMDS1 
          CALL      RPFMDS1 
          BRANCH    OVRCD,HFMDSA85
          PACK      KEY5,FMDSCODE,SP70                                          
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HFMDSA36
          CALL      RPFMDS1 
          BRANCH    OVRCD,HFMDSA37
          PACK      KEY5,FMDSCODE,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HFMDSA36 IF NOT EQUAL
HFMDSA37
          MOVE      ZERO,XITEMNO
          GOTO      HFMDSA10
HFMDSA40
          MOVE      ST1,KEY5 
          CALL      RDFMDS1 
          CALL      RPFMDS1 
          BRANCH    OVRCD,HFMDSA85
          PACK      KEY5,FMDSCODE,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMDSA08
HFMDSA45
          CALL      RKFMDS1 
          BRANCH    OVRCD,HFMDSA85
          PACK      KEY5,FMDSCODE,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY5,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSFMDS1 
HFMDSA46
          CALL      RPFMDS1 
          BRANCH    OVRCD,HFMDSA47
          PACK      KEY5,FMDSCODE,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HFMDSA46 IF NOT EQUAL
HFMDSA47
          MOVE      ZERO,XITEMNO
          GOTO      HFMDSA10
HFMDSA50  COMPARE   ZERO,CSEARCH
          GOTO      HFMDSA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMDSA08
HFMDSA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HFMDSA08
HFMDSA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HFMDSA61 IF EQUAL
          MATCH     SCHFLD01,FMDSCODE
          GOTO      HFMDSA72 IF NOT EQUAL
HFMDSA61
          MATCH     SP70,SCHFLD02
          GOTO      HFMDSA62 IF EQUAL
          MATCH     SCHFLD02,FMDSDESC
          GOTO      HFMDSA72 IF NOT EQUAL
HFMDSA62
HFMDSA63
HFMDSA64
HFMDSA65
HFMDSA66
HFMDSA67
HFMDSA68
HFMDSA69
HFMDSA70
          GOTO      HFMDSA75
HFMDSA72  MOVE      ONE,OVRCD
HFMDSA75  RETURN
HFMDSA85
          LOAD      KEY5,XITEMNO,ST1,ST2,ST3,ST4,ST5:
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY5,FMDSCODE                                               
          CALL      RDFMDS1 
HFMDSA90
.                                                                               
          BEEP
          GOTO      HFMDSA20
HFMDSA91
          MOVE      SP70,FMDSDESC
          MOVE      SP70,FMDSSPAR
          MOVE      SP70,KEY5 
          MOVE      SP70,FMDSCODE
          GOTO      HFMDSA96
HFMDSA92
          MOVE      SP70,KEY5 
          MOVE      SP70,FMDSCODE
          LOAD      KEY5,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY5,FMDSCODE                                               
HFMDSA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HFMDSA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Financial Management System        
.   Include   :   FMSDISKY
.   Routine   :   KFMDSA00 Keyin Dissection          
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KFMDSA00 TOPIC     "FMDSCODE"
          MOVE      UNDLN70,FMDSCODE
          KEYIN     *PCCOL:CVERT,*DV,FMDSCODE:                                  
                    *PCCOL:CVERT,*V2LON,*JR,FMDSCODE:                           
                    *PCCOL:CVERT,*DV,FMDSCODE
.
          ENDSET    FMDSCODE
          APPEND    SP70,FMDSCODE
          RESET     FMDSCODE
.
          MATCH     SP70,FMDSCODE
          GOTO      KFMDSA90 IF EQUAL
.
          ENDSET    FMDSCODE
          CMATCH    EXITCHAR,FMDSCODE
          GOTO      KFMDSA95 IF EQUAL
.
          CMATCH    QUEST,FMDSCODE
          GOTO      KFMDSA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HFMDSA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    FMDSCODE
          APPEND    SP70,FMDSCODE
          RESET     FMDSCODE
.
          MATCH     SP70,FMDSCODE
          GOTO      KFMDSA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,FMDSCODE                                
KFMDSA10
          RESET     FMDSCODE
          PACK      KEY5,FMDSCODE                                               
          CALL      RAFMDS1 
          BRANCH    OVRCD,KFMDSA80
          BRANCH    CKEYTYP,KFMDSA75
          CALL      RDFMDS1 
KFMDSA70
          MOVE      ZERO,EXIT
          GOTO      KFMDSA99
KFMDSA75
          MOVE      "Dissection Already Exist - ",DISPMSG                                            
          CALL      MESSAGE1
          GOTO      KFMDSA00
KFMDSA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KFMDSA99,KFMDSA99
          MOVE      "Dissection Does Not Exist - ",DISPMSG                                           
          CALL      MESSAGE1
          GOTO      KFMDSA00
KFMDSA90
          MOVE      ONE,EXIT
          GOTO      KFMDSA99
KFMDSA95
          MOVE      TWO,EXIT
KFMDSA99 TOPIC    PRGID
          RETURN
