          WRITE     ITMTEMPB,KEY29;URNUMBER,CLAIMNUM,ITMNUMBR,SERVDATE:
                                   DICOUNTR,VISITNUM,TITEMCNT
          RETURN
.
DETEMPB   DELETE    ITMTEMPB,KEY29
