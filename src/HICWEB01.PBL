.***************************************************************************
.*                              PRTBTC00                                   *
.*                      Print batch report                                 *
.***************************************************************************
PRTBTC00  CALL      OPNPRINT
          PACK      SCHDPRNT,ANSS,SP70          * Spool File
          MATCH     SP70,SELPRINT
          IF        !@EQUAL
            MOVE      SELPRINT,SCHDPRNT      * printer code
          ELSE
            MOVE      SCHDPRNT,SELPRINT
          ENDIF
.
          MOVE      HICCLCON,SETDFPRG
          MOVE      "1",SETDFRPT
          CALL      SETDEF00
.
          CALL      HICPRCLM         * print batch report
          CALL      CLSPRINT
.
          MOVE      ZERO,EXIT
.
PRTBTC99  RETURN
