            ADD       ONE,COUNTDAT
            LOAD      DIM10,COUNTDAT,DSUN,DMON,DTUE,DWED,DTHU,DFRI,DSAT
            GOTO      PRNT0500
          ENDIF
.
          IF        FOUNDX=0
            DISPLAY   *P1:24,*B,*EF,"Nothing to Print - ";
            CALL      HITENTER
          ENDIF
.
          CALL      TRAN0000                     * export export file
.
