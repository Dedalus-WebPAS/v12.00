. PRNT - print report for SICA                      Called by ML
.********************************************************************
PRNT0000  READ      CONTROLF,TWENTY4;*164,HFIVCOLM
.......   READ      CONTROLF,SIXTY3;*133,INCOSI40,*142,SNCO40PL
          READ      CONTROLF,SIXTY3;*142,SNCO40PL                      *I-f0903
          PACK      INCOSI40,INCOSI40,SP70
          REP       " 0",INCOSI40
.
          PACK      DISPMSG,SP70,SP70            * save line 24
          CALL      MESSAGE2
.
          MATCH     FCOST,TCOST                  * check range
          IF        @LESS
            DISPLAY   *P1:24,*B,*EF,"Illegal Cost Centre Range - ";
            CALL      HITENTER
            GOTO      PRNT9500
          ENDIF
.
          DISPLAY   *P1:24,*EL,*P50:24,"Printing :   ";
          MOVE      ZERO,CPAGENO                 * reset page no.
          CLOCK     TIME,CTIMEIS                 * get current time
          MOVE      "999",CLNO                   * init last line number
          MOVE      TEN6,FORM2
          MOVE      ONE,CNOUNDLN                 * no underlines
          MOVE      ZERO,FOUNDX
.
          IF        ZERO=DAYWEEK
            MOVE      ONE,COUNTDAT               * start at day 1
          ELSE
            MOVE      DAYWEEK,COUNTDAT           * start at selected day
          ENDIF

          CALL      MAKEXP00                    * create export file
.
PRNT0500  CALL      FILE0000                    * go to correct position in file
.
PRNT1000  CALL      NREC0000                     * get next record
          BRANCH    OVRCD,PRNT8000               * end of file ?
.
          DISPLAY   *P61:24,*V2LON,SICACODE,*EL;
.
          CALL      WRIT0000                     * print details
          GOTO      PRNT1000                     * get next record
.
