                      *120,"!"                                         *C-f0903
            IF        SNCO40PL = 1
              PRINT     *1,"-------":
                        *11,"------------------------------":
                        *42,"---------------":
                        *58,"----",*63,"--------":
                        *72,"---------",*82,"---------":
                        *92,"---------",*102,"---------",*112,"---------":
                        *122,"----------"
              ADD       ONE,CLNO         * add one to line number      *I-f0903
              GOTO      WRIT9000                                       *I-f0903
            ENDIF                                                      *I-f0903
          ELSE
            MOVE      SIIAAVC,F12P2
            PRINT     *1,SIIMCAT,*9,DIM1,*11,SIIADES,*72,SIIAISS,*90,SIIMSEQ:
                      *95,QTYFORM8,*104,"!",*112,"!":
                      *N,*11,SIICPNO,*72,F12P2,*104,"!",*112,"!"      *C-f0903
            IF        SNCO40PL = 1
              PRINT     *1,"-------",*11,"------------------------------":
                                         "------------------------------":
                        *72,"---------------",*90,"----",*95,"--------":
                        *104,"---------",*114,"------------------"
              ADD       ONE,CLNO         * add one to line number      *I-f0903
              GOTO      WRIT9000                                       *I-f0903
            ENDIF                                                      *I-f0903
          ENDIF
. ----------------------------------------- * Start of Deletion         D-f0903
.....     IF        SNCO40PL=1
.....       PRINT     "----------------------------------------------":
.....                 "----------------------------------------------":
.....                 "---------------------------------------"
.....       ADD       ONE,CLNO         * add one to line number
.....       GOTO      WRIT9000
.....     ENDIF
. ----------------------------------------- * End of Deletion           D-f0903
          MATCH     "1",INCOSI40
          IF        @EQUAL
            PRINT     "----------------------------------------------":
                      "----------------------------------------------":
                      "---------------------------------------"
            ADD       ONE,CLNO         * add one to line number
          ENDIF
.
WRIT9000  ADD       ONE,FORM2
          ADD       TWO,CLNO           * add two to line number
          CALL      WRTEXP00           * write data to export file
          GOTO      WRIT1000
.
WRIT9999  RETURN
.********************************************************************
. BKLIN - Break up KEY60 into two neat halves        Called by lots
.         Requires - KEY60   (original line)
.         Returns  - KEY30   (line 1)
.                    KEY60   (line 2)
.********************************************************************
BKLIN000  RESET     KEY60,31           * reset to length(line1)+1
          CMATCH    SP70,KEY60
          WHILE     !@EQUAL&!@EOS
            BUMP      KEY60,-1
            IF        !@EOS
              CMATCH    SP70,KEY60
            ENDIF
          DO
.
          IF        @EOS
            SETLPTR   KEY60
            RESET     KEY60
            UNPACK    KEY60,KEY30,KEY60
          ELSE
            LENSET    KEY60
            RESET     KEY60
            MOVE      KEY60,KEY30
            PACK      KEY30,KEY30,SP70
.
            ENDSET    KEY60
            SETLPTR   KEY60
            BUMP      KEY60
            PACK      KEY60,KEY60,SP70
          ENDIF
.
BKLIN999  RETURN
.------------------------------------------------------------
. File       : casetran
.
. Functions  : This file contains standard routines for
.              creating and transfering TAB delimited
.              export files to a PC using the unix script
.              keatran.
.
. Routines   : MAKEXP00  - Create the Export File and Headings
.              WRTEXP00  - Write Export details Record to File
.              TRAN0000  - Transfer file to PC using script keatran
.
. Note       : The Variable EXPORTYN is used to determine if a
.              export file is to be created. 0=No, 1=Yes.
.
. Variables to be Declared
. ------------------------
. EXPORTFL  FILE      ASCII, FIXED=400     * Export File (Adjust Length to Suit)
. EXPORTFN  DIM       8                    * Export File Name
. TABDELM   EQU       0x09                 * Tab Delimiter
.------------------------------------------------------------
. Create Export File
.------------------------------------------------------------
MAKEXP00  COMPARE   ONE,EXPORTYN
          GOTO      MAKEXP99 IF NOT EQUAL
          MOVE      "EXPORT",KEY6
          PACK      EXPORTFN,KEY6,PORT
          REP       " 0",EXPORTFN
          PREP      EXPORTFL,EXPORTFN
.
          IF        HFIVCOLM=1
            WRITE     EXPORTFL,SEQ;"Cost Centre Code",TABDELM:
                                   "Cost Centre Description",TABDELM:
                                   "Imprest Delivery Day",TABDELM:
                                   "Catalog Code",TABDELM:
                                   "Catalog Description",TABDELM:
                                   "Part No.",TABDELM:
                                   "Unit of Issue",TABDELM:
                                   "Seq. No",TABDELM:
                                   "Imprest Quantity",TABDELM:
                                   "Stock Required"
          ELSE
            WRITE     EXPORTFL,SEQ;"Cost Centre Code",TABDELM:
                                   "Cost Centre Description",TABDELM:
                                   "Imprest Delivery Day",TABDELM:
                                   "Catalog Code",TABDELM:
                                   "Catalog Description",TABDELM:
                                   "Part No.",TABDELM:
                                   "Unit of Issue",TABDELM:
                                   "Avg. Cost",TABDELM:
                                   "Seq. No",TABDELM:
                                   "Imprest Quantity",TABDELM:
                                   "Counted Stock"
          ENDIF


MAKEXP99  RETURN
.------------------------------------------------------------
. Write to Export File
.------------------------------------------------------------
WRTEXP00  COMPARE   ONE,EXPORTYN
          GOTO      WRTEXP99 IF NOT EQUAL
.
          IF        HFIVCOLM=1
            WRITE     EXPORTFL,SEQ;SICACODE,TABDELM:
                                   SICADESC,TABDELM:
                                   DIM10,TABDELM:
                                   SIIMCAT,TABDELM:
                                   SIIADES,TABDELM:
                                   SIICPNO,TABDELM:
                                   SIIAISS,TABDELM:
                                   SIIMSEQ,TABDELM:
                                   QTYFORM8
          ELSE
            WRITE     EXPORTFL,SEQ;SICACODE,TABDELM:
                                   SICADESC,TABDELM:
                                   DIM10,TABDELM:
                                   SIIMCAT,TABDELM:
                                   SIIADES,TABDELM:
                                   SIICPNO,TABDELM:
                                   SIIAISS,TABDELM:
                                   SIIAAVC,TABDELM:
                                   SIIMSEQ,TABDELM:
                                   QTYFORM8
          ENDIF

WRTEXP99  RETURN
.------------------------------------------------------------
. Execute Transfer Script eg softerm
.------------------------------------------------------------
TRAN0000  COMPARE   ONE,EXPORTYN
          GOTO      TRAN9999 IF NOT EQUAL
          MOVE      YES,DEFYESNO
          CLOSE     EXPORTFL
          DISPLAY   *P1:24,*EL,"Execute Transfer Now (":
                               *V2LON,"Y",*HOFF,"/":
                               *V2LON,"N",*HOFF,") ? "
          MOVE      "24",CVERT
          MOVE      "30",CCOL
          CALL      YORN0000
          COMPARE   ZERO,FYESNO
          GOTO      TRAN9999 IF EQUAL   *  No Transfer
.
          MOVE      ZERO,HLEF
          CALL      GETSCR00
.
          CLEAR     KEY80
          APPEND    "keatran ",KEY80
          APPEND    EXPORTFN,KEY80
          APPEND    ".txt",KEY80
          RESET     KEY80
          EXECUTE   KEY80,TASKID
.
          CALL      PUTSCR00
.
TRAN9998  DISPLAY   *P1:24,*EL,"Transfer Completed Ok (":
                               *V2LON,"Y",*HOFF,"/":
                               *V2LON,"N",*HOFF,") ? "
          MOVE      "24",CVERT
          MOVE      "31",CCOL
          CALL      YORN0000
          COMPARE   ZERO,FYESNO,
          GOTO      TRAN0000 IF EQUAL   *  Not ok
.
          PREP      EXPORTFL,EXPORTFN
          CLOSE     EXPORTFL
.
TRAN9999  RETURN
.----------------------------------------------------------------------
. keyin print seq
.----------------------------------------------------------------------
KPSEQ000
          PACK      DISPMSG,SP70,SP70
          CALL      MESSAGE2
          DISPLAY   *P1:24,*V2LON:
                    "0",*HOFF," = Exit Program, ",*V2LON:
                    "1",*HOFF," = Cost Centre Code Sequence, ",*V2LON:
                    "2",*HOFF," = Cost Centre Description Sequence";
.
KPSEQ100
          KEYIN     *PCCOL:CVERT,"_":
                    *PCCOL:CVERT,*V2LON,PRTSEQ
.
          MOVE      "                                        ",DPRTSEQ
          COMPARE   ZERO,PRTSEQ
          GOTO      KPSEQ950 IF EQUAL
          MOVE      "Cost Centre Code Sequence               ",DPRTSEQ
          COMPARE   ONE,PRTSEQ
          GOTO      KPSEQ900 IF EQUAL
          MOVE      "Cost Centre Description Sequence        ",DPRTSEQ
          COMPARE   TWO,PRTSEQ
          GOTO      KPSEQ900 IF EQUAL
.
          BEEP
          GOTO      KPSEQ100
.
KPSEQ900  MOVE      ZERO,EXIT
          GOTO      KPSEQ999
.
KPSEQ950  MOVE      TWO,EXIT
.
KPSEQ999
          CALL      MESSAGE3
          RETURN
.----------------------------------------------------------------------
. keyin day of week
.----------------------------------------------------------------------
KDAYS000
          PACK      DISPMSG,SP70,SP70
          CALL      MESSAGE2
          DISPLAY   *P1:24,*V2LON:
                    "0",*HOFF," = All, ",*V2LON:
                    "1",*HOFF," = Sun, ",*V2LON:
                    "2",*HOFF," = Mon, ",*V2LON:
                    "3",*HOFF," = Tue, ",*V2LON:
                    "4",*HOFF," = Wed, ",*V2LON:
                    "5",*HOFF," = Thu, ",*V2LON:
                    "6",*HOFF," = Fri, ",*V2LON:
                    "7",*HOFF," = Sat";
.
KDAYS100
          KEYIN     *PCCOL:CVERT,"_":
                    *PCCOL:CVERT,*V2LON,DAYWEEK
.
          COMPARE   ZERO,DAYWEEK
          GOTO      KDAYS150 IF NOT EQUAL
.
          MOVE      "All       ",DDAYWEEK
          MOVE      ONE,COUNTDAT
          GOTO      KDAYS250
.
KDAYS150  BRANCH    DAYWEEK,KDAYS200,KDAYS200,KDAYS200,KDAYS200,KDAYS200:
                            KDAYS200,KDAYS200
          BEEP
          GOTO      KDAYS100
.
KDAYS200  LOAD      COUNTDAT USING DAYWEEK OF ONE,TWO,THREE,FOUR,FIVE,SIX:
                                             SEVEN
          LOAD      DDAYWEEK USING COUNTDAT OF DSUN,DMON,DTUE,DWED,DTHU,DFRI:
                                              DSAT
KDAYS250  MOVE      SP20,DIM10
          LOAD      DIM10 USING COUNTDAT OF DSUN,DMON,DTUE,DWED,DTHU,DFRI:
                                              DSAT
.
KDAYS900  MOVE      ZERO,EXIT
          GOTO      KDAYS999
.
KDAYS950  MOVE      TWO,EXIT
.
KDAYS999  CALL      MESSAGE3
          RETURN
.----------------------------------------------------------------------
. Keyin from code (SICA)
.----------------------------------------------------------------------
KCSTF000
          CALL      KSICAA00                     * keyin SICA
          BRANCH    EXIT,KCSTF100,KCSTF950
.
          PACK      FROMCOST,SICACODE,SP70       * item entered
          PACK      FCOST,SICACODE,SP70
          PACK      FCDESC,SICADESC,SP70
          GOTO      KCSTF900
.
KCSTF100  PACK      FROMCOST,START,SP70          * nothing entered
          PACK      FCOST,SP70
          PACK      FCDESC,SP70
          GOTO      KCSTF900
.
KCSTF900  MOVE      ZERO,EXIT                    * entry ok
          GOTO      KCSTF999
.
KCSTF950  MOVE      TWO,EXIT                     * quit screen
.
KCSTF999  RETURN
.----------------------------------------------------------------------
. Keyin to code (SICA)
.----------------------------------------------------------------------
KCSTT000
          CALL      KSICAA00                     * keyin SICA
          BRANCH    EXIT,KCSTT100,KCSTT950
.
          PACK      TOCOST,SICACODE,SP70         * item entered
          PACK      TCOST,SICACODE,SP70
          PACK      TCDESC,SICADESC,SP70
          GOTO      KCSTT900
.
KCSTT100  PACK      TOCOST,END,SP70              * nothing entered
          PACK      TCOST,Z70
          PACK      TCDESC,SP70
          GOTO      KCSTT900
.
KCSTT900  MOVE      ZERO,EXIT                    * entry ok
          GOTO      KCSTT999
.
KCSTT950  MOVE      TWO,EXIT                     * quit screen
.
KCSTT999  RETURN
