.
          MATCH     "A",UPDTTYPE            * Add
          GOTO      AECI1000 IF EQUAL
.
          MATCH     "U",UPDTTYPE            * Update
          GOTO      AECI2000 IF EQUAL
.
          MATCH     "D",UPDTTYPE            * Delete
          GOTO      AECI3000 IF EQUAL
.
          GOTO      AECI9000
.
.         ************ ADD *************
.
