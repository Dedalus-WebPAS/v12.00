.***************************************************************************
          DEFPROC   PTYPSELC
.
          INC       NZPPTYFD/INC
.
          OPEN      NZPPTYA2,"nzpptyaf"
.
          PACK      KEY33,SP70
          CALL      RSNZPTY2
PRTYPS10  CALL      RKNZPTY2
          BRANCH    OVRCD,PRTYPS99
.
          MATCH     NZPTPTYP,PMHCPTYP
          IF        @EQUAL
            WRITE     HTMLFILE;"<option value=#"",NZPTPTYP,"#" selected>":
                                 NZPTDESC,"</option>"
          ELSE
            MATCH     "1",NZPTACTV
            GOTO      PRTYPS10 IF EQUAL
.
            WRITE     HTMLFILE;"<option value=#"",NZPTPTYP,"#">":
                                 NZPTDESC,"</option>"
          ENDIF
          GOTO      PRTYPS10
.
          INC       NZPPTYIO/INC
          INC       IBAOVRIO/INC
.
PRTYPS99  ENDPROC
