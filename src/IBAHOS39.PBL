          IF        IBCNMHOS=1
            MATCH     PTHSHOSP,PTWDHOSN     * Different Hospital
            GOTO      AWR1000 IF NOT EQUAL
          ENDIF
.
          BRANCH    WINPUT,AWR3000          * Ward only
          MOVE      WWARD,SAWARD            * save ward just read in
