          GOTO      AWR9999
.
AWR2000   IF        IBCNMHOS=1
            MATCH     PTHSHOSP,PTWDHOSN     * Different Hospital ?
            GOTO      AWR1000 IF NOT EQUAL
          ENDIF
.
          MATCH     SP3,WBED
          IF        @EQUAL
            MOVE      WACTIVE,WHOLWRD
          ENDIF
