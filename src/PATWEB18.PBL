          BRANCH    OVRCD,LNEP9999
.
          MATCH     SP70,PMNEP002
          IF        !@EQUAL
            MATCH     PMNEP002,PMNEVTYP              * Visit type
            GOTO      LNEP9999 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,PMNEP001
          IF        !@EQUAL
            MATCH     PMNEP001,PMNECNTR              * COntract ID
            GOTO      LNEP1000 IF NOT EQUAL
          ENDIF
          GOTO      LNEP5000
