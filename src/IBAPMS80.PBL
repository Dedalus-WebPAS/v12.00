          OPEN      PATMX1A1,"patmx1af"
.
          DISPLAY   *P65:24,"patalrtf";
          OPEN      PATALRT1,"patalrtf"
.
          DISPLAY   *P65:24,"patalrtf";
          OPEN      PATALRT1,"patalrtf"
          OPEN      PATALRT2,"patalrtf"
.
          DISPLAY   *P65:24,"patalrtf";
          OPEN      PATALRT3,"patalrtf"
.
          DISPLAY   *P65:24,"pmspx2af";
