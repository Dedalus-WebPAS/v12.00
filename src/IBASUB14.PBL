CHKORDZZ  ENDPROC
.----------------------------------------------------------------------
. Keyin AMT
.----------------------------------------------------------------------
KAMT0000
          KEYIN     *PCCOL:CVERT,*V2LON,F8
          MOVE      F8,SIQAQTY
.
          IF        SIQAQTY<0
            CLEAR     DISPMSG
            APPEND    "Quantity must be greater than zero - ",DISPMSG
            CALL      MESSAGE1
            GOTO      KAMT0000
          ENDIF
.
          IF        SIQAQTY=0
            GOTO      KAMT0000
          ELSE
            MOVE      ZERO,KEEPCC
            MOVE      ZERO,ACCEPT
            MOVE      ZERO,EXIT
          ENDIF
.
KAMT9999  RETURN
.----------------------------------------------------------------------
. KSICD - Keyin Code For Category "PRC"
.         Returns : SIQASTA   (code keyed in)
.                   SICDDESC  (code description)
.----------------------------------------------------------------------
KSICD000
          MOVE      "PRC",SICDCATA          * set up category
          CALL      KSICDB000               * keyin code
          BRANCH    EXIT,KSICD500,KSICD999
.
          MOVE      SICDCODE,SIQASTA        * save code entered
          GOTO      KSICD900
.
KSICD500  UNPACK    SP70,SIQASTA,SICDDESC   * no code entered
          GOTO      KSICD900
.
KSICD900  MOVE      ZERO,EXIT               * continue
          GOTO      KSICD999
.
KSICD950  MOVE      TWO,EXIT                * quit
.
KSICD999  RETURN
.----------------------------------------------------------------------
. KUNIT - Keyin unit if no catalog entered
.----------------------------------------------------------------------
KUNIT000
          MOVE      SP70,SIIAISS
          PACK      KEY7,SIQACAT,SP70
          CALL      RDSIIA1                 * get default if possible
.
          MATCH     SP70,SIIAISS
          IF        @EQUAL
            KEYIN     *PCCOL:CVERT,*V2LON,*RV,SIQAUNT
            MATCH     EXITCHAR,SIQAUNT      * exitchar entered ?
            GOTO      KUNIT950 IF EQUAL
          ELSE
            MOVE      SIIAISS,SIQAUNT
            IF        ACCEPT=0
              BEEP
              CLEAR     DISPMSG
              APPEND    "Issue unit set up in catalog - ",DISPMSG
              CALL      MESSAGE1
            ENDIF
          ENDIF
.
          GOTO      KUNIT900
.
KUNIT900  MOVE      ZERO,EXIT               * continue
          GOTO      KUNIT999
.
KUNIT950  MOVE      TWO,EXIT                * quit
.
KUNIT999  RETURN
.----------------------------------------------------------------------
. KDESC - Keyin description if no catalog entered
.----------------------------------------------------------------------
KDESC000
          MOVE      SP70,SIIADES
          PACK      KEY7,SIQACAT,SP70
          CALL      RDSIIA1                 * get default if possible
.
          MATCH     SP70,SIIADES            * if no unit set - key one in
          IF        @EQUAL
            KEYIN     *PCCOL:CVERT,*V2LON,*RV,SIQAPD
            MATCH     EXITCHAR,SIQAPD       * exitchar entered ?
            GOTO      KDESC950 IF EQUAL
          ELSE
            MOVE      SIIADES,SIQAPD
            IF        ACCEPT=0
              BEEP
              CLEAR     DISPMSG
              APPEND    "Description set up in catalog - ",DISPMSG
              CALL      MESSAGE1
            ENDIF
          ENDIF
.
          GOTO      KDESC900
.
KDESC900  MOVE      ZERO,EXIT               * continue
          GOTO      KDESC999
.
KDESC950  MOVE      TWO,EXIT                * quit
.
KDESC999  RETURN
.----------------------------------------------------------------------
. KPART - Keyin part number if no catalog entered
.----------------------------------------------------------------------
KPART000
          PACK      KEY27,SIQACAT,SIIAPSC,SP70
          CALL      RSSIIC1                 * look for default
          CALL      RKSIIC1
          BRANCH    OVRCD,KPART800
          MATCH     SIQACAT,SIICCAT
          GOTO      KPART800 IF NOT EQUAL
          MATCH     SP70,SIICPNO
          GOTO      KPART800 IF EQUAL
.
          MOVE      SIICPNO,SIQAPN
          IF        ACCEPT=0
            BEEP
            CLEAR     DISPMSG
            APPEND    "Part number set up in catalog - ",DISPMSG
            CALL      MESSAGE1
          ENDIF
.
          GOTO      KPART900
.
KPART800  KEYIN     *PCCOL:CVERT,*V2LON,*RV,SIQAPN
          MATCH     EXITCHAR,SIQAPN         * exitchar entered ?
          GOTO      KPART950 IF EQUAL
.
          GOTO      KPART900
.
KPART900  MOVE      ZERO,EXIT               * continue
          GOTO      KPART999
.
KPART950  MOVE      TWO,EXIT                * quit
.
KPART999  RETURN
.----------------------------------------------------------------------
. Keyin SICA (security check)
.----------------------------------------------------------------------
KSICS000  CALL      CSICS000           * check security
.                                      * SICSSEC 0=none 1=1 cc 2=>1 cc 3=all cc
          BRANCH    SICSSEC,KSICS100,KSICS100,KSICS100
          GOTO      KSICS940
.
KSICS100  IF        SICSSEC=1
            DISPLAY   *PCCOL:CVERT,*V2LON,SICACODE;
          ELSE
            CALL      KSICSA00                   * keyin SICA
            BRANCH    EXIT,KSICS950,KSICS950
          ENDIF
.
          MATCH     SP70,SICAMES                 * display warning message
          IF        !@EQUAL
            CLEAR     DISPMSG
            APPEND    SICAMES,DISPMSG
            BEEP
            CALL      MESSAGE1
          ENDIF
.
KSICS900  MOVE      ZERO,EXIT
          GOTO      KSICS999
.
KSICS930  CLEAR     DISPMSG
          APPEND    "Cost centre locked - ",DISPMSG
          BEEP
          CALL      MESSAGE1
          BRANCH    SICSSEC,KSICS950   * access to only one cc ?
          GOTO      KSICS100
.
KSICS940  CLEAR     DISPMSG
          APPEND    "You do not have access to any cost centres - ",DISPMSG
          BEEP
          CALL      MESSAGE1
          GOTO      KSICS950
.
KSICS950  MOVE      TWO,EXIT
.
KSICS999  RETURN
.----------------------------------------------------------------------
. Check cost centre security
. Returns SICSSEC : 0 = no access to any cost centres
.                   1 = access to one cost centre
.                   2 = access to more than one cost centre
.                   3 = access to all cost centres
.----------------------------------------------------------------------
CSICS000
          MOVE      "3",SICSSEC             * check for all cc's
          PACK      KEY9,PASSCODE,SP70
          CALL      RDSICS1
          IF        OVRCD=1
            MOVE      "0",SICSSEC           * check for no cc's
            CALL      RKSICS1
            BRANCH    OVRCD,CSICS999
            MATCH     SICSUID,PASSCODE
            GOTO      CSICS999 IF NOT EQUAL
.
            PACK      KEY5,SICSCST,SP70     * read data for first cc
            CALL      RDSICA1
            MOVE      "1",SICSSEC
            CALL      RKSICS1               * check for more than one cc
            BRANCH    OVRCD,CSICS999
            MATCH     SICSUID,PASSCODE
            GOTO      CSICS999 IF NOT EQUAL
.
            MOVE      "2",SICSSEC
          ENDIF
.
CSICS999  RETURN
