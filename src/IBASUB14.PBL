          CALL     RKSIQB1
          BRANCH   OVRCD,POWP9999
          PACK     KEY16,SIQBCST,SIQBRID,SIQBLIN,SP70
          MATCH    KEY16,KEY13
          GOTO     POWP9999 IF NOT EQUAL
          CALL     DESIQB1                       * delete extra record
          GOTO     POWP3000
.
POWP9999  RETURN
.----------------------------------------------------------------------
. Keyin SICA
.----------------------------------------------------------------------
KCOST000
          PACK      SIQARID,PORT,Z70             * set up dummy id number
          BRANCH    KEEPCC,KCOST900,KCOST950     * 1=keep last cost centre
.                                                * 2=get outta here !!
          MOVE      SP70,SIQARID
          CALL      KSICS000                     * keyin SICA
          BRANCH    EXIT,KCOST950,KCOST950
.
KCOST900  MOVE      ZERO,EXIT
          MOVE      SICACODE,SIQACST
.
          MOVE      SP70,SICADESC
          PACK      KEY5,SIQACST,SP70
          CALL      RDSICA1
          GOTO      KCOST999
.
KCOST950  MOVE      TWO,EXIT
.
KCOST999
          MOVE      ZERO,KEEPCC
          RETURN
.----------------------------------------------------------------------
. Keyin SIIA
.----------------------------------------------------------------------
KSIIA000  MOVE      ONE,ETOEXIT                  * Standard Variable
          CALL      KSIIAA00                     * keyin SIIA
          BRANCH    EXIT,KSIIA500,KSIIA950
.
          MATCH     SP70,SIIACAT
          GOTO      KSIIA500 IF EQUAL
.
          PROC      CHKORD00                     * Check for Back Order etc
          BRANCH    EXIT,KSIIA000
.
          GOTO      KSIIA900
.
KSIIA500  UNPACK    SP70,SIIACAT,SIIADES         * nothing entered
          GOTO      KSIIA900
.
KSIIA900  MOVE      ZERO,EXIT
          MOVE      ONE,ACCEPT                   * go into accept mode
          MOVE      SIIACAT,SIQACAT
          COMPARE   SIIASUS,ZERO                                       *I-50167
          GOTO      KSIIA999 IF EQUAL                                  *I-50167
.         IF        SIIASUS > 0                                        *D-50167
.
          CLEAR   DISPMSG
          APPEND  "Catalog Item ",DISPMSG
          APPEND  SIIACAT,DISPMSG
          APPEND  " suspended",DISPMSG
          APPEND  " - code ",DISPMSG
          APPEND  SIIASUS,DISPMSG
          APPEND  ". ",DISPMSG
          CALL    MESSAGE1
          GOTO    KSIIA000
.
.         ENDIF                                                        *D-50167
.         GOTO      KSIIA999                                           *D-50167
.
KSIIA950  MOVE      TWO,EXIT
          LOAD      KEEPCC,SICSSEC,TWO           * quit if access to only 1 cc
.
KSIIA999  RETURN
.
.
.
          DEFPROC   CHKORD00
          INC       SINBACFD/INC
          INC       SINOQAFD/INC
          INC       SINPOCFD/INC
          INC       SINPRQFD/INC
.------------------------------------------------------------------
. Check Catalog Entered is not Already on Order or Back Order etc
.------------------------------------------------------------------
CHKORD00  CALL      CHKPR000
          BRANCH    EXIT,CHKORD99
.
          BRANCH    SIIANON,CHKORD50
.
. Stock - Check Picking Lists
.----------------------------
          CALL      CHKPL000
.
          GOTO      CHKORD99
.
. Non Stock - Check Orders
.-------------------------
CHKORD50  CALL      CHKOR000
.
CHKORD99  GOTO      CHKORDZZ
.--------------------------------------------------
. Check Outstanding Purchase Requisitions
.--------------------------------------------------
CHKPR000  PACK      KEY13,SICACODE,SP70
          OPEN      SINOQAA1,"sinoqaaf"
          CALL      RSSIQA1
CHKPR100  CALL      RKSIQA1
          BRANCH    OVRCD,CHKPR999
          MATCH     SICACODE,SIQACST
          GOTO      CHKPR900 IF NOT EQUAL
.
          MATCH     SIIACAT,SIQACAT
          GOTO      CHKPR100 IF NOT EQUAL
.
          MOVE      SP70,SECUSER
          PACK      KEY4,SIQAUID,SP70
          CALL      RDPASS1
.
          MOVE      SP70,SICDDESC
          MOVE      "PRC",SICDCATA
          PACK      KEY6,SICDCATA,SIQASTA,SP70
          CALL      RDSICD1
          UNPACK    SIQADAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
.
CHKPR200  MOVE      "16",HTOP
          MOVE      "1",HLEF
          MOVE      "80",HRIG
          MOVE      "24",HBOT
          CALL      GETSCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HRIG
          MOVE      ZERO,HBOT
          BEEP
          DISPLAY   *P1:16,*EF
          HLINE     *G37,16,1,80
          HLINE     *G37,23,1,80
          BEEP
          DISPLAY  *P20:17,*V2LON,*ULON:
                   "WARNING: Outstanding Purchase Req. for this Catalog"
          DISPLAY  *P1:18,"Purchase Req : ",*V2LON,SIQARID,*HOFF:
                   *P1:19,"Date         : ",*V2LON,CPCDATE,*HOFF:
                   *P1:20,"Ordered by   : ",*V2LON,SECUSER,*HOFF:
                   *P1:21,"Status       : ",*V2LON,SICDDESC,*HOFF:
                   *P1:22,"Quantity     : ",*V2LON,SIQAQTY
          KEYIN    *P1:24,"Do you want to request more (":
                           *V2LON,"Y",*HOFF,"/":
                           *V2LON,"N",*HOFF,") ? ",*V2LON,ANS
          CALL     PUTSCR00
          REP      UPPLOW,ANS
          MATCH    ANSY,ANS
          GOTO     CHKPR900 IF EQUAL
          MATCH    ANSN,ANS
          GOTO     CHKPR200 IF NOT EQUAL
          MOVE     ONE,EXIT
          GOTO     CHKPR999
.
CHKPR900  MOVE      ZERO,EXIT
CHKPR999  RETURN
.------------------------------------------------------------
. Check For Items on Order
.------------------------------------------------------------
CHKOR000  OPEN      SINPOCA5,"sinpocaf"
          PACK      KEY23,SICACODE,ONE,SP70
          CALL      RSSIPC5
CHKOR100  CALL      RKSIPC5
          BRANCH    OVRCD,CHKOR900
          MATCH     SICACODE,SIPCCST
          GOTO      CHKOR900 IF NOT EQUAL
          MATCH     SP70,SIPCEDD
          GOTO      CHKOR100 IF EQUAL
          MATCH     SIIACAT,SIPCCAT
          GOTO      CHKOR100 IF NOT EQUAL
.
CHKOR200  MOVE      "16",HTOP
          MOVE      "1",HLEF
          MOVE      "80",HRIG
          MOVE      "24",HBOT
          CALL      GETSCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HRIG
          MOVE      ZERO,HBOT
          BEEP
          DISPLAY   *P1:16,*EF
          HLINE     *G37,16,1,80
          HLINE     *G37,23,1,80
          BEEP
          DISPLAY  *P20:17,*V2LON,*ULON:
                   "WARNING: Outstanding Order for this Catalog"
          UNPACK   SIPCEDD,CCENT,CYEAR,CMON,CDAY
          CALL     PACDATE
          DISPLAY  *P1:18,"Purchase Order    : ",*V2LON,SIPCPON,*HOFF:
                   *P1:19,"Purchase Line     : ",*V2LON,SIPCITM,*HOFF:
                   *P1:20,"Expected Delivery : ",*V2LON,CPCDATE,*HOFF:
                   *P1:21,"Quantity          : ",*V2LON,SIPCOQT,*HOFF:
                   *P1:22,"Price             : ",*V2LON,SIPCECT
          KEYIN    *P1:24,"Do you want to request more (":
                           *V2LON,"Y",*HOFF,"/":
                           *V2LON,"N",*HOFF,") ? ",*V2LON,ANS
          CALL     PUTSCR00
          REP      UPPLOW,ANS
          MATCH    ANSY,ANS
          GOTO     CHKOR900 IF EQUAL
          MATCH    ANSN,ANS
          GOTO     CHKOR200 IF NOT EQUAL
          MOVE     ONE,EXIT
          GOTO     CHKOR999
.
CHKOR900  MOVE     ZERO,EXIT
CHKOR999  RETURN
.------------------------------------------------------------
. Check For Items on Requisition
.------------------------------------------------------------
CHKPL000  OPEN      SINPRQA2,"sinprqaf"
          PACK      KEY83,SIIACAT,SP70
          CALL      RSSIPR2
CHKPL100  CALL      RKSIPR2
          BRANCH    OVRCD,CHKPL190
          MATCH     SIIACAT,SIPRCAT
          GOTO      CHKPL190 IF NOT EQUAL
          MATCH     SICACODE,SIPRCST
          GOTO      CHKPL100 IF NOT EQUAL
.
CHKPL110  MOVE      "16",HTOP
          MOVE      "1",HLEF
          MOVE      "80",HRIG
          MOVE      "24",HBOT
          CALL      GETSCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HRIG
          MOVE      ZERO,HBOT
          BEEP
          DISPLAY   *P1:16,*EF
          HLINE     *G37,16,1,80
          HLINE     *G37,23,1,80
          BEEP
          DISPLAY  *P20:17,*V2LON,*ULON:
                   "WARNING: Outstanding Requisition for this Catalog"
          DISPLAY  *P1:18,"Requisition No.   : ",*V2LON,SIPRTYP,SIPRREQ,*HOFF:
                   *P1:19,"Delivery          : ",*V2LON,SIPRDEL,*HOFF:
                   *P1:20,"Quantity Req.     : ",*V2LON,SIPRRQT,*HOFF:
                   *P1:21,"Issue Qty         : ",*V2LON,SIPRAQT,*HOFF:
                   *P1:22,"Backorder Qty     : ",*V2LON,SIPRBQT
          KEYIN    *P1:24,"Do you want to request more (":
                           *V2LON,"Y",*HOFF,"/":
                           *V2LON,"N",*HOFF,") ? ",*V2LON,ANS
          CALL     PUTSCR00
          REP      UPPLOW,ANS
          MATCH    ANSY,ANS
          GOTO     CHKPL190 IF EQUAL
          MATCH    ANSN,ANS
          GOTO     CHKPL110 IF NOT EQUAL
          MOVE     ONE,EXIT
          GOTO     CHKPL999
.
CHKPL190  OPEN     SINBACA1,"sinbacaf"
          MOVE     ZERO,F12P2
          PACK     KEY25,SICACODE,SIIACAT,SP70
          CALL     RSSIBA1
CHKPL200  CALL     RKSIBA1
          BRANCH   OVRCD,CHKPL290
          MATCH    SIBACST,SICACODE
          GOTO     CHKPL290 IF NOT EQUAL
          MATCH    SIBACAT,SIIACAT
          GOTO     CHKPL290 IF NOT EQUAL
.
          ADD      SIBAQTY,F12P2
          GOTO     CHKPL200
CHKPL290  COMPARE  ZERO,F12P2
          GOTO     CHKPL900 IF EQUAL
.
          MOVE      "20",HTOP
          MOVE      "1",HLEF
          MOVE      "80",HRIG
          MOVE      "24",HBOT
          CALL      GETSCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HRIG
          MOVE      ZERO,HBOT
          BEEP
          DISPLAY   *P1:20,*EF
          HLINE     *G37,20,1,80
          HLINE     *G37,23,1,80
          DISPLAY  *P20:21,*V2LON,*ULON:
                   "WARNING: Back Order Requisition Exist for this Catalog"
          DISPLAY  *P1:22,"Back Order Qty    : ",*V2LON,F12P2
          KEYIN    *P1:24,"Do you want to request more (":
                           *V2LON,"Y",*HOFF,"/":
                           *V2LON,"N",*HOFF,") ? ",*V2LON,ANS
          CALL     PUTSCR00
          REP      UPPLOW,ANS
          MATCH    ANSY,ANS
          GOTO     CHKPL900 IF EQUAL
          MATCH    ANSN,ANS
          GOTO     CHKPL290 IF NOT EQUAL
          MOVE     ONE,EXIT
          GOTO     CHKPL999
.
CHKPL900  MOVE     ZERO,EXIT
.
CHKPL999  RETURN
.
          INC       SINBACIO/INC
