.----------------------------------------------------------------------
. Post Data - note SIQA contains a dummy id until next id no. is read
.----------------------------------------------------------------------
POST0000  MOVE      "1",KEEPCC
.
          PACK      SIQACAT,SIQACAT,SP70    * check if any data to post
          MATCH     SP70,SIQACAT
          GOTO      POST1000 IF NOT EQUAL
.
          PACK      SIQAPD,SIQAPD,SP70
          MATCH     SP70,SIQAPD
          GOTO      POST1000 IF NOT EQUAL
.
          PACK      SIQAPN,SIQAPN,SP70
          MATCH     SP70,SIQAPN
          GOTO      POST1000 IF NOT EQUAL
.
          PACK      SIQAUNT,SIQAUNT,SP70
          MATCH     SP70,SIQAUNT
          GOTO      POST1000 IF NOT EQUAL
.
          CLOSE     TEMPWORD                     * open temp file
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      TEMPWORD,FILSIQB1
          TRAPCLR   IO
          COMPARE   ONE,OVRCD
          GOTO      POST0200 IF EQUAL
.
POST0100  READ      TEMPWORD,SEQ;SIQBCOM
          GOTO      POST0200 IF OVER
.
          PACK      SIQBCOM,SIQBCOM,SP70
          MATCH     SP70,SIQBCOM
          GOTO      POST0100 IF EQUAL
          GOTO      POST1000
.
POST0200  CLEAR     DISPMSG
          APPEND    "No Data to Post - ",DISPMSG
          CALL      MESSAGE1
          GOTO      POST9999
.
POST1000  PACK      KEY5,SIQACST,SP70            * lock cost centre
          REPEAT
            CALL      RLSICA1
            BRANCH    OVRCD,POST9999             * should be there !!
            IF        OVRCD=2
              PACK      DISPMSG,SP70,SP70
              CALL      MESSAGE2
              KEYIN     *P1:24,*EL,"Cost Centre Locked - (":
                        *V2LON,"R",*HOFF,")etry or e(":
                        *V2LON,"X",*HOFF,")it ? ",*V2LON,ANS
              CALL      MESSAGE3
              PACK      ANS,ANS,SP70
              REP       UPPLOW,ANS
              MATCH     "E",ANS
              GOTO      POST9999 IF EQUAL
              MATCH     "X",ANS
              GOTO      POST9999 IF EQUAL
            ENDIF
          UNTIL     OVRCD=0
.
          MOVE      ZERO,F8                 * get next id no
          MOVE      SICARID,F8
          REPEAT
            ADD       ONE,F8
            MOVE      F8,SICARID
            REP       " 0",SICARID
            PACK      KEY13,SIQACST,SICARID,SP70
            UNPACK    KEY13,SIQACST,SIQARID
            CALL      RDSIQA1
          UNTIL     OVRCD=1
          CALL      UPSICA1
          CALL      UUSICA1
.
          CALL      POWP0000                * post wp data
.
          PACK      SIQARID,SICARID,SP70    * save data
          PACK      SIQAUID,PASSCODE,SP70
          PACK      SIQADAT,CCC,CYY,CMM,CDD,SP70
          REP       " 0",SIQADAT
          CLOCK     TIME,SIQATIM
          PACK      SIQAPOR,PORT,SP70
          CALL      WRSIQA1
.
POST9999  RETURN
