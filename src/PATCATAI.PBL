              MATCH     SAVDATE1,SAVDATE2
              IF        !@EQUAL
                CALL      GJGB0000
              ENDIF
            ENDIF
            MOVE      SAVDATE2,SAVDATE1
            MOVE      PTDTUNIQ,SAVEUNIQ
          ENDIF
