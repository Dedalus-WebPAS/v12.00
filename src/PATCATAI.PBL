          GOTO      D5WEB100 IF EQUAL
.
          IF        PTCNINVL=24 | PTCNINVL=25
            MATCH     SP70,RCBDPAYD
            IF        !@EQUAL
              PACK      KEY25,RCBDPAYD   * Payee details
            ENDIF
          ENDIF
          GOTO      D5WEB200
