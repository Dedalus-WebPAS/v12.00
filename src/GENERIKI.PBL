.   Program       : GENERIKI
.   Modifications : 
.   V10.00.00 15/02/1011  Mike Laming   CAR 230710
.              Created program
.----------------------------------------------------------------------
.
.----------------------------------------------------------------------
.  data includes
.----------------------------------------------------------------------
.
          INC       STDGENDF
          INC       PRIITMFD/INC
          INC       PRIIKIFD/INC
.
.----------------------------------------------------------------------
.  local variables
.----------------------------------------------------------------------
.
ITEMTYP   DIM       2
ITEMNO    DIM       9
.
PRGID     INIT      "GENERIKI"
PRGNAM    INIT      "Fix priikiaf"
VERSION   INIT      "V12.00.00"
.
.----------------------------------------------------------------------
.  program code
.----------------------------------------------------------------------
.
.---- initialise screen and confirm desire to execute program ----
.
ML0000    CALL      DISPHEAD
.
          DISPLAY   *P1:10,"Generate AMA/MBD Item Keyword records."
          CALL      CONTQST
          SUB       ONE,CEXIT
          BRANCH    CEXIT,ML9999,ML9999
.
.---- open files ----
.
          OPEN      PRIITEM1,"priitemf"
          OPEN      PRIIKIA1,"priikiaf"
          OPEN      PRIIKIA2,"priikiaf"
.
.---- loop through priikiaf ----
.
          MOVE      ZERO,SECTOR
.
          PACK      KEY22,SP70
ML1000    CALL      RSPRIT1
ML1100    CALL      RKPRIT1
          BRANCH    OVRCD,ML9999
.
          ADD       ONE,SECTOR
          IF        SECTOR%100=1
            DISPLAY   *P1:24,*V2LON,SECTOR," - ",PRITITMN,*EF;
          ENDIF
.
          MOVE      PRITFLAG,ITEMTYP        * Input var for PRIIKIUP
          MOVE      PRITITMN,ITEMNO         * Input var for PRIIKIUP
          PROC      PRIIKIUP
          GOTO      ML1100
.
.---- exit program ----
.
ML9999    CHAIN     PGM
.----------------------------------------------------------------------
.  code includes
.----------------------------------------------------------------------
.
          INC       PRIITMIO/INC
          INC       PRIIKIIO/INC
          INC       PRIIKIPR
          INC       STDGENCD
.
