.-------------------------------------------------------------
. Link to Prev Group (Printer & Printer Department)
.-------------------------------------------------------------
PREVGR00  MOVE      NORECORD,KEY2
          REP       " +",KEY2
          ASSIGN    STARTNUM-NORECORD,F3
          IF        F3<0
            MOVE      ZERO,F3
          ENDIF
          MOVE      F3,KEY3
          REP       " +",KEY3
          UNPACK    DIM127,D1,FLDPARA,DIM127
          MOVE      REPORTNO,F2
.
          REP       " +",STARTKEY
          WRITE     HTMLFILE;"comweb05.pbl":
                                 "?reportno=",F2:
                                 "&template=",FLDPARA:
                                 "&startnum=",KEY3:
                                 "&startkey=",STARTKEY:
                                 "&deprt001=",DEPRT001:
                                 "&norecord=",KEY2:
                                 "&filthosp=",FILTHOSP;               *I-251218
          REP       "+ ",STARTKEY
.
PREVGR99  RETURN
.
