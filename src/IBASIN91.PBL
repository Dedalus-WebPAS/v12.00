.********************************************************************
. PRNT - print report for SIBA                      Called by ML
.         Requires : FROMCATA,TOCATA (print range display labels)
.                    FROMCOD7,TOCOD7 (print range)
.         Returns  : EXIT            (1=illegal range entered)
.********************************************************************
PRNT0000  DISPLAY   *P1:24,*EL,*P50:24,"Printing :   ";
          MOVE      ZERO,CPAGENO                 * reset page no.
          CLOCK     TIME,CTIMEIS                 * get current time
          MOVE      ZERO,TOTALREC
          PACK      LASTCOD7,SP70
          PACK      LASTCOD5,SP70
          MOVE      "999",CLNO                   * init last line number
.
          CALL      MAKEXP00                     * Create export file
          CALL      FILE0000                    * go to correct position in file
.
PRNT1000  CALL      NREC0000                     * get next record
          BRANCH    OVRCD,PRNT8000               * end of file ?
.
          MATCH     STRWAR,SIBAWAR
          GOTO      PRNT1000 IF LESS             * warehouse out of range ?
.
          MATCH     SIBAWAR,ENDWAR
          GOTO      PRNT1000 IF LESS             * warehouse out of range ?
.
. check warehouse security
.
          MATCH     SP70,SIBAWAR
          IF        !@EQUAL
            PACK      KEY9,PASSCODE,SIBAWAR,SP70
            CALL      RDSIWS1
            BRANCH    OVRCD,PRNT1000
          ENDIF
.
          DISPLAY   *P61:24,*V2LON,SIBACAT,SLASH,SIBACST,*EL;
.
          CALL      WRIT0000                     * print details
          GOTO      PRNT1000                     * get next record
.
PRNT8000  COMPARE   ZERO,CPAGENO
          CALL      HEAD0000 IF EQUAL          * print header if nothing printed
.
          MATCH     SP70,LASTCOD7
          GOTO      PRNT8900 IF NOT EQUAL        * print end of cost centre
.
          MATCH     SP70,LASTCOD5
          GOTO      PRNT8900 IF NOT EQUAL        * print end of cost centre
.
PRNT8100  CALL      UND132                       * print line
          PRINT     "****   End of Report   ****"
.
          CALL      TRAN0000
          GOTO      PRNT9000
.
PRNT8900  CALL      ECCA0000
          GOTO      PRNT8100
.
PRNT9000  MOVE      ZERO,EXIT                    * exit screen
          GOTO      PRNT9999
.
PRNT9500  MOVE      ONE,EXIT                     * continue
.
PRNT9999  RETURN
