            CALL      UND132
            CALL      NCCA0000                   * print catalog heading
            CALL      UND132
          ENDIF
.
          GOTO      WRIT0300
.
WRIT0200  MATCH     SP70,LASTCOD5
          CALL      ECCA0000 IF NOT EQUAL        * print end of catalog
          PACK      LASTCOD5,SIBAWAR,SP70
          IF        40<CLNO
            MOVE      "999",CLNO
          ELSE
            CALL      UND132
            CALL      NCCA0000                   * print catalog heading
            CALL      UND132
          ENDIF
.
          GOTO      WRIT0300
