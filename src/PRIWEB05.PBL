SEQU0000  MOVE      PRTDSEQN,KEY3
          MOVE      PRTDTYPE,KEY1
          MOVE      PRTDTMID,KEY4
          WRITE     HTMLFILE;"<td><select name=dummy ":
                             "onChange=setSeq(this);>"
          PACK      KEY8,PRTDTMID,PRTDTYPE,SP70
          CALL      RDPRTDT1
