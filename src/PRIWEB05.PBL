          BRANCH    OVRCD,SEQU9000
.
          MATCH     PRTDTMID,KEY4
          GOTO      SEQU9000 IF NOT EQUAL
.
          MATCH     PRTDTYPE,KEY1
          GOTO      SEQU9000 IF NOT EQUAL
.
          MOVE      PRTDSEQN,FORM3 
          MATCH     PRTDSEQN,KEY3
          IF        @EQUAL
            WRITE     HTMLFILE;"<option value=#"",PRTDSEQN,"#" selected>":
                               FORM3,"</option>"
          ELSE
            WRITE     HTMLFILE;"<option value=#"",PRTDSEQN,"#">":
                               FORM3,"</option>"
          ENDIF
          GOTO      SEQU0100
.
SEQU9000  WRITE     HTMLFILE;"</select>"
          PACK      KEY8,KEY4,KEY1,KEY3,SP70
          CALL      RSPRTDT1
          MOVE      KEY3,PRTDSEQN
          MOVE      KEY1,PRTDTYPE
          MOVE      KEY4,PRTDTMID

