            MOVE      QRYDATA,DELTSTKY
            GOTO      SETPAR99
          ENDIF
.
          MATCH     "newseqnc=",QRYNAME
          IF        @EQUAL
            MOVE      QRYDATA,NEWSEQNC
            PACK      NEWSEQNC,NEWSEQNC,SP70
            GOTO      SETPAR99
          ENDIF
.
