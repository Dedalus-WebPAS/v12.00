          IF        @EQUAL
            MOVE      QRYDATA,VALDEFDT
            TYPE      VALDEFDT
            IF        @EQUAL
              PACK      VALDEFDT,VALDEFDT,SP70
            ELSE
              UNPACK    QRYDATA,CDAY,KEY1,MTHNAM3,KEY1,CCENT,CYEAR
              CALL      SETMTH00                * To Format Month
              PACK      VALDEFDT,CCENT,CYEAR,CMON,CDAY
            ENDIF
            GOTO      SETPAR99
          ENDIF
.
          MATCH     "primg",QRYNAME
          IF        @EQUAL
            CALL      STMGP000
            GOTO      SETPAR99 
          ENDIF
.
          MATCH     "pritm",QRYNAME
          IF        @EQUAL
            CALL      STITM000
            GOTO      SETPAR99 
          ENDIF
.
          MATCH     "prigl",QRYNAME
          IF        @EQUAL
            CALL      STIGL000
            GOTO      SETPAR99 
