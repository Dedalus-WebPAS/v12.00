          MOVE     SCPSITM,FIELDNO
          BRANCH    FIELDNO,PS00L999,PS00L999,PS00L999,PS00L999:
                            PS00L999,PS00L101,PS00L999,PS00L999:
                            PS00L999,PS00L999,PS00L999,PS00L999:
                            PS00L999,PS00L999
          GOTO     PS00L999
.
PS00L101
          MOVE      SECUSER,VAR      
          CALL      DISPITEM
          GOTO     PS00L999
PS00L999
          RETURN
.---------------------------------------------------------
. Selections for : Main Screen                        
.---------------------------------------------------------
SF00L000  PACK     KEY13,PRGID,SCRID,SP70
          CALL     RSSCSL1
SF00L001  BRANCH   ACCEPT,SF00L020
.
SF00L005  CALL     MTSTQ000
          MOVE     SCSLITM,FIELDNO
          BRANCH    FIELDNO,SF00L006,SF00L101,SF00L102,SF00L103:
                            SF00L006,SF00L006,SF00L104,SF00L006:
                            SF00L105,SF00L106,SF00L107,SF00L108:
                            SF00L109,SF00L110
SF00L006  BEEP
          GOTO     SF00L005
SF00L020  CALL     RKSCSL1
          BRANCH   OVRCD,SF00L090
          MATCH    PRGID,SCSLPID
          GOTO     SF00L090 IF NOT EQUAL
          MATCH    SCRID,SCSLSID
          GOTO     SF00L090 IF NOT EQUAL
          MOVE     SCSLITM,FIELDNO
          BRANCH    FIELDNO,SF00L020,SF00L020,SF00L020,SF00L020:
                            SF00L020,SF00L020,SF00L104,SF00L020:
                            SF00L020,SF00L020,SF00L020,SF00L020:
                            SF00L020,SF00L020
          GOTO     SF00L020
SF00L090  MOVE     ZERO,ACCEPT
          MOVE      "SIWS1",NAMSSFFI
          MOVE      "    7",ITMSSFFI
          CALL      FP00L000
          GOTO     SF00L001
.
SF00L101
          MOVE      ONE,EXIT
          GOTO      SF00L999
.
SF00L102
          MOVE      ONE,EXIT
          GOTO      SF00L999
.
SF00L103
          CALL      POST0000
          MOVE      ONE,EXIT
          GOTO      SF00L999
.
SF00L104
          CALL      SELECTED
          BRANCH    EXIT,SF00L001
          MOVE      "SIWS1",NAMSSFFI
          MOVE      SCPSITM,ITMSSFFI
          IF        ACCEPT=0
            PACK      KEY5,SCPSROW,SCPSCOL
            CALL      TRSIWS11
            BRANCH    OVRCD,SF00L001
            MATCH     SCTMROW,SCSLROW
            GOTO      SF00L001 IF NOT EQUAL
            MATCH     SCTMCOL,SCSLCOL
            GOTO      SF00L001 IF NOT EQUAL
            CALL      SM00L000 * Change Line
          ELSE
            IF        SIWS1AFL=0
              MOVE      "SIWS1",NAMSSFFI
              MOVE     "    7",ITMSSFFI
              CALL      AD00L000 * Add Line
            ENDIF
          ENDIF
          GOTO     SF00L001
.
SF00L105
          MOVE      "SIWS1",NAMSSFFI
          MOVE      "    7",ITMSSFFI
          CALL      NP00L000
          GOTO     SF00L001
.
SF00L106
          MOVE      "SIWS1",NAMSSFFI
          MOVE      "    7",ITMSSFFI
          CALL      PP00L000
          GOTO     SF00L001
.
SF00L107
          MOVE      "SIWS1",NAMSSFFI
          MOVE      "    7",ITMSSFFI
          CALL      AD00L000
          GOTO     SF00L001
.
SF00L108
          CALL      DELE0000
          GOTO      SF00L001
.
SF00L109
          MOVE      "SIWS1",NAMSSFFI
          MOVE      "    7",ITMSSFFI
          CALL      FP00L000
          GOTO     SF00L001
.
SF00L110
          MOVE      "SIWS1",NAMSSFFI
          MOVE      "    7",ITMSSFFI
          CALL      LP00L000
          GOTO     SF00L001
SF00L990  MOVE      ONE,EXIT * Exit Char Restart
SF00L999  RETURN
.@@@@@ Function Restart Point @@@@@
.----------------------------------------------------------
. Display Warehouse Description
.----------------------------------------------------------
DWAR0000  PACK      SIWADES,SP70
          PACK      KEY5,SIWSWAR,SP70
          CALL      RDSIWA1
.
DWAR9999  RETURN
.----------------------------------------------------------
. Save Changes
.----------------------------------------------------------
. delete record before write
.
POST0000  PACK      KEY9,SECODE,SP70
POST0100  CALL      RSSIWS1
          CALL      RKSIWS1
          BRANCH    OVRCD,POST1000
.
          MATCH     SIWSUID,SECODE
          GOTO      POST1000 IF NOT EQUAL
.
          PACK      KEY9,SIWSUID,SIWSWAR,SP70
          CALL      DESIWS1
.
          GOTO      POST0100
.
. write record
.
POST1000  PACK      KEY5,SP70,SP70
          CALL      TRSIWS11
          COMPARE   ZERO,OVRCD
          GOTO      POST1200 IF EQUAL
.
POST1100  CALL      TKSIWS11
          BRANCH    OVRCD,POST9999
.
POST1200  PACK      KEY9,SECODE,SIWSWAR,SP70
          UNPACK    KEY9,SIWSUID,SIWSWAR
          CALL      RASIWS1
          IF        OVRCD=0
            CALL      UPSIWS1
          ELSE
            CALL      WRSIWS1
          ENDIF
.
          GOTO      POST1100
.
POST9999  RETURN
.----------------------------------------------------------
. Enter Security Code
.----------------------------------------------------------
ESEC0000  CALL      KIBPSA00
          MOVE      SECODE,SIWSUID
.
ESEC9999  RETURN
.----------------------------------------------------------
. Enter Warehouse Code
.----------------------------------------------------------
EWAR0000  PACK      SIWAWAR,SP70
          CALL      KSIWAA00
          MOVE      SIWAWAR,SIWSWAR
.
EWAR9999  RETURN
.---------------------------------------------------------
. Delete Warehouse Details (copied from original generated
. function called DL00L000, so can display an appropriate
. Description to user.
.---------------------------------------------------------
..          MOVE      SCSLDES,DISPMSG
DELE0000  MOVE      "Select Item to Delete ",DISPMSG
          CALL      MESSAGE2
DELE0100
          CALL      MDELQ000
          BRANCH    EXIT,DELE9900,DELE9900
          MOVE      SCSLITM,FIELDNO
          MATCH     ITMSSFFI,SCSLITM
          GOTO      DELE0200 IF NOT EQUAL
          CALL      SELECTED
          MATCH     "SIWS1",SCPSMTY
          IF        @EQUAL
            PACK      KEY5,SCSLROW,SCSLCOL
            CALL      TRSIWS11
            IF        OVRCD=0
              CALL      TDSIWS12
            ELSE
              BEEP
              GOTO      DELE0100
            ENDIF
            PACK      KEY5,SP1,ONE,SP70
            CALL      TSSIWS11
            CALL      TKSIWS11
            MOVE     KEY5,SIWS1NXT
            CALL      CT00L000
            MOVE      SIWS1NXT,KEY5
            CALL      TSSIWS12
            CALL      TPSIWS12
            CALL      DP00L000
            GOTO      DELE0100
          ENDIF
DELE0200  MOVE      "Invalid Selection. - ",DISPMSG
          CALL      MESSAGE1
          GOTO      DELE0100
.
DELE9900  CALL      MESSAGE3
.
DELE9999  RETURN
.
