          IF        !@EQUAL
            MOVE      SP70,ALRTREAD
            MOVE      "wn",KEY2
            PACK      KEY5,KEY2,PTALREAC       * Key is category
            CALL      RDCODE1                  * Read patient codes file
            IF        OVRCD=0
              MOVE      TDESC,ALRTREAD
            ENDIF
            STRIP     ALRTREAD
            WRITE     HTMLFILE;*+,", #"reaction#": [ { " :
                      "  #"manifestation#": [ {":
                      "    #"coding#": [ {":
                      "      #"system#": #"%BASEURL/ValueSet/Category-wn#",":
                      "      #"code#": #"",PTALREAC,"#",":
                      "      #"display#": #"",ALRTREAD,"#"":
                      "    } ]":
                      "  } ]":
                      "} ]";
          ENDIF
          GOTO      PALT9999
.
