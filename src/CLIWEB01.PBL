            WRITE     HTMLFILE;*+,"   #"reporter#": {":
                      "#"reference#": #"Practitioner/HCP-",PTALRQBY,"#" },";
          ENDIF
          GOTO      PALT9999
.
PALT4300  MOVE      ZERO,F1
          PACK      KEY19,PTALURNO,PTALCATG,PTALCODE,PTALCNTR,SP70
          CALL      RSPMALN1              * alert comments
