          BRANCH    OVRCD,PALT4390
          MATCH     PURNO,PMANURNO
          GOTO      PALT4390 IF NOT EQUAL
          MATCH     PTALCATG,PMANACAT
          GOTO      PALT4390 IF NOT EQUAL
          MATCH     PTALCODE,PMANACOD
          GOTO      PALT4390 IF NOT EQUAL
          MATCH     PTALCNTR,PMANCNTR
          GOTO      PALT4390 IF NOT EQUAL
          IF        F1=0
            MOVE      ONE,F1
            WRITE     HTMLFILE;*+,",   #"note#": [ { #"text#" : #"";
          ENDIF
          WRITE     HTMLFILE;*+,PMANCOMM;
          GOTO      PALT4310
.
PALT4390  IF        F1=1
            WRITE     HTMLFILE;*+,"#"} ] ";
          ENDIF
          GOTO      PALT9999
.
