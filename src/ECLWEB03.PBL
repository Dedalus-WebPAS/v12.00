          CALL      WEBINT00       * Initialise Fifo Etc.
.
          CALL      INIT0000       * Open Files
.
MAIN0000  COMMIT
.
          BEGIN
.
          DISPLAY   *W2
.
          MATCH     "1",PTCNWSIN
          GOTO      MAIN0000 IF NOT EQUAL
.
          COMPARE   GETTOKF4,GATTIMER
          IF        @LESS
            ADD       ONE,GATTIMER
          ENDIF
.
          MOVE      ZERO,EXIT
          PACK      KEY71,SP70,SP70
          CALL      RSWBB2B1
MAIN1000  CALL      RKWBB2B1
          BRANCH    OVRCD,MAIN9999
.
          MATCH     SP70,WBB2DPTH
          GOTO      MAIN1000 IF EQUAL
.
          CALL      PROPN000
.
          MATCH     "1",PRODAIND
          GOTO      MAIN1000 IF EQUAL
.
          OPEN      WBSPROPF,PRDAPATH
          OPEN      WBSACTVF,ACTVPATH
          OPEN      WBSTOKNF,TOKNPATH
          OPEN      WBSACTIF,ACTIPATH
          OPEN      WBSTOKIF,TOKIPATH 
          OPEN      WBSACTEF,ACTEPATH
.
. **********************************************
. POLL webPAS_PRODA_ActivateDevice_curl_response
. **********************************************
.
MAIN2000  MOVE      SP70,WBSACTIN
.
          READ      WBSACTIF,SEQ;WBSACTIN
          GOTO      MAIN3000 IF OVER
.
          MATCH     "1",WBSACTIN
          GOTO      MAIN3000 IF NOT EQUAL
.
          MOVE      SP70,WBSACTIN

          CLOSE     WBSACTIF
          OPEN      WBSACTIF,ACTIPATH
.
          WRITE     WBSACTIF,SEQ;WBSACTIN
.
          CALL      PRDACT00
.
          CALL      IBACLOCK
          PACK      WBB2UDAT,CCC,CYY,CMM,CDD
          REP       " 0",WBB2UDAT
          MOVE      CTIMEIS,WBB2UTIM
          MOVE      USERID,WBB2UUID
.
          CALL      UPWBB2B1
.
. **********************************************
. POLL webPAS_PRODA_GetAccessToken_curl_response
. **********************************************
.
MAIN3000  MOVE      SP70,WBSTOKIN
.
          READ      WBSTOKIF,SEQ;WBSTOKIN
          GOTO      MAIN4000 IF OVER
.
          MATCH     "1",WBSTOKIN
          GOTO      MAIN4000 IF NOT EQUAL
.
          MOVE      SP70,WBSTOKIN
.
          CLOSE     WBSTOKIF
          OPEN      WBSTOKIF,TOKIPATH
.
          WRITE     WBSTOKIF,SEQ;WBSTOKIN
.
          CALL      PRDTKN00
.
          CALL      IBACLOCK
          PACK      WBB2UDAT,CCC,CYY,CMM,CDD
          REP       " 0",WBB2UDAT
          MOVE      CTIMEIS,WBB2UTIM
          MOVE      USERID,WBB2UUID
.
          CALL      UPWBB2B1
.
. ************************************
. Send GetAccessToken every 30 minutes
. ************************************
.
MAIN4000  COMPARE   GETTOKF4,GATTIMER
          GOTO      MAIN5000 IF LESS
.
          SCAN      "ERROR",WBB2ADKS
          GOTO      MAIN5000 IF EQUAL
.
          MATCH     SP70,WBB2DPTH
          GOTO      MAIN5000 IF EQUAL
          GOTO      MAIN5000 IF EOS
.
          CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_PRODA_run GetAccessToken ",CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/",CMDLINE
          RESET     CMDLINE
.
          EXECUTE   CMDLINE,TASKID 
.
. *******************************
. Check and Send RefreshKeyDevice
. *******************************
.
MAIN5000  SCAN      "ERROR",WBB2ADKS
          GOTO      MAIN6000 IF EQUAL
.
          MATCH     SP70,WBB2ADKE
          GOTO      MAIN6000 IF EQUAL
          GOTO      MAIN6000 IF EOS
.
          CALL      IBACLOCK
          PACK      CDYSFDTE,CCC,CYY,CMM,CDD
          REP       " 0",CDYSFDTE
.
          UNPACK    WBB2ADKE,CCENT,CYEAR,ANS,CMON,ANS,CDAY
          PACK      CDYSTDTE,CCENT,CYEAR,CMON,CDAY
          REP       " 0",CDYSTDTE
.
          CALL      CALCDAYS
.
          COMPARE   REFDEVF6,CDYSDAYS
          GOTO      MAIN6000 IF NOT LESS
.
          MATCH     SP70,WBB2DPTH
          GOTO      MAIN6000 IF EQUAL
          GOTO      MAIN6000 IF EOS
.
          CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_PRODA_run RefreshKeyDevice ",CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/",CMDLINE
          RESET     CMDLINE
.
          EXECUTE   CMDLINE,TASKID
.
. ************************************************
. Email Service for approaching device expiry date
. ************************************************
.
MAIN6000  MATCH     SP70,WBB2DEDT
          GOTO      MAIN9000 IF EQUAL
          GOTO      MAIN9000 IF EQUAL
.
          READ      WBSACTEF,SEQ;WBSACTE1,WBSACTE2,WBSACTE3
          GOTO      MAIN9000 IF OVER
.
          MOVE      SP70,KEY3
          PACK      KEY3,WBSACTE1,WBSACTE2,WBSACTE3,SP70
.
          CALL      IBACLOCK
          PACK      CURRDATE,CCC,CYY,CMM,CDD
          REP       " 0",CDYSFDTE
.
          UNPACK    WBB2DEDT,CCENT,CYEAR,ANS,CMON,ANS,CDAY
          PACK      CDYSTDTE,CCENT,CYEAR,CMON,CDAY
          REP       " 0",CDYSTDTE
.
          CALL      CALCDAYS
.
          COMPARE   CDYSDAYS,THIRTY
.         GOTO      MAIN9000 IF LESS
          IF        @LESS
.
            CLOSE     WBSACTEF
            OPEN      WBSACTEF,ACTEPATH
.
            MATCH     "117",KEY3
            IF        !@EQUAL
              WRITE     WBSACTEF,SEQ;ONE,ONE,SEVEN
            ENDIF
.
            GOTO      MAIN9000
          ENDIF
.
          CALL      CLWBSR00
          CALL      RDWBSR00
.
          IF        CDYSDAYS <= THIRTY & CDYSDAYS > TEN4 
.
            MATCH     "1",WBSACTE1
            IF        @EQUAL
.
              UNPACK    WBB2DEDT,DIM10
.
              CLEAR     CMDLINE
              APPEND    "wbsprodd.us1",CMDLINE  
              APPEND    SP1,CMDLINE
              APPEND    WBB2DNAM,CMDLINE
              APPEND    SP1,CMDLINE
              APPEND    DIM10,CMDLINE
              APPEND    SP1,CMDLINE
              APPEND    CDYSDAYS,CMDLINE
              APPEND    SP1,CMDLINE
              APPEND    WBSNOTEM,CMDLINE
.
              APPEND    SP1,CMDLINE
              MOVE      WBB2ORGN,DIM40
              SQUEEZE   DIM40
              APPEND    DIM40,CMDLINE
.
              APPEND    SP1,CMDLINE
              MOVE      WBSPRORG,DIM20
              APPEND    DIM20,CMDLINE
.
              APPEND    SP70,CMDLINE
              RESET     CMDLINE
              EXECUTE   CMDLINE,TASKID
            ENDIF
            MOVE      SP70,WBSACTE1
.
          ELSE
.
            IF        CDYSDAYS <= TEN4 & CDYSDAYS > SEVEN
.
              MATCH     "1",WBSACTE2
              IF        @EQUAL
.
                UNPACK    WBB2DEDT,DIM10
.
                CLEAR     CMDLINE
                APPEND    "wbsprodd.us1",CMDLINE
                APPEND    SP1,CMDLINE
                APPEND    WBB2DNAM,CMDLINE
                APPEND    SP1,CMDLINE
                APPEND    DIM10,CMDLINE
                APPEND    SP1,CMDLINE
                APPEND    CDYSDAYS,CMDLINE
                APPEND    SP1,CMDLINE
                APPEND    WBSNOTEM,CMDLINE
.
                APPEND    SP1,CMDLINE
                MOVE      WBB2ORGN,DIM40
                SQUEEZE   DIM40
                APPEND    DIM40,CMDLINE
.
                APPEND    SP1,CMDLINE
                MOVE      WBSPRORG,DIM20
                APPEND    DIM20,CMDLINE
.
                APPEND    SP70,CMDLINE
                RESET     CMDLINE
                EXECUTE   CMDLINE,TASKID
              ENDIF
              MOVE      SP70,WBSACTE2
.
            ELSE
.
              IF        CDYSDAYS <= SEVEN
.
                MATCH     SP70,WBSACTE3
                GOTO      MAIN6100 IF EQUAL
.
                MATCH     "0",WBSACTE3
                GOTO      MAIN6100 IF EQUAL
.
                MOVE      ZERO,FORM1
                MOVE      WBSACTE3,FORM1
.
                COMPARE   CDYSDAYS,FORM1
                IF        @EQUAL
.
                UNPACK    WBB2DEDT,DIM10
.
                  CLEAR     CMDLINE
                  APPEND    "wbsprodd.us1",CMDLINE
                  APPEND    SP1,CMDLINE
                  APPEND    WBB2DNAM,CMDLINE
                  APPEND    SP1,CMDLINE
                  APPEND    DIM10,CMDLINE
                  APPEND    SP1,CMDLINE
                  APPEND    CDYSDAYS,CMDLINE
                  APPEND    SP1,CMDLINE
                  APPEND    WBSNOTEM,CMDLINE
.
                  APPEND    SP1,CMDLINE
                  MOVE      WBB2ORGN,DIM40
                  SQUEEZE   DIM40
                  APPEND    DIM40,CMDLINE
.
                  APPEND    SP1,CMDLINE
                  MOVE      WBSPRORG,DIM20
                  APPEND    DIM20,CMDLINE
.
                  APPEND    SP70,CMDLINE
                  RESET     CMDLINE
                  EXECUTE   CMDLINE,TASKID
.
                  ASSIGN    (FORM1-1),FORM1
                  MOVE      FORM1,WBSACTE3
.
                ELSE
.
                  COMPARE   FORM1,CDYSDAYS
                  IF        @LESS
                    ASSIGN    (FORM1-1),FORM1
                    MOVE      FORM1,WBSACTE3
                  ENDIF
.
                ENDIF
              ENDIF
            ENDIF
          ENDIF
.
MAIN6100  CLOSE     WBSACTEF
          OPEN      WBSACTEF,ACTEPATH
          WRITE     WBSACTEF,SEQ;WBSACTE1,WBSACTE2,WBSACTE3
.
.
MAIN9000  CLOSE     WBSPROPF
          CLOSE     WBSACTVF
          CLOSE     WBSTOKNF
          CLOSE     WBSACTIF
          CLOSE     WBSTOKIF
          CLOSE     WBSACTEF
.
          GOTO      MAIN1000
.
MAIN9999  COMPARE   GETTOKF4,GATTIMER
          IF        !@LESS
            MOVE      ZERO,GATTIMER
          ENDIF
.
          CLOSE     WBSPROPF
          CLOSE     WBSACTVF
          CLOSE     WBSTOKNF
          CLOSE     WBSACTIF
          CLOSE     WBSTOKIF
          CLOSE     WBSACTEF
.
          GOTO      MAIN0000
.
.------------------------------------------------------------
.  Open PRODA Text Files
.------------------------------------------------------------
PROPN000  MOVE      SP70,PRODAIND
.
          CLEAR     PRDAPATH
          APPEND    WBB2DPTH,PRDAPATH
          RESET     PRDAPATH
          STRIP     PRDAPATH
          ENDSET    PRDAPATH
          APPEND    SLASH,PRDAPATH
          APPEND    PRDAFILE,PRDAPATH
          RESET     PRDAPATH
.
          MOVE      ZERO,PRODAIND
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      WBSPROPF,PRDAPATH
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      ONE,PRODAIND
            GOTO      PROPN999
          ENDIF
          CLOSE     WBSPROPF
.
          CLEAR     ACTVPATH
          APPEND    WBB2DPTH,ACTVPATH
          RESET     ACTVPATH
          STRIP     ACTVPATH
          ENDSET    ACTVPATH
          APPEND    SLASH,ACTVPATH
          APPEND    ACTVFILE,ACTVPATH
          RESET     ACTVPATH
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      WBSACTVF,ACTVPATH
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      ONE,PRODAIND
            GOTO      PROPN999
          ENDIF
          CLOSE     WBSACTVF
.
          CLEAR     TOKNPATH
          APPEND    WBB2DPTH,TOKNPATH
          RESET     TOKNPATH
          STRIP     TOKNPATH
          ENDSET    TOKNPATH
          APPEND    SLASH,TOKNPATH
          APPEND    TOKNFILE,TOKNPATH
          RESET     TOKNPATH
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      WBSTOKNF,TOKNPATH
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      ONE,PRODAIND
            GOTO      PROPN999
          ENDIF
          CLOSE     WBSTOKNF
.
          CLEAR     ACTIPATH
          APPEND    WBB2DPTH,ACTIPATH
          RESET     ACTIPATH
          STRIP     ACTIPATH
          ENDSET    ACTIPATH
          APPEND    SLASH,ACTIPATH
          APPEND    ACTIFILE,ACTIPATH
          RESET     ACTIPATH
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      WBSACTIF,ACTIPATH
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      ONE,PRODAIND
            GOTO      PROPN999
          ENDIF
          CLOSE     WBSACTIF
.
          CLEAR     TOKIPATH
          APPEND    WBB2DPTH,TOKIPATH
          RESET     TOKIPATH
          STRIP     TOKIPATH
          ENDSET    TOKIPATH
          APPEND    SLASH,TOKIPATH
          APPEND    TOKIFILE,TOKIPATH
          RESET     TOKIPATH
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      WBSTOKIF,TOKIPATH
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      ONE,PRODAIND
            GOTO      PROPN999
          ENDIF
          CLOSE     WBSTOKIF
.
          CLEAR     ACTEPATH
          APPEND    WBB2DPTH,ACTEPATH
          RESET     ACTEPATH
          STRIP     ACTEPATH
          ENDSET    ACTEPATH
          APPEND    SLASH,ACTEPATH
          APPEND    ACTEFILE,ACTEPATH
          RESET     ACTEPATH
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      WBSACTEF,ACTEPATH
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      ONE,PRODAIND
            GOTO      PROPN999
          ENDIF
          CLOSE     WBSACTEF
.
PROPN999  RETURN
