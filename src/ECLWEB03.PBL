INIT8000  MOVE      ZERO,GETTOKF4
          MOVE      ZERO,FORM4
          MOVE      ZERO,FORM3
          SQUEEZE   PTCNTKMN
          MOVE      PTCNTKMN,FORM3
.
          IF        FORM3=ZERO
            MOVE      "900",GETTOKF4
          ELSE
            ASSIGN    (FORM3*60),FORM4
            ASSIGN    (FORM4/2),GETTOKF4
          ENDIF
.
          MOVE      ZERO,REFDEVF6
          SQUEEZE   PTCNRKDY
          MOVE      PTCNRKDY,REFDEVF6
.
          IF        ZERO=REFDEVF6
            MOVE      FIVE,REFDEVF6
          ENDIF
.
          RETURN
