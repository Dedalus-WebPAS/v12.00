.------------------------------------------------------------
.  Update Access Token Response from Medicare PRODA
.------------------------------------------------------------
PRDTKN00  PACK      ACCTKLAB,SP70
          PACK      ACCTKVAL,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70:
                             SP70,SP70,SP70
          PACK      ACCDELAB,SP70
          PACK      ACCDEVAL,SP70
          PACK      ACCKELAB,SP70
          PACK      ACCKEVAL,SP70
          PACK      ACCTTLAB,SP70
          PACK      ACCTTVAL,SP70
          PACK      ACCEILAB,SP70
          PACK      ACCEIVAL,SP70
.
PRDTKN10  READ      WBSTOKNF,SEQ;VAR
          GOTO      PRDTKN99 IF OVER
.
          SCAN      "access_token",VAR
          GOTO      PRDTKN20 IF EQUAL
.
          SCAN      "error_description",VAR
          GOTO      PRDTKN30 IF EQUAL
.
          GOTO      PRDTKN99
.
PRDTKN20  
.          CLOSE     WBSTOKNF
.          OPEN      WBSTOKNF,TOKNPATH
..
.          READ      WBSTOKNF,SEQ;OPENBRAC,ACCTKLAB,ACCTKVAL,ACCDELAB,ACCDEVAL:
.                                 ACCKELAB,ACCKEVAL,ACCTTLAB,ACCTTVAL,ACCEILAB:
.                                 ACCEIVAL,CLOSBRAC
..
.          PACK      WBB2GTTK,ACCTKVAL,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70:
.                                      SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70
.          PACK      WBB2GTDE,ACCDEVAL,SP70
.          PACK      WBB2GTKE,ACCKEVAL,SP70
.          PACK      WBB2GTBR,ACCTTVAL,SP70
.          PACK      WBB2GTEX,ACCEIVAL,SP70      
..
.
          MOVE      "ACTIVE",WBB2DSTA
          MOVE      "ACTIVE",WBB2GTTK
.
          GOTO      PRDTKN99
.
PRDTKN30  CLOSE     WBSTOKNF
          OPEN      WBSTOKNF,TOKNPATH
.
          MOVE      "ERROR",WBB2DSTA
.
          MOVE      "ERROR",WBB2GTTK
          PACK      WBB2GTTK,WBB2GTTK,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70:
                                      SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70
          PACK      WBB2GTDE,SP70
          PACK      WBB2GTKE,SP70
          PACK      WBB2GTBR,SP70
          PACK      WBB2GTEX,SP70
.
          GOTO      PRDTKN99
.
PRDTKN99  RETURN
