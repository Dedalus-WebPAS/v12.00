.------------------------------------------------------------
.  Update Activate Device Response from Medicare PRODA
.------------------------------------------------------------
PRDACT00  PACK      VAR,SP70,SP70
.
PRDACT10  READ      WBSACTVF,SEQ;VAR
          GOTO      PRDACT30 IF OVER 
.
          SCAN      "orgId",VAR           
          GOTO      PRDACT20 IF EQUAL
.
          SCAN      "errors",VAR
          GOTO      PRDACT30 IF EQUAL
.
          GOTO      PRDACT10
.
PRDACT20  CLOSE     WBSACTVF
          OPEN      WBSACTVF,ACTVPATH
.
          READ      WBSACTVF,SEQ;OPENBRAC 
          READ      WBSACTVF,SEQ;VAR
          READ      WBSACTVF,SEQ;VAR
          READ      WBSACTVF,SEQ;DEVISLAB,DEVISVAL
          READ      WBSACTVF,SEQ;KEYSTLAB,KEYSTVAL
          READ      WBSACTVF,SEQ;KEYEXLAB,KEYEXVAL
          READ      WBSACTVF,SEQ;DEVIXLAB,DEVIXVAL
          READ      WBSACTVF,SEQ;CLOSBRAC
.
          PACK      WBB2DSTA,DEVISVAL,SP70
          PACK      WBB2ADKS,KEYSTVAL,SP70
          PACK      WBB2ADKE,KEYEXVAL,SP70
          PACK      WBB2ADDE,DEVIXVAL,SP70
          PACK      WBB2DEDT,DEVIXVAL,SP70
.
          GOTO      PRDACT99
.
PRDACT30  CLOSE     WBSACTVF
          OPEN      WBSACTVF,ACTVPATH
.
          MOVE      "ERROR",WBB2DSTA
          MOVE      "ERROR",WBB2ADKS
          MOVE      SP70,WBB2DEDT
          MOVE      SP70,WBB2ADKE
          MOVE      SP70,WBB2ADDE
.
          GOTO      PRDACT99
.
PRDACT99  RETURN
