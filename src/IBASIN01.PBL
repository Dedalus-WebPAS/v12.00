          MATCH     "RT ",SICDCATA
          IF        @EQUAL
            UNPACK    SICDCODE,D1,D2
            MATCH     SP2,D2
            IF        !@EQUAL
              MOVE      "Code Must be Only One Character Long - ",DISPMSG
              CALL      MESSAGE1
              GOTO      ECODE000
            ENDIF
          ENDIF
.
          MATCH     "CAT",SICDCATA
          IF        @EQUAL
            UNPACK    SICDCODE,D1,D2
            MATCH     SP2,D2
            IF        !@EQUAL
              MOVE      "Code must be 1-6 - ",DISPMSG
              CALL      MESSAGE1
              GOTO      ECODE000
            ENDIF
            MATCH     "1",D1
            GOTO      ECODE999 IF EQUAL
            MATCH     "2",D1
            GOTO      ECODE999 IF EQUAL
            MATCH     "3",D1
            GOTO      ECODE999 IF EQUAL
            MATCH     "4",D1
            GOTO      ECODE999 IF EQUAL
            MATCH     "5",D1
            GOTO      ECODE999 IF EQUAL
            MATCH     "6",D1
            GOTO      ECODE999 IF EQUAL
            MOVE      "Code must be 1-6 - ",DISPMSG
            CALL      MESSAGE1
            GOTO      ECODE000
          ENDIF
.
ECODE999  RETURN
.----------------------------------------------
. Save Code
.----------------------------------------------
SCOD0000  PACK        KEY6,SICDCATA,SICDCODE
          CALL        RASICD1
          IF          OVRCD=0
            CALL        UPSICD1
          ELSE
            CALL        WRSICD1
          ENDIF
          RETURN
.
.----------------------------------------------
. Delete Category
.----------------------------------------------
DCOD0000  MOVE      SP70,DISPMSG
          CALL      MESSAGE2
          CALL      DELQST
          CALL      MESSAGE3
          BRANCH    CEXIT,DCOD1000,DCOD9000,DCOD9100
.
DCOD1000  PACK      KEY6,SICDCATA,SICDCODE
          CALL      RASICD1
          IF        OVRCD=0
            CALL       DESICD1
          ENDIF
          MOVE     TWO,EXIT
          GOTO     DCOD9999
.
DCOD9000  MOVE     ONE,EXIT
          GOTO     DCOD9999
DCOD9100  MOVE     TWO,EXIT
          GOTO     DCOD9999
DCOD9999  RETURN
.
.----------------------------------------------------------------------
. Keyin CANC
.----------------------------------------------------------------------
KCANC000
          MATCH     "RT ",SICDCATA
          IF        @EQUAL
            MOVE      "N",DEFYESNO
            CALL      YORN0000
            MOVE      FYESNO,SICDCANC
          ELSE
            MOVE      ZERO,FYESNO
            MOVE      ZERO,SICDCANC
            MOVE      NO,YESNO
            IF        ACCEPT=0
              CLEAR     DISPMSG
              APPEND    "This item only valid for category RT - ",DISPMSG
              CALL      MESSAGE1
            ENDIF
          ENDIF
.
KCANC999  RETURN
.----------------------------------------------------------------------
. Keyin Other SICDOTHR
.----------------------------------------------------------------------
KOTHR000  MATCH     "IS ",SICDCATA
          GOTO      KOTHR999 IF NOT EQUAL
.
          TYPE      SICDOTHR
          IF        @EQUAL & CKEYTYP=0
            MOVE      SICDOTHR,OTHR7P2
          ELSE
            MOVE      ZERO,OTHR7P2
          ENDIF
.
          KEYIN     *PCCOL:CVERT,*V2LON,*DV,SP10:
                    *PCCOL:CVERT,*RV,OTHR7P2
          MOVE      OTHR7P2,SICDOTHR
.
KOTHR999  RETURN
.-------------------------------
. Includes for I/O and Keyins
.-------------------------------
          INC       SINCODIO/INC
          INC       SINCODKY    
.
          INC       STDGENCD
.
