.
          MATCH    "APIM6",NAMSSFFI
          GOTO     LP03L105 IF NOT EQUAL
          PACK     KEY41,APIM6SKY,SP70
          CALL     RSAPIM6  * End
          IF       OVRCD=1
            CALL     RKAPIM6 
          ENDIF
          MOVE     ZERO,SECTOR
          WHILE    SECTOR<APIM6SNO
            CALL     RKAPIM6 
            IF       OVRCD=1
              MOVE     APIM6SNO,SECTOR
            ENDIF
            PACK     APIM6MKY,APIMOUT,APIMCRD,APIMREF,APIMDOC,APIMBCH                                        
            MATCH    APIM6EKY,APIM6MKY
            IF       !@EQUAL
              MOVE     APIM6SNO,SECTOR
