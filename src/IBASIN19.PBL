          MOVE     ZERO,SECTOR
          IF       APIM6CNT=APIM6SNO
            SUB      ONE,APIM6CNT
          ENDIF
          WHILE    SECTOR<=APIM6CNT
            CALL     RKAPIM6 
            IF       OVRCD=1
              MOVE     APIM6SNO,SECTOR
            ENDIF
            PACK     APIM6MKY,APIMOUT,APIMCRD,APIMREF,APIMDOC,APIMBCH                                        
            MATCH    APIM6EKY,APIM6MKY
