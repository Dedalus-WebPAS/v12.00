          CALL      HEAD0000 IF LESS        * Print report header
.
          PACK      ERRMSG,ERRMSG,SP70
          UNPACK    WMDATE1,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          MOVE      CPCDATE,DATOLIST
          PRINT     *1,"|",WMURNO,"|",WMCODE:
                    "| ",DATOLIST," |",ERRMSG,"|"
          ADD       ONE,CLNO
          PACK      ERRMSG,SP70
.
PRNT9000  MOVE      ZERO,EXIT
PRNT9999  RETURN
