.******************************************************************************
.*                                  OVER0000              Called by: PROC0000 *
.*         Calculate the total number of not ready for care days              *
.******************************************************************************
OVER0000  MOVE      "2",OVERDUE
          MOVE      ZERO,TOTALDAY
          IF        WMSTAT<4
            MOVE      WTCENSUS,TOTALDAY
          ELSE
            MOVE      WTREM,TOTALDAY
          ENDIF
.
          MATCH     "1",CLURGR
          GOTO      OVER1000 IF NOT EQUAL
.
          COMPARE   "30",TOTALDAY
          IF        @LESS|@EQUAL
            MOVE      "2",OVERDUE
          ELSE
            MOVE      "1",OVERDUE
          ENDIF
          GOTO      OVER9999
.
OVER1000  MATCH     "2",CLURGR
          GOTO      OVER2000 IF NOT EQUAL
.
          COMPARE   "90",TOTALDAY
          IF        @LESS|@EQUAL
            MOVE      "2",OVERDUE
          ELSE
            MOVE      "1",OVERDUE
          ENDIF
          GOTO      OVER9999
.
OVER2000  MATCH     "3",CLURGR
          GOTO      OVER9999 IF NOT EQUAL
.
          COMPARE   "365",TOTALDAY
          IF        @LESS|@EQUAL
            MOVE      "2",OVERDUE
          ELSE
            MOVE      "1",OVERDUE
          ENDIF
          GOTO      OVER9999
.
OVER9999  RETURN
