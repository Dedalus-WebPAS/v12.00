          IF        STARTNUM>=RECNUMB
            GOTO      ENDTE100
          ENDIF
.
          CALL      IENROW00                * Output row
.
          ADD       ONE,DISNUMB
          COMPARE   NORECORD,DISNUMB
          GOTO      ENDTE100 IF NOT EQUAL
.
          CALL      RPPMIHI4                * read next record
          BRANCH    OVRCD,ENDTE910          * end of file
.
          MOVE      ZERO,ENDOFLST
          GOTO      ENDTE920
.
