.        INCLUDE NAME : WATLETIO.INC
.
.        FILE I/O REQUESTS FOR WAITING LISTS LETTER FILE
.
.OVRCD   FORM     1        0 = NOT OVER
.                          1 = OVER
.KEY6    DIM      6        KEY FOR WATLETR1 AND WATLETR2
.
RDALET1  MOVE     ZERO,OVRCD
         RESET    KEY6
         READ     WATLETR1,KEY6;ANS
         GOTO     OVERCOND IF OVER
         RETURN
.
RDSLET1  MOVE     ZERO,OVRCD
         RESET    KEY6
         READ     WATLETR1,KEY6;;
         RETURN
.
RDKLET1  MOVE     ZERO,OVRCD
         READKS   WATLETR1;DWLETNO,DWLINNO,WLTEXT,WLVAR,WLETPBOT,WLETPTOP:
                           WLETPPAG,WLETPTAB,WLETMMFN,WLETVALD,WLETAUDL:
                           WLETACTV,WLETCOMM,WLETCONF,WLETSPAR
         GOTO     OVERCOND IF OVER
         MOVE     DWLETNO,WLETNO
         MOVE     DWLINNO,WLINNO
         RETURN
.
