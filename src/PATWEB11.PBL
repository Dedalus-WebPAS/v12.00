            GOTO      GETVIS60
          ELSE
            IF        EMVISTAT=1
              APPEND    EMGSTAT1,VSTATUS
              GOTO      GETVIS60
            ENDIF
          ENDIF
.
          MATCH     SP70,DISCDATE
          GOTO      GETVIS60 IF EQUAL
.
          PACK      VSTATUS,SP70
          CLEAR     VSTATUS
          APPEND    INPSTA3A,VSTATUS
.
          MATCH     SP70,PTVADESC
          IF        !@EQUAL
            MOVE      PTVADESC,KEY10
            APPEND    KEY10,VSTATUS
          ELSE
            MATCH     SP70,EMVIDSTA
            IF        !@EQUAL
              PACK      KEY5,ANSE,ANSD,EMVIDSTA,SP70
              CALL      RDCODE1
              IF        OVRCD<>1
                MOVE      TDESC,KEY20
                APPEND    KEY20,VSTATUS
              ENDIF
            ENDIF
