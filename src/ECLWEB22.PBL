          CALL      WEBINT00       * Initialise Fifo Etc.
.
          CALL      INIT0000       * Open Files
.
MAIN0000  COMMIT
.
          BEGIN
.
          DISPLAY   *W2
.
          MATCH     "1",PTCNWSIN
          GOTO      MAIN0000 IF NOT EQUAL
.
.         MATCH     "1",PTCNIMCW
.         GOTO      MAIN0000 IF NOT EQUAL
.
          MOVE      ZERO,EXIT
.
          PACK      KEY51,ZERO,SP1,FIVE,SP70
          CALL      RSWBETR1
MAIN1000  CALL      RKWBETR1
          BRANCH    OVRCD,MAIN9999
.
          MATCH     "0",WBETSTAT
          GOTO      MAIN9999 IF NOT EQUAL
.
          MATCH     " 5",WBETTYPE
          GOTO      MAIN9999 IF NOT EQUAL
.
          PACK      KEY71,WBETLOCN,SP70          * Read B2B Device Table
          CALL      RSWBB2B2
          CALL      RKWBB2B2
          BRANCH    OVRCD,MAIN1000
.
          MATCH     WBETLOCN,WBB2LOCN            * Same Hospital
          GOTO      MAIN1000 IF NOT EQUAL
.
          MATCH     "ACTIVE",WBB2DSTA            * Device Active
          GOTO      MAIN1000 IF NOT EQUAL
.
          MATCH     SP70,WBB2DPTH
          GOTO      MAIN1000 IF EQUAL
.
          CALL      PROPN000                     * Valid Properties File?
.
          MATCH     "1",PRODAIND
          GOTO      MAIN1000 IF EQUAL
.
          PACK      SAVKEY51,WBETSTAT,WBETTYPE,WBETLOCN,WBETCDTE,WBETCTIM:
                             WBETTRID,SP70
.
          OPEN      WBSPROPF,PRDAPATH
.
. Parse _erawrtvw_response file (ERAW RTVW Response JSON file)
. ************************************************************
.
MAIN2000  CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/eraw/",CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    WBETSTRD,CMDLINE
          APPEND    RTVRLITR,CMDLINE
          RESET     CMDLINE
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      RTVRFILE,CMDLINE
          TRAPCLR   IO
          IF        OVRCD=1
            GOTO      MAIN3000
          ENDIF
.
          CALL      RTVPRS00                   * Parse ERAW RTVW Response JSON
.
          GOTO      MAIN8000
.
. Parse _eraw_errors file (ERAW RTVW Stderrs/Stdout file)
. ******************************************************
.
MAIN3000  CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/eraw/",CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    WBETSTRD,CMDLINE
          APPEND    RTVELITR,CMDLINE
          RESET     CMDLINE
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      RTVEFILE,CMDLINE
          TRAPCLR   IO
          IF        OVRCD=1
            MOVE      "Response files not found.",RTVELINE  
            GOTO      MAIN3500
          ENDIF
.
MAIN3100  READ      RTVEFILE,SEQ;RTVELINE
          IF        @OVER
            MOVE      "Refer to ERAW RTVW Details.",RTVELINE
            GOTO      MAIN3500 
          ENDIF
.
          SCAN      "error",RTVELINE
          IF        @EQUAL
            GOTO      MAIN3500
          ENDIF
.
          GOTO      MAIN3100
.
MAIN3500  MOVE      "1",RTVEINDC
.
          MOVE      SAVKEY51,KEY51
          CALL      RDWBETR1
.
          MOVE      "EROR",WBETEROR
.
          CALL      IBACLOCK
          PACK      WBETUDTE,CCC,CYY,CMM,CDD
          REP       " 0",WBETUDTE
          MOVE      CTIMEIS,WBETUTIM
          MOVE      USERID,WBETUUID

          CALL      UPWBETR1
.
          GOTO      MAIN8000
.
. ******************************************************************************
.
MAIN8000  MOVE      SAVKEY51,KEY51
          CALL      RDWBETR1
.
          MATCH     "1",RTVEINDC
          IF        @EQUAL
            MOVE      "E",WBETSTAT
          ELSE
            MOVE      "1",WBETSTAT
          ENDIF
.
          CALL      IBACLOCK
          PACK      WBETUDTE,CCC,CYY,CMM,CDD
          REP       " 0",WBETUDTE
          MOVE      CTIMEIS,WBETUTIM
          MOVE      USERID,WBETUUID

          CALL      UPWBETR1
.
MAIN9000  CLOSE     WBSPROPF
          CLOSE     RTVRFILE
          CLOSE     RTVEFILE
.
          GOTO      MAIN1000
.
MAIN9999  CLOSE     WBSPROPF
          CLOSE     RTVRFILE
          CLOSE     RTVEFILE
.
          GOTO      MAIN0000
+
.------------------------------------------------------------
.  Parse ERAW RTVW Response JSON
.------------------------------------------------------------
.
RTVPRS00  MOVE      SP70,JSONCODE
          PACK      JSONTEXT,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70
          MOVE      SP70,RTVEINDC
.
RTVPRS01  READ      RTVRFILE,SEQ;RTVRLINE
          GOTO      RTVPRS99 IF OVER
.
. ***********************************************************
. Errors
. ***********************************************************
.
          SCAN      "highestSeverity",RTVRLINE
          IF        @EQUAL
.
            MOVE      "1",RTVEINDC
.
RTVPRS05    READ      RTVRFILE,SEQ;RTVRLINE
            GOTO      RTVPRS90 IF OVER
.
.           *************************************************
.           serviceMessage
.           *************************************************
.
            SCAN      "serviceMessage",RTVRLINE
            IF        @EQUAL
.
RTVPRS07      READ      RTVRFILE,SEQ;RTVRLINE
              GOTO      RTVPRS20 IF OVER
.
.             ***********************************************
.             {    
.             ***********************************************
.
              SCAN      "{",RTVRLINE
              IF        @EQUAL
                MOVE      SP70,JSONCODE
                PACK      JSONTEXT,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70
.
                GOTO      RTVPRS07
.
              ENDIF
.
.             ***********************************************
.             code
.             ***********************************************
.
              SCAN      "code",RTVRLINE
              IF        @EQUAL
                RESET     RTVRLINE
.               UNPACK    RTVRLINE,DIM14,JSONCODE
.
                SCAN      ":",RTVRLINE
                IF        @EQUAL
                  MOVE      RTVRLINE,RTVRWORK
                  REP       ": ",RTVRWORK
                  REP       "#" ",RTVRWORK
                  LJUSTIFY  RTVRWORK
                  MOVE      RTVRWORK,JSONCODE
                  RESET     RTVRLINE
                ENDIF
.
                GOTO      RTVPRS07
.
              ENDIF
.
.             ***********************************************
.             }
.             ***********************************************
.
              SCAN      "}",RTVRLINE
              IF        @EQUAL
.
                MOVE      SAVKEY51,KEY51
                CALL      RDWBETR1
.
                MOVE      JSONCODE,WBETEROR
.
                CALL      IBACLOCK
                PACK      WBETUDTE,CCC,CYY,CMM,CDD
                REP       " 0",WBETUDTE
                MOVE      CTIMEIS,WBETUTIM
                MOVE      USERID,WBETUUID

                CALL      UPWBETR1
.
                GOTO      RTVPRS07
.
              ENDIF
.
.             ***********************************************
.             ]
.             ***********************************************
.
              SCAN      "]",RTVRLINE
              IF        @EQUAL
                GOTO      RTVPRS90
              ENDIF
.
              GOTO      RTVPRS07
.
            ENDIF
.
RTVPRS20    GOTO      RTVPRS05   
.
          ENDIF
.
