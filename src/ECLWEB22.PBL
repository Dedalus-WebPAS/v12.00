.---------------------------------------------------------------
. Get the Report Key from the ERAW RTVW JSON Response
. And reposition again at "content"
.---------------------------------------------------------------
.
RKEY0000  READ      RTVRFILE,SEQ;RTVRLINE
          GOTO      RKEY5000 IF OVER
.
.         *************************************************
.         transactionId
.         *************************************************
.
          
          SCAN      "transactionId",RTVRLINE
          IF        @EQUAL
            RESET     RTVRLINE
.           UNPACK    RTVRLINE,DIM23,WBEIFTID
.
            SCAN      ":",RTVRLINE
            IF        @EQUAL
              MOVE      RTVRLINE,RTVRWORK
              REP       ": ",RTVRWORK
              REP       "#" ",RTVRWORK
              LJUSTIFY  RTVRWORK
              MOVE      RTVRWORK,WBEIFTID
              RESET     RTVRLINE
            ENDIF
.
            REP       "#" ",WBEIFTID
            REP       ", ",WBEIFTID
.
            RESET     WBEIFTID
            PACK      WBEIFTID,WBEIFTID,SP70
.
            MATCH     WBEIFTID,WBETTRID 
            IF        !@EQUAL
              MOVE      SP70,WBEIFTID
            ENDIF
.
          ENDIF
.
.         *************************************************
.         remittanceAdviceId
.         *************************************************
.
          SCAN      "remittanceAdviceId",RTVRLINE
          IF        @EQUAL
            RESET     RTVRLINE
.           UNPACK    RTVRLINE,DIM30,WBEIRADV
.
            SCAN      ":",RTVRLINE
            IF        @EQUAL
              MOVE      RTVRLINE,RTVRWORK
              REP       ": ",RTVRWORK
              REP       "#" ",RTVRWORK
              LJUSTIFY  RTVRWORK
              MOVE      RTVRWORK,WBEIRADV
              RESET     RTVRLINE
            ENDIF
.
            REP       "#" ",WBEIRADV
            REP       ", ",WBEIRADV
          ENDIF
.
.         *************************************************
.         partNumber
.         *************************************************
.
          SCAN      "partNumber",RTVRLINE
          IF        @EQUAL
            RESET     RTVRLINE
.           UNPACK    RTVRLINE,DIM21,WBEIPNUM
.
            SCAN      ":",RTVRLINE
            IF        @EQUAL
              MOVE      RTVRLINE,RTVRWORK
              REP       ": ",RTVRWORK
              REP       "#" ",RTVRWORK
              LJUSTIFY  RTVRWORK
              MOVE      RTVRWORK,WBEIPNUM
              RESET     RTVRLINE
            ENDIF
.
            REP       "#" ",WBEIPNUM
            REP       ", ",WBEIPNUM
          ENDIF
.
          GOTO      RKEY0000
.
.         close and reopen rtvw response json file and read from the start
.         ----------------------------------------------------------------
.
RKEY5000  CLOSE     RTVRFILE
.
          CLEAR     CMDLINE
          APPEND    WBB2DPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/eraw/",CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
