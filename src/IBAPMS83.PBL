          MATCH     LSTDATE,TDATE
          GOTO      TRAN30 IF EQUAL
.
TRAN20    UNPACK    TDATE,XCENT,XYEAR,XMON,XDAY
.
          CMATCH    ANSD,TMOVE
          GOTO      TRAN50 IF EQUAL
          CMATCH    ANSL,TMOVE
          GOTO      TRAN25 IF EQUAL
.
          MOVE      TRATEH,DAYRATE
          ADD       TRATEF,DAYRATE
          ADD       PTTRADPT,DAYRATE
          ADD       PTTRADRB,DAYRATE
          SUB       TDISC,DAYRATE
          SUB       TALLW,DAYRATE
.
          MATCH     STATYPE,TATYPE
          GOTO      TRAN30 IF EQUAL
          MOVE      TDATE,FROMDATE
          GOTO      TRAN30
