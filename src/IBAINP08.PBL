          MOVE      PTEDCODE,DIM1
          TYPE      DIM1                  * numeric ?
          GOTO      EXHD1500 IF EQUAL
.
          MOVE      DIM1,ANS
          REP       "V1M2E3",ANS
          TYPE      ANS                   * Test for V,M or E codes
          IF        !@EQUAL
            UNPACK    PTEDCODE,DIM1,PTEDCODE * Unknown code process as numeric
            PACK      PTEDCODE,PTEDCODE,SP30
            GOTO      EXHD1500
          ENDIF
