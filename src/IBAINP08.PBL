.*                        Called : ML0000                             *
.**********************************************************************
+
SCRN0000  DISPLAY   *P1:3,*EF:
                    *P1:4,*V2LON,ZERO,*HOFF," = Exit":
                    *P1:5,*V2LON,ONE,*HOFF:
                          " = Run Data Extraction "
.
SCRN1000  KEYIN     *P1:7,"Select Option : ",*EF,*DV,UNDLN1:
                    *P17:7,*V2LON,OPTION;
.
          BRANCH    OPTION,SCRN8000
.
          COMPARE   ZERO,OPTION
          GOTO      SCRN9000 IF EQUAL
.
          BEEP
          GOTO      SCRN1000
.
SCRN8000  MOVE      FALSE,EXIT
