.
.          MATCH     SP70,PTEDOSET
.          IF        @EQUAL
.            MOVE      "9",HDONSET
.          ELSE
            MOVE      PTEDOSET,HDONSET
.          ENDIF
.
          MOVE      FORM2,HDCOUNT
          SQUEEZE   HDCOUNT
.
          MOVE      PTEDCODE,DIM9
...       MOVE      PTEDUNQN,RECORDN
          MOVE      PTEDDESC,HDDODESC
          MOVE      PTEDDESC,HDDOLDES
.
          MATCH     "10",HDCODSYS
          IF        @EQUAL
            CALL      ITEN0000              * Coding is in ICD10
            GOTO      EXHD1900
