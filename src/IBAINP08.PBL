GAFL0000  MOVE      SP1,KEY1
          IF        PTCNAFLG = 34
            MOVE      AUSR1,KEY1
          ENDIF
          IF        PTCNAFLG = 36
            MOVE      AUSR2,KEY1
          ENDIF
          IF        PTCNAFLG = 38
            MOVE      AUSR3,KEY1
          ENDIF
          IF        PTCNAFLG = 40
            MOVE      AUSR4,KEY1
          ENDIF
          IF        PTCNAFLG = 42
            MOVE      AUSR5,KEY1
          ENDIF
          IF        PTCNAFLG = 44
            MOVE      AUSR6,KEY1
          ENDIF
          IF        PTCNAFLG = 46
            MOVE      AUSR7,KEY1
          ENDIF
          IF        PTCNAFLG = 99
            MOVE      PTMIUSR8,KEY1
          ENDIF
          IF        PTCNAFLG = 101
           MOVE      PTMIUSR9,KEY1 
          ENDIF
          IF        PTCNAFLG = 103
            MOVE      PTMIUSR0,KEY1
          ENDIF
          IF        PTCNAFLG = 118
            MOVE      PTMIUYN1,KEY1
          ENDIF
          IF        PTCNAFLG = 119
            MOVE      PTMIUYN2,KEY1
          ENDIF
          REP       "0N1Y",KEY1                  * only want Yes/No
