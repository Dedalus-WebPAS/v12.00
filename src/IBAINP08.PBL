          MOVE      ZERO,CANCERPT
          MATCH     ECAFDATE,ENDDATE
          GOTO      CHGF9000 IF LESS
.
          MATCH     "1",ECAFSENT            * already sent ?
          GOTO      CHGF1000 IF EQUAL       * yes - ignore
.
          MOVE      ECAFADMN,KEY8
          CALL      RDMISS1                 * admission record on file ?
          BRANCH    OVRCD,CHGF1000          * no - ignore
.
          MOVE      AURNO,KEY8
