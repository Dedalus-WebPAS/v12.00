.------------------------------------------------------------
. Add fields for allied health RSA Table
.------------------------------------------------------------
ALRA0000  BRANCH    FLDITMNO,ALRA0100,ALRA0200,ALRA0300,ALRA0400,ALRA0500:
                             ALRA0600,ALRA0700,ALRA0800
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      ALRA9999
.
.
ALRA0100  WRITE     HTMLFILE;ALRAVISN;
          GOTO      ALRA9999
.
ALRA0200  WRITE     HTMLFILE;ALRALADM;
          GOTO      ALRA9999
.
ALRA0300  MATCH     ALRACDAT,SP70
          GOTO      ALRA9999 IF EQUAL
          UNPACK    ALRACDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR
          GOTO      ALRA9999
.
ALRA0400  WRITE     HTMLFILE;ALRACTIM; 
          GOTO      ALIV9999
.
ALRA0500  MATCH     SP70,ALRACUID
          GOTO      ALRA9999 IF EQUAL
          PACK      KEY10,ALRACUID
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      ALRACUID,WBSENAM
          ENDIF
          WRITE     HTMLFILE;WBSENAM;
          GOTO      ALRA9999
.
ALRA0600  MATCH     ALRAUDAT,SP70
          GOTO      ALRA9999 IF EQUAL
          UNPACK    ALRAUDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR
          GOTO      ALRA9999
.
ALRA0700  WRITE     HTMLFILE;ALRAUTIM;
          GOTO      ALRA9999
.
ALRA0800  MATCH     SP70,ALRAUUID
          GOTO      ALRA9999 IF EQUAL
          PACK      KEY10,ALRAUUID
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      ALRAUUID,WBSENAM
          ENDIF
          WRITE     HTMLFILE;WBSENAM;
          GOTO      ALRA9999
.
ALRA9999  RETURN
.
