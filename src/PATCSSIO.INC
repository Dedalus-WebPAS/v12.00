.        INCLUDE NAME : PATCSSIO.INC
.
.        FILE I/O REQUESTS FOR CLINICAL STATISTICS SUMMARY
.
.OVRCD   FORM     1        0 = NOT OVER
.                          1 = OVER
.KEY8    DIM     8         KEY FOR PATCSSA1
.
RAPTCSS1 MOVE     ZERO,OVRCD
         RESET    KEY8 
         READ     PATCSSA1,KEY8;ANS
         GOTO     OVERCOND IF OVER
         RETURN
.
RSPTCSS1 RESET    KEY8
         READ     PATCSSA1,KEY8;;
         RETURN
.
RKPTCSS1 MOVE     ZERO,OVRCD
         READKS   PATCSSA1;PTCSDATE,PTCSTYPC,PTCSTYPP,PTCSSTRT,PTCSADMT:
                           PTCSDSCH,PTCSDIED,PTCSEND,PTCSBDAY,PTCSDYCS:
                           PTCSDBDY,PTCSRNG1,PTCSRNG2,PTCSRNG3,PTCSRNG4:
                           PTCSWAIT,PTCSSPAR
         GOTO     OVERCOND IF OVER
.
         RETURN
.
RPPTCSS1 MOVE     ZERO,OVRCD
         READKP   PATCSSA1;PTCSDATE,PTCSTYPC,PTCSTYPP,PTCSSTRT,PTCSADMT:
                           PTCSDSCH,PTCSDIED,PTCSEND,PTCSBDAY,PTCSDYCS:
                           PTCSDBDY,PTCSRNG1,PTCSRNG2,PTCSRNG3,PTCSRNG4:
                           PTCSWAIT,PTCSSPAR
         GOTO     OVERCOND IF OVER
.
         RETURN
.
RDPTCSS1 MOVE     ZERO,OVRCD
         RESET    KEY8
         READ     PATCSSA1,KEY8;PTCSDATE,PTCSTYPC,PTCSTYPP,PTCSSTRT,PTCSADMT:
                           PTCSDSCH,PTCSDIED,PTCSEND,PTCSBDAY,PTCSDYCS:
                           PTCSDBDY,PTCSRNG1,PTCSRNG2,PTCSRNG3,PTCSRNG4:
                           PTCSWAIT,PTCSSPAR
         GOTO     OVERCOND IF OVER
.
         RETURN
.
UPPTCSS1 UPDATE   PATCSSA1;PTCSDATE,PTCSTYPC,PTCSTYPP,PTCSSTRT,PTCSADMT:
                           PTCSDSCH,PTCSDIED,PTCSEND,PTCSBDAY,PTCSDYCS:
                           PTCSDBDY,PTCSRNG1,PTCSRNG2,PTCSRNG3,PTCSRNG4:
                           PTCSWAIT,PTCSSPAR
         RETURN
.
DEPTCSS1 RESET    KEY8
         DELETE   PATCSSA1,KEY8
         RETURN
.
WRPTCSS1 MOVE     ZERO,OVRCD
         RESET    KEY8
         WRITE    PATCSSA1,KEY8;PTCSDATE,PTCSTYPC,PTCSTYPP,PTCSSTRT,PTCSADMT:
                           PTCSDSCH,PTCSDIED,PTCSEND,PTCSBDAY,PTCSDYCS:
                           PTCSDBDY,PTCSRNG1,PTCSRNG2,PTCSRNG3,PTCSRNG4:
                           PTCSWAIT,PTCSSPAR
.
         RETURN
.
