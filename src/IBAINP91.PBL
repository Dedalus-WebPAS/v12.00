          CALL      RDCODE1                 * Cat."A "
          BRANCH    OVRCD,EXTD1000
.
          MATCH     ANSX,TCINDC5
          GOTO      EXTD1000 IF EQUAL       * Exclude from extract
.
          MATCH     ANSB,TCINDC3
          GOTO      EXTD1000 IF EQUAL       * Ignore boarder & unqual patients
.
          IF        OPTION<>5
            CALL      VCHF0000              * Validate claim code & health fund
            BRANCH    EXIT,EXTD1000
          ENDIF
.
