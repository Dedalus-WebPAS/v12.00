                      *N,"Number of Warnings Found    : ",WARNGCNT;
            IF        OPTION=5
              PRINT     *N,"Extract File Name           : ",INPFNAME;
            ELSE
              PRINT     *N,"Extract File Name           : ",HCPFNAME;
            ENDIF
            PRINT     *N,*N,"*** End of Report ***"
            DISPLAY   *P1:24,*EL,EXTRDCNT," records extracted. ":
