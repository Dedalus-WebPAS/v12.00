            IF        SNAPRECS = 1
              WRITAB    ANSPFIL1,ZERO;*28,DRGNOSNP *Update no of rec. written
            ENDIF
.
            CALL      UND80
            PRINT     *N,"Number of Records Read      : ",TOTRDCNT:
