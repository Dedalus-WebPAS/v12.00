          IF        @EQUAL
            MOVE      TWO,XINTRP                 * no
            GOTO      LANG9999
          ENDIF
.
          MATCH     "1",PMVXINTR                 * interpreter req'd ?
          IF        @EQUAL
            MOVE      ONE,XINTRP                 * yes
            GOTO      LANG9999
