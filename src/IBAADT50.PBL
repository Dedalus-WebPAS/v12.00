          MATCH     "70",XSEPTP
          IF        @EQUAL
            MATCH     "0000",XLEVDY
            IF        @EQUAL
              CLEAR     ERRDESC
              APPEND    "Separation mode discharge from leave & ",ERRDESC
              APPEND    "leave days = 0",ERRDESC
              RESET     ERRDESC
              CALL      WRTEMP
            ENDIF
          ENDIF
