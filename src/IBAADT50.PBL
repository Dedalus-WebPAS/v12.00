ACTV0000  KEYIN     *P1:11,*EL,"Sending Activity codes (",*V2LON,*DV,ANSY,*HOFF:
                    "/",*V2LON,*DV,ANSN,*HOFF,") :":
                    *P32:11,*V2LON,ANS
.
          PACK      ANS,ANS,SP1
          REP       UPPLOW,ANS
.
          MATCH     ANSY,ANS                     * yes - entered ?
          IF        @EQUAL
            MOVE      ZERO,SENDFLAG              * yes
            GOTO      ACTV9999
          ENDIF
.
          MATCH     ANSN,ANS                     * no - entered ?
          IF        @EQUAL
            MOVE      ONE,SENDFLAG               * yes
            GOTO      ACTV9999
          ENDIF
.
          BEEP                                   * invalid entry
          GOTO      ACTV0000
.
