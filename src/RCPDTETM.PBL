.------------------------------------------------------------
. Receipt details file standard tags
.------------------------------------------------------------
RCDE0000  BRANCH    FLDITMNO,RCDE0010,RCDE0020,RCDE0030,RCDE0040,RCDE0050:
                             RCDE0060,RCDE0070,RCDE0080,RCDE0090,RCDE0100:
                             RCDE0110,RCDE0120,RCDE0130,RCDE0140,RCDE0150:
                             RCDE0160,RCDE0170,RCDE0180,RCDE0190,RCDE0200:
                             RCDE0210,RCDE0220,RCDE0230,RCDE0240,RCDE0250:
                             RCDE0260,RCDE0270,RCDE0280,RCDE0290,RCDE0300:
                             RCDE0310,RCDE0320,RCDE0330,RCDE0340,RCDE0350
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      RCDE9999
.
RCDE0010  MATCH     RCDTTDAT,SP70
          GOTO      RCDE9999 IF EQUAL
.
          UNPACK    RCDTTDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      RCDE9999
.
RCDE0020  WRITE     HTMLFILE;RCDTRECN;
          GOTO      RCDE9999
.
RCDE0030  WRITE     HTMLFILE;RCDTTCNT;
          GOTO      RCDE9999
.
RCDE0040  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      ZERO,F1
          MOVE      KEY1,F1
          BRANCH    F1,RCDE0045
          WRITE     HTMLFILE;RCDTREGI;
          GOTO      RCDE9999
.
RCDE0045  MOVE      RCDTREGI,KEY3
          CALL      RDREGA1
          IF        OVRCD=1
            MOVE      RCDTREGI,REGDESC
          ENDIF
          WRITE     HTMLFILE;REGDESC;
          GOTO      RCDE9999
.
RCDE0050  WRITE     HTMLFILE;RCDTINVN;
          GOTO      RCDE9999
.
RCDE0060  WRITE     HTMLFILE;RCDTVIST;
          GOTO      RCDE9999
.
RCDE0070  WRITE     HTMLFILE;RCDTURNO;
          GOTO      RCDE9999
.
RCDE0080  WRITE     HTMLFILE;RCDTNAME;
          GOTO      RCDE9999
.
RCDE0090  WRITE     HTMLFILE;RCDTADD1;
          GOTO      RCDE9999
.
RCDE0100  WRITE     HTMLFILE;RCDTADD2;
          GOTO      RCDE9999
.
RCDE0110  WRITE     HTMLFILE;RCDTADD3;
          GOTO      RCDE9999
.
RCDE0120  WRITE     HTMLFILE;RCDTADD4;
          GOTO      RCDE9999
.
RCDE0130  WRITE     HTMLFILE;RCDTPOST;
          GOTO      RCDE9999
.
RCDE0140  WRITE     HTMLFILE;RCDTSFLG;
          GOTO      RCDE9999
.
RCDE0150  WRITE     HTMLFILE;RCDTREFR;
          GOTO      RCDE9999
.
RCDE0160  WRITE     HTMLFILE;RCDTINCA;
          GOTO      RCDE9999
.
RCDE0170  WRITE     HTMLFILE;RCDTBNKA;
          GOTO      RCDE9999
.
RCDE0180  WRITE     HTMLFILE;RCDTAMNT;
          GOTO      RCDE9999
.
RCDE0190  WRITE     HTMLFILE;RCDTDISC;
          GOTO      RCDE9999
.
RCDE0200  WRITE     HTMLFILE;RCDTMHOS;
          GOTO      RCDE9999
.
RCDE0210  WRITE     HTMLFILE;RCDTMDHS;
          GOTO      RCDE9999
.
RCDE0220  WRITE     HTMLFILE;RCDTGSTC;
          GOTO      RCDE9999
.
RCDE0230  WRITE     HTMLFILE;RCDTGSTA;
          GOTO      RCDE9999
.
RCDE0240  WRITE     HTMLFILE;RCDTALLO;
          GOTO      RCDE9999
.
RCDE0250  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      ZERO,F1
          MOVE      KEY1,F1
          BRANCH    F1,RCDE0255
.
          MATCH     RCDTRELD,SP70
          GOTO      RCDE9999 IF EQUAL
.         
          UNPACK    RCDTRELD,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;  
          GOTO      RCDE9999
.
RCDE0255  MOVE      "Unreleased",KEY10
          MATCH     SP70,RCDTRELD
          IF        !@EQUAL
            MOVE      "Released",KEY10
          ENDIF
          WRITE     HTMLFILE;KEY10;
          GOTO      RCDE9999
.
RCDE0260  WRITE     HTMLFILE;RCDTRELT;
          GOTO      RCDE9999
.
RCDE0270  UNPACK    SP70,D1,KEY1
          UNPACK    DIM127,KEY1
          IF        HSYS3<>1
            GOTO      RCDE9999 * don't read pripractf if not using 
.                              * Private Practice
          ENDIF
          MATCH     ".",KEY1
          IF        @EQUAL
            UNPACK    DIM127,KEY1,D1,DIM127
            MATCH     SP1,D1
            IF        !@EQUAL
              MOVE      ZERO,F2
              MOVE      D1,F1
              BRANCH    F1,RCDE0272
            ENDIF
          ENDIF
          WRITE     HTMLFILE;RCDTPRAC;
          GOTO      RCDE9999
.
RCDE0272  PACK      KEY6,RCDTPRAC,SP70
          CALL      RDPRPR1
          BRANCH    OVRCD,RCDE9999
.
          WRITE     HTMLFILE;PRPRDESC;
          GOTO      RCDE9999
.
RCDE0280  MATCH     RCDTCDAT,SP70
          GOTO      RCDE9999 IF EQUAL
.
          UNPACK    RCDTCDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      RCDE9999
.
RCDE0290  WRITE     HTMLFILE;RCDTCTIM;
          GOTO      RCDE9999
.
RCDE0300  MATCH     SP70,RCDTCUID
          GOTO      RCDE9999 IF EQUAL
.
          PACK      KEY10,RCDTCUID,SP70
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      RCDTCUID,WBSENAM
          ENDIF
          WRITE     HTMLFILE;WBSENAM;
          GOTO      RCDE9999
.
RCDE0310  MATCH     RCDTUDAT,SP70
          GOTO      RCDE9999 IF EQUAL
.
          UNPACK    RCDTUDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          WRITE     HTMLFILE;CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR;
          GOTO      RCDE9999
.
RCDE0320  WRITE     HTMLFILE;RCDTUTIM;
          GOTO      RCDE9999
.
RCDE0330  MATCH     SP70,RCDTUUID
          GOTO      RCDE9999 IF EQUAL
.
          PACK      KEY10,RCDTUUID,SP70
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      RCDTUUID,WBSENAM
          ENDIF
          WRITE     HTMLFILE;WBSENAM;
          GOTO      RCDE9999
.
RCDE0340  MOVE      "DP",CATEGORY
          MOVE      RCDTDPTY,CODE
          CALL      CODITM00
          GOTO      RCDE9999
.
RCDE0350  UNPACK    DIM127,D1,KEY1,DIM127
          MOVE      ZERO,F1
          MOVE      KEY1,F1
          BRANCH    F1,RCDE0352
          PACK      KEY10,RCDTSDOC,SP30
          CALL      RDPMHCP1
          BRANCH    OVRCD,RCDE9999
          MOVE      PMHCTITL,FMTTITLE
          MOVE      PMHCGNAM,FMTGNAME
          MOVE      PMHCSNAM,FMTSNAME
          CALL      DOCNM000
          WRITE     HTMLFILE;DOCFNAME;
          GOTO      RCDE9999
.
RCDE0352  WRITE     HTMLFILE;RCDTSDOC;
          GOTO      RCDE9999
.
RCDE9999  RETURN
+
