.
.         INC       PATADTIO.INC
.
.         FILE  I/O REQUESTS FOR ALL THEATRE FEE AUDIT FILES
.
.OVRCD  FORM            1             0=NOT OVER
.                                     1= OVER
.
.         NOTE:-
.                  UPAAAX  IS THE WRITE CALL
.                  RDAAAX  IS THE READ CALL
.                  RDSAAAX  IS THE READ SECTOR 0 CALL
.                  WRSAAAX  IS THE WRITE ONE TO SECTOR 0 CALL
.
.
RDTAUD  READ       PATTAUD,SECTOR;ADD,AMM,AYY,TIMEIS,ACTION,OPERCODE:
                           TFCLM,TFHFUND,TFHFTAB,TFDAYC,TFENDMBS:
                           TFHF1,TFPAT1,TFHF2,TFPAT2,TFHF3,TFPAT3:
                           TFHF4,TFPAT4,TFHF5,TFPAT5,TFHF6,TFPAT6,TFNINV
        RETURN
.
RDTAUDX READ       PATTAUD,SECTOR;ADD,AMM,AYY,TIMEIS,ACTION,OPERCODE:
                        STFCLM,STFHFUND,STFHFTAB,STFDAYC,STFENDMB:
                        STFHF1,STFPAT1,STFHF2,STFPAT2,STFHF3,STFPAT3:
                        STFHF4,STFPAT4,STFHF5,STFPAT5,STFHF6,STFPAT6,STFNINV
        RETURN
.
RDSTAUD  
        READ       PATTAUD,ZERO;SECTOR
        RETURN
.
WRSTAUD 
        CLOSE      PATTAUD
        PREP       PATTAUD,"pattaud"
        CLOSE      PATTAUD
        PREP       PATTAUD,"pattaud"
        WRITE      PATTAUD,ZERO;SEC1
        WEOF       PATTAUD,SEC1
        RETURN
.
UPTAUD  
        CLOCK      TIME,TIMEIS
        READ       PATTAUD,ZERO;SECTOR
.
        WRITE      PATTAUD,SECTOR;CDD,CMM,CYY,TIMEIS,ACTION,PASSCODE:
                           TFCLM,TFHFUND,TFHFTAB,TFDAYC,TFENDMBS:
                           TFHF1,TFPAT1,TFHF2,TFPAT2,TFHF3,TFPAT3:
                           TFHF4,TFPAT4,TFHF5,TFPAT5,TFHF6,TFPAT6,TFNINV
        ADD        ONE,SECTOR
        WEOF       PATTAUD,SECTOR
        WRITE      PATTAUD,ZERO;SECTOR
        RETURN
.  
UPTAUDX 
        CLOCK      TIME,TIMEIS
        READ       PATTAUD,ZERO;SECTOR
.
        WRITE      PATTAUD,SECTOR;CDD,CMM,CYY,TIMEIS,ACTION,PASSCODE:
                        STFCLM,STFHFUND,STFHFTAB,STFDAYC,STFENDMB:
                        STFHF1,STFPAT1,STFHF2,STFPAT2,STFHF3,STFPAT3:
                        STFHF4,STFPAT4,STFHF5,STFPAT5,STFHF6,STFPAT6,STFNINV
.
        ADD        ONE,SECTOR
        WEOF       PATTAUD,SECTOR
        WRITE      PATTAUD,ZERO;SECTOR
        RETURN
