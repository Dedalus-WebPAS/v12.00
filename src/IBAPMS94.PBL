          ADD       ONE,NOOFMEMB
          MOVE      ZERO,FORM9
          MOVELPTR  DIM400,FORM9
          ADD       FORM9,NOOFBYTE
          ADD       ONE,NOOFBYTE            * add byte for "end of record"
          GOTO      DSCEX100
