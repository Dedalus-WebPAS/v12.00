RSTEMP1   READ      ADMTEMP1,KEY9;;             
          RETURN
.
RKTEMP1   MOVE      ZERO,OVRCD
          READKS    ADMTEMP1;XADRAWRD,XADRABED,XADRUNIQ,XADRNAME,XADRPDOB:
                             XADRADAT,XADRMEMB,XADRWRDD,XADRSPAR
          GOTO      OVERCOND IF OVER
          RETURN
