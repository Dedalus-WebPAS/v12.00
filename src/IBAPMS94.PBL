          APPEND    "Error renaming Pre-admission txt File - '",PRNTLINE
          APPEND    TASKID,PRNTLINE
          APPEND    "' for Command - ",PRNTLINE
          STRIP     LCMDLINE
          APPEND    LCMDLINE,PRNTLINE
          CALL      PRNT0000
