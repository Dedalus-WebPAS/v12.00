                             PIPE,PIPE,PIPE,PIPE,PIPE,PIPE:           *I-265283
                             PIPE,PIPE,PIPE,PIPE                      *I-265283
            WRITE     EXTFILE2,SEQ;*+,DIM400                          *I-265283
            CLEAR     PRNTLINE
            CALL      PRNT0000
            MOVE      "End of Discharge extract - No data found ",PRNTLINE
            CALL      PRNT0000
