.-----------------------------------------------------------------
. Kill the Temporary Usage Calculation File
.-----------------------------------------------------------------
KLALL000  PACK      SINALLNM,SINALLAA,PORT
          REP       " 0",SINALLNM
          CLEAR     CMDLINE
          APPEND    "iserase ",CMDLINE
          APPEND    SINALLNM,CMDLINE
          RESET     CMDLINE
.
          CLOSE     SINALLA1
          CLOSE     SINALLA2
          CLOSE     SINALLA3
          CLOSE     SINALLA4
          CLOSE     SINALLA5
          EXECUTE   CMDLINE,TASKID
.
KLALL999  RETURN
.-----------------------------------------------------------------
. Create the Temporary Usage Calculation File
.-----------------------------------------------------------------
CRALL000  CALL      KLALL000
          CLEAR     CMDLINE
          APPEND    "isbuild ",CMDLINE
          APPEND    SINALLNM,CMDLINE
          APPEND    " 45 UC6-10,16-22,1-5,11-15,23-28 ",CMDLINE
          APPEND    "UC1-5,6-10,23-28,11-15,16-22 ",CMDLINE
          APPEND    "UC1-5,16-22,23-28,11-15,6-10 ",CMDLINE
          APPEND    "DC16-22,23-28 ",CMDLINE
          APPEND    "DC16-22,1-5,23-28",CMDLINE
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
.
          OPEN      SINALLA1,SINALLNM
          OPEN      SINALLA2,SINALLNM
          OPEN      SINALLA3,SINALLNM
          OPEN      SINALLA4,SINALLNM
          OPEN      SINALLA5,SINALLNM
.
CRALL999  RETURN
