          DISPLAY   *P80:3,*+,SIPESDT
.
          OPEN      FMSAMAA1,"fmsamaaf"
.
RDCON999  RETURN
.----------------------------------------------------------------------
. Check warehouse security
. Returns SECSIWS : 0 = no access to any warehouse
.                   1 = access to one warehouse
.                   2 = access to more than one warehouse
.                   3 = access to all warehouse(Not Applicable - for future use)
.----------------------------------------------------------------------
..                                            * (for future use)
..CSIWS000  MOVE      "3",SECSIWS             * default to all warehouse's
.
..          PACK      KEY9,SP70               * any security set up ???
..          CALL      RSSIWS1
..          CALL      RKSIWS1
..          BRANCH    OVRCD,CSIWS999
.
CSIWS000  MOVE      "0",SECSIWS             * check for no warehouse's
          PACK      KEY9,PASSCODE,SP70
          CALL      RSSIWS1
          CALL      RKSIWS1
          BRANCH    OVRCD,CSIWS999
.
          MATCH     SIWSUID,PASSCODE
          GOTO      CSIWS999 IF NOT EQUAL
.
          MOVE      "1",SECSIWS             * check for one warehouse's
          CALL      RKSIWS1
          BRANCH    OVRCD,CSIWS999
.
          MATCH     SIWSUID,PASSCODE
          GOTO      CSIWS999 IF NOT EQUAL
.
          MOVE      "2",SECSIWS             * more than one warehouse
.
CSIWS999  RETURN
.
ITMDET00  MOVE      SIRTCAT,KEY7
          CALL      RDSIIA1
          MOVE      SIRTWAR,KEY5
          CALL      RDSIWA1
          PACK      KEY12,SIRTCAT,SIRTWAR
          CALL      RDSIIE1
          CALL      NCATA000
          RETURN
NSICC000  MOVE    SP70,NOTESICC
          PACK    KEY8,SICACODE,SP70
          CALL    RSSICC1
NSICC100  CALL    RKSICC1
          BRANCH  OVRCD,NSICC999
          MATCH   SP70,SICCCOM
          GOTO    NSICC100 IF EQUAL
          MOVE    "Notes",NOTESICC
NSICC999  RETURN
EREQT000  CALL      VCATRT00        * Verify Category Ok
          BRANCH    REQTSTAT,EREQT800,EREQT900
.
          CALL      REQTYP00
          BRANCH    EXIT,EREQT999,EREQT999
          MOVE      SICDDESC,REQTDESC
          GOTO      EREQT999
.
EREQT800  MOVE      ZERO,EXIT       * Only One Requisition Type
          GOTO      EREQT999
.
EREQT900  MOVE      "Requisition Type Codes Missing. (Cat. RT ) - ",DISPMSG
          CALL      MESSAGE1
          MOVE      TWO,EXIT
          GOTO      EREQT999
.
EREQT999  RETURN
.
VCATRT00  MOVE      "RT    ",KEY6
          CALL      RDSICD1
          BRANCH    OVRCD,VCATRT90
          CALL      RKSICD1
          BRANCH    OVRCD,VCATRT90
          MATCH     "RT ",SICDCATA
          GOTO      VCATRT90 IF NOT EQUAL
.
VCATRT10
          CALL      RKSICD1
          BRANCH    OVRCD,VCATRT80
          MATCH     "RT ",SICDCATA
          GOTO      VCATRT80 IF NOT EQUAL
          MATCH     "R  ",SICDCODE
          GOTO      VCATRT10 IF EQUAL
          MOVE      ZERO,REQTSTAT
          GOTO      VCATRT99
.
VCATRT80  CALL      RPSICD1
          MOVE      SICDCODE,REQTYPE
          MATCH     ANSR,REQTYPE         * Type "R" is Reserved for Returns
          GOTO      VCATRT90 IF EQUAL
          MOVE      SICDDESC,REQTDESC
          MOVE      ONE,REQTSTAT
          GOTO      VCATRT99
.
VCATRT90  MOVE      TWO,REQTSTAT   * Nothing Setup
          GOTO      VCATRT99
.
VCATRT99  RETURN
.
REQTYP00  KEYIN     *PCCOL:CVERT,*V2LON,*RV,*+,REQTYPE
          MOVE      REQTYPE,SICDCODE
          ENDSET    SICDCODE
          APPEND    SP70,SICDCODE
          RESET     SICDCODE
.
          MATCH     SP70,SICDCODE
          GOTO      REQTYP90 IF EQUAL
.
          CMATCH    EXITCHAR,SICDCODE
          GOTO      REQTYP95 IF EQUAL
.
          CMATCH    QUEST,SICDCODE
          GOTO      REQTYP10 IF NOT EQUAL
.
          MOVE      "RT ",SICDCATA
          MOVE      CCITEM,SCCITEM
          CALL      HSICDB00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    SICDCODE
          APPEND    SP70,SICDCODE
          RESET     SICDCODE
.
          MATCH     SP70,SICDCODE
          GOTO      REQTYP00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,SICDCODE
REQTYP10
          MOVE      "RT ",SICDCATA
          PACK      KEY6,SICDCATA,SICDCODE
          CALL      RDSICD1
          BRANCH    OVRCD,REQTYP80
.
          MATCH     "1",SICDCANC         * check if type has been cancelled
          IF        @EQUAL
            MOVE      SP70,SICDCODE
            BEEP
            CLEAR     DISPMSG
            APPEND    "Requisition Type Has Been Disabled - ",DISPMSG
            CALL      MESSAGE1
            GOTO      REQTYP00
          ENDIF
.
          MOVE      SICDCODE,REQTYPE
          MATCH     ANSR,REQTYPE         * Type "R" is Reserved for Returns
          GOTO      REQTYP70 IF EQUAL
          MATCH     ANSB,REQTYPE         * Type "B" is Reserved for Backorders
          GOTO      REQTYP71 IF EQUAL
          MATCH     ANSI,REQTYPE         * Type "I" is Reserved for Imprest
          GOTO      REQTYP72 IF EQUAL
          MATCH     ANST,REQTYPE         * Type "T" is Reserved for Transfers
          GOTO      REQTYP73 IF EQUAL
          MOVE      ZERO,EXIT
          GOTO      REQTYP99
.
REQTYP70  MOVE      "Requisition Type is for Return of Stock - ",DISPMSG
          CALL      MESSAGE1
          GOTO      REQTYP00
.
REQTYP71  MOVE      "Requisition Type is for Backorders - ",DISPMSG
          CALL      MESSAGE1
          GOTO      REQTYP00
.
REQTYP72  MOVE      "Requisition Type is for Imprest - ",DISPMSG
          CALL      MESSAGE1
          GOTO      REQTYP00
.
REQTYP73  MOVE      "Requisition Type is for Transfer - ",DISPMSG
          CALL      MESSAGE1
          GOTO      REQTYP00
.
REQTYP80  MOVE      "Requisition Type Does Not Exist - ",DISPMSG
          CALL      MESSAGE1
          GOTO      REQTYP00
REQTYP90  MOVE      ONE,EXIT
          GOTO      REQTYP99
REQTYP95  MOVE      TWO,EXIT
REQTYP99  RETURN
.
EREQN000  MOVE      ZERO,EXIT
          MOVE      SP70,REQNUMB
          IF        HAUTOREQ=TRUE
            CALL      AUTORQ00
            IF        EXIT=1
             GOTO       EREQN950
            ELSE
              GOTO      EREQN999
            ENDIF
          ENDIF
.
          STRIP     REQNUMB
          KEYIN     *PCCOL:CVERT,*V2LON,*MASK,*JR,*DE,*RV,REQNUMB
          PACK      REQNUMB,REQNUMB,SP70
          MATCH     SP70,REQNUMB
          GOTO      EREQN900 IF EQUAL
          MOVE      REQNUMB,FORM7
          MOVE      FORM7,REQNUMB
          PACK      KEY8,REQTYPE,REQNUMB
          CALL      RDSIRA1
          IF        OVRCD=0
            MOVE      "Requisition Already on File - ",DISPMSG
            BEEP
            CALL       MESSAGE1
            GOTO      EREQN000
          ENDIF
          MOVE      REQNUMB,VAR
          CALL      DISPITEM
.
          MOVE      "    6",FLDID        * Verify if field on screen
          CALL      GETPOSIT
          IF        EXIT=0
            KEYIN     *PCCOL:CVERT,*V2LON,*JR,VREQNUMB
            MOVE      "    2",FLDID
            CALL      GETPOSIT
            MOVE      VREQNUMB,KEY7
            MATCH     REQNUMB,KEY7
            IF        !(@EQUAL)
              BEEP
              GOTO      EREQN000
            ENDIF
          ENDIF
          MOVE      "    2",FLDID
          CALL      GETPOSIT
          MOVE      REQNUMB,SIRAREQ
          MOVE      ZERO,EXIT
          GOTO      EREQN999
.
EREQN900  IF        REQTSTAT=0
            MOVE      ONE,EXIT
          ELSE
            MOVE      TWO,EXIT
          ENDIF
          GOTO      EREQN999
.
EREQN950  MOVE      ONE,EXIT
.
EREQN999  RETURN
.----------------------------------------------------------------------
. Automatic Requisition Number Creation
.----------------------------------------------------------------------
AUTORQ00  MOVE      ZERO,EXIT
          PACK      KEY8,REQTYPE,Z20
          CALL      RSSIRA1
          CALL      RPSIRA1
          MATCH     SIRATYP,KEY8
          IF        @EQUAL & OVRCD=0
            MOVE      SIRAREQ,FORM7
            ADD       ONE,FORM7
          ELSE
            MOVE      ONE,FORM7
          ENDIF
          MATCH     "9999999",SIRAREQ
          GOTO      AUTORQ95 IF EQUAL
          MOVE      FORM7,REQNUMB
          MOVE      FORM7,VREQNUMB
          MOVE      FORM7,SIRAREQ
.
          MOVE      REQNUMB,VAR
          CALL      DISPITEM
          MOVE      "    6",FLDID
          CALL      GETPOSIT
          MOVE      VREQNUMB,VAR
          CALL      DISPITEM
          MOVE      "    2",FLDID        * Verify if field on screen
          CALL      GETPOSIT
          GOTO      AUTORQ99
.
AUTORQ95  MOVE      "All Requisition Numbers for this type are used - ",DISPMSG
          BEEP
          CALL      MESSAGE1
          MOVE      ONE,EXIT
          GOTO      AUTORQ99
.
AUTORQ99  RETURN
ECOST000  MOVE     ZERO,CKEYTYP
          CALL     KSICSA00
          BRANCH   EXIT,ECOST999,ECOST999
          IF       SICALOC=1
            BEEP
            MOVE     "Cost Centre Not Available for Requisitions - ",DISPMSG
            CALL     MESSAGE1
            GOTO     ECOST000
          ENDIF
.
          MATCH    SP70,SICAMES
          IF       !(@EQUAL)
            BEEP
            MOVE     SICAMES,DISPMSG
            CALL     MESSAGE1
          ENDIF
          MOVE     SICACODE,SIRACST
          MOVE     SP70,SIRADEL
          MOVE     ONE,OVRCD
          MOVE     ONE,ACCEPT
          MOVE     ONE,COUNTER3
.
ECOST999  RETURN
.------------------------------------------------------------
. Enter Requisition Date
.------------------------------------------------------------
ERADT000  CALL      KEYDATE
          PACK      SIRADAT,CCENT,CYEAR,CMON,CDAY
          REP       " 0",SIRADAT
          MOVE      SIRADAT,SIPEDATE
          CALL      RSIPE000   * Get Period End Date
          IF        OVRCD=1
            MOVE      "Period Not on File. Invalid Date - ",DISPMSG
            BEEP
            CALL      MESSAGE1
            GOTO      ERADT000
          ENDIF
          IF        SIPELOC=1
            MOVE      "Period Locked. - ",DISPMSG
            BEEP
            CALL      MESSAGE1
            GOTO      ERADT000
          ENDIF
.
          MATCH     SIPEYEAR,SIPECYR
          IF        !(@EQUAL)
            MOVE      "WARNING : Period Not in Current Year. - ",DISPMSG
            BEEP
            CALL      MESSAGE1
          ENDIF
.
          RETURN
.------------------------------------------------------------
. Enter Requisition Line Details
.------------------------------------------------------------
ELINE000  COMPARE   ZERO,ACCEPT
          GOTO      ELINE500 IF EQUAL
          MOVE      COUNTER3,SIRTUNI
.
ELINE100  MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
          CALL      ECATA000
          BRANCH    EXIT,ELINE950,ELINE999
.
          CALL      EREQU000
          IF        EXIT=1
            CALL      CLRLIN00
            GOTO      ELINE100
          ENDIF
          COMPARE   TWO,EXIT
          GOTO      ELINE999 IF EQUAL
.
          CALL      EISSU000
.
          MOVE      "   15",FLDID
          CALL      GETMLPOS
          MOVE      ZERO,EXIT
          ADD       ONE,COUNTER3
          GOTO      ELINE999
.
ELINE500  PACK      KEY5,SIRTWAR
          CALL      RDSIWA1
          PACK      KEY7,SIRTCAT
          CALL      RDSIIA1
          PACK      KEY12,SIRTCAT,SIRTWAR
          CALL      RDSIIE1
          CALL      NCATA000
          CALL      DCATA000     * Display Static Fields After Catalog Entry
.
          CALL      EREQU000
          BRANCH    EXIT,ELINE500,ELINE500
.
          CALL      EISSU000
          MOVE       "   15",FLDID
          CALL      GETMLPOS
          MOVE      ZERO,EXIT
          GOTO      ELINE999
.
ELINE900  MOVE      ONE,EXIT
          GOTO      ELINE999
.
ELINE950  MOVE      TWO,EXIT
.
ELINE999  RETURN
.------------------------------------------------------------
. Enter Warehouse if Item is on more that one warehouse
.------------------------------------------------------------
EWARE000  MOVE       "   14",FLDID
          CALL      GETMLPOS
          MOVE      SIIEWAR,DEFTWAR
.
EWARE005  CALL      RKSIIE1               * Check > 1 warehouse
          BRANCH    OVRCD,EWARE100        * No so just default
.
          MATCH     SIIECAT,SIIACAT
          GOTO      EWARE100 IF NOT EQUAL
          IF        SIIESUS=1|SIIESUS=3
            GOTO      EWARE005            * Suspended
          ENDIF
.
EWARE010  MATCH     DEFLTWH,SP70
          IF        @EQUAL
            CALL      DWARE000
            KEYIN     *PCCOL:CVERT,*V2LON,*MASK,*RV,DEFTWAR
            CALL      PUTSCR00
          ELSE
            PACK      KEY12,SIIACAT,DEFLTWH
            CALL      RDSIIE1
            IF        OVRCD<>0
              KEYIN     *PCCOL:CVERT,*V2LON,*MASK,*RV,DEFTWAR
              MATCH     QUEST,DEFTWAR
              IF        @EQUAL
                CALL      DWARE000
                MOVE      SP70,DEFTWAR
                KEYIN     *PCCOL:CVERT,*V2LON,*MASK,*RV,DEFTWAR
                CALL      PUTSCR00
              ENDIF
            ELSE
              MOVE      SIIEWAR,DEFTWAR
            ENDIF
          ENDIF
.
EWARE100  MOVE      DEFTWAR,SIWAWAR
.
          CMATCH    EXITCHAR,SIWAWAR
          IF        @EQUAL
            MOVE      SP70,SIWAWAR
            DISPLAY   *PCCOL:CVERT,*V2LON,SIWAWAR
            GOTO      EWARE900
          ENDIF
          DISPLAY   *PCCOL:CVERT,*V2LON,SIWAWAR
.
EWARE110  PACK      KEY5,SIWAWAR
          CALL      RDSIWA1
          BRANCH    OVRCD,EWARE800
.
. check warehouse security
.
          PACK      KEY9,PASSCODE,SIWAWAR,SP70
          CALL      RDSIWS1
          BRANCH    OVRCD,EWARE890
.
          MOVE      SIWAWAR,SIRTWAR
          PACK      KEY12,SIIACAT,SIRTWAR
          CALL      RDSIIE1
          IF        OVRCD=1
            BEEP
            CLEAR     DISPMSG
            APPEND    "Catalog Not Stocked in ",DISPMSG
            MOVE      SIWADES,KEY25
            APPEND    KEY25,DISPMSG
            APPEND    " - ",DISPMSG
            RESET     DISPMSG
            CALL      MESSAGE1
            GOTO      EWARE010
          ENDIF
.
          PACK      KEY12,SIIACAT,SIRTWAR,SP70
          CALL      SISIWK1
          IF        OVRCD=0
            BEEP
            CLEAR     DISPMSG
            APPEND    "Item in Stocktake for Warehouse ",DISPMSG
            APPEND    SIRTWAR,DISPMSG
            APPEND    " - ",DISPMSG
            RESET     DISPMSG
            CALL      MESSAGE1
            GOTO      EWARE900
          ENDIF
.
          IF        SIIESUS=1|SIIESUS=3
            BEEP
            MOVE      "Catalog Suspended in Warehouse. - ",DISPMSG
            CALL      MESSAGE1
            GOTO      EWARE900
          ENDIF
.
          MOVE      SIWAWAR,VAR
          CALL      DISPITEM
          MOVE      ZERO,EXIT
          GOTO      EWARE999
.
EWARE800  MOVE      "Warehouse Does Not Exist - ",DISPMSG
          BEEP
          CALL      MESSAGE1
          GOTO      EWARE010
.
EWARE890  BEEP
          CLEAR     DISPMSG
          APPEND    "You have no access for Warehouse ",DISPMSG
          APPEND    SIWAWAR,DISPMSG
          APPEND    " - ",DISPMSG
          CALL      MESSAGE1
          GOTO      EWARE900
.
EWARE900  MOVE      ONE,EXIT
          MOVE      SP70,VAR
          CALL      DISPITEM
          GOTO      EWARE999
EWARE950  MOVE      TWO,EXIT
.
EWARE999  RETURN
.------------------------------------------------------------
. Enter Catalog Code
.------------------------------------------------------------
ECATA000  MOVE      "   15",FLDID
          CALL      GETMLPOS
          MOVE      "S",CKEYSTCK
.
          MOVE      ZERO,CKEYTYP
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
          CALL      KSIIAA00
          BRANCH    EXIT,ECATA999,ECATA999
          MOVE      SIIACAT,VAR
          CALL      DISPITEM
.
          MOVE      "21",HTOP
          MOVE      "11",HLEF
          MOVE      "21",HBOT
          MOVE      "80",HRIG
          CALL      GETSCR00
          DISPLAY   *P11:21,*EL,*V2LON,SIIADES
.
          CALL      CLRCATA
          CALL      FCATA000     * Check Flags and Warehouse
          BRANCH    EXIT,ECATA000
.
          MATCH     SP70,DEFLTWH
          GOTO      ECATA105 IF EQUAL
.
          PACK      KEY12,SIIACAT,DEFLTWH,SP70
          CALL      RDSIIE1
          IF        OVRCD=1
            BEEP
            CLEAR     DISPMSG
            APPEND    "Catalog ",DISPMSG
            APPEND    SIIACAT,DISPMSG
            APPEND    " Not in Warehouse ",DISPMSG
            APPEND    DEFLTWH,DISPMSG
            RESET     DISPMSG
            CALL      MESSAGE4
            BRANCH    EXIT,ECATA000
          ENDIF
          IF        SIIESUS=1|SIIESUS=3
            BEEP
            CLEAR     DISPMSG
            APPEND    "Catalog ",DISPMSG
            APPEND    SIIACAT,DISPMSG
            APPEND    " Suspended ",DISPMSG
            RESET     DISPMSG
            CALL      MESSAGE1
            GOTO      ECATA000
          ENDIF
          GOTO      ECATA120
.
ECATA105  PACK      KEY12,SIIACAT,SP70
          CALL      RSSIIE1
.
ECATA110  CALL      RKSIIE1                * Check for warehouse
          BRANCH    OVRCD,ECATA900
          MATCH     SIIECAT,SIIACAT
          GOTO      ECATA900 IF NOT EQUAL
          IF        SIIESUS=1|SIIESUS=3
            MATCH     SP5,DEFLTWH
            IF        @EQUAL
              GOTO      ECATA110
            ENDIF
            MATCH     SIIEWAR,DEFLTWH
            IF        !@EQUAL
              GOTO      ECATA110             * Suspended
            ELSE
              GOTO      ECATA950
            ENDIF
          ENDIF
.
ECATA120  CALL      CBACK000     * Check for any back orders
          BRANCH    EXIT,ECATA000
.
          CALL      PUTSCR00
.
          CALL      EWARE000
          BRANCH    EXIT,ECATA000,ECATA999
.
          CALL      CHKFMS00
          IF        EXIT=1
            MOVE      SP70,VAR
            CALL      DISPITEM
            GOTO      ECATA000
          ENDIF
.
          MOVE      SIIACAT,SIRTCAT
          MOVE      SP70,NOTECATA
          CALL      NCATA000     * Check for Notes
          CALL      DCATA000     * Display Static Fields After Catalog Entry
          CALL      SWARN000     * Stock Level Warning
          MOVE      ZERO,EXIT
          GOTO      ECATA999
.
ECATA900  MOVE      "Catalog is NOT available in any Warehouse - ",DISPMSG
          BEEP
          CALL      MESSAGE1
          CALL      PUTSCR00
          GOTO      ECATA000
.
ECATA950  MOVE      "Catalog is suspended in selected Warehouse - ",DISPMSG
          BEEP
          CALL      MESSAGE1
          CALL      PUTSCR00
          GOTO      ECATA000
.
ECATA999  RETURN
.------------------------------------------------------------
. Stock Level Warning
.------------------------------------------------------------
SWARN000  COMPARE   ONE,SNCOIMS
          GOTO      SWARN999 IF NOT EQUAL
.
          IF        SIIESOH<SIIEROL
            BEEP
            CLEAR     DISPMSG
            APPEND    "WARNING: LOW STOCK ON HAND (ROL=",DISPMSG
            APPEND    SIIEROL,DISPMSG
            APPEND    ") - ",DISPMSG
            RESET     DISPMSG
            CALL      MESSAGE1
          ENDIF
.
SWARN999  RETURN
.------------------------------------------------------------
. Check for Backorders
.------------------------------------------------------------
CBACK000  MOVE      ZERO,F12P2
          PACK      KEY25,SICACODE,SIIACAT,SP70
          PACK      KEY12,SICACODE,SIIACAT,SP70
          CALL      RSSIBA1
.
CBACK100  CALL      RKSIBA1
          BRANCH    OVRCD,CBACK200
          PACK      KEY25,SIBACST,SIBACAT,SIBAREQ,SIBAWAR,SP70
          MATCH     KEY12,KEY25
          GOTO      CBACK200 IF NOT EQUAL
.
          ADD       SIBAQTY,F12P2
          GOTO      CBACK100
.
CBACK200  IF        F12P2>0
            BEEP
            CLEAR     DISPMSG
            APPEND    F12P2,DISPMSG
            APPEND    " Already on Backorder ",DISPMSG
            APPEND    "for this CC - ",DISPMSG
            CALL      MESSAGE4
            BRANCH    EXIT,CBACK950
          ENDIF
.
CBACK900  MOVE      ZERO,EXIT
          GOTO      CBACK999
.
CBACK950  CALL      PUTSCR00
          MOVE      ONE,EXIT
.
CBACK999  RETURN
.------------------------------------------------------------
. Check Flags for Catalog
.------------------------------------------------------------
FCATA000  MOVE      ZERO,EXIT
.
          IF        SIIANON=1
            BEEP
            MOVE      "Catalog is a Non-Stock Item. - ",DISPMSG
            CALL      MESSAGE1
            CALL      PUTSCR00
            MOVE      ONE,EXIT
            GOTO      FCATA999
          ENDIF
          IF        SIIASUS=1|SIIASUS=3
            BEEP
            MOVE      "Catalog is Suspended - ",DISPMSG
            CALL      MESSAGE1
            CALL      PUTSCR00
            MOVE      ONE,EXIT
          ENDIF
.
FCATA999  RETURN
.
. Display Fields After Catalog
.-----------------------------
DCATA000  PACK      SAVKEY15,SCPSPID,SCPSSID,SCPSROW,SCPSCOL
          MATCH     SP70,NOTECATA
          IF        !(@EQUAL)
            MOVE      "   34",FLDID
            CALL      GETPOSIT
            IF        EXIT=0
              MOVE      NOTECATA,VAR
              CALL      DISPITEM
            ENDIF
            MOVE      "   26",FLDID
            CALL      GETMLPOS
            IF        EXIT=0
              MOVE      NOTECATA,VAR
              CALL      DISPITEM
            ENDIF
          ENDIF
.
          MOVE      "   30",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            IF        SIIANON=0
              MOVE      SIIERAS,VAR
              CALL      DISPITEM
            ELSE
              MOVE      SP70,VAR
              CALL      DISPITEM
            ENDIF
          ENDIF
.
          MOVE      "   29",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      SIIACAT,VAR
            CALL      DISPITEM
          ENDIF
.
          MOVE      "   28",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            MOVE      SIIADES,VAR
            CALL      DISPITEM
          ENDIF
.
          MOVE      "   24",FLDID
          CALL      GETMLPOS
          IF        EXIT=0
            MOVE      SIIADES,VAR
            CALL      DISPITEM
          ENDIF
.
          MOVE      "   25",FLDID
          CALL      GETMLPOS
          IF        EXIT=0
            MOVE      SIIAISS,VAR
            CALL      DISPITEM
          ENDIF
.
          MOVE      "   27",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            IF        SIIANON=0
              MOVE      SIIESOH,VAR
              CALL      DISPITEM
            ELSE
              MOVE      SP70,VAR
              CALL      DISPITEM
            ENDIF
          ENDIF
.
          MOVE      "   33",FLDID
          CALL      GETPOSIT
          IF        EXIT=0
            IF        SIIANON=0
              MOVE      SIIAAVC,VAR
              CALL      DISPITEM
            ELSE
              MOVE      SP70,VAR
              CALL      DISPITEM
            ENDIF
          ENDIF
.
          MOVE      SAVKEY15,KEY15
          CALL      RDSCPS1
          RETURN
.
. Check for Notes
.----------------
NCATA000  MOVE      SP70,NOTECATA
          PACK      KEY10,SIIACAT,SP70
          CALL      RSSIIB1
          PACK      KEY15,SIIACAT,SIWAWAR,SP70
          CALL      RSSIIG1
NCATA100
          CALL      RKSIIB1
          BRANCH    OVRCD,NCATA200
          MATCH     SIIACAT,SIIBCAT
          GOTO      NCATA200 IF NOT EQUAL
          MATCH     SP70,SIIBCOM
          GOTO      NCATA100 IF EQUAL
          MOVE      "C",NOTECATA
NCATA200
          CALL      RKSIIG1
          BRANCH    OVRCD,NCATA300
          MATCH     SIIACAT,SIIGCAT
          GOTO      NCATA300 IF NOT EQUAL
          MATCH     SIWAWAR,SIIGWAR
          GOTO      NCATA300 IF NOT EQUAL
          MATCH     SP70,SIIGCOM
          GOTO      NCATA200 IF EQUAL
          MATCH     "C",NOTECATA
          IF        @EQUAL
            MOVE      "B",NOTECATA
          ELSE
            MOVE      "W",NOTECATA
          ENDIF
.
NCATA300  RETURN
.------------------------------------------------------------
. Enter Requested
.------------------------------------------------------------
EREQU000  MOVE       "   16",FLDID
          CALL      GETMLPOS
          MOVE      SP70,VAR
          CALL      DISPITEM
          IF        ACCEPT=1
            MOVE      ZERO,QTYFORM8
          ELSE
            MOVE      SIRTREQ,QTYFORM8
          ENDIF
.
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,QTYFORM8
          MOVE      QTYFORM8,SIRTREQ
          MOVE      SIRTREQ,VAR
          CALL      DISPITEM
          MOVE      ZERO,EXIT
          IF        QTYFORM8=0
            MOVE      ONE,EXIT
            GOTO      EREQU999
          ENDIF
          IF        QTYFORM8<1
            BEEP
            MOVE      "Required must be Greater Than 0 - ",DISPMSG
            CALL      MESSAGE1
            GOTO      EREQU000
          ENDIF
          READ      CONTROLF,TWENTY3;*120,HBACK
          IF        (HBACK<>TRUE) &  (QTYFORM8>SIIESOH)
            BEEP
            MOVE      "ERROR - Required greater than stock on hand - ",DISPMSG
            CALL      MESSAGE1
            GOTO      EREQU000
          ENDIF
EREQU999  RETURN
