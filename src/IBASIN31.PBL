EISSU000  MOVE       "   17",FLDID
          CALL      GETMLPOS
          BRANCH    EXIT,EISSU999
          IF        SIWAPIC=1
            MOVE      ZERO,SIRTISS
            MOVE      ZERO,QTYFORM8
            MOVE      ZERO,EXIT
            GOTO      EISSU999
          ENDIF
.
          MOVE      SP70,VAR
          CALL      DISPITEM
          MOVE      SIRTREQ,QTYFORM8
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,QTYFORM8
          MOVE      QTYFORM8,SIRTISS
          MOVE      SIRTISS,VAR
          CALL      DISPITEM
.
          IF        SIIESOH<QTYFORM8
            MOVE      "Issued Qty. Greater Than Stock on Hand. - ",DISPMSG
            BEEP
            CALL      MESSAGE1
            GOTO      EISSU000
          ENDIF
.
          IF        QTYFORM8>SIRTREQ
            MOVE      "WARNING - Issued Qty. Greater Than Required. - ",DISPMSG
            BEEP
            CALL      MESSAGE1
          ENDIF
.
          IF        QTYFORM8<0
            MOVE      "Issued Quantity Must Be Greater Than 0 - ",DISPMSG
            BEEP
            CALL      MESSAGE1
            GOTO      EISSU000
          ENDIF
          MOVE      ZERO,EXIT
.
EISSU999  RETURN
