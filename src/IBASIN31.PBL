. ------------------------------------------------------------
. Read Catalog Code Keyin Control Information
.
. INCOCATE  FORM      1         67      Catalog Code Type
. 0 Normal Keyin,   1 *JR *DE,   2 Generated ,  3 Generated with Check Digit
.------------------------------------------------------------
PRTSEL00  MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF FORMAT
          READ      CONTROLF,SIXTY3;*67,INCOCATE,*149,SNCOPTYP:
                                    *150,SNCOPRS,SNCOPPR,SNCOIMS
          TRAPCLR   FORMAT
          IF        OVRCD=1
            MOVE      ZERO,INCOCATE
            MOVE      ZERO,SNCOPTYP
            MOVE      ZERO,SNCOPRS
            MOVE      ZERO,SNCOPPR
            MOVE      ZERO,SNCOIMS
            WRITAB    CONTROLF,SIXTY3;*67,INCOCATE,*149,SNCOPTYP:
                                    *150,SNCOPRS,SNCOPPR,SNCOIMS
          ENDIF
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF FORMAT
          READ      CONTROLF,TWENTY3;*120,HBACK
          TRAPCLR   FORMAT
          IF        OVRCD=1
            MOVE      ZERO,HBACK
            WRITAB    CONTROLF,TWENTY3;*120,HBACK
          ENDIF
.
          READ      CONTROLF,TWENTY4;*198,HACCCODE
.
          COMPARE   ONE,SNCOPTYP
          GOTO      PRTSEL99 IF NOT EQUAL
.
          OPEN      IBACODE1,"ibacodef"
.         OPEN      IBAPRNT1,"ibaprntf"
          MOVE      SP70,SPLFILE
          CALL      SELPRINT
.
PRTSEL10  KEYIN     *P1:24,*EL,*P20:24,"Do You Require A Line Up Print (":
                    *V2LON,"Y",*HOFF,"/",*V2LON,"N",*HOFF,") ? ",*V2LON,ANS;
.
          PACK      ANS,ANS,SP10
          REP       UPPLOW,ANS
          CMATCH    "Y" TO ANS
          GOTO      PRTSEL20 IF EQUAL
.
          CMATCH    "N" TO ANS
          GOTO      PRTSEL99 IF EQUAL
.
          BEEP
          GOTO      PRTSEL10
.
PRTSEL20  CALL      OPNPRINT
          PRINT     *N,*N,*N,*N,*33,"PICKING SLIP",*71,CDATE:
                    *N,*N,*N,*1,"Department ","XXXXX":
                    *56,"Requisition No. ","A999999":
                    *N,*12,"XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX":
                    *55,"Requisition Date ",CDATE:
                    *N,*N,*1,"========================================":
                             "=======================================":
                    *N,*N,*N,*N,*N:
                    *N,*1,"9999999",*10,"XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX":
                    *42,"99999",*48,"9999.99":
                    *56,"99999",*63,"99999",*69,"XXXXXXXX":
                    *N,*10,"XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX",*N:
                    *N,*46,"=========":
                    *N,*46,"999999.99":
                    *N,*46,"=========":
                    *N,*N,*N,*N,*N,*N,*N,*N,*N,*N,*N,*N,*N,*N,*N,*N,*N:
                    *N,*N,*N,*N,*N,*N,*N,*N,*N:
                    *N,*1,"9999999",*10,"XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX":
                    *42,"99999",*48,"9999.99":
                    *56,"99999",*63,"99999",*69,"XXXXXXXX":
                    *N,*N
          CALL      CLSPRINT
          GOTO      PRTSEL10
.
