.------------------------------------------------------------
. Read patient details and write the patient line           
.------------------------------------------------------------
PRTROW00  PACK      KEY8,MREDVIS,SP70
          CALL      RDMISS1
          IF        OVRCD=1
            MOVE      "Invalid Admission",FORMATNM
          ENDIF
.
          PACK      KEY8,AURNO,SP70
          CALL      RDMAST1
          IF        OVRCD=1
            MOVE      "Invalid U/R",FORMATNM
          ELSE
            IF        PCEASE=1
              MOVE      PDECDTE,AGEDATE            * Date of Death
            ELSE
              PACK      AGEDATE,CCC,CYY,CMM,CDD    * Today's date
            ENDIF
            REP       " 0",AGEDATE
            CALL      CALCAGE
            CALL      PATCN000
          ENDIF
.
          PACK      KEY8,AADMNO,SP70
          CALL      RDDSCH1
          IF        OVRCD=1
            MOVE      SP70,DISPDATE
            GOTO      PRTROW99
          ELSE
            UNPACK    DDATE,CCENT,CYEAR,CMON,CDAY
            MOVE      CMON,F2
            LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
            PACK      DISPDATE,CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR
          ENDIF
.
          CALL      GETDRGZ                 * get Grouper version     *I-137125
          PACK      KEY7,PTMIPDRG,DRGVERZ,SP10                        *C-137125
          CALL      RDPTDGW1
          IF        OVRCD=1
            MOVE      SP70,PTDWDESC
          ENDIF
.
          MATCH    "1",MREDDEL
          IF       @EQUAL
            MOVE     "Yes",DELTSTAT
          ELSE
            MOVE     "No ",DELTSTAT
          ENDIF
.
          ADD       ONE,CLNO
          COMPARE   FIFTY5,CLNO
          CALL      PRTHD000 IF EQUAL
.
          PRINT     *1,"|",*3,MREDURNO,*13,"|":
                    *14,MREDVIS,*24,"|":
                    *26,FORMATNM,*56,"|":
                    *58,DELTSTAT,*66,"|":
                    *68,DISPDATE,*80,"|"
.
PRTROW99  RETURN
