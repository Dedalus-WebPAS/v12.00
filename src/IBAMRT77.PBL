.         Create the temporary index file
.*********************************************************************
CTPI0000  CALL      DTPI0000
          CLEAR     CMDLINE
          APPEND    "isbuild ",CMDLINE
          APPEND    FNAMEI,CMDLINE
          APPEND    " 91 u31-36,1-22,72-79",CMDLINE                    *C-240716
          RESET     CMDLINE
          EXECUTE   CMDLINE,TASKID
          OPEN      PSTIDX,FNAMEI
CTPI9999  RETURN
