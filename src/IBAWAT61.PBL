          CALL      HEAD0000 IF LESS        * Print report header
.
          PACK      ERRMSG,ERRMSG,SP127
          PRINT     *1,"|",TMPURNO,"|",TMPROCOD,"|",TMPROCNT," |",ERRMSG,"|"
          ADD       ONE,CLNO
          ADD       ONE,ERRCNT              * Increment the error counter
          PACK      ERRMSG,SP127
