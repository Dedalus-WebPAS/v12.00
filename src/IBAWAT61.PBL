          ENDIF
.
          MATCH     "A",URGCAT
          GOTO      VALD1600 IF EQUAL
          MATCH     "B",URGCAT
          GOTO      VALD1600 IF EQUAL
          MATCH     "C",URGCAT
          GOTO      VALD1600 IF EQUAL
          MATCH     "D",URGCAT
          GOTO      VALD1600 IF EQUAL
.
          MOVE      "Invalid Current urgency category",ERRMSG
          CALL      PRNT0000              * Print error message
