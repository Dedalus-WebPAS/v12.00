          ADD       SICFBUD,BUDPERXX
          GOTO      EPROD100
.
EPROD900  MOVE      ACCEPT,OVRCD
          IF        ACCEPT=1
            MOVE      ZERO,ACCEPT
            CALL      DF02L000
            MOVE      ONE,ACCEPT
            MOVE      "    1",FLDID
            CALL      GETPOSIT
          ENDIF
          GOTO      EPROD999
.
EPROD990  MOVE      TWO,EXIT
.
EPROD999  RETURN
ECOMM000  CALL      KSIGAA00
          BRANCH    EXIT,ECOMM990,ECOMM990
.
          MOVE      ONE,ACCEPT
          MOVE      ZERO,BUDPERXX
          MOVE      ZERO,F2
ECOMM100  ADD       ONE,F2
          IF        F2>SIPENUM
            GOTO      ECOMM900
          ENDIF
          PACK      SICEDAT,FINYEAR,F2
          REP       " 0",SICEDAT
          PACK      KEY17,SICACODE,SIGACOD,SICEDAT                     *C-59696
          CALL      RDSICE2
          IF        OVRCD=0
            MOVE      ZERO,ACCEPT
          ELSE
            MOVE      ZERO,SICEBUD
          ENDIF
.
          STORE     SICEBUD,F2,BUDPER01,BUDPER02,BUDPER03,BUDPER04:
                               BUDPER05,BUDPER06,BUDPER07,BUDPER08:
                               BUDPER09,BUDPER10,BUDPER11,BUDPER12,BUDPER13
          ADD       SICEBUD,BUDPERXX
          GOTO      ECOMM100
.
ECOMM900  MOVE      ACCEPT,OVRCD
          IF        ACCEPT=1
            MOVE      ZERO,ACCEPT
            CALL      DF03L000
            MOVE      ONE,ACCEPT
            MOVE      "    1",FLDID
            CALL      GETPOSIT
          ENDIF
          GOTO      ECOMM999
.
ECOMM990  MOVE      TWO,EXIT
.
ECOMM999  RETURN
