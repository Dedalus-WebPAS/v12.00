          GOTO      PMIG9999
.
PMIG1500  WRITE     HTMLFILE;PMIGSTAI;
          GOTO      PMIG9999
.
PMIG9999  RETURN
.
.-------------------------------------------------------------------------------
. Move Impairment Group cgi variables to file variables
.-------------------------------------------------------------------------------
MVPMIG00  MATCH     Z70,PMIGR001
          IF        !@EQUAL
            MOVE      PMIGR001,PMIGCODE
          ENDIF
.
          MATCH     Z70,PMIGR002
          IF        !@EQUAL
            MOVE      PMIGR002,PMIGDESC
          ENDIF
.
          COMPARE   ZERO,PMIGR003
          IF        !@EQUAL
            MOVE      PMIGR003,PMIGEATI
          ENDIF
.
          COMPARE   ZERO,PMIGR004
          IF        !@EQUAL
            MOVE      PMIGR004,PMIGROOM
          ENDIF
.
          COMPARE   ZERO,PMIGR005
          IF        !@EQUAL
            MOVE      PMIGR005,PMIGBATH
          ENDIF
.
          COMPARE   ZERO,PMIGR006
          IF        !@EQUAL
            MOVE      PMIGR006,PMIGUPBD
          ENDIF
.
          COMPARE   ZERO,PMIGR007
          IF        !@EQUAL
            MOVE      PMIGR007,PMIGLWBD
          ENDIF
.
          COMPARE   ZERO,PMIGR008
          IF        !@EQUAL
            MOVE    PMIGR008,PMIGTOIL
          ENDIF
.
          COMPARE   ZERO,PMIGR009
          IF        !@EQUAL
            MOVE    PMIGR009,PMIGBLAD
          ENDIF
.
          COMPARE   ZERO,PMIGR010
          IF        !@EQUAL
            MOVE      PMIGR010,PMIGBOWE
          ENDIF
.
          COMPARE   ZERO,PMIGR011
          IF        !@EQUAL
            MOVE      PMIGR011,PMIGTBED
          ENDIF
.
          COMPARE   ZERO,PMIGR012
          IF        !@EQUAL
            MOVE      PMIGR012,PMIGTTOI
          ENDIF
.
          COMPARE   ZERO,PMIGR013
          IF        !@EQUAL
            MOVE      PMIGR013,PMIGTTUB
          ENDIF
.
          COMPARE   ZERO,PMIGR014
          IF        !@EQUAL
            MOVE      PMIGR014,PMIGWALK
          ENDIF
.
          COMPARE   ZERO,PMIGR015
          IF        !@EQUAL
            MOVE      PMIGR015,PMIGSTAI
          ENDIF
.
MVPMIG99  RETURN
.
