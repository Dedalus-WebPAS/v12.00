SPPMIG15  MOVE      QRYDATA,PMIGR015
          GOTO      SPPMIG99
.
SPPMIG99  RETURN
.
.-------------------------------------------------------------------------------
. Output Impairment Group tags
.-------------------------------------------------------------------------------
PMIG0000  BRANCH    FLDITMNO,PMIG0100,PMIG0200,PMIG0300,PMIG0400,PMIG0500:
                             PMIG0600,PMIG0700,PMIG0800,PMIG0900,PMIG1000:
                             PMIG1100,PMIG1200,PMIG1300,PMIG1400,PMIG1500
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      PMIG9999
.
PMIG0100  WRITE     HTMLFILE;PMIGCODE;
          GOTO      PMIG9999
.
PMIG0200  WRITE     HTMLFILE;PMIGDESC;
          GOTO      PMIG9999
.
PMIG0300  WRITE     HTMLFILE;PMIGEATI;
          GOTO      PMIG9999
.
PMIG0400  WRITE     HTMLFILE;PMIGROOM;
          GOTO      PMIG9999
.
PMIG0500  WRITE     HTMLFILE;PMIGBATH;
          GOTO      PMIG9999
.
PMIG0600  WRITE     HTMLFILE;PMIGUPBD;
          GOTO      PMIG9999
.
PMIG0700  WRITE     HTMLFILE;PMIGLWBD;
          GOTO      PMIG9999
.
PMIG0800  WRITE     HTMLFILE;PMIGTOIL;
          GOTO      PMIG9999
.
PMIG0900  WRITE     HTMLFILE;PMIGBLAD;
          GOTO      PMIG9999
.
PMIG1000  WRITE     HTMLFILE;PMIGBOWE;
          GOTO      PMIG9999
.
PMIG1100  WRITE     HTMLFILE;PMIGTBED;
          GOTO      PMIG9999
.
PMIG1200  WRITE     HTMLFILE;PMIGTTOI;
          GOTO      PMIG9999
.
PMIG1300  WRITE     HTMLFILE;PMIGTTUB;
          GOTO      PMIG9999
.
