.------------------------------------------------------
. Note  : Keyin Routine Modified to accept *DE only
.------------------------------------------------------
.KCCSTA10
.          TYPE      CCSTSTY
.          GOTO      KCCSTA00 IF NOT EQUAL
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSSTYKY
.   Routine   :   HCCSTA00 Casemix Type        
.   Return    :           
.-------------------------------------------------------------------------------
HCCSTA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "30",HLEF
            MOVE      "80",HRIG
          ENDIF
HCCSTA04
          CALL      GSCRN000
          BRANCH    EXIT,HCCSTA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Casemix Type"       
.
          MOVE      STDCOL01,STDCOL02
          ADD       "13",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "35",STDCOL03
HCCSTA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,CCSTSTY 
          PACK      KEY3,CCSTSTY,SP70                                           
.                                                                               
.                                                                               
          CALL      RSCCST1 
HCCSTA10
          CALL      RKCCST1 
          BRANCH    OVRCD,HCCSTA15
          PACK      KEY3,CCSTSTY,SP70                                           
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HCCSTA12
          CALL      HCCSTA60
          BRANCH    OVRCD,HCCSTA10
HCCSTA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,CCSTSTY 
          DISPLAY   *PSTDCOL02:STDROW,CCSTDES 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HCCSTA10 IF NOT EQUAL
HCCSTA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HCCSTA20
          CALL      HSCRI000
          BRANCH    EXIT,HCCSTA30,HCCSTA35,HCCSTA40,HCCSTA45,HCCSTA04:
                              HCCSTA91,HCCSTA50
          GOTO      HCCSTA92
HCCSTA30
          CALL      RKCCST1 
          BRANCH    OVRCD,HCCSTA85
          PACK      KEY3,CCSTSTY,SP70                                           
.                                                                               
.                                                                               
          CALL      RPCCST1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCSTA10
HCCSTA35
          MOVE      ST1,KEY3 
          CALL      RDCCST1 
          CALL      RPCCST1 
          BRANCH    OVRCD,HCCSTA85
          PACK      KEY3,CCSTSTY,SP70                                           
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HCCSTA36
          CALL      RPCCST1 
          BRANCH    OVRCD,HCCSTA37
          PACK      KEY3,CCSTSTY,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCSTA36 IF NOT EQUAL
HCCSTA37
          MOVE      ZERO,XITEMNO
          GOTO      HCCSTA10
HCCSTA40
          MOVE      ST1,KEY3 
          CALL      RDCCST1 
          CALL      RPCCST1 
          BRANCH    OVRCD,HCCSTA85
          PACK      KEY3,CCSTSTY,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCSTA08
HCCSTA45
          CALL      RKCCST1 
          BRANCH    OVRCD,HCCSTA85
          PACK      KEY3,CCSTSTY,SP70                                           
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY3,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSCCST1 
HCCSTA46
          CALL      RPCCST1 
          BRANCH    OVRCD,HCCSTA47
          PACK      KEY3,CCSTSTY,SP70                                           
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HCCSTA46 IF NOT EQUAL
HCCSTA47
          MOVE      ZERO,XITEMNO
          GOTO      HCCSTA10
HCCSTA50  COMPARE   ZERO,CSEARCH
          GOTO      HCCSTA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCSTA08
HCCSTA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HCCSTA08
HCCSTA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HCCSTA61 IF EQUAL
          MATCH     SCHFLD01,CCSTSTY 
          GOTO      HCCSTA72 IF NOT EQUAL
HCCSTA61
          MATCH     SP70,SCHFLD02
          GOTO      HCCSTA62 IF EQUAL
          MATCH     SCHFLD02,CCSTDES 
          GOTO      HCCSTA72 IF NOT EQUAL
HCCSTA62
HCCSTA63
HCCSTA64
HCCSTA65
HCCSTA66
HCCSTA67
HCCSTA68
HCCSTA69
HCCSTA70
          GOTO      HCCSTA75
HCCSTA72  MOVE      ONE,OVRCD
HCCSTA75  RETURN
HCCSTA85
          LOAD      KEY3,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,CCSTSTY                                                
          CALL      RDCCST1 
HCCSTA90
.                                                                               
          BEEP
          GOTO      HCCSTA20
HCCSTA91
          MOVE      SP70,CCSTDES 
          MOVE      ZERO,CCSTLEN 
          MOVE      SP70,CCSTJR  
          MOVE      SP70,CCSTZF  
          MOVE      SP70,CCSTDE  
          MOVE      SP70,CCSTSPA 
          MOVE      SP70,KEY3 
          MOVE      SP70,CCSTSTY 
          GOTO      HCCSTA96
HCCSTA92
          MOVE      SP70,KEY3 
          MOVE      SP70,CCSTSTY 
          LOAD      KEY3,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY3,CCSTSTY                                                
HCCSTA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HCCSTA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   Clinical Costing System            
.   Include   :   CCSSTYKY
.   Routine   :   KCCSTA00 Keyin Casemix Type        
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KCCSTA00 TOPIC     "CCSTSTY "
          MOVE      UNDLN70,CCSTSTY 
          KEYIN     *PCCOL:CVERT,*DV,CCSTSTY:                                   
                    *PCCOL:CVERT,*V2LON,*JR,*ZF,CCSTSTY:                        
                    *PCCOL:CVERT,*DV,CCSTSTY 
.
          ENDSET    CCSTSTY 
          APPEND    SP70,CCSTSTY 
          RESET     CCSTSTY 
.
          MATCH     SP70,CCSTSTY 
          GOTO      KCCSTA90 IF EQUAL
.
          ENDSET    CCSTSTY 
          CMATCH    EXITCHAR,CCSTSTY 
          GOTO      KCCSTA95 IF EQUAL
.
          CMATCH    QUEST,CCSTSTY 
          GOTO      KCCSTA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HCCSTA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    CCSTSTY 
          APPEND    SP70,CCSTSTY 
          RESET     CCSTSTY 
.
          MATCH     SP70,CCSTSTY 
          GOTO      KCCSTA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,CCSTSTY                                 
KCCSTA10
          RESET     CCSTSTY 
          PACK      KEY3,CCSTSTY                                                
          CALL      RACCST1 
          BRANCH    OVRCD,KCCSTA80
          BRANCH    CKEYTYP,KCCSTA75
          CALL      RDCCST1 
KCCSTA70
          MOVE      ZERO,EXIT
          GOTO      KCCSTA99
KCCSTA75
          MOVE      "Casemix Type Already Exist - ",DISPMSG                                          
          CALL      MESSAGE1
          GOTO      KCCSTA00
KCCSTA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KCCSTA99,KCCSTA99
          MOVE      "Casemix Type Does Not Exist - ",DISPMSG                                         
          CALL      MESSAGE1
          GOTO      KCCSTA00
KCCSTA90
          MOVE      ONE,EXIT
          GOTO      KCCSTA99
KCCSTA95
          MOVE      TWO,EXIT
KCCSTA99 TOPIC    PRGID
          RETURN
