             GOTO       CMEF9500
           ELSE
             MATCH      DIM3A,PSTATCM
             GOTO       CMEF9000 IF EQUAL
             MATCH      DIM3B,PSTATCM
             GOTO       CMEF9000 IF EQUAL
             MATCH      DIM3C,PSTATCM
             GOTO       CMEF9000 IF EQUAL
             MATCH      DIM3D,PSTATCM
             GOTO       CMEF9000 IF EQUAL
             MATCH      DIM3E,PSTATCM
             GOTO       CMEF9000 IF EQUAL
.... CAR 46081
             MOVE       ZERO,FINDFLG
....
             GOTO       CMEF9999
           ENDIF
CMEF9000   MOVE         ONE,FINDFLG
           GOTO         CMEF9999
          ENDIF      
....
... exclusive (PMCCFTDS =3)
.
          MATCH      SP70,PSTATCM
          GOTO       CMEF9550 IF EQUAL
.
          MATCH      SP3,DIM3A
          IF         !@EQUAL
            MATCH      DIM3A,PSTATCM
            GOTO       CMEF9500 IF EQUAL
          ENDIF
.
          MATCH      SP3,DIM3B
          IF         !@EQUAL
            MATCH      DIM3B,PSTATCM
            GOTO       CMEF9500 IF EQUAL
          ENDIF
          
          MATCH      SP3,DIM3C
          IF         !@EQUAL
            MATCH      DIM3C,PSTATCM
            GOTO       CMEF9500 IF EQUAL
          ENDIF

          MATCH      SP3,DIM3D
          IF         !@EQUAL
            MATCH      DIM3D,PSTATCM
            GOTO       CMEF9500 IF EQUAL
          ENDIF
