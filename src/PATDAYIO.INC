.        INCLUDE NAME : PATDAYIO.INC
.
.        FILE I/O REQUESTS FOR DAILY PATIENT LIST FILE
.
.OVRCD   FORM     1        0 = NOT OVER
.                          1 = OVER
.KEY12   DIM      12       KEY FOR PATDAYA1
.
RAPTDAY1 MOVE     ZERO,OVRCD
         RESET    KEY12 
         READ     PATDAYA1,KEY12;ANS
         GOTO     OVERCOND IF OVER
         RETURN
.
RSPTDAY1 RESET    KEY12
         READ     PATDAYA1,KEY12;;
         RETURN
.
RKPTDAY1 MOVE     ZERO,OVRCD
         READKS   PATDAYA1;PTDYDATE,DPTDYADM
         MOVE     DPTDYADM,PTDYADMN
         GOTO     OVERCOND IF OVER
.
         RETURN
.
RPPTDAY1 MOVE     ZERO,OVRCD
         READKP   PATDAYA1;PTDYDATE,DPTDYADM
         MOVE     DPTDYADM,PTDYADMN
         GOTO     OVERCOND IF OVER
.
         RETURN
.
RDPTDAY1 MOVE     ZERO,OVRCD
         RESET    KEY12
         READ     PATDAYA1,KEY12;PTDYDATE,DPTDYADM
         MOVE     DPTDYADM,PTDYADMN
         GOTO     OVERCOND IF OVER
.
         RETURN
.
UPPTDAY1 MOVE     PTDYADMN,DPTDYADM
         UPDATE   PATDAYA1;PTDYDATE,DPTDYADM
         RETURN
.
DEPTDAY1 RESET    KEY12
         DELETE   PATDAYA1,KEY12
         RETURN
.
WRPTDAY1 MOVE     ZERO,OVRCD
         RESET    KEY12
         MOVE     PTDYADMN,DPTDYADM
         WRITE    PATDAYA1,KEY12;PTDYDATE,DPTDYADM
.
         RETURN
.
RDPTDBY1 MOVE     ZERO,OVRCD
         RESET    KEY12
         READ     PATDBYB1,KEY12;PTDYDATE,DPTDYADM
         MOVE     DPTDYADM,PTDYADMN
         GOTO     OVERCOND IF OVER
.
         RETURN
.
WRPTDBY1 MOVE     ZERO,OVRCD
         RESET    KEY12
         MOVE     PTDYADMN,DPTDYADM
         WRITE    PATDBYB1,KEY12;PTDYDATE,DPTDYADM
.
         RETURN
.
