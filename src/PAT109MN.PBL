. ICD10 Edition 9 Procedure Code: Search Table
.-----------------------------------------------------------
SPT9OA00  WRITE     HTMLFILE;"<tr align=center>":
                             "<td class=HeadingCell colspan=2>":
                             "<b>You Searched for ":
                               KEYWORDS,"</td>":
                             "</tr>":
                             "<tr>":
                             "<td class=HeadingCell width=15%><b>Code</b></td>":
                             "<td class=HeadingCell><b>Description</b></td>":
                             "</tr>"
.
          CALL      GETWRD00
.
          UNPACK    DIM127,D1,LINKPRG,DIM127
          UNPACK    DIM127,D1,LINKREP,DIM127
          UNPACK    DIM127,D1,LINKTEM,DIM127
.
          IF        NORECORD=0
            MOVE      PTCNNRTD,NORECORD      * Set Default No Records to Display
          ENDIF
          MOVE      ZERO,RECNUMB
          MOVE      ZERO,DISNUMB
.
....      UNPACK    PTCNGDV9,KEY4,KEY5
....      MATCH     "9999",KEY4          * is ICD10 Ed.9 in use yet?
....      GOTO      SPT9OA90 IF EQUAL
.
          STRIP     KEYWRD01
          PACK      KEY24,KEYWRD01,SP70
          CALL      RSPT9O2
SPT9OA10  CALL      RKPT9O2
          BRANCH    OVRCD,SPT9OA90
.
          CALL      SPT9OB00
          BRANCH    OVRCD,SPT9OA10,SPT9OA90
.
          MOVE      PTCOITM,KEY9
          CALL      RDPT19O1
          BRANCH    OVRCD,SPT9OA10
.
          ADD       ONE,RECNUMB
          IF        STARTNUM>=RECNUMB
            GOTO      SPT9OA10
          ENDIF
.
          BRANCH    LNK2PRT,SPT9OA20,SPT9OA30
          GOTO      SPT9OA99
.
SPT9OA20  WRITE     HTMLFILE;"<tr><td nowrap><A HREF='javascript:showICD02":
                             "(#"",LINKPRG,"#.pbl":
                             "?reportno=",LINKREP:
                             "&template=",LINKTEM:
                             "&ptico001=",OPCODE,"#")'>":
                             "<img src=../images/MaintenanceIcon.gif hspace=4":
                             " border=0 align=absmiddle></a>":
                             OPCODE,"</td>":
                             "<td>",PT0ODSC2,"</td>":
                             "</tr>"
.
          GOTO      SPT9OA80
.
SPT9OA30  WRITE     HTMLFILE;"<tr valign=top>":
                              "<td><a href=#"#" ":
                              "OnClick=#"updateParent('",PT0ODSC2:
                              "','",OPCODE,"')#">":
                              "<img src=../images/MaintenanceIcon.gif hspace=4":
                              " border=0 align=absmiddle></a>":
                              OPCODE,"</td>":
                              "<td>",PT0ODSC2,"</td>":
                             "</tr>"
.
SPT9OA80  ADD       ONE,DISNUMB
          COMPARE   NORECORD,DISNUMB
          GOTO      SPT9OA10 IF NOT EQUAL
          GOTO      SPT9OA99
.
SPT9OA90  CALL      NOMORE00      * Display No More Records Message
.
SPT9OA99  RETURN
.------------------------------------------------------------
. Check ICD10 Edition 9 Procedure Record
.------------------------------------------------------------
SPT9OB00  MATCH     KEYWRD01,PTCOKWD
          GOTO      SPT9OB92 IF NOT EQUAL
          IF        KEYWRDNO>1
            PACK      SAVKEY24,PTCOKWD,PTCOITM
            CALL      SPT9OC00
            BRANCH    EXIT,SPT9OB91
            MOVE      SAVKEY24,KEY24
            CALL      RDPT9O2
          ENDIF
.
. Insert Other Check for Valid Record Here i.e. Suspended or In Active
.   eg    COMPARE    ONE,INACTIVE
.         GOTO       SPT9OB91 IF EQUAL
.
          GOTO      SPT9OB90
.
SPT9OB90  MOVE      ZERO,OVRCD
          GOTO      SPT9OB99
SPT9OB91  MOVE      ONE,OVRCD
          GOTO      SPT9OB99
SPT9OB92  MOVE      TWO,OVRCD
SPT9OB99  RETURN
.------------------------------------------------------------
. Check for Matching ICD10 Edition 9 Procedure Words
.------------------------------------------------------------
SPT9OC00  MOVE      ONE,F2
          MOVE      ZERO,EXIT
          PACK      SAVKEY9,PTCOITM,SP70
.
SPT9OC10  COMPARE   KEYWRDNO,F2           * Loop through each Key Word in Search
          GOTO      SPT9OC99 IF EQUAL     * Exit OK
          ADD       ONE,F2
          LOAD      KEYWRDXX,F2,KEYWRD01,KEYWRD02,KEYWRD03,KEYWRD04,KEYWRD05:
                             KEYWRD06,KEYWRD07,KEYWRD08,KEYWRD09,KEYWRD10
          PACK      KEY24,PTCOITM,KEYWRDXX
          CALL      RDPT9O1               * Check for Exact Match
          COMPARE   ZERO,OVRCD            * Ok so Continue Loop
          GOTO      SPT9OC10 IF EQUAL
.
          CALL      RKPT9O1               * Not Exact so Read Next Record
          BRANCH    OVRCD,SPT9OC90        * No Match So Exit No Match
          PACK      KEY9,PTCOITM,SP70
          MATCH     SAVKEY9,KEY9        * Match Key Fields
          GOTO      SPT9OC90 IF NOT EQUAL * No Match So Exit
          STRIP     KEYWRDXX
          MATCH     PTCOKWD,KEYWRDXX         * Check Key Word
          GOTO      SPT9OC90 IF NOT EQUAL * No Match So Exit
          GOTO      SPT9OC10              * Check Next Search Word
.
SPT9OC90  MOVE      ONE,EXIT
SPT9OC99  RETURN
