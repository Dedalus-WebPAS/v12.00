.------------------------------------------------------------------------------
. ICD10 Edition 9 Diagnosis Coding Maintenance: Display,Add,Update,Delete
.------------------------------------------------------------------------------
ICD9DI00  RESET     PTICD001
          MOVE      "9",ICD10EDN
.                                           * make sure files are open
          OPEN      PATI19D1,"pat10d9f"     * Open ICD10 Ed.9 Diseases
          OPEN      PATK9DA1,"patkcd9f"     * ICD10 Ed.9 Key Word Diag.
          OPEN      PATK9DA2,"patkcd9f"     * ICD10 Ed.9 Key Word Diag.
.
          RESET     UPDTTYPE
          MATCH     SP1,UPDTTYPE
          GOTO      ICD9DI40 IF EQUAL
.
          MATCH     "A",UPDTTYPE    * Add formaction
          GOTO      ICD9DI10 IF EQUAL
.
          MATCH     "U",UPDTTYPE    * Update formaction
          GOTO      ICD9DI20 IF EQUAL
.
          MATCH     "D",UPDTTYPE    * Delete formaction
          GOTO      ICD9DI30 IF EQUAL
.
          MATCH     "V",UPDTTYPE    * Validate Formaction
          IF        @EQUAL
            CALL      VAL9CD00
            GOTO      ICD9DI99
          ENDIF
.
          GOTO      ICD9DI93
.
ICD9DI10  CALL      CHKDIS00  * Checks mandatory fields & checks for blank
          BRANCH    EXIT,ICD9DI99
          PACK      KEY9,PTICD001,SP70  * ADD FORMACTION
          CALL      RDPT19D1
          COMPARE   ZERO,OVRCD
          GOTO      ICD9DI92 IF EQUAL
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      WRPT19D1      * Writes away the data
          PROC      PATK9DUP      * Update Keyword table
.
          CALL      ADDCAN00      * Add Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD9DI99
.
ICD9DI20  PACK      KEY9,PTICD001,SP70  * UPDATE FORMACTION
          CALL      RDPT19D1
          BRANCH    OVRCD,ICD9DI91
          CALL      CHKDIS00      * Checks mandatory fields
          BRANCH    EXIT,ICD9DI99
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      UPPT19D1      * Writes away the data
          PROC      PATK9DUP      * Update Key word Table
.
          CALL      UPDCAN00      * Update Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD9DI99
.
ICD9DI30  PACK      KEY9,PTICD001,SP70   * DELETE FORMACTION
          CALL      RDPT19D1
          BRANCH    OVRCD,ICD9DI91
          CALL      DEPT19D1      * Deletes the data
          PROC      PATK9DUP      * Update Key word Table
.
          CALL      DELCAN00      * Delete Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD9DI99
.
ICD9DI40  PACK      KEY9,PTICD001,SP70
          CALL      RDPT19D1
          IF        OVRCD=1
            CALL      CLRDIS00    * Clear all the file variables
          ENDIF
.
          MOVE      ONE,CANCERCD
          PACK      KEY9,PTICD001,SP70
          CALL      RDPTCAN1
          IF        OVRCD=1
            MOVE     ZERO,CANCERCD
            MOVE     ZERO,PTCCLATR
          ENDIF
.
          CLEAR     WEBTITLE
          MOVE      "ICD10 Edition 9 Diagnosis Maintenance",WEBTITLE
          RESET     WEBTITLE
.
          CALL      WEBHED00   * Create Output File and Write HTML Page Header
          BRANCH    EXIT,ICD9DI99
          CALL      WEBBOD00   * Process Web Body
          CALL      WEBEND00   * Process End of HTML File
          MOVE      ONE,EXIT
          GOTO      ICD9DI99
.
ICD9DI91  MOVE      "ICD10 Edition 9 Diagnosis Code does not exist",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD9DI99
.
ICD9DI92  MOVE      "ICD10 Edition 9 Diagnosis Code exists",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD9DI99
.
ICD9DI93  MOVE      "Invalid Form Action.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD9DI99
.
ICD9DI99  RETURN
+
