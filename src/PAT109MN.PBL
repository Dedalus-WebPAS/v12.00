.------------------------------------------------------------------------------
. Online ICD10 Edition 9 Code Validation
.------------------------------------------------------------------------------
VAL9CD00  MOVE      SP70,VALDNAME
          MATCH     SP70,VALDCODE
          GOTO      VAL9CD90 IF EQUAL
.
          MOVE      "Invalid Code Entered",VALDNAME
          BRANCH    VALDTYPE,VAL9CD10,VAL9CD20
          MOVE      "Invalid Validation Type",VALDNAME
          GOTO      VAL9CD90
.
VAL9CD10  MOVE      VALDCODE,KEY9
          CALL      RDPT19O1
          IF        OVRCD=0
            MOVE      PT0ODSC2,VALDNAME
          ENDIF
          GOTO      VAL9CD90
.
VAL9CD20  MOVE      VALDCODE,KEY9
          CALL      RDPT19D1
          IF        OVRCD=0
            MOVE      PT0DDSC2,VALDNAME
          ENDIF
          GOTO      VAL9CD90
.
VAL9CD90  CALL      OPENHTML
          BRANCH    EXIT,VAL9CD99
          WRITE     HTMLFILE;"<h3 align=center>Checking Database</h3>":
                             "<script type=#"text/javascript#">":
                             "{ opener.ValidateName.value=#"",VALDNAME,"#";":
                             "  opener.ValidateCode.value=#"",VALDCODE,"#";"
.
          MATCH     "Invalid",VALDNAME
          IF        @EQUAL
            WRITE     HTMLFILE;"  alert(#"Invalid Code Entered#");";
          ENDIF
.
          WRITE     HTMLFILE;"  close(); } </script>"
          CALL      CLOSHTML
.
VAL9CD99  RETURN
