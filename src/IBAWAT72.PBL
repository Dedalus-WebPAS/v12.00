          BRANCH    EXIT,OPTN9500
          MATCH     JULY2025,TODAY
          IF        !@LESS
            MOVE      ONE,DOYR2025
          ENDIF
          GOTO      OPTN9000
.
OPTN9000  MOVE      ZERO,EXIT
          CALL      CREAT000
          GOTO      OPTN9999
.
OPTN9500  MOVE      ONE,EXIT
.
OPTN9999  RETURN
