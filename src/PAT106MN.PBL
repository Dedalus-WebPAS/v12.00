.------------------------------------------------------------------------------
. ICD10 Edition 6 Diagnosis Coding Maintenance: Display,Add,Update,Delete
.------------------------------------------------------------------------------
ICD6DI00  RESET     PTICD001
          MOVE      "6",ICD10EDN
.                                           * make sure files are open
          OPEN      PATI16D1,"pat10d6f"     * Open ICD10 Ed.6 Diseases
          OPEN      PATK6DA1,"patkcd6f"     * ICD10 Ed.6 Key Word Diag.
          OPEN      PATK6DA2,"patkcd6f"     * ICD10 Ed.6 Key Word Diag.
.
          RESET     UPDTTYPE
          MATCH     SP1,UPDTTYPE
          GOTO      ICD6DI40 IF EQUAL
.
          MATCH     "A",UPDTTYPE    * Add formaction
          GOTO      ICD6DI10 IF EQUAL
.
          MATCH     "U",UPDTTYPE    * Update formaction
          GOTO      ICD6DI20 IF EQUAL
.
          MATCH     "D",UPDTTYPE    * Delete formaction
          GOTO      ICD6DI30 IF EQUAL
.
          MATCH     "V",UPDTTYPE    * Validate Formaction
          IF        @EQUAL
            CALL      VAL6CD00
            GOTO      ICD6DI99
          ENDIF
.
          GOTO      ICD6DI93
.
ICD6DI10  CALL      CHKDIS00  * Checks mandatory fields & checks for blank
          BRANCH    EXIT,ICD6DI99
          PACK      KEY9,PTICD001,SP70  * ADD FORMACTION
          CALL      RDPT16D1
          COMPARE   ZERO,OVRCD
          GOTO      ICD6DI92 IF EQUAL
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      WRPT16D1      * Writes away the data
          PROC      PATK6DUP      * Update Keyword table
.
          CALL      ADDCAN00      * Add Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD6DI99
.
ICD6DI20  PACK      KEY9,PTICD001,SP70  * UPDATE FORMACTION
          CALL      RDPT16D1
          BRANCH    OVRCD,ICD6DI91
          CALL      CHKDIS00      * Checks mandatory fields
          BRANCH    EXIT,ICD6DI99
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      UPPT16D1      * Writes away the data
          PROC      PATK6DUP      * Update Key word Table
.
          CALL      UPDCAN00      * Update Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD6DI99
.
ICD6DI30  PACK      KEY9,PTICD001,SP70   * DELETE FORMACTION
          CALL      RDPT16D1
          BRANCH    OVRCD,ICD6DI91
          CALL      DEPT16D1      * Deletes the data
          PROC      PATK6DUP      * Update Key word Table
.
          CALL      DELCAN00      * Delete Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD6DI99
.
ICD6DI40  PACK      KEY9,PTICD001,SP70
          CALL      RDPT16D1
          IF        OVRCD=1
            CALL      CLRDIS00    * Clear all the file variables
          ENDIF
.
          MOVE      ONE,CANCERCD
          PACK      KEY9,PTICD001,SP70
          CALL      RDPTCAN1
          IF        OVRCD=1
            MOVE     ZERO,CANCERCD
            MOVE     ZERO,PTCCLATR
          ENDIF
.
          CLEAR     WEBTITLE
          MOVE      "ICD10 Edition 6 Diagnosis Maintenance",WEBTITLE
          RESET     WEBTITLE
.
          CALL      WEBHED00   * Create Output File and Write HTML Page Header
          BRANCH    EXIT,ICD6DI99
          CALL      WEBBOD00   * Process Web Body
          CALL      WEBEND00   * Process End of HTML File
          MOVE      ONE,EXIT
          GOTO      ICD6DI99
.
ICD6DI91  MOVE      "ICD10 Edition 6 Diagnosis Code does not exist",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD6DI99
.
ICD6DI92  MOVE      "ICD10 Edition 6 Diagnosis Code exists",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD6DI99
.
ICD6DI93  MOVE      "Invalid Form Action.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD6DI99
.
ICD6DI99  RETURN
+
