SPT6OA00  WRITE     HTMLFILE;"<tr align=center>":
                             "<td class=HeadingCell colspan=2>":
                             "<b>You Searched for ":
                               KEYWORDS,"</td>":
                             "</tr>":
                             "<tr>":
                             "<td class=HeadingCell width=15%><b>Code</b></td>":
                             "<td class=HeadingCell><b>Description</b></td>":
                             "</tr>"
.
          CALL      GETWRD00
.
          UNPACK    DIM127,D1,LINKPRG,DIM127
          UNPACK    DIM127,D1,LINKREP,DIM127
          UNPACK    DIM127,D1,LINKTEM,DIM127
.
          IF        NORECORD=0
            MOVE      PTCNNRTD,NORECORD       * Set Default No Records to Display
          ENDIF
          MOVE      ZERO,RECNUMB
          MOVE      ZERO,DISNUMB
.
....      UNPACK    PTCNGDV6,KEY4,KEY5
....      MATCH     "9999",KEY4          * is ICD10 Ed.6 in use yet?
....      GOTO      SPT6OA90 IF EQUAL
.
          STRIP     KEYWRD01
          PACK      KEY24,KEYWRD01,SP70
          CALL      RSPT6O2
.
SPT6OA10  CALL      RKPT6O2
          BRANCH    OVRCD,SPT6OA90
.
          CALL      SPT6OB00
          BRANCH    OVRCD,SPT6OA10,SPT6OA90
.
          MOVE      PTCOITM,KEY9
          CALL      RDPT16O1
          BRANCH    OVRCD,SPT6OA10
.
          ADD       ONE,RECNUMB
          IF        STARTNUM>=RECNUMB
            GOTO      SPT6OA10
          ENDIF
.
          BRANCH    LNK2PRT,SPT6OA20,SPT6OA30
          GOTO      SPT6OA99
.
SPT6OA20  WRITE     HTMLFILE;"<tr><td nowrap><A HREF='javascript:showICD02":
                             "(#"",LINKPRG,"#.pbl":
                             "?reportno=",LINKREP:
                             "&template=",LINKTEM:
                             "&ptico001=",OPCODE,"#")'>":
                             "<img src=../images/MaintenanceIcon.gif hspace=4":
                             " border=0 align=absmiddle></a>":
                             OPCODE,"</td>":
                             "<td>",PT0ODESC,"</td>":
                             "</tr>"
.
          GOTO      SPT6OA80
.
SPT6OA30  WRITE     HTMLFILE;"<tr valign=top>":
                              "<td><a href=#"#" ":
                              "OnClick=#"updateParent('",PT0ODESC:
                              "','",OPCODE,"')#">":
                              "<img src=../images/MaintenanceIcon.gif hspace=4":
                              " border=0 align=absmiddle></a>":
                              OPCODE,"</td>":
                              "<td>",PT0ODESC,"</td>":
                             "</tr>"
.
SPT6OA80  ADD       ONE,DISNUMB
          COMPARE   NORECORD,DISNUMB
          GOTO      SPT6OA10 IF NOT EQUAL
          GOTO      SPT6OA99
.
SPT6OA90  CALL      NOMORE00           * Display No More Records Message
.
SPT6OA99  RETURN
.------------------------------------------------------------
. Check ICD10 Edition 6 Procedure Record
.------------------------------------------------------------
SPT6OB00  MATCH     KEYWRD01,PTCOKWD
          GOTO      SPT6OB92 IF NOT EQUAL
          IF        KEYWRDNO>1
            PACK      SAVKEY24,PTCOKWD,PTCOITM
            CALL      SPT6OC00
            BRANCH    EXIT,SPT6OB91
            MOVE      SAVKEY24,KEY24
            CALL      RDPT6O2
          ENDIF
.
. Insert Other Check for Valid Record Here i.e. Suspended or In Active
.   eg    COMPARE    ONE,INACTIVE
.         GOTO       SPT6OB91 IF EQUAL
.
          GOTO      SPT6OB90
.
SPT6OB90  MOVE      ZERO,OVRCD
          GOTO      SPT6OB99
SPT6OB91  MOVE      ONE,OVRCD
          GOTO      SPT6OB99
SPT6OB92  MOVE      TWO,OVRCD
SPT6OB99  RETURN
.------------------------------------------------------------
. Check for Matching ICD10 Edition 6 Procedure Words
.------------------------------------------------------------
SPT6OC00  MOVE      ONE,F2
          MOVE      ZERO,EXIT
          PACK      SAVKEY9,PTCOITM,SP70
.
SPT6OC10  COMPARE   KEYWRDNO,F2           * Loop through each Key Word in Search
          GOTO      SPT6OC99 IF EQUAL     * Exit OK
          ADD       ONE,F2
          LOAD      KEYWRDXX,F2,KEYWRD01,KEYWRD02,KEYWRD03,KEYWRD04,KEYWRD05:
                             KEYWRD06,KEYWRD07,KEYWRD08,KEYWRD09,KEYWRD10
          PACK      KEY24,PTCOITM,KEYWRDXX
          CALL      RDPT6O1               * Check for Exact Match
          COMPARE   ZERO,OVRCD            * Ok so Continue Loop
          GOTO      SPT6OC10 IF EQUAL
.
          CALL      RKPT6O1               * Not Exact so Read Next Record
          BRANCH    OVRCD,SPT6OC90        * No Match So Exit No Match
          PACK      KEY9,PTCOITM,SP70
          MATCH     SAVKEY9,KEY9        * Match Key Fields
          GOTO      SPT6OC90 IF NOT EQUAL * No Match So Exit
          STRIP     KEYWRDXX
          MATCH     PTCOKWD,KEYWRDXX         * Check Key Word
          GOTO      SPT6OC90 IF NOT EQUAL * No Match So Exit
          GOTO      SPT6OC10              * Check Next Search Word
.
SPT6OC90  MOVE      ONE,EXIT
SPT6OC99  RETURN
