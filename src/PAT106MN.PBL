.------------------------------------------------------------------------------
. Online ICD10 Edition 6 Code Validation
.------------------------------------------------------------------------------
VAL6CD00  MOVE      SP70,VALDNAME
          MATCH     SP70,VALDCODE
          GOTO      VAL6CD90 IF EQUAL
.
          MOVE      "Invalid Code Entered",VALDNAME
          BRANCH    VALDTYPE,VAL6CD10,VAL6CD20
          MOVE      "Invalid Validation Type",VALDNAME
          GOTO      VAL6CD90
.
VAL6CD10  MOVE      VALDCODE,KEY9
          CALL      RDPT16O1
          IF        OVRCD=0
            MOVE      PT0ODESC,VALDNAME
          ENDIF
          GOTO      VAL6CD90
.
VAL6CD20  MOVE      VALDCODE,KEY9
          CALL      RDPT16D1
          IF        OVRCD=0
            MOVE      PT0DDESC,VALDNAME
          ENDIF
          GOTO      VAL6CD90
.
VAL6CD90  CALL      OPENHTML
          BRANCH    EXIT,VAL6CD99
          WRITE     HTMLFILE;"<h3 align=center>Checking Database</h3>":
                             "<script type=#"text/javascript#">":
                             "{ opener.ValidateName.value=#"",VALDNAME,"#";":
                             "  opener.ValidateCode.value=#"",VALDCODE,"#";"
.
          MATCH     "Invalid",VALDNAME
          IF        @EQUAL
            WRITE     HTMLFILE;"  alert(#"Invalid Code Entered#");";
          ENDIF
.
          WRITE     HTMLFILE;"  close(); } </script>"
          CALL      CLOSHTML
.
VAL6CD99  RETURN
