.------------------------------------------------------------------------
.   Clear Parameters for IBACONFD
.------------------------------------------------------------------------
CPSY0000  MOVE      Z70,SYSCN001
.
CPSY9999  RETURN
.
.------------------------------------------------------------
. Display fields from IBACONFD - Sector 0
.------------------------------------------------------------
SYCN0000  BRANCH    FLDITMNO,SYCN1010
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      SYCN9999
.
SYCN1010  MOVE      "FI",CATEGORY             * Misc Charge Code Group
          PACK      CODE,IBCNPBXI,SP70
          CALL      CODITM00
          GOTO      SYCN9999
.
SYCN9999  RETURN
.
.------------------------------------------------------------
.   Set Parameters for IBACONFD
.------------------------------------------------------------
SPSYS000  MOVE      ZERO,EXIT
          UNPACK    QRYNAME,KEY5,KEY3
          MOVE      KEY3,F3
          BRANCH    F3,SPSYS001
.
          MOVE      ONE,EXIT
          GOTO      SPSYS999
.
SPSYS001  MOVE      QRYDATA,SYSCN001
          GOTO      SPSYS999
.
SPSYS999  RETURN
.
.------------------------------------------------------------
.  Update IBACONFD file variables
.------------------------------------------------------------
MVSY0000  MATCH     Z70,SYSCN001 
          IF        !@EQUAL
            MOVE      SYSCN001,IBCNPBXI     * Misc Charge for Phone Call Billing
          ENDIF
.
MVSY9999  RETURN
.
