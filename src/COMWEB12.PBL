            CALL      PATLN000
          ENDIF
.
          MATCH     "1",SUPERLEV
          IF        @EQUAL
            CALL     LIRE0000
            CALL     LIDE0000
            WRITE     HTMLFILE;"t.addRow(#"",H7CIDTTM,"#",":
                             "#"",H7CIMETP,"#",":
                             "#"",STATWORD,"#",":
                             "#"",FORMATNM,"#",":
                             "#"",PATLINK,"#",":
                             "#"",H7CIURNO,"#",":
                             "#"",DH7CIMES,"#",":
                             "#"",H7CIDDAT,"#",":
