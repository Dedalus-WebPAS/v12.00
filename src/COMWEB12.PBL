.-------------------------------------------------------------
. [FUNCTION] Match HL7 Messages to Call App. Icons
.-------------------------------------------------------------
HLMT0000  CLEAR     HL7POPUP
          CLEAR     HL7POPU2
          CLEAR     H7DATTYP
          MATCH     "A14",H7CIMETP
          IF        @EQUAL
            MATCH     "1",PTCNH7A5
            IF      @EQUAL
              MOVE      "A05",H7CIMETP
            ENDIF
          ENDIF
          MATCH     "A31",H7CIMETP
          IF        @EQUAL
            MATCH     SP2,PTCNA31X              
            IF        !@EQUAL
              PACK      H7CIMETP,ANSA,PTCNA31X
            ENDIF
          ENDIF
          CALL      CHHLTY00
          MOVE      COUNTER2,UNIQNUM
          SQUEEZE   UNIQNUM
          SQUEEZE   DH7CIMES
          APPEND    "<a href=javascript:AssessLink('",HL7POPU2
