          MATCH     "INV",H7DATTYP
          IF        !@EQUAL
            APPEND    "<a href=javascript:AssessLink('",HL7POPUP
            APPEND    DH7CIMES,HL7POPUP
            APPEND    "','",HL7POPUP
            APPEND    H7DATTYP,HL7POPUP
            APPEND    "')><img src='../images/AppointmentIcon.gif'",HL7POPUP
            APPEND    " border=0 hspace=4></a>",HL7POPUP
          ENDIF
