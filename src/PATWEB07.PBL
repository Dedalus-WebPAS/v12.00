          GOTO      MULBAT92 IF EQUAL
.
          PACK      KEY8,URNUMBER,SP70     * Read Master File
          CALL      RDMAST1
          BRANCH    OVRCD,MULBAT92
.
          PACK      KEY8,URNUMBER,SP70     * Read Master File Ext 2
          CALL      RDPMPX21
          IF        OVRCD=1
