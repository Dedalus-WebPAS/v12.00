          GOTO      PHSP1040 IF EQUAL
.
          OPEN      PATHSPA1,"pathspaf"
          MOVE      PMVXMHOS,KEY3
          CALL      RDPTHSP1
          BRANCH    OVRCD,PHSP1040
.
          MATCH     "1",PTHSHOST             * Public hospital
          GOTO      PHSP1040 IF NOT EQUAL    * no
