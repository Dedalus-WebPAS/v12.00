NEXTAD00  MOVE      NORECORD,KEY2
          REP       " +",KEY2
          ASSIGN    STARTNUM+NORECORD,F3
          MOVE      F3,KEY3
          REP       " +",KEY3
          UNPACK    DIM127,D1,FLDPARA,DIM127
          MOVE      REPORTNO,F1
          REP       " +",PMDATYPE
.
          WRITE     HTMLFILE;"patweb07.pbl":
                                 "?reportno=",F1:
                                 "&template=",FLDPARA:
                                 "&startnum=",KEY3:
                                 "&norecord=",KEY2:
                                 "&pmdaa001=",PMDATYPE;
          REP       "+ ",PMDATYPE
.
NEXTAD99  RETURN
.-------------------------------------------------------------
. Link to Prev Group of Address
.-------------------------------------------------------------
PREVAD00  MOVE      NORECORD,KEY2
          REP       " +",KEY2
          ASSIGN    STARTNUM-NORECORD,F3
          IF        F3<0
            MOVE      ZERO,F3
          ENDIF
          MOVE      F3,KEY3
          REP       " +",KEY3
          UNPACK    DIM127,D1,FLDPARA,DIM127
          MOVE      REPORTNO,F1
          REP       " +",PMDATYPE
.
          WRITE     HTMLFILE;"patweb07.pbl":
                                 "?reportno=",F1:
