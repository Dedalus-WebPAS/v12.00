              CALL      RDCODE1
              IF        OVRCD=0
                MOVE      "Credit-",KEY7
                PACK      EFTDESC,KEY7,TDESC,SP70
              ENDIF
            ENDIF
            UNPACK    PAYEE,KEY28
            PRINT     *1,DDREFER,*30,EFTDESC,*80,KEY28,*110,CHQAMNT
          ENDIF
