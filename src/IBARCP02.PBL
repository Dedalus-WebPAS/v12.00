            CALL      RDPTHSP1
            IF        OVRCD=0
              MOVE      "Hospital:",KEY9
              MOVE      "-",KEY1
              PACK      XHDROW05,KEY9,WBSEHOSP,KEY1,PTHSNAME,COMMA,SP70
            ENDIF
          ENDIF
          WRITE     CSVOFILE,SEQ;*+,XHDROW05
