LISTOT96  PRINT     *65,"----------------",*N:
                    *40,"Total Bank Deposit",*65,TOTTOTAL,*N:
                    *65,"----------------",*N
          ADD       "4",CLNO
.
          MOVE      ZERO,TOTCASH
          MOVE      ZERO,TOTCHQ
          MOVE      ZERO,TOTCRED
          MOVE      ZERO,TOTDDBT
          MOVE      ZERO,TOTMONOR
          MOVE      ZERO,TOTEFTDB
          MOVE      ZERO,TOTEFTCR
          MOVE      ZERO,TOTHCAPS
          MOVE      ZERO,TOTBPAY
