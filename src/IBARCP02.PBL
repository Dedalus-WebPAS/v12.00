            CALL      RDDRTMP2
          ENDIF
.
          IF        OVRCD=0
            GOTO      WRDAT500
          ENDIF
.
          MOVE      RCBNCDRW,DRAWERID
          MOVE      RCDTREGI,REGISTER                                  *I-59171
          MOVE      RCDTRECN,RECEIPTN
