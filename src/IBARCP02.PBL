          MOVE      ZERO,FRSTFLAG
          MOVE      SP8,CONTIN
          MOVE      SP70,SAVCSHDR
          MOVE      ZERO,PHEDFLAG
          PACK      KEY17,SP20
          CALL      RSDRTMP4
SUMREC10  CALL      RKDRTMP4
          BRANCH    OVRCD,SUMREC90
.
          IF        FRSTFLAG=0
            MOVE      DRAWERID,SAVDRAWR
            MOVE      REGISTER,SAVREG
            MOVE      HEADTYPE,SAVHTYPE
