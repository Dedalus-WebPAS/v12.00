.
.         INC       OUTPHAIO.INC
.
.         FILE  I/O REQUESTS FOR SITE CODE FILE
.
.OVRCD  FORM            1             0=NOT OVER
.                                     1= OVER
.KEY11  DIM            11             KEY FOR OUTPHOA1
.KEY41  DIM            41             KEY FOR OUTPHOA2
.KEY11  DIM            11             KEY FOR OUTPHOA3
.
.         NOTE:-
.                  RDAAAX  IS OUT READ CALL ON FILE OUTAAAX
.                  RDKAAAX  IS OUT READKS CALL
.                  WRAAAX  IS OUT WRITE CALL
.                  UPAAAX  IS OUT UPDATE CALL
.                  DEAAAX  IS OUT DELETE CALL
. 
.         NOTE  OVRCD=1   RECORD DOSENT EXIST
.                     2   RECORD IS LOCKED
.
RDAPHOA1 RESET     KEY11
         MOVE      ZERO,OVRCD
         READ      OUTPHOA1,KEY11;ANS
         GOTO      OVERCOND IF OVER
         RETURN  
.
RDSPHOA1 RESET     KEY11
         READ      OUTPHOA1,KEY11;;
         RETURN  
.
RDPHOA1  MOVE      ZERO,OVRCD
         RESET     KEY11
         READ      OUTPHOA1,KEY11;OPHDATE,OPHDESC,OPHHOSP,OPHSPARE
         GOTO      OVERCOND IF OVER
         MOVE      OPHDATE,SOPHDATE
         MOVE      OPHDESC,SOPHDESC
         MOVE      OPHHOSP,SOPHHOSP
         RETURN  
.
RDKPHOA1 MOVE      ZERO,OVRCD
         READKS    OUTPHOA1;OPHDATE,OPHDESC,OPHHOSP,OPHSPARE
         GOTO      OVERCOND IF OVER
         MOVE      OPHDATE,SOPHDATE
         MOVE      OPHDESC,SOPHDESC
         MOVE      OPHHOSP,SOPHHOSP
         RETURN  
.
RDPPHOA1 MOVE      ZERO,OVRCD
         READKP    OUTPHOA1;OPHDATE,OPHDESC,OPHHOSP,OPHSPARE
         GOTO      OVERCOND IF OVER
         MOVE      OPHDATE,SOPHDATE
         MOVE      OPHDESC,SOPHDESC
         MOVE      OPHHOSP,SOPHHOSP
         RETURN  
.
WRPHOA1  RESET     KEY11
         WRITE     OUTPHOA1,KEY11;OPHDATE,OPHDESC,OPHHOSP,OPHSPARE
.
         RETURN  
.
UPPHOA1  UPDATE    OUTPHOA1;OPHDATE,OPHDESC,OPHHOSP,OPHSPARE
.
         RETURN  
.
DEPHOA1  RESET     KEY11
         MOVE      ZERO,OVRCD
         DELETE    OUTPHOA1,KEY11
.
         RETURN
.
