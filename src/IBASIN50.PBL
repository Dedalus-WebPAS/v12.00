            IF        !@EQUAL&(SIVALED>0)
              GOTO      EEXDT999
            ENDIF
          ENDIF
          UNPACK    SIPCEDD,CCENT,CYEAR,CMON,CDAY
          CALL      KEYDATE
          PACK      SIPCEDD,CCENT,CYEAR,CMON,CDAY
          MATCH     SP2,CDAY
          IF        @EQUAL
            MOVE      SP8,SIPCEDD
          ENDIF
