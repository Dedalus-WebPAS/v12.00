CHKWAR00  MOVE      ZERO,EXIT
          MATCH     SP70,IMPTWAR
          GOTO      CHKWAR99 IF EQUAL
.
          PACK      KEY5,IMPTWAR,SP70
          CALL      RDSIWA1                     * Read Master Record
          IF        OVRCD=1
            BEEP
            CLEAR     DISPMSG
            APPEND    "Invalid Warehouse ",DISPMSG
            APPEND    IMPTWAR,DISPMSG
            APPEND    " Record No ",DISPMSG
            APPEND    RECCOUNT,DISPMSG
            APPEND    " - ",DISPMSG
            RESET     DISPMSG
            CALL      MESSAGE1
            MOVE      ONE,EXIT
          ENDIF
.
