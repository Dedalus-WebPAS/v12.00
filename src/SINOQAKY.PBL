.---------------------------------------------------------------------------
. External Routines
.---------------------------------------------------------------------------
ESIQA100  UNPACK    SIQADAT,CCENT,CYEAR,CMON,CDAY
          CALL      PACDATE
          RETURN
.---------------------------------------------------------------------------
.   System    :   Supply and Inventory               
.   Include   :   SINOQAKY
.   Routine   :   KSIQAA00 Keyin Purchase Req. ID    
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KSIQAA00 TOPIC     "SIQARID "
          MOVE      UNDLN70,SIQARID 
          KEYIN     *PCCOL:CVERT,*DV,SIQARID:                                   
                    *PCCOL:CVERT,*V2LON,*JR,*ZF,SIQARID:                        
                    *PCCOL:CVERT,*DV,SIQARID 
.
          ENDSET    SIQARID 
          APPEND    SP70,SIQARID 
          RESET     SIQARID 
.
          MATCH     SP70,SIQARID 
          GOTO      KSIQAA90 IF EQUAL
.
          ENDSET    SIQARID 
          CMATCH    EXITCHAR,SIQARID 
          GOTO      KSIQAA95 IF EQUAL
.
          CMATCH    QUEST,SIQARID 
          GOTO      KSIQAA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HSIQAA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    SIQARID 
          APPEND    SP70,SIQARID 
          RESET     SIQARID 
.
          MATCH     SP70,SIQARID 
          GOTO      KSIQAA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,SIQARID                                 
KSIQAA10
          RESET     SIQARID 
          PACK      KEY13,SIQACST,SIQARID                                       
          CALL      RASIQA1 
          BRANCH    OVRCD,KSIQAA80
          BRANCH    CKEYTYP,KSIQAA75
          CALL      RDSIQA1 
KSIQAA70
          MOVE      ZERO,EXIT
          GOTO      KSIQAA99
KSIQAA75
          MOVE      "Purchase Req. ID Already Exist - ",DISPMSG                                      
          CALL      MESSAGE1
          GOTO      KSIQAA00
KSIQAA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KSIQAA99,KSIQAA99
          MOVE      "Purchase Req. ID Does Not Exist - ",DISPMSG                                     
          CALL      MESSAGE1
          GOTO      KSIQAA00
KSIQAA90
          MOVE      ONE,EXIT
          GOTO      KSIQAA99
KSIQAA95
          MOVE      TWO,EXIT
KSIQAA99 TOPIC    PRGID
          RETURN
.---------------------------------------------------------------------------
.   System    :   Supply and Inventory               
.   Include   :   SINOQAKY
.   Routine   :   HSIQAA00 Purchase Req. Number
.   Return    :   SIQARID 
.-------------------------------------------------------------------------------
HSIQAA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "35",HLEF
            MOVE      "80",HRIG
          ENDIF
HSIQAA04
          CALL      GSCRN000
          BRANCH    EXIT,HSIQAA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Purch Req ID"       
.
          MOVE      STDCOL01,STDCOL02
          ADD       "14",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Catalog"            
.
          MOVE      STDCOL02,STDCOL03
          ADD       " 9",STDCOL03
          DISPLAY   *PSTDCOL03:STDROW,*V2LON,*ULON,"       Quantity"    
.
          MOVE      STDCOL03,STDCOL04
          ADD       "17",STDCOL04
          DISPLAY   *PSTDCOL04:STDROW,*V2LON,*ULON,"Date Entered"       
.
          MOVE      STDCOL04,STDCOL05
          ADD       "14",STDCOL05
          DISPLAY   *PSTDCOL05:STDROW,*V2LON,*ULON,"Time Entered"       
.
          MOVE      STDCOL05,STDCOL06
          ADD       " 7",STDCOL06
HSIQAA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,SIQARID 
          PACK      KEY13,SIQACST,SIQARID,SP70                                  
          PACK      SAVKEY80,SIQACST,SIQARID,SP70                               
          SETLPTR   SAVKEY80,5                                                  
          CALL      RSSIQA1 
HSIQAA10
          CALL      RKSIQA1 
          BRANCH    OVRCD,HSIQAA15
          PACK      KEY13,SIQACST,SIQARID,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HSIQAA15 IF NOT EQUAL                                       
          BRANCH    CSEARCH,HSIQAA12
          CALL      HSIQAA60
          BRANCH    OVRCD,HSIQAA10
HSIQAA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY13,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,SIQARID 
          DISPLAY   *PSTDCOL02:STDROW,SIQACAT 
          DISPLAY   *PSTDCOL03:STDROW,SIQAQTY 
          CALL      ESIQA100
          DISPLAY   *PSTDCOL04:STDROW,CPCDATE 
          DISPLAY   *PSTDCOL05:STDROW,SIQATIM 
          COMPARE   MAXITEM,XITEMNO
          GOTO      HSIQAA10 IF NOT EQUAL
HSIQAA15
          UNPACK    SAVKEY80,SIQACST,SIQARID                                    
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HSIQAA20
          CALL      HSCRI000
          BRANCH    EXIT,HSIQAA30,HSIQAA35,HSIQAA40,HSIQAA45,HSIQAA04:
                              HSIQAA91,HSIQAA50
          GOTO      HSIQAA92
HSIQAA30
          CALL      RKSIQA1 
          BRANCH    OVRCD,HSIQAA85
          PACK      KEY13,SIQACST,SIQARID,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HSIQAA85 IF NOT EQUAL                                       
          CALL      RPSIQA1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HSIQAA10
HSIQAA35
          MOVE      ST1,KEY13
          CALL      RDSIQA1 
          CALL      RPSIQA1 
          BRANCH    OVRCD,HSIQAA85
          PACK      KEY13,SIQACST,SIQARID,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HSIQAA85 IF NOT EQUAL                                       
          MATCH     SP70,SIQARID                                                
          GOTO      HSIQAA85 IF EQUAL                                           
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HSIQAA36
          CALL      RPSIQA1 
          BRANCH    OVRCD,HSIQAA37
          PACK      KEY13,SIQACST,SIQARID,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HSIQAA37 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HSIQAA36 IF NOT EQUAL
HSIQAA37
          MOVE      ZERO,XITEMNO
          GOTO      HSIQAA10
HSIQAA40
          MOVE      ST1,KEY13
          CALL      RDSIQA1 
          CALL      RPSIQA1 
          BRANCH    OVRCD,HSIQAA85
          PACK      KEY13,SIQACST,SIQARID,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HSIQAA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HSIQAA08
HSIQAA45
          CALL      RKSIQA1 
          BRANCH    OVRCD,HSIQAA85
          PACK      KEY13,SIQACST,SIQARID,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HSIQAA85 IF NOT EQUAL                                       
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY13,SAVKEY80,Z70                                          
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSSIQA1 
HSIQAA46
          CALL      RPSIQA1 
          BRANCH    OVRCD,HSIQAA47
          PACK      KEY13,SIQACST,SIQARID,SP70                                  
          MATCH     SAVKEY80,KEY13                                              
          GOTO      HSIQAA47 IF NOT EQUAL                                       
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HSIQAA46 IF NOT EQUAL
HSIQAA47
          MOVE      ZERO,XITEMNO
          GOTO      HSIQAA10
HSIQAA50  COMPARE   ZERO,CSEARCH
          GOTO      HSIQAA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          KEYIN     *PSTDCOL04:STDROW,*V2LON,SCHFLD04
          RESET     SCHFLD04
          KEYIN     *PSTDCOL05:STDROW,*V2LON,SCHFLD05
          RESET     SCHFLD05
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HSIQAA08
HSIQAA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HSIQAA08
HSIQAA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HSIQAA61 IF EQUAL
          MATCH     SCHFLD01,SIQARID 
          GOTO      HSIQAA72 IF NOT EQUAL
HSIQAA61
          MATCH     SP70,SCHFLD02
          GOTO      HSIQAA62 IF EQUAL
          MATCH     SCHFLD02,SIQACAT 
          GOTO      HSIQAA72 IF NOT EQUAL
HSIQAA62
HSIQAA63
          CALL      ESIQA100
          MATCH     SP70,SCHFLD04
          GOTO      HSIQAA64 IF EQUAL
          MATCH     SCHFLD04,CPCDATE 
          GOTO      HSIQAA72 IF NOT EQUAL
HSIQAA64
          MATCH     SP70,SCHFLD05
          GOTO      HSIQAA65 IF EQUAL
          MATCH     SCHFLD05,SIQATIM 
          GOTO      HSIQAA72 IF NOT EQUAL
HSIQAA65
HSIQAA66
HSIQAA67
HSIQAA68
HSIQAA69
HSIQAA70
          GOTO      HSIQAA75
HSIQAA72  MOVE      ONE,OVRCD
HSIQAA75  RETURN
HSIQAA85
          LOAD      KEY13,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY13,SIQACST,SIQARID                                       
          CALL      RDSIQA1 
HSIQAA90
          UNPACK    SAVKEY80,SIQACST,SIQARID                                    
          BEEP
          GOTO      HSIQAA20
HSIQAA91
          MOVE      SP70,SIQACAT 
          MOVE      SP70,SIQAUNT 
          MOVE      SP70,SIQAPD  
          MOVE      SP70,SIQAPN  
          MOVE      ZERO,SIQAQTY 
          MOVE      SP70,SIQASTA 
          MOVE      SP70,SIQAUID 
          MOVE      SP70,SIQADAT 
          MOVE      SP70,SIQATIM 
          MOVE      SP70,SIQAPOR 
          MOVE      SP70,SIQASPA 
          MOVE      SP70,KEY13
          MOVE      SP70,SIQARID 
          GOTO      HSIQAA96
HSIQAA92
          MOVE      SP70,KEY13
          MOVE      SP70,SIQARID 
          LOAD      KEY13,CCITEM,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY13,SIQACST,SIQARID                                       
HSIQAA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HSIQAA99
          RETURN
