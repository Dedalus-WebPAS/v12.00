             MOVE     BUDPERXX,VAR
             CALL     DISPITEM
           ENDIF
           CALL     SELECTED
           RETURN
ADDBUD00  MOVE      ZERO,BUDPERXX
          ASSIGN    BUDPERXX+BUDPER01+BUDPER02+BUDPER03+BUDPER04,BUDPERXX
          ASSIGN    BUDPERXX+BUDPER05+BUDPER06+BUDPER07+BUDPER08,BUDPERXX
          ASSIGN    BUDPERXX+BUDPER09+BUDPER10+BUDPER11+BUDPER12,BUDPERXX
          IF        SIPENUM=13
            ADD       BUDPER13,BUDPERXX
          ENDIF
          RETURN
EYEAR000  CALL      RINPECON
          MOVE      SIPECYR,FINYEAR
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,*JR,FINYEAR
          PACK      FINYEAR,FINYEAR,SP70
.
          MATCH     SP70,FINYEAR
          GOTO      EYEAR990 IF EQUAL
          PACK      KEY4,SP3,EXITCHAR * exitchar entered ?
          MATCH     KEY4,FINYEAR
          GOTO      EYEAR990 IF EQUAL
.
          TYPE      FINYEAR           * valid numeric ?
          GOTO      EYEAR000 IF NOT EQUAL
.
          MOVE      FINYEAR,SIPEYF4
          IF        SIPEYF4<100
            SQUEEZE   FINYEAR
            MOVE      FINYEAR,FORM2
            PACK      FINYEAR,CCC,FORM2,SP70
          ENDIF
.
          REP       " 0",FINYEAR
          MOVE      ONE,ACCEPT
          MOVE      ZERO,BUDPERXX
.
          MOVE      ZERO,F2
EYEAR100  ADD       ONE,F2
          IF        F2>SIPENUM
            GOTO      EYEAR900
          ENDIF
          PACK      SISBDAT,FINYEAR,F2
          REP       " 0",SISBDAT
          PACK      KEY11,SISACODE,SISBDAT
          CALL      RDSISB1
          IF        OVRCD=0
            MOVE      ZERO,ACCEPT
          ELSE
            MOVE      ZERO,SISBUBD
            MOVE      ZERO,SISBUAM
          ENDIF
          STORE     SISBUBD,F2,BUDPER01,BUDPER02,BUDPER03,BUDPER04:
                               BUDPER05,BUDPER06,BUDPER07,BUDPER08:
                               BUDPER09,BUDPER10,BUDPER11,BUDPER12,BUDPER13
          ADD       SISBUBD,BUDPERXX
          GOTO      EYEAR100
.
EYEAR900  MOVE      ACCEPT,OVRCD
          IF        ACCEPT=1
            MOVE      ZERO,ACCEPT
            CALL      DF01L000
            MOVE      ONE,ACCEPT
            MOVE      "    1",FLDID
            CALL      GETPOSIT
          ENDIF
          GOTO      EYEAR999
EYEAR990  MOVE      TWO,EXIT
EYEAR999  RETURN
ANNUS000  MOVE      "   26",FLDID
          CALL      GETPOSIT
          IF        EXIT=1
            BEEP
            GOTO      ANNUS999
          ENDIF
          KEYIN     *PCCOL:CVERT,*RV,BUDPERXX
          IF        SIPENUM=12
            ASSIGN    BUDPERXX/12.0,BUDPER01
            ASSIGN    BUDPERXX/12.0,BUDPER02
            ASSIGN    BUDPERXX/12.0,BUDPER03
            ASSIGN    BUDPERXX/12.0,BUDPER04
            ASSIGN    BUDPERXX/12.0,BUDPER05
            ASSIGN    BUDPERXX/12.0,BUDPER06
            ASSIGN    BUDPERXX/12.0,BUDPER07
            ASSIGN    BUDPERXX/12.0,BUDPER08
            ASSIGN    BUDPERXX/12.0,BUDPER09
            ASSIGN    BUDPERXX/12.0,BUDPER10
            ASSIGN    BUDPERXX/12.0,BUDPER11
            ASSIGN    BUDPERXX-BUDPER01-BUDPER02-BUDPER03-BUDPER04,BUDPER12
            ASSIGN    BUDPER12-BUDPER05-BUDPER06-BUDPER07-BUDPER08,BUDPER12
            ASSIGN    BUDPER12-BUDPER09-BUDPER10-BUDPER11,BUDPER12
          ELSE
            ASSIGN    BUDPERXX/13.0,BUDPER01
            ASSIGN    BUDPERXX/13.0,BUDPER02
            ASSIGN    BUDPERXX/13.0,BUDPER03
            ASSIGN    BUDPERXX/13.0,BUDPER04
            ASSIGN    BUDPERXX/13.0,BUDPER05
            ASSIGN    BUDPERXX/13.0,BUDPER06
            ASSIGN    BUDPERXX/13.0,BUDPER07
            ASSIGN    BUDPERXX/13.0,BUDPER08
            ASSIGN    BUDPERXX/13.0,BUDPER09
            ASSIGN    BUDPERXX/13.0,BUDPER10
            ASSIGN    BUDPERXX/13.0,BUDPER11
            ASSIGN    BUDPERXX/13.0,BUDPER12
            ASSIGN    BUDPERXX-BUDPER01-BUDPER02-BUDPER03-BUDPER04,BUDPER13
            ASSIGN    BUDPER13-BUDPER05-BUDPER06-BUDPER07-BUDPER08,BUDPER13
            ASSIGN    BUDPER13-BUDPER09-BUDPER10-BUDPER11-BUDPER12,BUDPER13
          ENDIF
ANNUS999  RETURN
POS10000  MOVE      ZERO,F2
POS1L100  ADD       ONE,F2
          IF        F2>SIPENUM
            GOTO      POS1L900
