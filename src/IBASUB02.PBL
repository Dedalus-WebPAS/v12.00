          PACK      KEY11,SISACODE,SISBDAT
          UNPACK    KEY11,SISBSUB,SISBDAT
          CALL      RDSISB1
          IF        OVRCD=0
            LOAD      SISBUBD,F2,BUDPER01,BUDPER02,BUDPER03,BUDPER04:
                                 BUDPER05,BUDPER06,BUDPER07,BUDPER08:
                                 BUDPER09,BUDPER10,BUDPER11,BUDPER12,BUDPER13
            CALL      UPSISB1
