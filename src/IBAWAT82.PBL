.*****************************************************************************
.                            RJUST000
.                      Right Justify Fields - SRF 13227
.*****************************************************************************
RJUST000  RESET     DIM8
          STRIP     DIM8         
          MOVE      DIM8,DIM8A      * Save value
          MOVELPTR  DIM8,F2         * Determine the length 
          ASSIGN    8-F2,F2         * Determine how many spaces trailing
          CLEAR     DIM8
          MOVE      SP8,DIM8        * Clear DIM8 for new right justified value
          RESET     DIM8,F2
          APPEND    DIM8A,DIM8
          RESET     DIM8
RJUST999  RETURN
