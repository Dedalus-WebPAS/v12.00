          MATCH     SP8,BEHCUID
          IF        @EQUAL
            MOVE      "E4. Invalid HCU identifier. ",ERRMSG
            CALL      PRTL0000
          ENDIF
.
          MATCH     "05",BEBSCOD
          IF        @EQUAL
            MATCH     SP8,BEDEFBY
