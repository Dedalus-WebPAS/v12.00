.*****************************************************************************
TAHS0000  PACK     KEY8,WMBOOK,SP8
          CALL     RDMISS1
          IF       OVRCD<>1
            PACK      KEY5,ANSH,ANSA,PTMIAGNC
            CALL      RDCODE1
            IF        OVRCD<>1
              MATCH     "20",BEBSCOD
              IF        @EQUAL
                MOVE      THCSCOD,BETRTAG       * Treatment agency
              ENDIF
            ENDIF
.
            PACK     KEY5,ANSA,SP1,ATYPE,SP8
            CALL     RDCODE1
