          PACK      KEY6,OPARNUF1,SP70           * Recovery Nurse 1 Front    
          CALL      RDOPNUR1
          IF        OVRCD = 0
            MOVE      OPNRGNAM,DIM1
            STRIP     OPNRSNAM
            PACK      FORMATNM,OPNRSNAM,COMM,DIM1
            MOVE      FORMATNM,PRINNUF1
          ENDIF
.
          PACK      KEY6,OPARNUF2,SP70           * Recovery Nurse 2 Front    
          CALL      RDOPNUR1
          IF        OVRCD = 0
            MOVE      OPNRGNAM,DIM1
            STRIP     OPNRSNAM
            PACK      FORMATNM,OPNRSNAM,COMM,DIM1
            MOVE      FORMATNM,PRINNUF2
