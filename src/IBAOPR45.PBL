.***************************************************************************
.*                               KEYC0000                                  *
.* Keyin Case key                                                          *
.***************************************************************************
.
KEYC0000    MOVE      ZERO,EXIT
            DISPLAY   *P1:9,*EF,"Please enter CaseKey :";
.
            KEYIN     *P25:9,*V2LON,CASEKEYS
.
            PACK      CASEKEYS,CASEKEYS,SP70
.
            MATCH     SP70,CASEKEYS
            GOTO      KEYC9000 IF EQUAL
.
            GOTO      KEYC9999
.
KEYC9000    MOVE      ONE,EXIT
.
KEYC9999    RETURN
