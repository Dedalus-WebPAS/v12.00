. -------- I/O routines for LEGDADA1 -------
.
RALGDAD1  MOVE      ZERO,OVRCD
          RESET     KEY8
          READ      LEGDADA1,KEY8;ANS
          GOTO      OVERCOND IF OVER
          RETURN
.
RSLGDAD1  RESET     KEY8
          READ      LEGDADA1,KEY8;;
          RETURN
.
RDLGDAD1  MOVE      ZERO,OVRCD
          RESET     KEY8
          READ      LEGDADA1,KEY8;DLPTDAAD,LPTDAADT,LPTDADCT,LPTDASPA
          GOTO      OVERCOND IF OVER
          MOVE      DLPTDAAD,LPTDAADM
          RETURN
.
RKLGDAD1  MOVE      ZERO,OVRCD
          READKS    LEGDADA1;DLPTDAAD,LPTDAADT,LPTDADCT,LPTDASPA
          GOTO      OVERCOND IF OVER
          MOVE      DLPTDAAD,LPTDAADM
          RETURN
.
RPLGDAD1  MOVE      ZERO,OVRCD
          READKP    LEGDADA1;DLPTDAAD,LPTDAADT,LPTDADCT,LPTDASPA
          GOTO      OVERCOND IF OVER
          MOVE      DLPTDAAD,LPTDAADM
          RETURN
