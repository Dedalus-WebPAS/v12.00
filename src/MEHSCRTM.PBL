.******************************************************************************
.          Supplementary Consumer Records list
.******************************************************************************
SCRLIS00  PACK      KEY16,URNUMBER,SP70
          CALL      RSMHSCR1
SCRLIS10  CALL      RKMHSCR1
          BRANCH    OVRCD,SCRLIS99
.
          MATCH     URNUMBER,MHSCURNO
          GOTO      SCRLIS99 IF NOT EQUAL
.
          PACK      WELLDESC,SP70
          MATCH     SP70,MHSCUC01
          IF        !@EQUAL
            PACK      KEY5,LANSS,ANSA,MHSCUC01,SP70
            CALL      RDCODE1
            IF        OVRCD<>1
              PACK      WELLDESC,PTCDLDES,SP70
            ENDIF
          ENDIF
.
          PACK      ACCMDESC,SP70
          MATCH     SP70,MHSCUC02
          IF        !@EQUAL
            PACK      KEY5,LANSS,ANSB,MHSCUC02,SP70
            CALL      RDCODE1
            IF        OVRCD<>1
              PACK      ACCMDESC,PTCDLDES,SP70
            ENDIF
          ENDIF
.
          PACK      EMPLDESC,SP70
          MATCH     SP70,MHSCUC03
          IF        !@EQUAL
            PACK      KEY5,LANSS,ANSC,MHSCUC03,SP70
            CALL      RDCODE1
            IF        OVRCD<>1
              PACK      EMPLDESC,PTCDLDES,SP70
            ENDIF
          ENDIF
.
          PACK      EDUCDESC,SP70
          MATCH     SP70,MHSCUC04
          IF        !@EQUAL
            PACK      KEY5,LANSS,ANSD,MHSCUC04,SP70
            CALL      RDCODE1
            IF        OVRCD<>1
              PACK      EDUCDESC,PTCDLDES,SP70
            ENDIF
          ENDIF
.
          UNPACK    MHSCSDAT,CCENT,CYEAR,CMON,CDAY
          MOVE      CMON,F2
          LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
          PACK      COLLDATE,CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR,SP70
.
          CLEAR     HTMLLINK
          APPEND    "javascript:updateSuppRecord('",HTMLLINK
          APPEND    MHSCURNO,HTMLLINK
          APPEND    "','",HTMLLINK
          APPEND    COLLDATE,HTMLLINK
          APPEND    "')",HTMLLINK
          RESET     HTMLLINK
.
          PACK      STRTSTRK,SP70
          PACK      ENDSTRK,SP70
          MATCH     "1",MHSCSTAT
          IF        @EQUAL
            MOVE      "<strike>",STRTSTRK
            MOVE      "</strike",ENDSTRK
          ENDIF
.
          WRITE     HTMLFILE;"t.addRow(#"",HTMLLINK,"#",":
                           "#"",MHSCSDAT,"#",":             1
                           "#"",STRTSTRK,WELLDESC,ENDSTRK,"#",":             2
                           "#"",STRTSTRK,ACCMDESC,ENDSTRK,"#",":             3
                           "#"",STRTSTRK,EMPLDESC,ENDSTRK,"#",":             4
                           "#"",STRTSTRK,EDUCDESC,ENDSTRK,"#",";             5
          MATCH     "1",MHSCSTAT
          IF        @EQUAL
            WRITE     HTMLFILE;"#"",STRTSTRK,"No",ENDSTRK,"#");";            6
          ELSE
            WRITE     HTMLFILE;"#"Yes#");";                                  6
          ENDIF
.
          GOTO      SCRLIS10

SCRLIS99  RETURN
