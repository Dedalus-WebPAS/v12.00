            PACK    XXCOFCAR,SP70
            GOTO    SHCO4000
          ENDIF
.
          MOVE      "FC",KEY2
          MOVE      SP70,DIM2
          MOVE      "hf",TCODE
          PACK      KEY5,TCODE,MHHNFOCU,SP70
          CALL      RDCODE1
          IF        OVRCD=0
            UNPACK    THCSCOD,DIM2              * Focus of Care
            REP       " 0",DIM2
          ENDIF
          PACK      XXCOFCAR,KEY2,DIM2,SP70
.
