          PACK      DIM170,XXRDFVER,XXRDRFID,XXRDSORG,XXRDORID,XXRDOTYP:
                           XXRDFDAT,XXRDTDAT,XXRDDELT,XXRDTEAM,XXRDEHCU:
                           XXRDPSEX,XXRDPDOB,XXRDRFFR,XXRDRFTO,XXRDECOD:
                           XXRDRSTR,XXRDREND,SP70,SP70
