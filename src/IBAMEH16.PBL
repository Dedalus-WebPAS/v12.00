            MOVE      FORM4,XXCOPVER            * Protocol version
            REP       " 0",XXCOPVER                                   *M-210949
          ENDIF
.
.                                                             * start *I-210949
          MOVE      "hs",TCODE
          PACK      KEY5,TCODE,MHHNHOTY,SP70
          CALL      RDCODE1
          IF        OVRCD=0
           MATCH     "C",TCINDC1
           GOTO      SHCO4000 IF EQUAL      * it' a Child - bypass FOCUS_OF_CARE
          ENDIF                                               * end   *I-210949
