          ADD       MSNPBAD,FORM10
          ADD       MSTRIN,FORM10
          ADD       MSTROUT,FORM10
          ADD       MSDSCH,FORM10
          ADD       MSNBDAY,FORM10
          ADD       MSBBDAY,FORM10
          ADD       MSDEATH,FORM10
          ADD       MSBPRAD,FORM10
          ADD       MSBPBAD,FORM10
