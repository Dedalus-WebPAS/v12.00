.------------------------------------------------------------------------
.   Clear Parameters for doctor referral
.------------------------------------------------------------------------
CPPRVA00  MOVE      SP70,PRVAF001
CPPRVA99  RETURN
.
.------------------------------------------------------------
.   Set Parameters for Doctor referral file
.------------------------------------------------------------
SPPRVA00  MOVE      ZERO,EXIT
          UNPACK    QRYNAME,KEY5,KEY3
          MOVE      KEY3,F3
          BRANCH    F3,SPPRVA01
.
          MOVE      ONE,EXIT
          GOTO      SPPRVA99
.
SPPRVA01  MOVE      QRYDATA,PRVAF001
          PACK      PRVAF001,PRVAF001,SP70
          GOTO      SPPRVA99
.
SPPRVA99  RETURN
.------------------------------------------------------------
. Clear General Variables
.------------------------------------------------------------
CLPRVA00  MOVE      SP70,PRVACLAM     * Claim number
CLPRVA99  RETURN
.------------------------------------------------------------
. Add General  Fields
.------------------------------------------------------------
PRVA0000  BRANCH    FLDITMNO,PRVA0100
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      PRVA9999
.
PRVA0100  WRITE     HTMLFILE;PRVACLAM; 
          GOTO      PRVA9999
.
PRVA9999  RETURN
.------------------------------------------------------------
.  move CGI variable to file variable 
.------------------------------------------------------------
MVPRVA00  MATCH     PRVAF001,SP70
          IF        !@EQUAL
            MOVE      PRVAF001,PRVACLAM
          ENDIF
.
MVPRVA99  RETURN
.
