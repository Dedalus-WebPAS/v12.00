          COMPARE   LENGTH,ZERO 
          IF        @EQUAL
            MOVE      ONE,LENGTH
          ENDIF     
.
          SFORMAT   VAR,LENGTH
          MOVE      VSMTCMNT,VAR
          WRITE     HTMLFILE;VAR
          SFORMAT   VAR,127
.
          GOTO      VSMD0910
