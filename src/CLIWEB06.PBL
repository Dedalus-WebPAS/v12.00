NXTPG000  IF        FHIRTYPE=1
            MATCH     "A",FORMACTN
            IF        @EQUAL
              CALL      FHREXT00              * FHIR Create Success
              GOTO      NXTPG999
            ENDIF
            MATCH     "D0",FORMACTN
            IF        @EQUAL
              CALL      FHREXT00              * FHIR Mark as Deleted Success
              GOTO      NXTPG999
            ENDIF
            MATCH     "D1",FORMACTN
            IF        @EQUAL
              CALL      FHREXT00              * FHIR Mark as Deleted Success
              GOTO      NXTPG999
            ENDIF
            MATCH     "D2",FORMACTN
            IF        @EQUAL
              CALL      FHREXT00              * FHIR Mark as Deleted Success
              GOTO      NXTPG999
            ENDIF
            MATCH     "D3",FORMACTN
            IF        @EQUAL
              CALL      FHREXT00              * FHIR Mark as Deleted Success
              GOTO      NXTPG999
            ENDIF
            MATCH     "D4",FORMACTN
            IF        @EQUAL
              CALL      FHREXT00              * FHIR Mark as Deleted Success
              GOTO      NXTPG999
            ENDIF
          ENDIF
.
          IF        NEXTPAGE=0
            CALL      WINCLS00              * Refresh parent and close window
          ENDIF
.
          IF        NEXTPAGE=1
            CALL      DISPLY00
          ENDIF
.
          IF        NEXTPAGE=2
            CALL      RDIREC00
          ENDIF
.
          IF        NEXTPAGE=3
            CALL      GOBACK10
          ENDIF
.
          IF        NEXTPAGE=4
            CALL      GOBACK20
          ENDIF
.
          IF        NEXTPAGE=5
            CALL      AJAXRT00              * AJAX Post Return
          ENDIF
.
          IF        NEXTPAGE=6
            CALL      WINEXT00              * Close Window
          ENDIF
.
