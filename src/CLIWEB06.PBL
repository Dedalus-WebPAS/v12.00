          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      DELPAT99
.
DELPAT91  MOVE      "User ID must be the same to strikeout this entry.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      DELPAT99
.
DELPAT98  MOVE      ZERO,EXIT
.
