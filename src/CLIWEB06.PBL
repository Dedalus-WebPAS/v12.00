.
          WRITE     HTMLFILE;"<tr><td nowrap><a href='javascript:ShowDetail02":
                             "(#"",LINKPRG,"#.pbl":
                             "?reportno=",LINKREP:
                             "&template=",LINKTEM:
.                             "&template=003":
                             "&urnumber=",URNUMBER:
                             "&notenumb=",OBMDNOTE,"#")'>":
                             "<img src=../images/MaintenanceIcon.gif hspace=4":
                             " border=0 align=absmiddle></a>":
                             STRIKETG,FBOKDATE,SP1,OBMDTIME,"</td>":
                             "<td>",STRIKETG,WBSENAM," - ",OBMDOCCG,"&nbsp;":
                             STRENDTG,"</td>":
                             "<td>",STRIKETG;
          MOVE      ZERO,CTR
          CALL      OBSNOT00 
.
          PACK      WBSENAM,SP70
          PACK      KEY10,OBMDDUSE,SP70
          CALL      RDWBSE1
          IF        OVRCD<>1
            STRIP     WBSENAM
          ELSE
            MOVE      OBMDDUSE,WBSENAM
          ENDIF
.
          WRITE     HTMLFILE;"&nbsp;",STRENDTG,"</td>":
                             "<td>",WBSENAM," - &nbsp;":
                             STRENDTG,"</td>":
                             "</tr>"
   
          REP       "+ ",URNUMBER
          REP       "+ ",OBMDNOTE 
          GOTO      OBSTAB10
