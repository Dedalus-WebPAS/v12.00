          BRANCH    OVRCD,PRCAN300
          MATCH     VSMDVISN,VSMTVISN
          GOTO      PRCAN300 IF NOT EQUAL
          MATCH     VSMDTYPE,VSMTTYPE
          GOTO      PRCAN300 IF NOT EQUAL
          MATCH     VSMDNOTE,VSMTNOTE
          GOTO      PRCAN300 IF NOT EQUAL
.
          STRIP     VSMTCMNT
          IF        LINBREAK=0
            WRITE     HTMLFILE;VSMTCMNT;
          ELSE
            WRITE     HTMLFILE;VSMTCMNT,"<br>";
          ENDIF
          GOTO      PRCAN200
PRCAN300
          WRITE     HTMLFILE;"</td></tr>"
          GOTO      PRCAN100
.
