          BRANCH    OVRCD,PRNOT300
          MATCH     OBCBVIS,OBCAVIS
          GOTO      PRNOT300 IF NOT EQUAL
          MATCH     OBCBCID,OBCACID
          GOTO      PRNOT300 IF NOT EQUAL
          STRIP     OBCBLDT
          IF        LINBREAK=0
            WRITE     HTMLFILE;OBCBLDT;
          ELSE
            WRITE     HTMLFILE;OBCBLDT,"<br>";
