          MOVE      VSMDTIME,KEY5
          WRITE     HTMLFILE;"<tr><td class=notedate><h4 class=notehead>":
                              CDAY,SP1,MTHNAM3,SP1,CCENT,CYEAR," at ",KEY5:
                             " by ",WBSENAM,"</h4></td></tr>":
                             "<tr><td class=notetext>";
.
          PACK      KEY20,VSMDVISN,VSMDTYPE,VSMDNOTE,SP70
          CALL      RSVSMTX1
