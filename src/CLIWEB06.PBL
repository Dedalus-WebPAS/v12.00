            MATCH     SP70,PMHCPRV1
            IF        !@EQUAL
              SQUEEZE   PMHCPRV1
              PACK      KEY80,PMHCPRV1,CARAT,PMHCSNAM,CARAT,PMHCGNAM,CARAT,CARAT,CARAT,PMHCTITL,SAUSPROV
