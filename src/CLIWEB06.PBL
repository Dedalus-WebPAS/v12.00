          BRANCH    OVRCD,PRCAN999
          MATCH     ADMISSNO,VSMDVISN
          GOTO      PRCAN999 IF NOT EQUAL
          MATCH     NOTETYPE,VSMDTYPE
          GOTO      PRCAN999 IF NOT EQUAL
