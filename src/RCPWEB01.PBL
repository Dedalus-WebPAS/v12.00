            STRIP     WBSENAM
            APPEND    WBSENAM,DIM50B
            APPEND    " - ",DIM50B
            APPEND    CDAY,DIM50B
            APPEND    SP1,DIM50B
            APPEND    MTHNAM3,DIM50B
            APPEND    SP1,DIM50B
            APPEND    CCENT,DIM50B
            APPEND    CYEAR,DIM50B
            APPEND    " @ ",DIM50B
