.------------------------------------------------------------
NEXTCS00  MOVE      NORECORD,KEY2
          REP       " +",KEY2
          ASSIGN    STARTNUM+NORECORD,F3
          MOVE      F3,KEY3
          REP       " +",KEY3
          UNPACK    DIM127,D1,FLDPARA,DIM127
          MOVE      REPORTNO,F1
.
          REP       " +",STARTKEY
          REP       " +",URNUMBER
          REP       " +",INVNUMBR
          REP       " +",CHEQUENO
.
          MOVE      RCPTDATE,CHNGDATE
          CALL      GETDAT00
