          MATCH     SP70,RCDTRELD
          IF        !@EQUAL
            MOVE      SP30,WBSENAM
            MOVE      "Unknown ",WBSENAM
            PACK      KEY10,RCDTRELU,SP70
            CALL      RDWBSE1
            
            UNPACK    RCDTRELD,CCENT,CYEAR,CMON,CDAY
            REP       " 0",CCENT
            REP       " 0",CYEAR
            MOVE      CMON,F2
            LOAD      MTHNAM3,F2,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,DEC
