DAEDT035  PACK      KEY10,EMVIREFG,SP70          * referring HCP description
          MATCH     SP8,EMVIREFG
          IF        !@EQUAL
            MOVE      "Invalid Code",PMHCSNAM
          ENDIF
          MOVE      SP30,PMHCSNAM
          CALL      RDPMHCP1
          PACK      VAR,PMHCSNAM,SP70,SP10
          STRIP     VAR
          ENDSET    VAR
          APPEND    SP1,VAR
          APPEND    PMHCGNAM,VAR
          APPEND    SP70,VAR
          RESET     VAR
          GOTO      DAEDT995
