          PRINT     *92,FORM12P2
          PRINT     *1,PRDTSTIM
          ADD       TWO,CLNO
          ADD       FORM12P2,TOTFEE
.
          CALL      EXTD0000                    * Print extra details
          GOTO      PRIT1000
