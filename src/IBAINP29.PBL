          MOVE      SP3,DRGVERZ                                       *I-137125
          CALL      DISPHEAD
          DISPLAY   *P1:3,*EF:
                    *P1:4,*V2LON,"0",*HOFF," = Exit":
                    *P1:5,*V2LON,"1",*HOFF," = Insert":
                    *P1:6,*V2LON,"2",*HOFF," = Change":
                    *P1:7,*V2LON,"3",*HOFF," = Delete":
                    *P1:8,*V2LON,"4",*HOFF," = Load New DRG File":
                    *P1:9,*V2LON,"5",*HOFF," = Regenerating DRG Keyword ":
                                           "Index":

                    *P1:11,"Select Option : ";
.
