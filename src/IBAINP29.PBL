.
          COMPARE   ZERO,OPTION
          GOTO      OPTN9500 IF EQUAL       * Exit
.
.                                                                     *C-137125
          BRANCH    OPTION,OPTN5000,OPTN5000,OPTN5000,OPTN5000,OPTN7000
.
          BEEP
          GOTO      OPTN1000
.
.******************************************** start of addition       *I-137125
