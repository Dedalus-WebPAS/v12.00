          IF        IBCNMHOS=1
            PACK      KEY8,AADMNO,SP70
            CALL      RDPMVX11
            BRANCH    OVRCD,NCOD1000
            MATCH     HOSPCODE,PMVXMHOS
            GOTO      NCOD1000 IF NOT EQUAL
          ENDIF

          MOVE      AADMNO,TEMPADMN
.
          PACK      KEY8,TEMPADMN,SP70
          CALL      RDTEMP1
          IF        OVRCD=1
            CALL      WRTEMP1
          ENDIF
          GOTO      NCOD1000
.
NCOD9999  RETURN
