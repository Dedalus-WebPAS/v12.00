.         INC       AAEPRIIO.PBL
.
.         FILE  I/O REQUESTS FOR PRIORITY 
.
.OVRCD  FORM            1             0=NOT OVER
.                                     1= OVER
.KEY24   DIM             22             KEY FOR AAEPRIA1
.
.        Modifications : 12/04/90 Graeme Williams
.                        Added RDKPPRIA1
.
RDSPRIA1 RESET     KEY24
         READ      AAEPRIA1,KEY24;;
         RETURN  
.
RDAPRIA1 MOVE      ZERO,OVRCD
         RESET     KEY24
         READ      AAEPRIA1,KEY24;ANS
         GOTO      OVERCOND IF OVER
         RETURN  
.
RDPRIA1  MOVE      ZERO,OVRCD
         RESET     KEY24
         READ      AAEPRIA1,KEY24;ADPPRTY,ADPDATE,ADPTIME,ADPNUMB,ADPSPAR
         GOTO      OVERCOND IF OVER
         RETURN  
.
RDKPRIA1 MOVE      ZERO,OVRCD
         READKS    AAEPRIA1;ADPPRTY,ADPDATE,ADPTIME,ADPNUMB,ADPSPAR
         GOTO      OVERCOND IF OVER
         RETURN  
.
RDPPRIA1 MOVE      ZERO,OVRCD
         READKP    AAEPRIA1;ADPPRTY,ADPDATE,ADPTIME,ADPNUMB,ADPSPAR
         GOTO      OVERCOND IF OVER
         RETURN  
.
WRPRIA1  RESET     KEY24
         WRITE     AAEPRIA1,KEY24;ADPPRTY,ADPDATE,ADPTIME,ADPNUMB,ADPSPAR
         RETURN  
.
UPPRIA1  UPDATE    AAEPRIA1;ADPPRTY,ADPDATE,ADPTIME,ADPNUMB,ADPSPAR
         RETURN  
.
DEPRIA1  RESET     KEY24
         DELETE    AAEPRIA1,KEY24
         RETURN
