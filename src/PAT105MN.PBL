.------------------------------------------------------------------------------
. Online ICD10 Edition 5 Code Validation
.------------------------------------------------------------------------------
VAL5CD00  MOVE      SP70,VALDNAME
          MATCH     SP70,VALDCODE
          GOTO      VAL5CD90 IF EQUAL
.
          MOVE      "Invalid Code Entered",VALDNAME
          BRANCH    VALDTYPE,VAL5CD10,VAL5CD20
          MOVE      "Invalid Validation Type",VALDNAME
          GOTO      VAL5CD90
.
VAL5CD10  MOVE      VALDCODE,KEY9
          CALL      RDPT15O1
          IF        OVRCD=0
            MOVE      ODESC,VALDNAME
          ENDIF
          GOTO      VAL5CD90
.
VAL5CD20  MOVE      VALDCODE,KEY9
          CALL      RDPT15D1
          IF        OVRCD=0
            MOVE      DDESC,VALDNAME
          ENDIF
          GOTO      VAL5CD90
.
VAL5CD90  CALL      OPENHTML
          BRANCH    EXIT,VAL5CD99
          WRITE     HTMLFILE;"<h3 align=center>Checking Database</h3>":
                             "<script type=#"text/javascript#">":
                             "{ opener.ValidateName.value=#"",VALDNAME,"#";":
                             "  opener.ValidateCode.value=#"",VALDCODE,"#";"
.
          MATCH     "Invalid",VALDNAME
          IF        @EQUAL
            WRITE     HTMLFILE;"  alert(#"Invalid Code Entered#");";
          ENDIF
.
          WRITE     HTMLFILE;"  close(); } </script>"
          CALL      CLOSHTML
.
VAL5CD99  RETURN
