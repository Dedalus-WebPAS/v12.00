.------------------------------------------------------------------------------
. ICD10 Edition 5 Diagnosis Coding Maintenance: Display,Add,Update,Delete
.------------------------------------------------------------------------------
ICD5DI00  RESET     PTICD001
          MOVE      "5",ICD10EDN
          RESET     UPDTTYPE
          MATCH     SP1,UPDTTYPE
          GOTO      ICD5DI40 IF EQUAL
.
          MATCH     "A",UPDTTYPE    * Add formaction
          GOTO      ICD5DI10 IF EQUAL
.
          MATCH     "U",UPDTTYPE    * Update formaction
          GOTO      ICD5DI20 IF EQUAL
.
          MATCH     "D",UPDTTYPE    * Delete formaction
          GOTO      ICD5DI30 IF EQUAL
.
          MATCH     "V",UPDTTYPE    * Validate Formaction
          IF        @EQUAL
            CALL      VAL5CD00
            GOTO      ICD5DI99
          ENDIF
.
          GOTO      ICD5DI93
.
ICD5DI10  CALL      CHKDIS00  * Checks mandatory fields & checks for blank
          BRANCH    EXIT,ICD5DI99
          PACK      KEY9,PTICD001,SP70  * ADD FORMACTION
          CALL      RDPT15D1
          COMPARE   ZERO,OVRCD
          GOTO      ICD5DI92 IF EQUAL
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      WRPT15D1      * Writes away the data
          PROC      PATK5DUP      * Update Keyword table
.
          CALL      ADDCAN00      * Add Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD5DI99
.
ICD5DI20  PACK      KEY9,PTICD001,SP70  * UPDATE FORMACTION
          CALL      RDPT15D1
          BRANCH    OVRCD,ICD5DI91
          CALL      CHKDIS00      * Checks mandatory fields
          BRANCH    EXIT,ICD5DI99
          CALL      DISSAV00      * Moves input variables to file variables
          CALL      UPPT15D1      * Writes away the data
          PROC      PATK5DUP      * Update Key word Table
.
          CALL      UPDCAN00      * Update Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD5DI99
.
ICD5DI30  PACK      KEY9,PTICD001,SP70   * DELETE FORMACTION
          CALL      RDPT15D1
          BRANCH    OVRCD,ICD5DI91
          CALL      DEPT15D1      * Deletes the data
          PROC      PATK5DUP      * Update Key word Table
.
          CALL      DELCAN00      * Delete Cancer Code Record
.
          MOVE      ZERO,EXIT
          GOTO      ICD5DI99
.
ICD5DI40  PACK      KEY9,PTICD001,SP70
          CALL      RDPT15D1
          IF        OVRCD=1
            CALL      CLRDIS00    * Clear all the file variables
          ENDIF
.
          MOVE      ONE,CANCERCD
          PACK      KEY9,PTICD001,SP70
          CALL      RDPTCAN1
          IF        OVRCD=1
            MOVE     ZERO,CANCERCD
            MOVE     ZERO,PTCCLATR
          ENDIF
.
          CLEAR     WEBTITLE
          MOVE      "ICD10 Edition 5 Diagnosis Maintenance",WEBTITLE
          RESET     WEBTITLE
.
          CALL      WEBHED00   * Create Output File and Write HTML Page Header
          BRANCH    EXIT,ICD5DI99
          CALL      WEBBOD00   * Process Web Body
          CALL      WEBEND00   * Process End of HTML File
          MOVE      ONE,EXIT
          GOTO      ICD5DI99
.
ICD5DI91  MOVE      "ICD10 Edition 5 Diagnosis Code does not exist",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD5DI99
.
ICD5DI92  MOVE      "ICD10 Edition 5 Diagnosis Code exists",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD5DI99
.
ICD5DI93  MOVE      "Invalid Form Action.",WEBTITLE
          CALL      WEBERR00
          MOVE      ONE,EXIT
          GOTO      ICD5DI99
.
ICD5DI99  RETURN
+
