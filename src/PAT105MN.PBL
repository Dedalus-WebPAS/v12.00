.-----------------------------------------------------------
SPT5OA00  WRITE     HTMLFILE;"<tr align=center>":
                            "<td class=HeadingCell colspan=2>":
                            "<b>You Searched for ":
                             KEYWORDS,"</td>":
                            "</tr>":
                            "<tr>":
                            "<td class=HeadingCell width=15%><b>Code</b></td> ":
                            "<td class=HeadingCell><b>Description</b></td>":
                            "</tr>"
.
          CALL      GETWRD00
.
          UNPACK    DIM127,D1,LINKPRG,DIM127
          UNPACK    DIM127,D1,LINKREP,DIM127
          UNPACK    DIM127,D1,LINKTEM,DIM127
.
          IF        NORECORD=0
            MOVE      PTCNNRTD,NORECORD   * Set Default No Records to Display
          ENDIF
          MOVE      ZERO,RECNUMB
          MOVE      ZERO,DISNUMB
.
          STRIP     KEYWRD01
          PACK      KEY24,KEYWRD01,SP70
          CALL      RSPT5O2
.
SPT5OA10  CALL      RKPT5O2
          BRANCH    OVRCD,SPT5OA90
.
          CALL      SPT5OB00
          BRANCH    OVRCD,SPT5OA10,SPT5OA90
.
          MOVE      PTCOITM,KEY9
          CALL      RDPT15O1
          BRANCH    OVRCD,SPT5OA10
.
          ADD       ONE,RECNUMB
          IF        STARTNUM>=RECNUMB
            GOTO      SPT5OA10
          ENDIF
.
          BRANCH    LNK2PRT,SPT5OA20,SPT5OA30
          GOTO      SPT5OA99
.
SPT5OA20  WRITE     HTMLFILE;"<tr><td nowrap><A HREF='javascript:showICD02":
                             "(#"",LINKPRG,"#.pbl":
                             "?reportno=",LINKREP:
                             "&template=",LINKTEM:
                             "&ptico001=",OPCODE,"#")'>":
                             "<img src=../images/MaintenanceIcon.gif hspace=4":
                             " border=0 align=absmiddle></a>":
                             OPCODE,"</td>":
                             "<td>",ODESC,"</td>":
                             "</tr>"
.
          GOTO      SPT5OA80
.
SPT5OA30  WRITE     HTMLFILE;"<tr valign=top>":
                              "<td><a href=#"#" ":
                              "OnClick=#"updateParent('",ODESC:
                              "','",OPCODE,"')#">":
                              "<img src=../images/MaintenanceIcon.gif hspace=4":
                              " border=0 align=absmiddle></a>":
                              OPCODE,"</td>":
                              "<td>",ODESC,"</td>":
                             "</tr>"
.
SPT5OA80  ADD       ONE,DISNUMB
          COMPARE   NORECORD,DISNUMB
          GOTO      SPT5OA10 IF NOT EQUAL
          GOTO      SPT5OA99
.
SPT5OA90  CALL      NOMORE00
.
SPT5OA99  RETURN
.------------------------------------------------------------
. Check ICD10 Edition 5 Procedure Record
.------------------------------------------------------------
SPT5OB00  MATCH     KEYWRD01,PTCOKWD
          GOTO      SPT5OB92 IF NOT EQUAL
          IF        KEYWRDNO>1
            PACK      SAVKEY24,PTCOKWD,PTCOITM
            CALL      SPT5OC00
            BRANCH    EXIT,SPT5OB91
            MOVE      SAVKEY24,KEY24
            CALL      RDPT5O2
          ENDIF
.
. Insert Other Check for Valid Record Here i.e. Suspended or In Active
.   eg    COMPARE    ONE,INACTIVE
.         GOTO       SPT5OB91 IF EQUAL
.
          GOTO      SPT5OB90
.
SPT5OB90  MOVE      ZERO,OVRCD
          GOTO      SPT5OB99
SPT5OB91  MOVE      ONE,OVRCD
          GOTO      SPT5OB99
SPT5OB92  MOVE      TWO,OVRCD
SPT5OB99  RETURN
.------------------------------------------------------------
. Check for Matching ICD10 Edition 5 Procedure Words
.------------------------------------------------------------
SPT5OC00  MOVE      ONE,F2
          MOVE      ZERO,EXIT
          PACK      SAVKEY9,PTCOITM,SP70
.
SPT5OC10  COMPARE   KEYWRDNO,F2           * Loop through each Key Word in Search
          GOTO      SPT5OC99 IF EQUAL     * Exit OK
          ADD       ONE,F2
          LOAD      KEYWRDXX,F2,KEYWRD01,KEYWRD02,KEYWRD03,KEYWRD04,KEYWRD05:
                             KEYWRD06,KEYWRD07,KEYWRD08,KEYWRD09,KEYWRD10
          PACK      KEY24,PTCOITM,KEYWRDXX
          CALL      RDPT5O1               * Check for Exact Match
          COMPARE   ZERO,OVRCD            * Ok so Continue Loop
          GOTO      SPT5OC10 IF EQUAL
.
          CALL      RKPT5O1               * Not Exact so Read Next Record
          BRANCH    OVRCD,SPT5OC90        * No Match So Exit No Match
          PACK      KEY9,PTCOITM,SP70
          MATCH     SAVKEY9,KEY9        * Match Key Fields
          GOTO      SPT5OC90 IF NOT EQUAL * No Match So Exit
          STRIP     KEYWRDXX
          MATCH     PTCOKWD,KEYWRDXX         * Check Key Word
          GOTO      SPT5OC90 IF NOT EQUAL * No Match So Exit
          GOTO      SPT5OC10              * Check Next Search Word
.
SPT5OC90  MOVE      ONE,EXIT
SPT5OC99  RETURN
