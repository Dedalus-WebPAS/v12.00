                             OPIT0310,OPIT0320,OPIT0330,OPIT0340,OPIT0350:
                             OPIT0360
.
          WRITE     HTMLFILE;"Invalid IBA Tag";
          GOTO      OPIT9999
.
OPIT0010  WRITE     HTMLFILE;OPITCODE;  * Item Code
          GOTO      OPIT9999
.
OPIT0020  WRITE     HTMLFILE;OPITDESC;  * Item Description
          GOTO      OPIT9999
.
OPIT0030  WRITE     HTMLFILE;OPITINDC;  * Indicator
