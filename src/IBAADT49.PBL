            IF        OVRCD = 0
              PACK      XSPLTY,THCSCOD,SP10      * Doctor type
            ENDIF
            MATCH     SP3,XSPLTY
            IF        @EQUAL
              MOVE      "Invalid speciality on admission ",ERRDESC
              CALL      WRTEMP
