          APPEND    "Doctor Code       : ",PRUNDO2
          APPEND    DOCTOR,PRUNDO2
          APPEND    DASH,PRUNDO2
          APPEND    DOCNAM,PRUNDO2
          RESET     PRUNDO2
          GOTO      KDOC8000
