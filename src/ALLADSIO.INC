. Requires - AUDTTYPE 1=write 
.                     2=before change
.                     3=after change
.                     4=delete
.**************************************
WAALAD00  READ      CONTROLF,HUND06;*202,ALCNAAUD
          MATCH     "1",ALCNAAUD
          GOTO      WAALAD99 IF NOT EQUAL
.
          PACK     ALADAUDD,CCC,CYY,CMM,CDD
          REP       " 0",ALADAUDD
          CLOCK     TIME,ALADAUDT
          MOVE      PORT,ALADAUDP
          LOAD      ALADAUDR,AUDTTYPE,ANSA,ANSB,ANSC,ANSD
          MOVE      ONE,ALADAUDS
          MOVE      PASSCODE,ALADAUDO
          PACK      KEY19,ALADAUDD,ALADAUDT,ALADAUDP,ALADAUDR
          MOVE      KEY19,AUDTSKEY
          CALL      AWALADS
WAALAD99  RETURN
.**************************************
. DAssff - Delete Before Change Audit 
.**************************************
DAALAD00  READ      CONTROLF,HUND06;*202,ALCNAAUD
          MATCH     "1",ALCNAAUD
          GOTO      DAALAD99 IF NOT EQUAL
.
          MOVE      AUDTSKEY,KEY19
          CALL      ADALADS
DAALAD99  RETURN
