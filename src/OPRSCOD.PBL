.
.     OPRSCOD : Display Operating Room File Entries
.
.     This include displays entries in the Operating Room file .
.
.     Modifications: 
.
.         Display Heading, and then loop over entries
.
OPRSCOD   DISPLAY   *P1:4,*EF,*P30:4,*V2LON,*ULON,*+,OPCNODSC,*-:
                                     " File Enteries"
          MOVE      FIVE,CVERT
          MOVE      ONE,CCOL
.
          PACK      KEY6,SP6
          CALL      RSOPOPR1
OPRSCOD1  CALL      RKOPOPR1
          BRANCH    OVRCD OF OPRSCOD4
.
          MOVE      OPRMDESC,DIM15
.
          DISPLAY   *PCCOL:CVERT,*V2LON,OPRMROOM,SP2,*HOFF,DIM15
          ADD       ONE,CVERT
          COMPARE   TWENTY4,CVERT
          GOTO      OPRSCOD1 IF LESS
.
          MOVE      FIVE,CVERT
          ADD       TWENTY6,CCOL
          COMPARE   "70",CCOL
          GOTO      OPRSCOD1 IF LESS
.
          BRANCH    CNEWDS,OPRSCOD2
.
          KEYIN     *P1:24,*EL," (",*V2LON,"E",*HOFF,")xit, (":
                    *V2LON,"F",*HOFF,")irst Screen, (":
                    *V2LON,"N",*HOFF,")ext Screen : ",*V2LON,ANS;
.
          CMATCH    ANSE,ANS
          GOTO      OPRSCOD5 IF EQUAL
          CMATCH    ANSF,ANS
          GOTO      OPRSCOD IF EQUAL
          GOTO      OPRSCOD3
.
OPRSCOD2  KEYIN     *P1:24,*EL," (",*V2LON,"C",*HOFF,")ontinue, (":
                    *V2LON,"F",*HOFF,")irst Screen, (":
                    *V2LON,"N",*HOFF,")ext Screen : ",*V2LON,ANS;
.
          CMATCH    ANSC,ANS
          GOTO      OPRSCOD5 IF EQUAL
          CMATCH    ANSF,ANS
          GOTO      OPRSCOD IF EQUAL
.
.         Clear the screen
.
OPRSCOD3  MOVE      FIVE,CVERT
          MOVE      ONE,CCOL
          DISPLAY   *P1:5,*EF
          GOTO      OPRSCOD1
.
.      End of codes
.
OPRSCOD4  BRANCH    CNEWDS,OPRSCOD5
          DISPLAY   *P1:24,*EL;
          CALL      HITENTER
.
OPRSCOD5  MOVE      ZERO,OVRCD
          RETURN
