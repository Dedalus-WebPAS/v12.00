.---------------------------------------------------------------------------
.   System    :   IBA Software                       
.   Include   :   IBAPASKY
.   Routine   :   KIBPSA00 Keyin                     
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.   Mods      :   Adapted From IBAMENKY
.-------------------------------------------------------------------------------
KIBPSA00
          MOVE      UNDLN70,SECODE
          KEYIN     *PCCOL:CVERT,*DV,SECODE:                                  
                    *PCCOL:CVERT,*V2LON,SECODE:                               
                    *PCCOL:CVERT,*DV,SECODE
.
          ENDSET    SECODE
          APPEND    SP70,SECODE
          RESET     SECODE
.
          MATCH     SP70,SECODE
          GOTO      KIBPSA90 IF EQUAL
.
          CMATCH    EXITCHAR,SECODE
          GOTO      KIBPSA95 IF EQUAL
.
          CMATCH    QUEST,SECODE
          GOTO      KIBPSA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HIBPSA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    SECODE
          APPEND    SP70,SECODE
          RESET     SECODE
.
          MATCH     SP70,SECODE
          GOTO      KIBPSA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,SECODE                                
KIBPSA10
          PACK      KEY4,SECODE                                               
          CALL      RDAPASS1
          BRANCH    OVRCD,KIBPSA80
          BRANCH    CKEYTYP,KIBPSA75
          CALL      RDPASS1
KIBPSA70
          MOVE      ZERO,EXIT
          GOTO      KIBPSA99
KIBPSA75
          DISPLAY   *P1:24,*EL,*B," Already Exist - ";                                                             
          CALL      HITENTER
          GOTO      KIBPSA00
KIBPSA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KIBPSA99,KIBPSA99
          DISPLAY   *P1:24,*EL,*B," Does Not Exist - ";                                                            
          CALL      HITENTER
          GOTO      KIBPSA00
KIBPSA90
          MOVE      ONE,EXIT
          GOTO      KIBPSA99
KIBPSA95
          MOVE      TWO,EXIT
KIBPSA99
          RETURN
.---------------------------------------------------------------------------
.   System    :   IBA Software                       
.   Include   :   IBAMENKY
.   Routine   :   HIBPSA00 Help Description    
.   Return    :           
.-------------------------------------------------------------------------------
HIBPSA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
          GOTO      HIBPSA04 IF NOT EQUAL
          MOVE      "10",HTOP
          MOVE      "24",HBOT
          MOVE      "40",HLEF
          MOVE      "80",HRIG
          ENDIF
HIBPSA04
          CALL      GSCRN000
          BRANCH    EXIT,HIBPSA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Code"               
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 8",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"User Name  "        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "20",STDCOL03
HIBPSA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,SECODE
          PACK      KEY4,SECODE,SP70                                          
.
          CALL      RDSPASS1
HIBPSA10
          CALL      RDKPASS1
          BRANCH    OVRCD,HIBPSA15
          PACK      KEY4,SECODE,SP70                                          
.
          BRANCH    CSEARCH,HIBPSA12
          CALL      HIBPSA60
          BRANCH    OVRCD,HIBPSA10
HIBPSA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY4,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,SECODE
          DISPLAY   *PSTDCOL02:STDROW,SECUSER
          COMPARE   MAXITEM,XITEMNO
          GOTO      HIBPSA10 IF NOT EQUAL
HIBPSA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HIBPSA20
          CALL      HSCRI000
          BRANCH    EXIT,HIBPSA30,HIBPSA35,HIBPSA40,HIBPSA45,HIBPSA04:
                              HIBPSA91,HIBPSA50
          GOTO      HIBPSA92
HIBPSA30
          CALL      RDKPASS1
          BRANCH    OVRCD,HIBPSA85
          PACK      KEY4,SECODE,SP70                                          
.
          CALL      RDPPASS1
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBPSA10
HIBPSA35
          MOVE      ST1,KEY4 
          CALL      RDPASS1
          CALL      RDPPASS1
          BRANCH    OVRCD,HIBPSA85
          PACK      KEY4,SECODE,SP70                                          
.
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HIBPSA36
          CALL      RDPPASS1
          BRANCH    OVRCD,HIBPSA37
          PACK      KEY4,SECODE,SP70                                          
.
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HIBPSA36 IF NOT EQUAL
HIBPSA37
          MOVE      ZERO,XITEMNO
          GOTO      HIBPSA10
HIBPSA40
          MOVE      ST1,KEY4 
          CALL      RDPASS1
          CALL      RDPPASS1
          BRANCH    OVRCD,HIBPSA85
          PACK      KEY4,SECODE,SP70                                          
.
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBPSA08
HIBPSA45
          CALL      RDKPASS1
          BRANCH    OVRCD,HIBPSA85
          PACK      KEY4,SECODE,SP70                                          
.
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY4,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RDSPASS1
HIBPSA46
          CALL      RDPPASS1
          BRANCH    OVRCD,HIBPSA47
          PACK      KEY4,SECODE,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HIBPSA46 IF NOT EQUAL
HIBPSA47
          MOVE      ZERO,XITEMNO
          GOTO      HIBPSA10
HIBPSA50  COMPARE   ZERO,CSEARCH
          GOTO      HIBPSA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBPSA08
HIBPSA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBPSA08
HIBPSA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HIBPSA61 IF EQUAL
          MATCH     SCHFLD01,SECODE
          GOTO      HIBPSA72 IF NOT EQUAL
HIBPSA61
          MATCH     SP70,SCHFLD02
          GOTO      HIBPSA62 IF EQUAL
          MATCH     SCHFLD02,SECUSER
          GOTO      HIBPSA72 IF NOT EQUAL
HIBPSA62
          GOTO      HIBPSA75
HIBPSA72  MOVE      ONE,OVRCD
HIBPSA75  RETURN
HIBPSA85
          LOAD      KEY4,XITEMNO,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY4,SECODE                                               
          CALL      RDPASS1
HIBPSA90
.                                                                               
          BEEP
          GOTO      HIBPSA20
HIBPSA91
          MOVE      SP70,KEY4 
          MOVE      SP70,SECODE
          GOTO      HIBPSA96
HIBPSA92
          MOVE      SP70,KEY4 
          MOVE      SP70,SECODE
          LOAD      KEY4,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY4,SECODE                                               
HIBPSA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HIBPSA99
          RETURN
