        GOTO     KEYHFN2
.
KEYHFN1 MOVE     STHFN,FRHFN
        DISPLAY  *P24:8,*V2LON,STHFN;
.
KEYHFN2 MOVE     SP3,EDHFN
        DISPLAY  *P41:8,*EL,"Ending Health Fund   : ";
.
        MOVE     EIGHT,EVERT
        MOVE     SIXTY4,ECOL
        MOVE     ZERO,CKYIMAND
        CALL     PATFNDKY
.
        MOVE     HCODE,EDHFN
