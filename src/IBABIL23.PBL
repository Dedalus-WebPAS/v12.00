            GOTO        DAYR00 IF NOT EQUAL         * Not a Patient O/S amt
          ELSE
            IF            PASSFLAG=2
              MATCH         "y",PATFLAG
              GOTO          DAYR00 IF NOT EQUAL    * Not a debt collection debt
              MATCH         SP70,SAVAGEN
              IF            !@EQUAL
                MATCH         DEBTAGEN,SAVAGEN
                CALL          AGENT00 IF NOT EQUAL
              ENDIF
            ELSE
              MATCH         SP70,PATFLAG
