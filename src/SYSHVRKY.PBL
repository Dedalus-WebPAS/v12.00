          BRANCH    CKEYTYP,KSYHVA99,KSYHVA99
          MOVE      "Help Id Keyin Does Not Exist - ",DISPMSG                                        
          CALL      MESSAGE1
          GOTO      KSYHVA00
KSYHVA90
