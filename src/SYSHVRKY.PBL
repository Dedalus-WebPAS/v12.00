          GOTO      KSYHVA99
KSYHVA95
          MOVE      TWO,EXIT
KSYHVA99 TOPIC    PRGID
          RETURN
.---------------------------------------------------------------------------
.   System    :   System Control                     
.   Include   :   SYSHVRKY
.   Routine   :   KSYHVB00 Keyin Help Routine        
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KSYHVB00 TOPIC     "SYHVHID "
          MOVE      UNDLN70,KEY5    
          KEYIN     *PCCOL:CVERT,*DV,KEY5:                                      
                    *PCCOL:CVERT,*V2LON,KEY5:                                   
                    *PCCOL:CVERT,*DV,KEY5    
.
          ENDSET    KEY5    
          APPEND    SP70,KEY5    
          RESET     KEY5    
.
          MATCH     SP70,KEY5    
          GOTO      KSYHVB90 IF EQUAL
.
          CMATCH    EXITCHAR,KEY5    
          GOTO      KSYHVB95 IF EQUAL
.
          CMATCH    QUEST,KEY5    
          GOTO      KSYHVB10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HSYHVB00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    KEY5    
          APPEND    SP70,KEY5    
          RESET     KEY5    
.
          MATCH     SP70,KEY5    
          GOTO      KSYHVB00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,KEY5                                    
KSYHVB10
          CALL      RASYHV1 
          BRANCH    OVRCD,KSYHVB80
          BRANCH    CKEYTYP,KSYHVB75
          CALL      RDSYHV1 
KSYHVB70
          MOVE      ZERO,EXIT
          GOTO      KSYHVB99
KSYHVB75
          MOVE      "Help Routine Already Exist - ",DISPMSG                                          
          CALL      MESSAGE1
          GOTO      KSYHVB00
KSYHVB80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KSYHVB99,KSYHVB99
          MOVE      "Help Routine Does Not Exist - ",DISPMSG                                         
          CALL      MESSAGE1
          GOTO      KSYHVB00
KSYHVB90
          MOVE      ONE,EXIT
          GOTO      KSYHVB99
KSYHVB95
          MOVE      TWO,EXIT
KSYHVB99 TOPIC    PRGID
          RETURN
