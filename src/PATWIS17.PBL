          CALL     RDCODE1
          BRANCH   OVRCD,C17TY900
.
          MATCH    "6",TCINDC4              * Check Program Funding Source
          GOTO     C17TY900 IF NOT EQUAL
.
          MATCH    "0   ",THCSCOD           * check care types
          GOTO     C17TY900 IF EQUAL
          MATCH    "1   ",THCSCOD
          GOTO     C17TY900 IF EQUAL
          MATCH    "2   ",THCSCOD
          GOTO     C17TY900 IF EQUAL
          MATCH    "6   ",THCSCOD
          GOTO     C17TY900 IF EQUAL
          MATCH    "7   ",THCSCOD
          GOTO     C17TY900 IF EQUAL
          MATCH    "8   ",THCSCOD
          GOTO     C17TY900 IF EQUAL
          MATCH    "9   ",THCSCOD
          GOTO     C17TY900 IF EQUAL
          MATCH    "5   ",THCSCOD
          GOTO     C17TY900 IF EQUAL
          MATCH    "3   ",THCSCOD
          GOTO     C17TY900 IF EQUAL
          MATCH    "E   ",THCSCOD
          GOTO     C17TY900 IF EQUAL
          MATCH    "F   ",THCSCOD
          GOTO     C17TY900 IF EQUAL
          MATCH    "J   ",THCSCOD
          GOTO     C17TY900 IF EQUAL
.
          MOVE     ZERO,EXIT
          GOTO     C17TY999
.
C17TY900  MOVE     ONE,EXIT                 * Not valid
C17TY999  RETURN
