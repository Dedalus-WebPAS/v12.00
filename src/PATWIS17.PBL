.******************************************************************************
.*                                  B17SW000              Called by: PATWIS17 *
.*                            Calculate Base WIES17                           *
.******************************************************************************
B17SW000  CALL      D17SC000                * length of stay category
          CALL      C17ST000                * calculate inlier status
.
          MATCH     "L",W17NLIER            * low outlier
          CALL      BW17L000 IF EQUAL       * calculate base WIES17
.
          MATCH     "I",W17NLIER            * inlier
          CALL      BW17N000 IF EQUAL       * calculate base WIES17
.
          MATCH     "H",W17NLIER            * high outlier
          CALL      BW17H000 IF EQUAL       * calculate base WIES17
.
B17SW999  RETURN
+
.******************************************************************************
.*                                  D17SC000              Called by: B17SW000 *
.*                      Determine Length Of Stay Category                     *
.******************************************************************************
D17SC000  MOVE      NBRDAYS,WIS17LOS        * length of stay from NHOSPDAY
          MOVE      "M",LOSCAT              * set LOS category to multiday
          MATCH     ADATE,DDATE             * check for sameday
          IF        @EQUAL
            MOVE      ONE,SAMEDAY           * sameday
            MOVE      ONE,WIS17LOS          * LOS = 1
            MOVE      "S",LOSCAT            * set LOS category to sameday
            GOTO      D17SC999
          ELSE
            MOVE      ZERO,SAMEDAY          * not sameday
          ENDIF
.
          IF        SAMEDAY = 0 & WIS17LOS = 1
            MOVE      "O",LOSCAT            * set LOS category to oneday
            GOTO      D17SC999
          ENDIF
.
          IF        WIS17LOS > 1825
            MOVE      "1825",WIS17LOS       * maximum LOS is 5 years
          ENDIF
.
D17SC999  RETURN
+
.******************************************************************************
.*                                  C17ST000              Called by: B17SW000 *
.*                           Calculate Inlier Status                          *
.******************************************************************************
C17ST000  MOVE      "I",W17NLIER            * set status to inlier
          IF        WIS17LOS < PT17LIBP
            MOVE      "L",W17NLIER          * set status to low outlier
            GOTO      C17ST999
          ENDIF
.
          ASSIGN    (PT17HIBP+ADJMVDAY),FORM12P4   * high bound. + mech.v days
          IF        WIS17LOS > FORM12P4
            MOVE      "H",W17NLIER          * set status to high outlier
          ENDIF
C17ST999  RETURN
+
.******************************************************************************
.*                                  BW17L000              Called by: B17SW000 *
.*                     Calculate Base WIES17 for Low Outlier                  *
.******************************************************************************
BW17L000  MATCH     "S",LOSCAT            * sameday
          IF        @EQUAL
            MOVE      PT17TSDW,TOTBWIES             * base WIES17
            ASSIGN    (TOTBWIES/PT17TIMW),TOTIES17  * IES17
.
            MOVE      PT17TSDW,WIS17WGT             * for display
            MOVE      "Same Day Wgt ",WIS17DSC
            GOTO      BW17L999
          ENDIF
.
          MATCH     "O",LOSCAT            * oneday
          IF        @EQUAL
            MOVE      PT17TODW,TOTBWIES             * base WIES17
            ASSIGN    (TOTBWIES/PT17TIMW),TOTIES17  * IES17
.
            MOVE      PT17TODW,WIS17WGT             * for display
            MOVE      "One Day Wgt  ",WIS17DSC
            GOTO      BW17L999
          ENDIF
.
          MATCH     "M",LOSCAT            * multiday
          IF        @EQUAL
            ASSIGN    (PT17TODW+((WIS17LOS-1)*PT17TLOM)),TOTBWIES
.                                                    * base WIES17
            ASSIGN    (TOTBWIES/PT17TIMW),TOTIES17   * IES17
.
            MOVE      PT17TLOM,WIS17WGT             * for display
            MOVE      "Low Outlr Wgt",WIS17DSC
            GOTO      BW17L999
          ENDIF
BW17L999  RETURN
+
.******************************************************************************
.*                                  BW17N000              Called by: B17SW000 *
.*                      Calculate Base WIES17 for Inlier                      *
.******************************************************************************
BW17N000  MOVE      ONE,TOTIES17          * IES17 = 1
          MATCH     "S",LOSCAT            * sameday
          IF        @EQUAL
            MOVE      PT17TSDW,TOTBWIES   * base WIES17
.
            MOVE      PT17TSDW,WIS17WGT             * for display
            MOVE      "Same Day Wgt ",WIS17DSC
            GOTO      BW17N999
          ENDIF
.
          MATCH     "O",LOSCAT            * oneday
          IF        @EQUAL
            MOVE      PT17TODW,TOTBWIES   * base WIES17
.
            MOVE      PT17TODW,WIS17WGT             * for display
            MOVE      "One Day Wgt  ",WIS17DSC
            GOTO      BW17N999
          ENDIF
.
          MATCH     "M",LOSCAT            * multiday
          IF        @EQUAL
            MOVE      PT17TIMW,TOTBWIES   * base WIES17
.
            MOVE      PT17TIMW,WIS17WGT             * for display
            MOVE      "Inlier Wgt   ",WIS17DSC
            GOTO      BW17N999
          ENDIF
BW17N999  RETURN
+
.******************************************************************************
.*                                  BW17H000              Called by: B17SW000 *
.*                    Calculate Base WIES17 for High Outlier                  *
.******************************************************************************
BW17H000  ASSIGN    ((WIS17LOS-PT17HIBP)-ADJMVDAY),HIGHDAYS  * high outlr days
          COMPARE   ZERO,HIGHDAYS
          IF        @LESS
            MOVE      ZERO,HIGHDAYS         * highdays must be >= zero
          ENDIF
.
          COMPARE   HIGHDAYS,HIHLOS
          IF        @LESS
            MOVE      HIHLOS,HITHDAYS       * get lesser of highdays and
          ELSE                              *  HIHLOS
            MOVE      HIGHDAYS,HITHDAYS
          ENDIF
.
          ASSIGN    (HIGHDAYS-HITHDAYS)*PT17THOM,TOTBWIES    *high WIES
          ASSIGN    (HITHDAYS*PT17THIH)+TOTBWIES,TOTBWIES    *HITH WIES
          ASSIGN    (PT17TIMW+TOTBWIES),TOTBWIES             *base WIES17
          ASSIGN    (TOTBWIES/PT17TIMW),TOTIES17             * IES17
.
          MOVE      PT17THOM,WIS17WGT                        * for display
          MOVE      "High Outl Wgt",WIS17DSC
BW17H999  RETURN
+
.******************************************************************************
.*                                  C17AB000              Called by: PATWIS17 *
.*                          Check for Aboriginality                           *
.******************************************************************************
C17AB000  MOVE      ZERO,ATSIWIES                * set ATSI WIES to zero
          IF        PNSWRACE<1 | PNSWRACE>14
            GOTO      C17AB999              * Invalid parameter
          ENDIF
.
          LOAD      TCODE,PNSWRACE,CATP1,CATP2,CATP3,CATP4,CATP5:
                                   CATP6,CATA1,CATA2,CATA3,CATA4:
                                   CATA5,CATA6,CATA7,CATVA
          LOAD      ACODE,PNSWRACE,PUSR1,PUSR2,PUSR3,PUSR4,PUSR5:
                                   PUSR6,AUSR1,AUSR2,AUSR3,AUSR4:
                                   AUSR5,AUSR6,AUSR7,PMPXABRG
.
          MATCH     ACODE,SP3
          GOTO      C17AB999 IF EQUAL
.
          PACK      KEY5,TCODE,ACODE
          CALL      RDCODE1                 * Read a code file record
          BRANCH    OVRCD,C17AB999
.
          UNPACK    THCSCOD,DIM1,KEY1       * use 2nd char of HDP Equiv
.>>>>     MOVE      THCSCOD,KEY1
          TYPE      KEY1
          GOTO      C17AB999 IF NOT EQUAL   * Non numeric, error
.
          MOVE      KEY1,FORM1
.
. ---- Calculate ATSI Loading if aboriginality = 1, 2, or 3 ----
.
          IF        FORM1=1 | FORM1=2 | FORM1=3
            MOVE      TOTBWIES,FORM12P4       * base WIES
            ADD       MVCOPAY,FORM12P4        * mechv copayment
            ADD       THCOPAY,FORM12P4        * thal  copayment
            ADD       AAACOPAY,FORM12P4       * AAA   copayment
            ADD       ASDCOPAY,FORM12P4       * ASD   copayment
            ASSIGN    (0.3*FORM12P4),ATSIWIES
          ENDIF
.
C17AB999  RETURN
+
