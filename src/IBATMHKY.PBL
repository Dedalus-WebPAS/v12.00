.   System    :   IBA Software                       
.   Include   :   IBATMHKY
.   Routine   :   KIBTHA00 Keyin Keyin               
.   Parameter :   CKEYTYP 0 = Keyin , 1 = Accept, 2 = Either
.   Return    :   EXIT = 1 null entry, 2 EXITCHAR entered
.-------------------------------------------------------------------------------
KIBTHA00 TOPIC     "IBTHSCOD"
          MOVE      UNDLN70,IBTHSCOD
          KEYIN     *PCCOL:CVERT,*DV,IBTHSCOD:                                  
                    *PCCOL:CVERT,*V2LON,IBTHSCOD:                               
                    *PCCOL:CVERT,*DV,IBTHSCOD
.
          ENDSET    IBTHSCOD
          APPEND    SP70,IBTHSCOD
          RESET     IBTHSCOD
.
          MATCH     SP70,IBTHSCOD
          GOTO      KIBTHA90 IF EQUAL
.
          CMATCH    EXITCHAR,IBTHSCOD
          GOTO      KIBTHA95 IF EQUAL
.
          CMATCH    QUEST,IBTHSCOD
          GOTO      KIBTHA10 IF NOT EQUAL
.
          MOVE      CCITEM,SCCITEM
          CALL      HIBTHA00
          MOVE      SCCITEM,CCITEM
.
          ENDSET    IBTHSCOD
          APPEND    SP70,IBTHSCOD
          RESET     IBTHSCOD
.
          MATCH     SP70,IBTHSCOD
          GOTO      KIBTHA00 IF EQUAL
.
          DISPLAY   *PCCOL:CVERT,*V2LON,IBTHSCOD                                
KIBTHA10
          PACK      KEY6,IBTHSCOD                                               
          CALL      RAIBTH1 
          BRANCH    OVRCD,KIBTHA80
          BRANCH    CKEYTYP,KIBTHA75
          CALL      RDIBTH1 
KIBTHA70
          MOVE      ZERO,EXIT
          GOTO      KIBTHA99
KIBTHA75
          MOVE      "Keyin Already Exist - ",DISPMSG                                                 
          CALL      MESSAGE1
          GOTO      KIBTHA00
KIBTHA80
          MOVE      ZERO,EXIT
          BRANCH    CKEYTYP,KIBTHA99,KIBTHA99
          MOVE      "Keyin Does Not Exist - ",DISPMSG                                                
          CALL      MESSAGE1
          GOTO      KIBTHA00
KIBTHA90
          MOVE      ONE,EXIT
          GOTO      KIBTHA99
KIBTHA95
          MOVE      TWO,EXIT
KIBTHA99 TOPIC    PRGID
          RETURN
