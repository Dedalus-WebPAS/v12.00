HIBTHA00
          MOVE      ZERO,CSCRSIZ
          IF        HTOP<3
            MOVE      "10",HTOP
            MOVE      "24",HBOT
            MOVE      "01",HLEF
            MOVE      "80",HRIG
          ENDIF
HIBTHA04
          CALL      GSCRN000
          BRANCH    EXIT,HIBTHA99
          MOVE      STDCOL00,STDCOL01
          ADD       " 5",STDCOL01
          DISPLAY   *PSTDCOL01:STDROW,*V2LON,*ULON,"Code"               
.
          MOVE      STDCOL01,STDCOL02
          ADD       " 7",STDCOL02
          DISPLAY   *PSTDCOL02:STDROW,*V2LON,*ULON,"Description"        
.
          MOVE      STDCOL02,STDCOL03
          ADD       "31",STDCOL03
          DISPLAY   *PSTDCOL03:STDROW,*V2LON,*ULON,"Type"               
.
          MOVE      STDCOL03,STDCOL04
          ADD       " 4",STDCOL04
HIBTHA08
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          MOVE      SP70,IBTHSCOD
          PACK      KEY6,IBTHSCOD,SP70                                          
.                                                                               
.                                                                               
          CALL      RSIBTH1 
HIBTHA10
          CALL      RKIBTH1 
          BRANCH    OVRCD,HIBTHA15
          PACK      KEY6,IBTHSCOD,SP70                                          
.                                                                               
.                                                                               
          BRANCH    CSEARCH,HIBTHA12
          CALL      HIBTHA60
          BRANCH    OVRCD,HIBTHA10
HIBTHA12
          ADD       ONE,XITEMNO
          ADD       ONE,STDROW
          STORE     KEY6,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          DISPLAY   *PSTDCOL00:STDROW,SP1,*V2LON,XITEMNO,DOT
          DISPLAY   *PSTDCOL01:STDROW,IBTHSCOD
          DISPLAY   *PSTDCOL02:STDROW,IBTHDESC
          DISPLAY   *PSTDCOL03:STDROW,IBTHSTYP
          COMPARE   MAXITEM,XITEMNO
          GOTO      HIBTHA10 IF NOT EQUAL
HIBTHA15
.                                                                               
          MOVE      ONE,DITEM
          MOVE      MINITEM,DROW
          ADD       ONE,DROW
          DISPLAY   *PSTDCOL00:DROW,*V2LON,*HON,SP1,DITEM,DOT
          CLIP
          FLUSH     HLEF,HTOP,HRIG,HBOT
.
HIBTHA20
          CALL      HSCRI000
          BRANCH    EXIT,HIBTHA30,HIBTHA35,HIBTHA40,HIBTHA45,HIBTHA04:
                              HIBTHA91,HIBTHA50
          GOTO      HIBTHA92
HIBTHA30
          CALL      RKIBTH1 
          BRANCH    OVRCD,HIBTHA85
          PACK      KEY6,IBTHSCOD,SP70                                          
.                                                                               
.                                                                               
          CALL      RPIBTH1 
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBTHA10
HIBTHA35
          MOVE      ST1,KEY6 
          CALL      RDIBTH1 
          CALL      RPIBTH1 
          BRANCH    OVRCD,HIBTHA85
          PACK      KEY6,IBTHSCOD,SP70                                          
.                                                                               
.                                                                               
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
HIBTHA36
          CALL      RPIBTH1 
          BRANCH    OVRCD,HIBTHA37
          PACK      KEY6,IBTHSCOD,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HIBTHA36 IF NOT EQUAL
HIBTHA37
          MOVE      ZERO,XITEMNO
          GOTO      HIBTHA10
HIBTHA40
          MOVE      ST1,KEY6 
          CALL      RDIBTH1 
          CALL      RPIBTH1 
          BRANCH    OVRCD,HIBTHA85
          PACK      KEY6,IBTHSCOD,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBTHA08
HIBTHA45
          CALL      RKIBTH1 
          BRANCH    OVRCD,HIBTHA85
          PACK      KEY6,IBTHSCOD,SP70                                          
.                                                                               
.                                                                               
          SUSPEND
          CLIP      1,1,CRIG,24
.
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          MOVE      ZERO,XITEMNO
          PACK      KEY6,Z70                                                    
          MOVE      MINITEM,STDROW
          MOVE      MAXITEM,XITEMNO
          CALL      RSIBTH1 
HIBTHA46
          CALL      RPIBTH1 
          BRANCH    OVRCD,HIBTHA47
          PACK      KEY6,IBTHSCOD,SP70                                          
.                                                                               
.                                                                               
          SUB       ONE,XITEMNO
          COMPARE   ZERO,XITEMNO
          GOTO      HIBTHA46 IF NOT EQUAL
HIBTHA47
          MOVE      ZERO,XITEMNO
          GOTO      HIBTHA10
HIBTHA50  COMPARE   ZERO,CSEARCH
          GOTO      HIBTHA55 IF EQUAL
          MOVE      ZERO,CSEARCH
          MOVE      ZERO,XITEMNO
          MOVE      MINITEM,STDROW
          ADD       ONE,STDROW
          UNPACK    SP70,SCHFLD01,SCHFLD02,SCHFLD03,SCHFLD04,SCHFLD05
          UNPACK    SP70,SCHFLD06,SCHFLD07,SCHFLD08,SCHFLD09,SCHFLD10
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          DISPLAY   *PSTDCOL00:STDROW,*V2LON,*HON,"Sch"
          KEYIN     *PSTDCOL01:STDROW,*V2LON,SCHFLD01
          RESET     SCHFLD01
          KEYIN     *PSTDCOL02:STDROW,*V2LON,SCHFLD02
          RESET     SCHFLD02
          KEYIN     *PSTDCOL03:STDROW,*V2LON,SCHFLD03
          RESET     SCHFLD03
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBTHA08
HIBTHA55  MOVE      ONE,CSEARCH
          PACK      KEY8,SP70
          SUSPEND
          CLIP      1,1,CRIG,24
          BOXCLR    CLEF,CTOP,CRIG,CBOT
          GOTO      HIBTHA08
HIBTHA60  MOVE      ZERO,OVRCD
          MATCH     SP70,SCHFLD01
          GOTO      HIBTHA61 IF EQUAL
          MATCH     SCHFLD01,IBTHSCOD
          GOTO      HIBTHA72 IF NOT EQUAL
HIBTHA61
          MATCH     SP70,SCHFLD02
          GOTO      HIBTHA62 IF EQUAL
          MATCH     SCHFLD02,IBTHDESC
          GOTO      HIBTHA72 IF NOT EQUAL
HIBTHA62
          MATCH     SP70,SCHFLD03
          GOTO      HIBTHA63 IF EQUAL
          MATCH     SCHFLD03,IBTHSTYP
          GOTO      HIBTHA72 IF NOT EQUAL
HIBTHA63
HIBTHA64
HIBTHA65
HIBTHA66
HIBTHA67
HIBTHA68
HIBTHA69
HIBTHA70
          GOTO      HIBTHA75
HIBTHA72  MOVE      ONE,OVRCD
HIBTHA75  RETURN
HIBTHA85
          LOAD      KEY6,XITEMNO,ST1,ST2,ST3,ST4,ST5:                           
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY6,IBTHSCOD                                               
          CALL      RDIBTH1 
HIBTHA90
.                                                                               
          BEEP
          GOTO      HIBTHA20
HIBTHA91
          MOVE      SP70,IBTHDESC
          MOVE      ZERO,IBTHWIDT
          MOVE      ZERO,IBTHLENG
          MOVE      SP70,IBTHSTYP
          MOVE      SP70,IBTHSPAR
          MOVE      SP70,KEY6 
          MOVE      SP70,IBTHSCOD
          GOTO      HIBTHA96
HIBTHA92
          MOVE      SP70,KEY6 
          MOVE      SP70,IBTHSCOD
          LOAD      KEY6,CCITEM,ST1,ST2,ST3,ST4,ST5:                            
                                ST6,ST7,ST8,ST9,ST10:
                                ST11,ST12,ST13,ST14,ST15:
                                ST16,ST17
          UNPACK    KEY6,IBTHSCOD                                               
HIBTHA96
          PUTSCR    CSSCRNUM,HLEF,HTOP,HRIG,HBOT
          CALL      FRESCR00
          MOVE      ZERO,HTOP
          MOVE      ZERO,HLEF
          MOVE      ZERO,HBOT
          MOVE      ZERO,HRIG
HIBTHA99
          RETURN
