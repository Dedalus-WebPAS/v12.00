            MOVE      PTFAP046,PTFPPIN9
          ENDIF
.
          MATCH     PTFAP047,Z70
          IF        !@EQUAL
            MOVE      PTFAP047,PTFPOCG9
          ENDIF
.
          MATCH     PTFAP048,Z70
          IF        !@EQUAL
            MOVE      PTFAP048,PTFPFAC0
          ENDIF
.
          MATCH     PTFAP049,Z70
          IF        !@EQUAL
            MOVE      PTFAP049,PTFPDFA0
          ENDIF
.
          MATCH     PTFAP050,Z70
          IF        !@EQUAL
            MOVE      PTFAP050,PTFPPLT0
          ENDIF
.
          MATCH     PTFAP051,Z70
          IF        !@EQUAL
            MOVE      PTFAP051,PTFPPIN0
          ENDIF
.
          MATCH     PTFAP052,Z70
          IF        !@EQUAL
            MOVE      PTFAP052,PTFPOCG0
          ENDIF
.
          RETURN
+
.------------------------------------------------------------
. Table of Hospital ID Maintanence
.------------------------------------------------------------
FAPTB000  UNPACK    DIM127,D1,LINKPRG,DIM127
          UNPACK    DIM127,D1,LINKREP,DIM127
          UNPACK    DIM127,D1,LINKTEM,DIM127
.
          CALL      FPHED000      * Output Table Heading
.
          IF        NORECORD=0
            MOVE      PTCNNRTD,NORECORD      * Set Default No Records to Display
          ENDIF
          MOVE      ZERO,RECNUMB
          MOVE      ZERO,DISNUMB
.
          PACK      KEY6,STARTKEY,SP70
          CALL      RDPTFAP1
          IF        OVRCD=0
            CALL      RPPTFAP1
          ENDIF
FAPTB100  CALL      RKPTFAP1
          BRANCH    OVRCD,FAPTB900
.
          ADD       ONE,RECNUMB
          IF        STARTNUM>=RECNUMB
            GOTO      FAPTB100
          ENDIF
.
          CALL      FPROW000      * Table Details Row
          ADD       ONE,DISNUMB
          COMPARE   NORECORD,DISNUMB
          GOTO      FAPTB100 IF NOT EQUAL
          GOTO      FAPTB999
.
FAPTB900  CALL      NOMORE00      * Display No More Records Message
.
FAPTB999  RETURN
.------------------------------------------------------------
. Action Plan Details Maintanance Details (TABLE HEADING)
.------------------------------------------------------------
FPHED000  WRITE     HTMLFILE;"<tr>":
                               "<td class=HeadingCell width=40%>":
                               "<b>Claim Code</b></td>":
                               "<td class=HeadingCell width=60%>":
                               "<b>Name</b></td>":
                             "</tr>"
.
FPHED999  RETURN
.------------------------------------------------------------
. Action Plan details Maintanance Details (TABLE ROW)
.------------------------------------------------------------
FPROW000  PACK      CLAIMDES,SP70
          PACK      KEY5,ANSC,ANSL,PTFPCLAI,SP70
          CALL      RDCODE1
          IF        OVRCD<>1
            PACK      CLAIMDES,TDESC,SP70
          ENDIF
.
          PACK      TDESC,SP70
          PACK      KEY5,ANSA,ANSK,PTFPACPL,SP70
          CALL      RDCODE1
.
          WRITE     HTMLFILE;"<tr><td nowrap><A HREF='javascript:ShowDetail":
                             "(#"",LINKPRG,"#.pbl":
                             "?reportno=",LINKREP:
                             "&template=",LINKTEM:
                             "&ptfap001=",PTFPCLAI:
                             "&ptfap002=",PTFPACPL,"#")'>":
                             "<img src=../images/MaintenanceIcon.gif hspace=4":
                             " border=0 align=absmiddle></a>":
                             CLAIMDES,"</td>":
                             "<td>",TDESC,"</td>":
                             "</tr>"
.
FPROW999 RETURN
+
.-----------------------------------------------------------
. Link to Next Group
.-----------------------------------------------------------
NEXTFP00  MOVE      NORECORD,KEY2
          REP       " +",KEY2
          ASSIGN    STARTNUM+NORECORD,F3
          MOVE      F3,KEY3
          REP       " +",KEY3
          UNPACK    DIM127,D1,FLDPARA,DIM127
          MOVE      REPORTNO,F2
          REP       " +",PTFAP001
          REP       " +",PTFAP002
.
          WRITE     HTMLFILE;"patweb79.pbl":
                                 "?reportno=",F2:
                                 "&template=",FLDPARA:
                                 "&startnum=",KEY3:
                                 "&norecord=",KEY2:
                                 "&ptfap001=",PTFAP001:
                                 "&ptfap002=",PTFAP002;
.
          REP       "+ ",PTFAP001
          REP       "+ ",PTFAP002
.
NEXTFP99  RETURN
.-------------------------------------------------------------
. Link to Prev Group
.-------------------------------------------------------------
PREVFP00  MOVE      NORECORD,KEY2
          REP       " +",KEY2
          ASSIGN    STARTNUM-NORECORD,F3
          IF        F3<0
            MOVE      ZERO,F3
          ENDIF
          MOVE      F3,KEY3
          REP       " +",KEY3
          UNPACK    DIM127,D1,FLDPARA,DIM127
          MOVE      REPORTNO,F2
          REP       " +",PTFAP001
          REP       " +",PTFAP002
.
          WRITE     HTMLFILE;"patweb79.pbl":
                                 "?reportno=",F2:
                                 "&template=",FLDPARA:
                                 "&startnum=",KEY3:
                                 "&norecord=",KEY2:
                                 "&ptfap001=",PTFAP001:
                                 "&ptfap002=",PTFAP002;
.
          REP       "+ ",PTFAP001
          REP       "+ ",PTFAP002
.
PREVFP99  RETURN
