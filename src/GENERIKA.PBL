.
.----------------------------------------------------------------------
.  data includes
.----------------------------------------------------------------------
.
          INC       STDGENDF
          INC       ALLINCFD/INC
          INC       ALLIKAFD/INC
.
.----------------------------------------------------------------------
.  local variables
.----------------------------------------------------------------------
.
.
PRGID     INIT      "GENERIKA"
PRGNAM    INIT      "Fix allikaaf"
VERSION   INIT      "V12.00.00"
.
.----------------------------------------------------------------------
.  program code
.----------------------------------------------------------------------
.
.---- initialise screen and confirm desire to execute program ----
.
ML0000    CALL      DISPHEAD
.
          DISPLAY   *P1:10,"Generate A/H Intervention Keyword records."
          CALL      CONTQST
          SUB       ONE,CEXIT
          BRANCH    CEXIT,ML9999,ML9999
.
.---- open files ----
.
          OPEN      ALLINCA1,"allincaf"
          OPEN      ALLIKAA1,"allikaaf"
          OPEN      ALLIKAA2,"allikaaf"
.
.---- loop through allincaf ----
.
          MOVE      ZERO,SECTOR
.
          PACK      KEY12,SP70
ML1000    CALL      RSALINC1           * read next record
ML1100    CALL      RKALINC1
          BRANCH    OVRCD,ML9999
.
          ADD       ONE,SECTOR
          IF        SECTOR%100=1
            DISPLAY   *P1:24,*V2LON,SECTOR," - ",ALINCODE,*EF;
          ENDIF
.
          PROC      ALLIKAUP
          GOTO      ML1100
.
.---- exit program ----
.
ML9999    CHAIN     PGM
.----------------------------------------------------------------------
.  code includes
.----------------------------------------------------------------------
.
          INC       ALLINCIO/INC
          INC       ALLIKAIO/INC
          INC       ALLIKAPR
          INC       STDGENCD
.
