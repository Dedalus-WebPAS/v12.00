          IF        !@EQUAL
            PACK      KEY5,CATSD,OTBBSRVD,SP70
            CALL      RDCODE1
            IF        OVRCD<>1
              MOVE      THCSCOD,KEY2
              PACK      XXXXSERD,KEY2,SP70                           * (90)
            ENDIF
          ENDIF
