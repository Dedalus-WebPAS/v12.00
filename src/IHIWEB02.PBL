MAIN0000  COMMIT
.
          BEGIN
.
          DISPLAY   *W2
.
          MATCH     "3",PTCNUNET
          GOTO      MAIN0000 IF NOT EQUAL
.
          MOVE      ZERO,EXIT
.
          PACK      KEY29,SP70
          CALL      RSPMNPL5
MAIN1000  CALL      RKPMNPL5
          BRANCH    OVRCD,MAIN9999
.
          MATCH     "0",PMNPRSTA                * Processed Record?
          GOTO      MAIN9999 IF NOT EQUAL
.
          MATCH     "10",PMNPRTYP
          GOTO      MAIN1000 IF EQUAL
.
          MOVE      SP70,ERRORIND
          PACK      ERRORDSC,SP70,SP70,SP70,SP70,SP70,SP70,SP70,SP70
.
          PACK      KEY53,PMNPHOSP,SP70          * Read B2B Device Table
          CALL      RSPMNDB2
          CALL      RKPMNDB2
          BRANCH    OVRCD,MAIN1000
.
          IF        IBCNMHOS=ONE
            MATCH     PMNPHOSP,PMNBHOSP            * Same Hospital
            GOTO      MAIN1000 IF NOT EQUAL
          ENDIF
.
          MATCH     SP70,PMNBDPTH
          GOTO      MAIN1000 IF EQUAL
.
          CALL      PROPN000                     * Valid Properties File?
.
          MATCH     "1",NHCIHIND
          GOTO      MAIN1000 IF EQUAL
.
          PACK      SAVKEY29,PMNPRSTA,PMNPRDAT,PMNPRTIM,PMNPURNO,PMNPCNTR,SP70
.
          OPEN      NHCPROPF,NIHCPATH
.
. ******************************************************************
. Call webPAS_NIHCIHI interface program to create a new Transaction Id
. Write new Transaction Id back to pmsnplaf
. ******************************************************************
.
MAIN1100  CALL      TFILENAM                     * Temp filename for Tran Id
          MOVE      TFILNAME,TRIDFILE
.
MAIN1140  CLEAR     CMDLINE
          APPEND    PMNBDPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_NIHCIHI_run GetTransactionId ",CMDLINE
          APPEND    PMNBDPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/ ",CMDLINE
          APPEND    TRIDFILE,CMDLINE
          RESET     CMDLINE
.
          EXECUTE   CMDLINE,TASKID              * Create Tran Id file
.
          CLEAR     TRIDPATH
          APPEND    PMNBDPTH,TRIDPATH
          RESET     TRIDPATH
          STRIP     TRIDPATH
          ENDSET    TRIDPATH
          APPEND    SLASH,TRIDPATH
          APPEND    "webPAS_messtranids/",TRIDPATH
          APPEND    TRIDFILE,TRIDPATH
          RESET     TRIDPATH
.
          MOVE      ZERO,OVRCD
          TRAP      OVERCOND IF IO
          OPEN      NHCTRIDF,TRIDPATH
          TRAPCLR   IO
          IF        OVRCD=1
            GOTO      MAIN1150
          ENDIF
.
          READ      NHCTRIDF,SEQ;TEMPTRID       * Read new Tran Id temp file
.
. Check if Tran Id already exists in pmsnplaf
.
          PACK      KEY52,TEMPTRID,SP70
          CALL      RSPMNPL2           
          CALL      RKPMNPL2
          IF        OVRCD=0
            MATCH     TEMPTRID,PMNPUNIQ
            IF        @EQUAL
              CLOSE     NHCTRIDF
              GOTO      MAIN1140
            ENDIF
          ENDIF
.
          MOVE      SAVKEY29,KEY29
          CALL      RDPMNPL5 
.
MAIN1150  CLOSE     NHCTRIDF
.
          CLEAR     CMDLINE
          APPEND    "rm -f ",CMDLINE
          APPEND    PMNBDPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_messtranids/",CMDLINE
          APPEND    TRIDFILE,CMDLINE
          RESET     CMDLINE
.
          EXECUTE   CMDLINE,TASKID              * Remove Tran Id file
.
          MOVE      TEMPTRID,PMNPUNIQ
          PACK      PMNPRCOD,TEMPTRID,SP70
.
          CALL      UPPMNPL5
.
. ******************************************************************
. Validate and Extract NIHCIHI files 
. [TransactionId].properties 
. [TransactionId]_nihcihi
. ******************************************************************
.
          CALL      NIHC0000
.
. ******************************************************************
.
          PACK      KEY29,PMNPRSTA,PMNPRDAT,PMNPRTIM,PMNPURNO,PMNPCNTR,SP7
          CALL      RDPMNPL5
.
          MATCH     "1",ERRORIND
          IF        @EQUAL
            MOVE      "3",PMNPRSTA
            MOVE      ERRORDSC,PMNPEROR
          ELSE
            MOVE      "1",PMNPRSTA
          ENDIF
.
          CALL      UPPMNPL5
.
MAIN9000  CLOSE     NHCPROPF
.
          GOTO      MAIN1000
.
MAIN9999  CLOSE     NHCPROPF
.
          GOTO      MAIN0000
.
