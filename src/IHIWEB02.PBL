          CLOSE     PROPFILE
.
.         Move .properties to NIHC Device nihcihi claims directory 
.
          CLEAR     CMDLINE
          APPEND    "chmod 777 ",CMDLINE
          APPEND    PROPFLNM,CMDLINE
          RESET     CMDLINE
.
          EXECUTE   CMDLINE,TASKID
.
          MATCH     "0       ",TASKID
          IF        !@EQUAL
            MOVE      "Unable to chmod 777 .properties file.",ERRORDSC
            GOTO      EXTR9100
          ENDIF 
.
          CLEAR     CMDLINE
          APPEND    "mv ",CMDLINE
          APPEND    PROPFLNM,CMDLINE
          APPEND    SP1,CMDLINE
          APPEND    PMNBDPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/webPAS_claims/nihcihi/.",CMDLINE 
          RESET     CMDLINE
.
          EXECUTE   CMDLINE,TASKID
.
          MATCH     "0       ",TASKID
          IF        !@EQUAL
            MOVE      "Unable to move .properties file to claims dir.",ERRORDSC
            GOTO      EXTR9100
