.*****************************************************************************
.*                               PROC0000                                    *
.*        Load the NIHCIHI variables with the relevant PMI data              *
.* Returns: EXIT   0 = Ok to continue                                        *
.*                 1 = error(s) encountered                                  *
.*****************************************************************************
.
. Collect date for .properties file
. ***************************************        
.
.         Load webpas.nihcihiTransactionId=
.
PROC0000  MATCH     SP70,PMNPUNIQ
          IF        @EQUAL 
            MOVE      "NHC.06-pmsnplaf transactionid is blank",ERRORDSC
            GOTO      PROC9100
          ELSE
            MOVE      PMNPUNIQ,NPRVAL01
          ENDIF
.
.         Collect nihc ihi userid
.
          MATCH     SP70,PMNPCUID
          IF        @EQUAL
            MOVE      "NHC.07-pmsnplaf created by userid is blank",ERRORDSC
            GOTO      PROC9100
          ENDIF
.
          PACK      KEY10,PMNPCUID,SP70
          CALL      RDWBSE1
          IF        OVRCD=1
            MOVE      "NHC.08-websecaf record not found",ERRORDSC
            GOTO      PROC9100
          ENDIF        
.
.         Collect nihc ihi role
.
          MATCH     SP70,WBSEOCG
          IF        @EQUAL | @EOS
            MOVE      "NHC.09-websecaf occupational group is blank",ERRORDSC
            GOTO      PROC9100
          ENDIF 
.
          PACK      KEY5,CATW0,WBSEOCG,SP70
          CALL      RDCODE1
          IF        OVRCD=1
            MOVE      "NHC.10-occupational group patcodes record not found",ERRORDSC
            GOTO      PROC9100
          ELSE
.
            MATCH     "1",TCINDC4
            IF        @EQUAL
              MOVE      "IHIAdmin",IHIVAL18
              GOTO      PROC1000
            ENDIF
.
            MATCH     "2",TCINDC4
            IF        @EQUAL
              MOVE      "MyHRAdmin",IHIVAL18
              GOTO      PROC1000
            ENDIF
.
            MATCH     "3",TCINDC4
            IF        @EQUAL
              MOVE      "DualIHIMyHRAdmin",IHIVAL18
              GOTO      PROC1000
            ENDIF
.
            MOVE      "NHC.11-nihcihi role is invalid",ERRORDSC
            GOTO      PROC9100
.
          ENDIF
.
PROC1000          
.
. Collect Data for _nihcihi file
. ***************************************
.
.         Load Role
.
          MOVE      "AuthenticatedUser",IHIVAL02
          PACK      IHIVAL02,IHIVAL02,SP70

.
.         Load Name
.
          MOVE      "AuthUserName",IHIVAL03
.
          MATCH     SP70,WBSENAM
          IF        !@EQUAL & !@EOS
            MOVE      WBSENAM,IHIVAL03
          ENDIF
.
          PACK      IHIVAL03,IHIVAL03,SP70
.
.         Load Login String (userid validated above)
.
          MATCH     SP70,PMNPHOSP
          IF        @EQUAL | @EOS
            MOVE      "NHC.12-pmsnplaf hospital is blank",ERRORDSC
            GOTO      PROC9100
          ENDIF
.
          CLEAR     IHIVAL04
          APPEND    "webPAS.",IHIVAL04
          APPEND    PMNPHOSP,IHIVAL04
          RESET     IHIVAL04
          STRIP     IHIVAL04
          ENDSET    IHIVAL04
          APPEND    ".",IHIVAL04
          APPEND    PMNPCUID,IHIVAL04
          RESET     IHIVAL04
.
.         Load HospitalCodeSystem
.
          MOVE      "pasFacCd",IHIVAL06
          PACK      IHIVAL06,IHIVAL06,SP70
.
.         Load DateOfBirth / dateOfBirth
.
          MATCH     SP70,PBDATE
          IF        @EQUAL
            MOVE      "NHC.01-DateOfBirth is blank",ERRORDSC
            GOTO      PROC9100
          ELSE
            UNPACK    PBDATE,CCENT,CYEAR,CMON,CDAY
            PACK      IHIVAL07,CDAY,DASH,CMON,DASH,CCENT,CYEAR
            PACK      IHIVAL17,CDAY,DASH,CMON,DASH,CCENT,CYEAR
          ENDIF
.
.         Load HospitalCode (validated above)
.
          MOVE      PMNPHOSP,IHIVAL08

.         Load FamilyName
.
          PACK      IHIVAL09,PTMASNAM,SP70
          MATCH     SP70,IHIVAL09
          IF        @EQUAL
            MOVE      "NHC.02-FamilyName is blank",ERRORDSC
            GOTO      PROC9100
          ENDIF
.
.         Load FirstName
.
          PACK      IHIVAL10,PMPXFNAM,SP70
          MATCH     SP70,IHIVAL10
          IF        @EQUAL
            MOVE      "ONLYNAME",IHIVAL10
            PACK      IHIVAL10,IHIVAL10,SP70
          ENDIF
.
.         Load Sex
.
          MOVE      "NotStatedOrInadequatelyDescribed",IHIVAL11
          MATCH     SP70,PSEX
          IF        !@EQUAL & !@EOS
.
            PACK      KEY5,CODEG,PSEX,SP70
            CALL      RDCODE1
            IF        OVRCD=0
.
              MATCH     "1",TCINDC13
              IF        @EQUAL
                MOVE      "Male",IHIVAL11
                GOTO      PROC2000
              ENDIF
.
              MATCH     "2",TCINDC13
              IF        @EQUAL
                MOVE      "Female",IHIVAL11
                GOTO      PROC2000
              ENDIF
.
              MATCH     "3",TCINDC13
              IF        @EQUAL
                MOVE      "IntersexOrIndeterminate",IHIVAL11
                GOTO      PROC2000
              ENDIF
.
              MATCH     "4",TCINDC13
              IF        @EQUAL
                MOVE      "NotStatedOrInadequatelyDescribed",IHIVAL11
                GOTO      PROC2000
              ENDIF
.
            ENDIF
.
          ENDIF         
.
PROC2000  PACK      IHIVAL14,PURNO,SP70
          MATCH     SP70,IHIVAL14
          IF        @EQUAL | @EOS
            MOVE      "NHC.13-PASID is blank",ERRORDSC
            GOTO      PROC9100   
          ENDIF
.
.
.         Load MedicareCardNum & MedicareIRN
.
          MOVE      SP70,IHIVAL12
          MOVE      SP70,IHIVAL13
.
          MATCH     SP70,PMEDI
          IF        !@EQUAL
            MOVE      PMEDI,IHIVAL12
            MATCH     SP70,PTMXMCCD
            IF        @EQUAL
              MOVE      "NHC.04-MedicareIRN is blank",ERRORDSC
              GOTO      PROC9100
            ELSE
              MOVE      PTMXMCCD,DIM2
              SQUEEZE   DIM2                     * remove blanks
              MATCH     "0",DIM2                 * leading zero ?
              IF        @EQUAL
                UNPACK    DIM2,ANS,IHIVAL13      * yes - take second char.
              ELSE
                MOVE      DIM2,IHIVAL13
              ENDIF
            ENDIF
          ELSE
.           MOVE      "NHC.05-MedicareNumber is blank",ERRORDSC
.           GOTO      PROC9100
          ENDIF
.
.         Load DVA Number
.
          PACK      IHIVAL16,PMCDCNUM,SP70
.
.         Load Ihi Number
.
          CALL      CIHI0000                   
.
.         Load Address Variables
.
          PACK      IHIVAL22,PADD1,SP70          
          PACK      IHIVAL23,PADD2,SP70
          PACK      IHIVAL24,PSUBRB,SP70
          PACK      IHIVAL25,PADD4,SP70
          PACK      IHIVAL28,PPOST,SP70
          MOVE      "H",IHIVAL27
          PACK      IHIVAL27,IHIVAL27,SP70
.
          MOVE      ZERO,EXIT
          GOTO      PROC9999
.
PROC9100  MOVE      "1",ERRORIND
          MOVE      ONE,EXIT
.
