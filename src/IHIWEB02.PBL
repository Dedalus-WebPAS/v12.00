          APPEND    "/webPAS_claims/nihcihi/.",CMDLINE
          RESET     CMDLINE
.
          EXECUTE   CMDLINE,TASKID
.
          MATCH     "0       ",TASKID
          IF        !@EQUAL
            MOVE      "Unable to move _nihcihi file to claims dir.",ERRORDSC
            GOTO      EXTR9100
          ENDIF
.
          MOVE      ZERO,EXIT
          GOTO      EXTR9999
.
EXTR9100  MOVE      "1",ERRORIND
          MOVE      ONE,EXIT
.
EXTR9999  RETURN
+
.*****************************************************************************
.*                               POST0000                                    *
.*                   Post the NIHCIHI JSON to NIHC Server                    *
.*****************************************************************************
POST0000  CLEAR     CMDLINE
          APPEND    PMNBDPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
.
          MATCH     "08",PMNPRTYP
          GOTO      POST0500 IF EQUAL
          MATCH     "09",PMNPRTYP
          GOTO      POST0500 IF EQUAL
.
          APPEND    "/webPAS_NIHCIHI_run PostNIHCIHI ",CMDLINE
          GOTO      POST1000
.
POST0500  APPEND    "/webPAS_NIHCIHI_run PostClearNIHCIHI ",CMDLINE
          GOTO      POST1000
.
POST1000  APPEND    PMNBDPTH,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    "/ ",CMDLINE
          APPEND    PMNPUNIQ,CMDLINE
          APPEND    SP1,CMDLINE
          APPEND    PMNPCUID,CMDLINE
          RESET     CMDLINE
          STRIP     CMDLINE
          ENDSET    CMDLINE
          APPEND    SP1,CMDLINE
          APPEND    IHIVAL18,CMDLINE
          RESET     CMDLINE
.
          EXECUTE   CMDLINE,TASKID
.
          MATCH     "0       ",TASKID
          IF        !@EQUAL
.
            MATCH     "08",PMNPRTYP
            IF        @EQUAL
              MOVE      "Unable to execute ./webPAS_NIHCIHI_run PostClearNIHCIHI.",ERRORDSC
              GOTO      POST9100
            ENDIF
.
            MATCH     "09",PMNPRTYP
            IF        @EQUAL
              MOVE      "Unable to execute ./webPAS_NIHCIHI_run PostClearNIHCIHI.",ERRORDSC
              GOTO      POST9100
            ENDIF
.
            MOVE      "Unable to execute ./webPAS_NIHCIHI_run PostNIHCIHI.",ERRORDSC
            GOTO      POST9100
          ENDIF
.
          MOVE      ZERO,EXIT
          GOTO      POST9999
.
POST9100  MOVE      "1",ERRORIND
          MOVE      ONE,EXIT
.
POST9999  RETURN
+
.----------------------------------------------------------------------
. Find the First IHI Number 
.----------------------------------------------------------------------
CIHI0000  MOVE      SP70,IHIVAL15
.
          PACK      KEY5,ANSA,ANSI,SP70
          CALL      RDSCODE1
CIHI1000  CALL      RDKCODE1
          BRANCH    OVRCD,CIHI9999
.
          MATCH     "AI",TCODE              * Cat AI - Alternate id
          GOTO      CIHI9999 IF NOT EQUAL
.
          MATCH     ANSI,PTCOACTV           * Skip inactive codes
          GOTO      CIHI1000 IF EQUAL
.
          MATCH     ANSI,TCINDC2            * IHI Code
          GOTO      CIHI1000 IF NOT EQUAL
.
          MOVE      ACODE,IHICATAI          * Save cat AI code for an IHI number
.
. Check if U/R already has an active IHI
.
CIHI2000  PACK      KEY31,PURNO,IHICATAI,SP70
          CALL      RSPMAID1
CIHI2100  CALL      RKPMAID1
          BRANCH    OVRCD,CIHI9999
.
          MATCH     PURNO,PMAIURNO          * Same U/R
          GOTO      CIHI9999 IF NOT EQUAL
.
          MATCH     IHICATAI,PMAITYPE       * Alternate Id
          GOTO      CIHI9999 IF NOT EQUAL
.
          MATCH     "1",PMAIASTA            * IHI Activation Status
          GOTO      CIHI2100 IF NOT EQUAL
.
          LJUSTIFY  PMAIALID
          MOVE      PMAIALID,IHIVAL15 
.
CIHI9999  RETURN
+
.------------------------------------------------------------------------------
. Get Concession Card (DVA Card)
.------------------------------------------------------------------------------
GCCAIH00  MOVE      "ct",DIM2
          PACK      KEY19,PURNO,SP70
          CALL      RSPMCCD1
GCCAIH20  CALL      RKPMCCD1
          BRANCH    OVRCD,GCCAIH80
.
          MATCH     PURNO,PMCDURNO                 * same U/R still ?
          GOTO      GCCAIH80 IF NOT EQUAL          * no - nothing found
.
.         Check if the card type is DVA or Pension (Cat ct, Indicator 1)
.
          PACK      KEY5,DIM2,PMCDCTYP
          CALL      RDCODE1                        * code on file ?
          BRANCH    OVRCD,GCCAIH20                 * no - ignore record
.
          MATCH     DIM1,TCINDC1                   * same concession card type?
          GOTO      GCCAIH20 IF NOT EQUAL          * no - ignore record
.
.         A concession card of the same type has been found, so check
.         if there is an expiry date
.
          MATCH     SP8,PMCDEXDT                 * expiry date blank ?
          GOTO      GCCAIH99 IF EQUAL            * yes - assume valid
.
          CALL      IBACLOCK
          PACK      DIM8,CCC,CYY,CMM,CDD
          REP       " 0",DIM8
.
.         There is an expiry date for the card, so check that the
.         card is current
.
          MATCH     PMCDEXDT,DIM8                * card valid for date ?
          GOTO      GCCAIH99 IF LESS             * no - nothing found
          GOTO      GCCAIH99 IF EQUAL
.
          GOTO      GCCAIH20
.
GCCAIH80  MOVE      SP70,PMCDCNUM
          MOVE      SP70,PMCDCTYP
          MOVE      SP70,PMCDEXDT
          MOVE      SP70,PMCDDVAC
          GOTO      GCCAIH99
.
GCCAIH99  RETURN
