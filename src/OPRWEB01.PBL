          APPEND    LINKTEM,HTMLLINK
          APPEND    "&sesskeys=",HTMLLINK
          APPEND    OPSEHOSP,HTMLLINK
          APPEND    OPSEDATE,HTMLLINK
          APPEND    OPSETIME,HTMLLINK
          APPEND    OPSECLIN,HTMLLINK
