          MOVE      CTIMEIS,BKRQUTIM
.
          CALL      UPBKRQ11
.
          CALL      CLBOKRAU
          MOVE      SIX,BKRUAUTY
          CALL      MVBOKRAU
.
          PACK      KEY27,BKRUAURQ,BKRUADAT,BKRUATIM,BKRUAUTY,SP70
          CALL      RABKRAU1
          IF        OVRCD=1
            CALL      WRBKRAU1
          ENDIF
.
          MOVE      ZERO,EXIT
          GOTO      THRQ9999
