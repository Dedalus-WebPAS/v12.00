.
          MATCH     OPDAUNIQ,WTENSADI
          GOTO      UPESIS90 IF NOT EQUAL
.
          MATCH     ANSS,WTENETYP
          GOTO      UPESIS90 IF NOT EQUAL
.
          MATCH     SP70,WTENEDAT
          GOTO      UPESIS10 IF NOT EQUAL
.
          PACK      SAVEVDT,WTENEVDT
          CALL      IBACLOCK
          PACK      WTENEVDT,CCC,CYY,CMM,CDD
          REP       " 0",WTENEVDT
          MOVE      PATADMDT,WTENEVAL
