          PACK      KEY5,SP70
          READ      SIPF1FL1,KEY5;;
          READKS    SIPF1FL1;SCPSROW,SCPSCOL,SIPF1MKY
          GOTO      OVERCOND IF OVER
          MATCH     SP70,SIPF1MKY
          GOTO      OVERCOND IF EQUAL
