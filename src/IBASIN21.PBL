          MOVE      ZERO,OVRCD
          PACK      KEY5,SCPSROW,SCPSCOL
          READ      SIQC3FL1,KEY5;SCPSROW,SCPSCOL,SIQC3MKY
          GOTO      OVERCOND IF OVER
          RETURN
RFTSIQC3
          MOVE      ZERO,OVRCD
          PACK      KEY5,SP70
          READ      SIQC3FL1,KEY5;;
          READKS    SIQC3FL1;SCPSROW,SCPSCOL,SIQC3MKY
          GOTO      OVERCOND IF OVER
          MATCH     SP70,SIQC3MKY
          GOTO      OVERCOND IF EQUAL
          RETURN
