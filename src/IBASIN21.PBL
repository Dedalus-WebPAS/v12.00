          MOVE      SIPCCST,KEY5
          CALL      RDSICA1
.
          MATCH     SP70,SIPCWAR
          IF        @EQUAL
            MOVE      "N/A",SIPCWAR
            MOVE      SP70,SIWADES
          ELSE
            MOVE      SIPCWAR,KEY5
            CALL      RDSIWA1
          ENDIF
.
          MOVE      SIPCPRD,KEY5
          CALL      RDSISA1
.
REDITB99  RETURN
.
