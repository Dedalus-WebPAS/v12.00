.------------------------------------------------------------
. Moves cgi variables into file variables
.------------------------------------------------------------
ICDSAV00  MOVE      PTICO001,OPCODE
          RESET     OPCODE           
.
          MOVE      PTICO002,ODESC
          MOVE      PTICO002,PT0ODESC
          MOVE      PTICO002,PT0ODSC2
          MATCH     "0",PTICO003         
          IF        @EQUAL
            MOVE      "P",OFLAG
          ELSE
            MOVE      "H",OFLAG
          ENDIF
          MOVE      PTICO004,PT0OBLKN
.
          MATCH     "0",PTICO005
          IF        @EQUAL
            MOVE      SP70,OAGEGP
          ELSE
            MOVE      PTICO005,OAGEGP
          ENDIF
.
          MOVE      PTICO006,OLOW
          MOVE      PTICO006,PTXOALL1
          MOVE      PTICO007,OHIGH
          MOVE      PTICO007,PTXOAHL1
.
