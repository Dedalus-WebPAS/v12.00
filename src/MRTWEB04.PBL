          IF        !@EQUAL
            MOVE       MRTDS005,MRDSLTST
          ENDIF
          MATCH     Z70,MRTDS006
          IF        !@EQUAL
            MOVE       MRTDS006,MRDSCOM1
          ENDIF
          MATCH     Z70,MRTDS007
          IF        !@EQUAL
            MOVE       MRTDS007,MRDSCOM2
          ENDIF
          MATCH     Z70,MRTDS008
          IF        !@EQUAL
            MOVE       MRTDS008,MRDSCOM3
          ENDIF
          MATCH     Z70,MRTDS009
          IF        !@EQUAL
            MOVE       MRTDS009,MRDSCOM4
          ENDIF
          MATCH     Z70,MRTDS010
          IF        !@EQUAL
            MOVE       MRTDS010,MRDSCOM5
