          IF        !@EQUAL
            MOVE       MRTDS002,DMRDSCNT            
            SQUEEZE    MRTDS002
            MOVE       MRTDS002,MRDSCNT
          ENDIF
          MATCH     Z70,MRTDS003
          IF        !@EQUAL
            MOVE       MRTDS003,MRDSDTRQ
