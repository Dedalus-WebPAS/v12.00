.********************************************************************
. WRIA - print data line                            Called by PRNA
.********************************************************************
WRIA0000
          COMPARE   "55",CLNO                    * end of page ?
          CALL      HEAA0000 IF NOT LESS         * start new page
.
          MOVE      ZERO,FORM12A
          MOVE      ZERO,FORM12B
          MOVE      ZERO,FORM12C
          MOVE      ZERO,FORM12D
          MOVE      ZERO,FORM12E
          MOVE      SP70,SKEY1
          MOVE      SP70,SKEY35
.
          PACK      KEY12,SIIACAT,WARECODE,SP70  * check if at warehouse
          CALL      RDSIIE1
          BRANCH    OVRCD,WRIA9999
.
          MOVE      SIIEROL,FORM12D
          MOVE      SIIEROQ,FORM12E
.
.---- supression flag set ? ----
.
          IF        SIIESIC=1
            MOVE      "Supression Flag Set",SKEY35
            GOTO      WRIA9000
          ENDIF
.
.---- check how old item is ----
.
          UNPACK    SP70,SIITWAR,SIITCAT         * get first item history record
          PACK      KEY28,WARECODE,SIIACAT,SP70
          CALL      RSSIIT1
          CALL      RKSIIT1
          PACK      KEY12,SIITWAR,SIITCAT,SP70
          MATCH     KEY12,KEY28
          IF        !@EQUAL
            MOVE      "999999",SIITPER
          ENDIF
.
          MATCH     SKEY6,SIITPER
          IF        !@LESS
            MOVE      "New Item - Insufficient History",SKEY35
            GOTO      WRIA9000
          ENDIF
.
.---- calculate usage ----
.
          PACK      SKEY18,SIIACAT,WARECODE,DAT6,SP70
          PACK      KEY18,SIIACAT,WARECODE,SKEY6,SP70
          CALL      RSSIIH1
          CALL      RPSIIH1
.
WRIA1000
          CALL      RKSIIH1
          BRANCH    OVRCD,WRIA2000
          PACK      KEY18,SIIHCAT,SIIHWAR,SIIHDAT,SP70
          MATCH     KEY18,SKEY18
          GOTO      WRIA2000 IF LESS
.
          UNPACK    SIIHDAT,KEY4,KEY2  * ignore discrepancy
          MATCH     "9999",KEY4
          GOTO      WRIA1000 IF EQUAL
.
          ADD       SIIHUQT,FORM12A
          IF        SIIHUQT < 0
            MOVE      "*",SKEY1
          ENDIF
          GOTO      WRIA1000
.
.---- no positive usage ? ----
.
WRIA2000
          IF        FORM12A < 0
            MOVE      "Negative Usage - ICL Not Calculated",SKEY35
            GOTO      WRIA9000
          ENDIF
.
          IF        FORM12A = 0
            MOVE      "Slow Moving Item",SKEY35
            GOTO      WRIA9000
          ENDIF
.
.---- calculate average daily usage ----
.
          ASSIGN    FORM12A/(HADUPERD*30),F12P5
.
.---- calculate Re-Order Level (ROL) ----
.
          UNPACK    SP70,SIICCAT,SIICSUP
          PACK      KEY27,SIIACAT,SIIAPSC,SP70
          CALL      RSSIIC1
          CALL      RKSIIC1
          PACK      KEY12,SIICCAT,SIICSUP,SP70
          MATCH     KEY12,KEY27
          IF        !@EQUAL
            MOVE      "Preferred Supplier Not Set Up",SKEY35
            GOTO      WRIA9000
          ENDIF
.
          IF        SUPORISS=1
            ASSIGN    F12P5*((HCONPERD*0.5)+SIICLTM)/SIFACTOR,F12X4
          ELSE
            ASSIGN    F12P5*((HCONPERD*0.5)+SIICLTM),F12X4
          ENDIF
          MOVE      F12X4,FORM12B
.
.---- calculate Re-Order Quantity (ROQ) ----
.
          MOVE      ONE,SIFACTOR
          PACK      KEY30,SIIAISS,SIICSUT,SP70
          CALL      RDSIFA1
          IF        SUPORISS=1
            ASSIGN    F12P5*(HCONPERD+SIICLTM)/SIFACTOR,F12X4
          ELSE
            ASSIGN    F12P5*(HCONPERD+SIICLTM),F12X4
          ENDIF
          MOVE      F12X4,FORM12C
.
.---- print data ----
.
WRIA9000
          MATCH     SP70,SKEY35
          IF        @EQUAL & (REPSEQ=0)
            GOTO      WRIA9999
          ENDIF
.
          MATCH     SP70,SKEY35
          IF        !@EQUAL & (REPSEQ=1)
            GOTO      WRIA9999
          ENDIF
.
          IF        (REPSEQ=1) & ( (FORM12B<>SIIEROL) | (FORM12C<>SIIEROQ) )
            PACK      KEY12,SIIACAT,WARECODE,SP70
            REPEAT
              CALL      RLSIIE1
              BRANCH    OVRCD,WRIA9999
              IF        OVRCD=2
                DISPLAY   *P1:24,*EL,*V2LON,SIIACAT,SLASH,WARECODE:
                          *HOFF," Locked",*W2;
              ENDIF
            UNTIL     OVRCD=0
            MOVE      "2",AUDTTYPE
            CALL      WASIIE00
            MOVE      FORM12B,SIIEROL
            MOVE      FORM12C,SIIEROQ
            CALL      UPSIIE1
            CALL      UUSIIE1
            MOVE      "3",AUDTTYPE
            CALL      WASIIE00
          ENDIF
.
          PACK      KEY60,SIIADES,SP70
          CALL      BKLIN000
          PACK      KEY55,KEY60,SP70
          PRINT     SKEY1,SP1,SIIACAT,SP2,KEY30,SP10,FORM12A:
                    SP3,FORM12D,SP3,FORM12E,SP4:
                    *+,SKEY35,*N:
                    SP2,SP7,SP2,KEY55,FORM12B,SP3,FORM12C
          ADD       "2",CLNO           * add 2 to line number
.
WRIA9999  RETURN
.********************************************************************
. BKLIN - Break up KEY60 into two neat halves        Called by lots
.         Requires - KEY60   (original line)
.         Returns  - KEY30   (line 1)
.                    KEY60   (line 2)
.********************************************************************
BKLIN000  RESET     KEY60,31           * reset to length(line1)+1
          CMATCH    SP70,KEY60
          WHILE     !@EQUAL&!@EOS
            BUMP      KEY60,-1
            IF        !@EOS
              CMATCH    SP70,KEY60
            ENDIF
          DO
.
          IF        @EOS
            SETLPTR   KEY60
            RESET     KEY60
            UNPACK    KEY60,KEY30,KEY60
          ELSE
            LENSET    KEY60
            RESET     KEY60
            MOVE      KEY60,KEY30
            PACK      KEY30,KEY30,SP70
.
            ENDSET    KEY60
            SETLPTR   KEY60
            BUMP      KEY60
            PACK      KEY60,KEY60,SP70
          ENDIF
.
BKLIN999  RETURN
KCONP000  IF        ACCEPT=1
             READ      CONTROLF,TWENTY4;*155,HCONPERD
          ENDIF
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,HCONPERD
          RETURN
HUSEP000  IF        ACCEPT=1
             READ      CONTROLF,TWENTY4;*158,HADUPERD
          ENDIF
          KEYIN     *PCCOL:CVERT,*V2LON,*RV,HADUPERD
          RETURN
