KREPS000
          PACK      DISPMSG,DISPMSG,SP70
          CALL      MESSAGE2
          DISPLAY   *P1:24,*EF,*V2LON:
                    "0",*HOFF," = ICLs Suppression Report, ",*V2LON:
                    "1",*HOFF," = Reset ICL Report, ":
                    "2",*HOFF," = Full Report (NO RESET)";
.
