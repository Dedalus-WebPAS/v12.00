#!/bin/bash
echo "Content-Type: text/html"
echo "Cache-Control: no-cache"
echo ""
echo ""
echo "<body style='background-color:white;'>"
for VAR in `echo $QUERY_STRING | tr "&" "\t"`
  do 
  NAME=$(echo $VAR | tr = " " | awk '{print $1}';);
  VALUE=$(echo $VAR | tr = " " | awk '{ print $2}' | tr + " ");
  echo $NAME " = " $VALUE  "<br>"
done
echo "REMOTE_ADDR = " $REMOTE_ADDR "<br>"
echo "REMOTE_USER = " $REMOTE_USER "<br>"
echo "HTTP_USER_AGENT = " $HTTP_USER_AGENT "<br>"
case $HTTP_USER_AGENT in
 *MSIE*)   echo "Internet Explorer"
           ;;
 *iPhone*) echo "iPhone"
           ;;
 *Safari*) echo "Safari"
           ;;
esac
echo "<pre>"
/usr/bin/env | sort
echo "</pre>"
echo "<a href=login.html>Link</a>"
echo "<a href=LoginPage.html>Link</a>"


