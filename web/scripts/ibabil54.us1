#!/bin/sh
#
FileName=`echo $1|sed 's/\.txt//'`
DateName=`date +%y%m%d-%H%M`
ExtractName="$FileName-$DateName.txt"
mv $1 $approot/extracts/$ExtractName
ExtractIndex="$approot/extracts/SAHealthDept.html"
echo "<br><font size=3>SA Health Department Extract Generated " >>$ExtractIndex
date                                                         >>$ExtractIndex
echo "<a href=$ExtractName>$ExtractName</a></font><br>"      >>$ExtractIndex

