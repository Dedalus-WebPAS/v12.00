#!/bin/sh

        DateName=`date +%y%m%d-%H%M`
        FileName="$1"

        cat $FileName | sed 's/^M//' > $FileName.unix
        mv $FileName.unix $FileName

ex -e - $FileName <<EOF
         /commence data input on next row/
         :1,.d
         :%s/^	//
         :%s/	/|/g
         :%s/$/||||/g
         :%s/[$]//g
         :%s/,//g
         :%s/'//g
         :%s/"//g
         :wq!
EOF



